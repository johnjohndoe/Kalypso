<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Workflow URI="http___www.tu-harburg.de_wb_kalypso_statistics__WF_KalypsoStatistics"
	xmlns="http://www.renew.de/workflow/base"
	xmlns:con="http://www.renew.de/workflow/contexts"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	defaultTask="org.kalypso.statistics.model.handlers.TestHandler">
	<Help>%KalypsoStatistics_help</Help>
	<Name>%KalypsoStatistics</Name>
	
	<!-- TASK DEFINITIONS -->
	<TaskGroup context="MainMap"
		URI="http___www.tu-harburg.de_wb_kalypso_statistics__Preprocessing">
		<Help>%KalypsoStatistics_Preprocessing_help</Help>
		<Name>%KalypsoStatistics_Preprocessing</Name>
		<TaskGroup context="MainMap"
			URI="http___www.tu-harburg.de_wb_kalypso_statistics__DataLoad">
			<Help>%KalypsoStatistics_Preprocessing_DataLoad_help</Help>
			<Name>%KalypsoStatistics_Preprocessing_DataLoad</Name>
			<Task context="ImportBaseMap"
				URI="org.kalypso.statistics.model.handlers.TestHandler">
				<Help>%KalypsoStatistics_Preprocessing_DataLoad_MapLoad_help</Help>
				<Name>%KalypsoStatistics_Preprocessing_DataLoad_MapLoad</Name>
			</Task>
			<Task context="ImportLanduseShapeDataWizard"
				URI="http___www.tu-harburg.de_wb_kalypso_statistics__ImportLanduseShapeData">
				<Help>%KalypsoStatistics_Preprocessing_DataLoad_ImportLanduseShapeData_help</Help>
				<Name>%KalypsoStatistics_Preprocessing_DataLoad_ImportLanduseShapeData</Name>
			</Task>
			<Task context="ImportWaterdepthRasterWizard"
				URI="http___www.tu-harburg.de_wb_kalypso_statistics__ImportWaterdepthRaster">
				<Help>%KalypsoStatistics_Preprocessing_DataLoad_ImportWaterdepthRaster_help</Help>
				<Name>%KalypsoStatistics_Preprocessing_DataLoad_ImportWaterdepthRaster</Name>
			</Task>
			<Task context="MainMap"
				URI="org.kalypso.statistics.model.handlers.WaterdepthCollectionsWidgetHandler">
				<Help>%KalypsoStatistics_Preprocessing_DataLoad_WaterdepthCollectionsWidgetHandler_help</Help>
				<Name>%KalypsoStatistics_Preprocessing_DataLoad_WaterdepthCollectionsWidgetHandler</Name>
			</Task>
			<Task context="MainMap"
				URI="org.kalypso.statistics.model.handlers.WaterdepthCoveragesWidgetHandler">
				<Help>%KalypsoStatistics_Preprocessing_DataLoad_WaterdepthCoveragesWidgetHandler_help</Help>
				<Name>%KalypsoStatistics_Preprocessing_DataLoad_WaterdepthCoveragesWidgetHandler</Name>
			</Task>
		</TaskGroup>
		<TaskGroup context="MainMap"
			URI="http___www.tu-harburg.de_wb_kalypso_statistics__Databases">
			<Help>%KalypsoStatistics_Preprocessing_Databases_help</Help>
			<Name>%KalypsoStatistics_Preprocessing_Databases</Name>
			<Task context="CalculationControlPanel"
				URI="http___www.tu-harburg.de_wb_kalypso_statistics__CalculationControlPanel">
				<Help>%KalypsoStatistics_Preprocessing_Databases_CalculationControlPanel_help</Help>
				<Name>%KalypsoStatistics_Preprocessing_Databases_CalculationControlPanel</Name>
			</Task>
			<Task context="RiskZonesPanel"
				URI="http___www.tu-harburg.de_wb_kalypso_statistics__RiskZonesPanel">
				<Help>%KalypsoStatistics_Preprocessing_Databases_RiskZonesPanel_help</Help>
				<Name>%KalypsoStatistics_Preprocessing_Databases_RiskZonesPanel</Name>
			</Task>
		</TaskGroup>
	</TaskGroup>
	<TaskGroup context="MainMap"
		URI="http___www.tu-harburg.de_wb_kalypso_statistics__Processing">
		<Help>%KalypsoStatistics_Processing_help</Help>
		<Name>%KalypsoStatistics_Processing</Name>
		<!-- 
		<Task type="ASYNCHRONOUS"
			context="ImportWaterdepthRasterContext"
			URI="http___www.tu-harburg.de_wb_kalypso_statistics__LanduseRasterizationHandler">
			<Help>%KalypsoStatistics_Processing_LanduseRasterizationHandler_help</Help>
			<Name>%KalypsoStatistics_Processing_LanduseRasterizationHandler</Name>
		</Task>
		 -->
		<Task type="SYNCHRONOUS"
			context="ImportWaterdepthRasterContext"
			URI="http___www.tu-harburg.de_wb_kalypso_statistics__DamagePotentialCalculationHandler">
			<Help>%KalypsoStatistics_Processing_DamagePotentialCalculationHandler_help</Help>
			<Name>%KalypsoStatistics_Processing_DamagePotentialCalculationHandler</Name>
		</Task>
		<Task type="SYNCHRONOUS"
			context="ImportWaterdepthRasterContext"
			URI="http___www.tu-harburg.de_wb_kalypso_statistics__RiskZonesCalculationHandler">
			<Help>%KalypsoStatistics_Processing_RiskZonesCalculationHandler_help</Help>
			<Name>%KalypsoStatistics_Processing_RiskZonesCalculationHandler_help</Name>
		</Task>
	</TaskGroup>
	<TaskGroup context="MainMap"
		URI="http___www.tu-harburg.de_wb_kalypso_statistics__Postprocessing">
		<Help>%KalypsoStatistics_Postprocessing_help</Help>
		<Name>%KalypsoStatistics_Postprocessing</Name>
		<Task context="RiskStatisticsView_Nodes"
			URI="org.kalypso.statistics.model.handlers.RiskShowResultStatisticHandler">
			<Help>%KalypsoStatistics_Postprocessing_RiskShowResultStatisticHandler_help</Help>
			<Name>%KalypsoStatistics_Postprocessing_RiskShowResultStatisticHandler</Name>
		</Task>
		<Task context="RiskStatisticsView_Timeseries"
			URI="org.kalypso.statistics.model.handlers.ExportSpecificDamageCoveragesWidgetHandler">
			<Help>%KalypsoStatistics_Postprocessing_ExportSpecificDamageCoveragesWidgetHandler_help</Help>
			<Name>%KalypsoStatistics_Postprocessing_ExportSpecificDamageCoveragesWidgetHandler</Name>
		</Task>
		<Task context="MainMap"
			URI="org.kalypso.statistics.model.handlers.ExportRiskZoneCoveragesWidgetHandler">
			<Help>%KalypsoStatistics_Postprocessing_ExportRiskZoneCoveragesWidgetHandler_help</Help>
			<Name>%KalypsoStatistics_Postprocessing_ExportRiskZoneCoveragesWidgetHandler</Name>
		</Task>
	</TaskGroup>
	
	
	
	<!-- PERSPECTIVE DEFINITIONS -->
	
	<Perspectives>
		<con:PerspectiveContext id="ContextKalypsoStatistics"
			perspectiveId="org.kalypso.statistics.gui.perspectives.KalypsoStatisticsPerspective">
			<con:ViewContext id="FeatureView"
				partId="org.kalypso.ui.views.featuretemplateview"
				parent="ContextKalypsoStatistics">
				<con:ExtensionContext id="CalculationControlPanel"
					parent="FeatureView"
					handlerFactoryId="kalypsoContextHandlerFactory">
					<con:Parameter name="type"
						value="featureViewInputContext" />
					<con:Parameter name="input"
						value="templates/CalculationControlPanel.gft" />
				</con:ExtensionContext>
				<con:ExtensionContext id="RiskZonesPanel"
					parent="FeatureView"
					handlerFactoryId="kalypsoContextHandlerFactory">
					<con:Parameter name="type"
						value="featureViewInputContext" />
					<con:Parameter name="input"
						value="templates/RiskZonesPanel.gft" />
				</con:ExtensionContext>
			</con:ViewContext>
			
			<con:ViewContext id="RiskStatisticsView_Nodes"
				partId="org.kalypso.statistics.gui.views.nodes.NodesListView"
				parent="ContextKalypsoStatistics">
			</con:ViewContext>
			<con:ViewContext id="RiskStatisticsView_Timeseries"
				partId="org.kalypso.statistics.gui.views.timeseries.TimeseriesListView"
				parent="ContextKalypsoStatistics">
			</con:ViewContext>
					
			<con:WorkbenchSiteContext id="MapContext"
				parent="ContextKalypsoStatistics">
				<con:ViewContext id="MapOutlineView"
					partId="org.kalypso.ui.views.outline" parent="MapContext" />
				<con:ViewContext id="MapView"
					partId="org.kalypso.ui.views.mapView" parent="MapContext">
					<con:ExtensionContext id="BaseMap"
						parent="MapContext"
						handlerFactoryId="kalypsoContextHandlerFactory">
						<con:WizardContext id="ImportBaseMap"
							wizardId="org.kalypso.ui.wizards.imports.baseMap.ImportBaseMapWizard"
							wizardType="NEW_WIZARD" parent="BaseMap" />
						<con:Parameter name="type"
							value="mapViewInputContext" />
						<con:Parameter name="input"
							value="maps/BackroundMap.gmt" />
					</con:ExtensionContext>
					<con:ExtensionContext
						id="ImportLanduseShapeDataContext" parent="MapContext"
						handlerFactoryId="kalypsoContextHandlerFactory">
						<con:WizardContext
							id="ImportLanduseShapeDataWizard"
							wizardId="org.kalypso.statistics.model.actions.dataImport.landuse.ImportLanduseWizard"
							wizardType="NEW_WIZARD" parent="ImportLanduseShapeDataContext" />
						<con:Parameter name="type"
							value="mapViewInputContext" />
						<con:Parameter name="input"
							value="maps/MainMap.gmt" />
					</con:ExtensionContext>
					<con:ExtensionContext
						id="ImportWaterdepthRasterContext" parent="MapContext"
						handlerFactoryId="kalypsoContextHandlerFactory">
						<con:WizardContext
							id="ImportWaterdepthRasterWizard"
							wizardId="org.kalypso.statistics.model.actions.dataImport.waterdepth.ImportWaterdepthWizard"
							wizardType="NEW_WIZARD" parent="ImportWaterdepthRasterContext" />
						<con:Parameter name="type"
							value="mapViewInputContext" />
						<con:Parameter name="input"
							value="maps/MainMap.gmt" />
					</con:ExtensionContext>
					<con:ExtensionContext id="MainMap"
						parent="MapContext"
						handlerFactoryId="kalypsoContextHandlerFactory">
						<con:Parameter name="type"
							value="mapViewInputContext" />
						<con:Parameter name="input"
							value="maps/MainMap.gmt" />
						<!-- 
						<con:ExtensionContext
							id="CoveragesWidgetContext" parent="MainMap"
							handlerFactoryId="kalypsoContextHandlerFactory">
							<con:Parameter name="type"
								value="themeContext" />
							<con:Parameter name="input"
								value="{http://www.opengis.net/gml}_Coverage" />
						</con:ExtensionContext>
						 -->
					</con:ExtensionContext>
				</con:ViewContext>
			</con:WorkbenchSiteContext>
			
			<con:ViewContext id="FeatureSelectionView"
				partId="org.kalypso.featureview.views.FeatureView"
				parent="ContextKalypsoStatistics" />
		</con:PerspectiveContext>
	</Perspectives>
</Workflow>