      <Axis id="NN_Axis" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear"/>
        <Label>Höhe [NN+m]</Label>
        <Direction>POSITIVE</Direction>
        <Position>LEFT</Position>
        <PreferredAdjustment before="5" range="90" after="5"/>
        <RendererRef ref="NumberAxisRenderer"/>
      </Axis>
      <Axis id="M3S_Axis" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear"/>
        <Label>Abfluss [m³/s]</Label>
        <Direction>POSITIVE</Direction>
        <Position>RIGHT</Position>
        <PreferredAdjustment before="40" range="50" after="10"/>
        <RendererRef ref="NumberAxisRenderer"/>
      </Axis>
      <Axis id="M1S_Axis" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear"/>
        <Label>Geschwindigkeit [m/s]</Label>
        <Direction>POSITIVE</Direction>
        <Position>RIGHT</Position>
        <PreferredAdjustment before="40" range="50" after="10"/>
        <RendererRef ref="NumberAxisRenderer"/>
      </Axis>
      <Axis id="Station_Axis" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear"/>
        <Label>Station [km]</Label>
        <Direction>NEGATIVE</Direction>
        <Position>BOTTOM</Position>
        <PreferredAdjustment before="1" range="98" after="1"/>
                <RendererRef ref="NumberAxisRenderer"/>
      </Axis>
    </Mappers>
    <Renderers>
      <AxisRenderer id="NumberAxisRenderer">
        <Provider epid="de.openali.odysseus.chart.ext.base.axisrenderer.provider.GenericNumberAxisRendererProvider">
          <Parameters>
            <Parameter name="inset_tick" value="2"/>
            <Parameter name="inset_label" value="2"/>
            <Parameter name="tick_length" value="5"/>
            <Parameter name="hide_cut" value="true"/>
            <Parameter name="gap" value="3"/>
          </Parameters>
        </Provider>
      </AxisRenderer>
    </Renderers>
