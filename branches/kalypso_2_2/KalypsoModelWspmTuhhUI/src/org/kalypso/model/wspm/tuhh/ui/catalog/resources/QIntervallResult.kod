<?xml version="1.0" encoding="Cp1252" standalone="yes"?>
<ChartConfiguration xmlns="http://www.openali.de/odysseus/chartconfig/0.2.0/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openali.de/odysseus/chartconfig/0.2.0/ ../../../../../../../../../../OdysseusChartFactory/etc/binding/schema/ChartConfiguration.xsd">
	
	
	
	<Chart id="runoffChart">
		<Title>Abfluss</Title>
		<Description>Abflusswerte der Berechnung</Description>
		<Layers>

			<Layer visible="true" id="runoffLayer">
				<Provider epid="org.kalypso.model.wspm.ui.featureview.TupleResultLineLayerProvider" >
					<Parameters>
						<Parameter name="featureKey" value="theResultFeatureKey" />
						<Parameter name="propertyName" value="{org.kalypso.model.wspm.tuhh}pointsMember" />
						<Parameter name="showPoints" value="true" />
						<Parameter name="showLines" value="false" />
						<Parameter name="domainComponentId"
							value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#Depth" />
						<Parameter name="valueComponentId"
							value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#Runoff" />
					</Parameters>
				</Provider>
				<Title>Abfluss</Title>
				<Description>Abfluss abhängig vom Wasserspiegel</Description>
				
				<MapperRefs>
					<DomainAxisRef ref="wsp_Axis" />
					<TargetAxisRef ref="runoff_Axis" />
				</MapperRefs>
				<Styles>
					<LineStyle role="line" isVisible="false">
						<Title></Title>
					</LineStyle>
					
					<PointStyle role="point">
						<Title>Abfluss</Title>
						<Width>8</Width>
						<Height>8</Height>
						<Stroke>
							<Width>1</Width>
							<LineColor>000000</LineColor>
						</Stroke>
						<OvalMarker>
						</OvalMarker>
						<FillColor>ff0000</FillColor>
					</PointStyle>
				</Styles>
				
			</Layer>

			<Layer visible="true" id="runoffPolynomeLayer">
				<Provider epid="org.kalypso.model.wspm.ui.featureview.PolynomeLayerProvider" >
					<Parameters>
						<Parameter name="featureKey" value="theResultFeatureKey" />
						<Parameter name="propertyName" value="{org.kalypso.model.wspm.tuhh}polynomialMember" />
						<Parameter name="domainId"
							value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#phenomenonRunoff" />
					</Parameters>
				</Provider>
				<Title>Abflusspolynome</Title>
				<Description>Abflusspolynome abhängig vom Wasserspiegel</Description>
				<MapperRefs>
					<DomainAxisRef ref="wsp_Axis" />
					<TargetAxisRef ref="runoff_Axis" />
				</MapperRefs>
				<Styles>
					<PointStyle role="point" isVisible="false">
						<Title></Title>
					</PointStyle>
					<LineStyle role="line">
						<Title>Abflusspolynome</Title>
						<Width>3</Width>
						<LineColor>00aaff</LineColor>
					</LineStyle>
				</Styles>
				
			</Layer>
