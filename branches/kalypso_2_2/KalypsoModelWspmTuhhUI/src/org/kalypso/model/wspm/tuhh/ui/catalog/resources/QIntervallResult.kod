		</Layers>
		<Mappers>
			<Axis id="wsp_Axis">
				<Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
				<Label>Wassertiefe [m]</Label>
				<Direction>POSITIVE</Direction>
				<Position>BOTTOM</Position>
				<NumberRange>
					<MinValue>0</MinValue>
					<MaxValue>100</MaxValue>
				</NumberRange>
				<RendererRef ref="NumberAxisRenderer"/>
			</Axis>
			<Axis id="runoff_Axis">
				<Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
				<Label>Abfluss [m³/s]</Label>
				<Direction>POSITIVE</Direction>
				<Position>LEFT</Position>
				<NumberRange>
					<MinValue>5</MinValue>
					<MaxValue>10</MaxValue>
				</NumberRange>
				<RendererRef ref="NumberAxisRenderer"/>
			</Axis>
			<Axis id="runoffDelta_Axis">
				<Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
				<Label>Abfluss-Delta [m³/s]</Label>
				<Direction>POSITIVE</Direction>
				<Position>LEFT</Position>
				<NumberRange>
					<MinValue>5</MinValue>
					<MaxValue>10</MaxValue>
				</NumberRange>
				<RendererRef ref="NumberAxisRenderer"/>
			</Axis>
		</Mappers>
		<Renderers>
			<AxisRenderer id="NumberAxisRenderer">
				<Provider epid="de.openali.odysseus.chart.ext.test.axisrenderer.provider.GenericNumberAxisRendererProvider">
					<Parameters>
						<Parameter name="inset_tick" value="1"/>
					</Parameters>
				</Provider>
			</AxisRenderer>
		</Renderers>
	</Chart>
	

