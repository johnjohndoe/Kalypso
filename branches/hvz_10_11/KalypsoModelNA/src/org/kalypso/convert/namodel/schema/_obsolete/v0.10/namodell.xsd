  </element>
  <complexType name="CatchmentFeatureType">
    <complexContent>
      <extension base="gml:AbstractFeatureType">
        <sequence>
          <!--
            ___________(inum,i5)____(ispk,i3)
            (text,a80)
            (flaech,*)
            (pns,a1)_(kurzzeit,a)_(namfn,a)_(langzeit,a)_(faktn,f5.2)
            (namft,a)_(namept,a)
            (namfzf,a)
            (namhydro,a)
            (snowtype,a15)(ftem,*)_(fver,*)
          -->
          <element name="Ort" type="gml:PolygonPropertyType">
            <annotation>
              <documentation xml:lang="de">
                <label>Geometrie</label>
                <tooltip>geometriescher Ort</tooltip>
                <description>Geometrie des Objektes</description>
              </documentation>
              <documentation xml:lang="en">
                <label>Geometry</label>
                <tooltip>Geometric position</tooltip>
                <description>Geometry of the object</description>
              </documentation>
            </annotation>
          </element>
          <element name="entwaesserungsStrangMember" type="na:ChannelAssociationType">
            <annotation>
              <documentation xml:lang="de">
                <label>Zielstrang</label>
                <tooltip>Relation zum Entwässerungsstrang</tooltip>
                <description>Relation zum dem Strang, in welchen das Teilgebiet entwässert.</description>
              </documentation>
              <documentation xml:lang="en">
                <label>Aim strand</label>
                <tooltip>Relation to drainage line</tooltip>
                <description>Relation to the line, in which the subcatchment drains.</description>
              </documentation>
            </annotation>
          </element>
          <element name="generateResult" type="boolean">
            <annotation>
              <documentation xml:lang="de">
                <label>berechnen</label>
                <tooltip>Berechnung bzw. Ergebnisse erzeugen</tooltip>
                <description>Auswahl bewirkt, dass bei einer Berechnung ein Ergebnis für dieses Gebiet erzeugt wird.</description>
              </documentation>
              <documentation xml:lang="en">
                <label>calculating</label>
                <tooltip>Calculation, producing results</tooltip>
                <description>Selection secures, that by single calculation one result will be produced for this catchment</description>
              </documentation>
            </annotation>
          </element>
          <element name="flaech" type="long">
            <annotation>
              <documentation xml:lang="de">
                <label>TG-Flaeche</label>
                <tooltip>Flaeche des Teilgebietes [m^2]</tooltip>
                <description>Gesamtfläche des Teilgebietes [m^2]</description>
              </documentation>
              <documentation xml:lang="en">
                <label>SC-Area</label>
                <tooltip>Area of the subcatchments [m^2]</tooltip>
                <description>Total area of the subcatchments[m^2]</description>
              </documentation>
            </annotation>
            <!-- JH, Flaeche kann auch aus der Hydrotopdatei hierher übernommen werden (wird im Modell ohnehin aus Hydrotopdatei übernommen)-->
          </element>
          <element name="faktn">
            <annotation>
              <documentation xml:lang="de">
                <label>Faktor Niederschlag</label>
                <tooltip>Korrekturfaktor des Niederschlages, multiplikativ</tooltip>
                <description>Korrekturfaktor mit dem alle Werte der Niederschlagszeitreihe zur Berechnung multipliziert werden</description>
              </documentation>
              <documentation xml:lang="en">
                <label>Precipitation coefficient</label>
                <tooltip>Correction coefficient of the precipitaition, multiplicative</tooltip>
                <description>Correction coefficient with all the values of the precipitation time series, which were multiplicated during the calculations</description>
              </documentation>
            </annotation>
            <simpleType>
              <restriction base="double">
                <minInclusive value="0" />
                <maxInclusive value="2" />
              </restriction>
            </simpleType>
          </element>
          <element name="snowtype" type="string">
            <annotation>
              <documentation xml:lang="de">
                <label>Schneetyp</label>
                <tooltip>Schneetyp Referenz DB</tooltip>
                <description>Schneetypen-Referenz auf Schneedatenbank</description>
              </documentation>
              <documentation xml:lang="en">
                <label>Snowtype</label>
                <tooltip>Snowtype Reference DB</tooltip>
                <description>Snowtypes-Reference on snowdatabase</description>
              </documentation>
            </annotation>
            <!-- TODO standard oder snowdatei uebernehmen -->
          </element>
          <element name="ftem">
            <annotation>
              <documentation xml:lang="de">
                <label>Temperaturverschiebung</label>
                <tooltip>Korrekturwert Temperatur, additiv [°C]</tooltip>
