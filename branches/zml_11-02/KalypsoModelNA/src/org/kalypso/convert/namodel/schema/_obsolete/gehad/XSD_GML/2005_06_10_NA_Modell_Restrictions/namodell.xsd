<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns:na="http://www.tuhh.de/kalypsoNA" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gml="http://www.opengis.net/gml" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:obslink="obslink.zml.kalypso.org" targetNamespace="http://www.tuhh.de/kalypsoNA" elementFormDefault="qualified" version="0.2">
	<import namespace="http://www.opengis.net/gml" schemaLocation="feature.xsd"/>
	<import namespace="obslink.zml.kalypso.org" schemaLocation="obslink.xsd"/>
	<import namespace="http://www.w3.org/1999/xlink" schemaLocation="xlinks.xsd"/>
	<!--<import namespace="http://www.opengis.net/gml" schemaLocation="http://schemas.opengis.net/gml/2.1.2/feature.xsd"/>
               <element name="Abfluss" type="obslink:TimeseriesLinkType/>
        jetzt: <element name="Abfluss" type="obslink:TimeseriesLinkFeatureProperty/>
	-->
	<element name="NaModell" type="na:NaModellType"/>
	<complexType name="NaModellType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element name="CatchmentCollectionMember" type="na:CatchmentCollectionAssociationType"/>
					<element name="ChannelCollectionMember" type="na:ChannelCollectionAssociationType"/>
					<element name="NodeCollectionMember" type="na:NodeCollectionAssociationType"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- __ Nodes _________________________________ -->
	<complexType name="NodeCollectionAssociationType">
		<complexContent>
			<restriction base="gml:FeatureAssociationType">
				<sequence>
					<element ref="na:NodeCollection"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<element name="NodeCollection" type="na:NodeCollectionType" substitutionGroup="gml:_Feature"/>
	<complexType name="NodeCollectionType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element name="nodeMember" type="na:NodeAssociationType" minOccurs="0" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="NodeAssociationType">
		<complexContent>
			<restriction base="gml:FeatureAssociationType">
				<sequence minOccurs="0">
					<element ref="na:Node"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<element name="Node" type="na:NodeFeatureType" substitutionGroup="gml:_Feature"/>
	<complexType name="NodeFeatureType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element name="Ort" type="gml:PointPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Ort</label>
								<tooltip>geometriescher Ort</tooltip>
								<description>Geometrie des Objektes</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Geometry</label>
								<tooltip>geometric position</tooltip>
								<description>Geometry of the Object</description>
							</documentation>
						</annotation>
					</element>
					<element name="num" type="integer">
						<annotation>
							<documentation xml:lang="de">
								<label>Nummer</label>
								<tooltip>Knoten Nummer</tooltip>
								<description>Jedem Knoten ist eine Knotennummer zugewiesen. Diese ist eindeutig und sollte nicht veraendert werden</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Number</label>
								<tooltip>Node Number</tooltip>
								<description>A node number is shown to each node.  This is unambiguous and should not be changed</description>
							</documentation>
						</annotation>
					</element>
					<element name="generateResult" type="boolean">
						<annotation>
							<documentation xml:lang="de">
								<label>berechnen</label>
								<tooltip>Berechnung bzw. Ergebnisse erzeugen</tooltip>
								<description>Auswahl bewirkt, dass bei einer Berechnung ein Ergebnis für diesen Knoten erzeugt wird.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>calculate</label>
								<tooltip>Produce calculations and/or results</tooltip>
								<description>Selection brings about that in a calculation a result is produced for this node.  </description>
							</documentation>
						</annotation>
					</element>
					<element name="downStreamChannelMember" type="na:ChannelAssociationType" minOccurs="0">
						<annotation>
							<documentation xml:lang="de">
								<label>Unterwasserstrang</label>
								<tooltip>Gewaesserstrang der Unterwasser anschliesst</tooltip>
								<description>Gewaesserstrang, welcher im weiteren Fliessverlauf Unterwasser anschliesst</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Downstream Strand</label>
								<tooltip>Water strand which connects the lower water</tooltip>
								<description>Water strand which connects lower water in the further discharge</description>
							</documentation>
						</annotation>
					</element>
					<element name="pegelZRRepository" type="obslink:TimeseriesLinkFeatureProperty">
						<annotation>
							<documentation xml:lang="de">
								<label>Pegel(rep)</label>
								<tooltip>Verknuepfung Pegel Repository</tooltip>
								<description>.</description>
							</documentation>
						</annotation>
					</element>
					<element name="pegelZR" type="obslink:TimeseriesLinkFeatureProperty">
						<annotation>
							<documentation xml:lang="de">
								<label>Pegel (lok)</label>
								<tooltip>Verknuepfung Pegel lokal</tooltip>
								<description>.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Level(rep)</label>
								<tooltip>Connection level Repository</tooltip>
								<description>.</description>
							</documentation>
						</annotation>
					</element>
					<!-- ZR berechnet, hier wird das ergebnis gespeichert-->
					<element name="qberechnetZR" type="obslink:TimeseriesLinkFeatureProperty">
						<annotation>
							<documentation xml:lang="de">
								<label>Abfluss berechnet</label>
								<tooltip>Berechnete Abflussganglinie</tooltip>
								<description>.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Calculated discharge</label>
								<tooltip>Calculated discharge line</tooltip>
								<description>.</description>
							</documentation>
						</annotation>
					</element>
					<element name="pegelBerechnetZRAblage" type="obslink:TimeseriesLinkFeatureProperty">
						<annotation>
							<documentation xml:lang="de">
								<label>ZR Ablage Berechnung</label>
								<tooltip>Verknuepfung zur Ablage des Ergebnisses in der Datenbank</tooltip>
								<description>.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>TS Storage calculation </label>
								<tooltip>Connection of the storage with the results in the database</tooltip>
								<description>.</description>
							</documentation>
						</annotation>
					</element>
					<!--ab hier kann entfernt werden, falls Branching verwendet wird-->
					<!-- ZR Zufluss falls zufluss vorhanden -->
					<element name="zuflussZRRepository" type="obslink:TimeseriesLinkFeatureProperty">
						<annotation>
							<documentation xml:lang="de">
								<label>Zufluss Eingang</label>
								<tooltip>Verknuepfte Zuflusszeitreihe, Eingangsdaten</tooltip>
								<description>.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Inflow input</label>
								<tooltip>Combined Inflow time series, Input data</tooltip>
								<description>.</description>
							</documentation>
						</annotation>
					</element>
					<element name="zuflussZRRepositoryVorhersage" type="obslink:TimeseriesLinkFeatureProperty">
						<annotation>
							<documentation xml:lang="de">
								<label>Zufluss Eingang</label>
								<tooltip>Verknuepfte Zuflusszeitreihe, Eingangsdaten Vorhersage</tooltip>
								<description>.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Inflow input</label>
								<tooltip>Combined Inflow time series, Input data forecast</tooltip>
								<description>.</description>
							</documentation>
						</annotation>
					</element>
					<element name="zuflussZR" type="obslink:TimeseriesLinkFeatureProperty">
						<annotation>
							<documentation xml:lang="de">
								<label>Zufluss Berechnung</label>
								<tooltip>Verknuepfte Zuflusszeitreihe, Berechnungsdaten</tooltip>
								<description>.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Inflow calculation</label>
								<tooltip>Combined Inflow time series, calculated data</tooltip>
								<description>.</description>
							</documentation>
						</annotation>
					</element>
					<!-- verzweigung -->
					<element name="zproz" type="double">
						<annotation>
							<documentation xml:lang="de">
								<label>Verzweigungsfaktor</label>
								<tooltip>Abflussanteil zum Verzweigungsknoten (0-1)</tooltip>
								<description>Anteil des Abflusses, der dem Verzweigungsknoten zugefuehrt wird (0-1)</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Branching factor</label>
								<tooltip>Outflow part going to the branching nodes (0-1)</tooltip>
								<description>The part of the outflow, which  is applied to the branching nodes (0-1)</description>
							</documentation>
						</annotation>
					</element>
					<element name="verzweigungNodeMember" type="na:NodeAssociationType" minOccurs="0">
						<annotation>
							<documentation xml:lang="de">
								<label>Verzweigungsknoten</label>
								<tooltip>Verknuepfung Zielknoten der Verzweigung</tooltip>
								<description>Zielknoten, dem der angegebene Verzweigungsanteil des Abflusses zugefuehrt wird</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Branchingnode</label>
								<tooltip>Connection aim nodes of the branching</tooltip>
								<description>Aim nodes, to which the given part of the branching of the outflow is applied</description>
							</documentation>
						</annotation>
					</element>
					<!--bis hier kann entfernt werden, falls Branching verwendet wird-->
					<!-- branching ab hier aktivieren
					<element name="qzug" type="double">
						<annotation>
							<documentation xml:lang="de">
								<label>Zugabemenge</label>
								<tooltip>kont. Zugabemenge [m^3/s]</tooltip>
								<description>kontinuierliche Zugabemenge [m^3/s] zum Knotenabfluss</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Additional discharge</label>
								<tooltip>continuous additional discharge [m^3/s]</tooltip>
								<description>continuous additional discharge [m^3/s] to the node outflow</description>
							</documentation>

						</annotation>
					</element>
					<element name="BranchingMember" type="na:BranchingAssociationType" minOccurs="0">
						<annotation>
							<documentation xml:lang="de">
								<label>Knoten zu Knoten</label>
								<tooltip>Verzweigung, Zufluss, Entnahme,Überlauf</tooltip>
								<description>Verbindung mit oder von einem Zielknoten (Verzweigung, Zufluss, Entnahme,Überlauf)</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Nodes to nodes</label>
								<tooltip>Branching, Inflow, Withdrawal,Overflow</tooltip>
								<description>Connection with or from one aim node (Branching, Inflow, Withdrawal,Overflow)</description>
							</documentation>

						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="BranchingAssociationType">
		<complexContent>
			<restriction base="gml:FeatureAssociationType">
				<sequence>
					<element ref="na:_Branching"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<element name="_Branching" type="na:AbstractBranchingType" abstract="true" substitutionGroup="gml:_Feature"/>
	<complexType name="AbstractBranchingType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element name="branchingNodeMember" type="na:NodeAssociationType">
						<annotation>
							<documentation xml:lang="de">
								<label>Relation Knoten</label>
								<tooltip>RelationVerzweigungsknoten</tooltip>
								<description>Relation zum Verzweigungsknoten der Knoten zu Knoten Beziehung</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Relation nodes</label>
								<tooltip>Relation connection nodes</tooltip>
								<description>Relation to connection nodes of node to node relationship</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<element name="kontEntnahme" type="na:ContExtractionFeatureType" substitutionGroup="na:_Branching"/>
	<complexType name="ContExtractionFeatureType">
		<complexContent>
			<extension base="na:AbstractBranchingType">
				<sequence>
					<element name="qabg" type="double">
						<annotation>
							<documentation xml:lang="de">
								<label>Entnahmemenge</label>
								<tooltip>kont. Entnahmemenge [m^3/s]</tooltip>
								<description>kont. Entnahmemenge [m^3/s], welche dem Zielknoten zugeschlagen wird</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Withdrawal discharge</label>
								<tooltip>continuous withdrawal discharge[m^3/s]</tooltip>
								<description>continuous withdrawal discharge[m^3/s], which is added to the nodes</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<element name="Ueberlauf" type="na:OverfallFeatureType" substitutionGroup="na:_Branching"/>
	<complexType name="OverfallFeatureType">
		<complexContent>
			<extension base="na:AbstractBranchingType">
				<sequence>
					<element name="queb" type="double">
						<annotation>
							<documentation xml:lang="de">
								<label>Überlaufhöhe</label>
								<tooltip>Überlaufhöhe [m^3/s]</tooltip>
								<description>Überlaufhöhe [m^3/s], bei Überschreitung wird der darüber liegende Abfluss dem Zielknoten zugeschlagen</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Overflowheight</label>
								<tooltip>Overflowheight [m^3/s]</tooltip>
								<description>Overflowheight [m^3/s], by exceeding it,the overflow will be added to the aim nodes.</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<element name="Zuflusspegel" type="na:InflowGageFeatureType" substitutionGroup="na:_Branching"/>
	<complexType name="InflowGageFeatureType">
		<complexContent>
			<extension base="na:AbstractBranchingType">
				<sequence>
					<element name="zuflussZRRepository" type="obslink:TimeseriesLinkFeatureProperty">
						<annotation>
							<documentation xml:lang="de">
								<label>Zufluss Eingang</label>
								<tooltip>Verknuepfte Zuflusszeitreihe, Eingangsdaten</tooltip>
								<description>.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Inflow input</label>
								<tooltip>Combined Inflow time series, Inputdata</tooltip>
								<description>.</description>
							</documentation>
						</annotation>
					</element>
					<element name="zuflussZRRepositoryVorhersage" type="obslink:TimeseriesLinkFeatureProperty">
						<annotation>
							<documentation xml:lang="de">
								<label>Zufluss Eingang</label>
								<tooltip>Verknuepfte Zuflusszeitreihe, Eingangsdaten Vorhersage</tooltip>
								<description>.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Inflow input</label>
								<tooltip>Combined Inflow time series, Inputdata forecast</tooltip>
								<description>.</description>
							</documentation>
						</annotation>
					</element>
					<element name="zuflussZR" type="obslink:TimeseriesLinkFeatureProperty">
						<annotation>
							<documentation xml:lang="de">
								<label>Zufluss Berechnung</label>
								<tooltip>Verknuepfte Zuflusszeitreihe, Berechnungsdaten</tooltip>
								<description>.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Calculating inflow</label>
								<tooltip>Combined Inflow time series, Calculation data</tooltip>
								<description>.</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<element name="Verzweigung" type="na:BranchingFeatureType" substitutionGroup="na:_Branching"/>
	<complexType name="BranchingFeatureType">
		<complexContent>
			<extension base="na:AbstractBranchingType">
				<sequence>
					<element name="zproz" type="double">
						<annotation>
							<documentation xml:lang="de">
								<label>Verzweigungsfaktor</label>
								<tooltip>Abflussanteil zum Verzweigungsknoten (0-1)</tooltip>
								<description>Anteil des Abflusses, der dem Verzweigungsknoten zugefuehrt wird (0-1)</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Branching factor</label>
								<tooltip>Outflow part going to the branching nodes (0-1)</tooltip>
								<description>The part of the outflow, which  is applied to the branching nodes (0-1)</description>
							</documentation>
						</annotation>
					</element>
-->
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- ________Catchments__________________ -->
	<complexType name="CatchmentCollectionAssociationType">
		<complexContent>
			<restriction base="gml:FeatureAssociationType">
				<sequence>
					<element ref="na:CatchmentCollection"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<element name="CatchmentCollection" type="na:CatchmentCollectionType" substitutionGroup="gml:_Feature"/>
	<complexType name="CatchmentCollectionType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element name="catchmentMember" type="na:CatchmentAssociationType" minOccurs="0" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<element name="Catchment" type="na:CatchmentFeatureType" substitutionGroup="gml:_Feature"/>
	<complexType name="CatchmentFeatureType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<!--
    ___________(inum,i5)____(ispk,i3)
(text,a80)
(flaech,*)
(pns,a1)_(kurzzeit,a)_(namfn,a)_(langzeit,a)_(faktn,f5.2)
(namft,a)_(namept,a)
(namfzf,a)
(namhydro,a)
(snowtype,a15)(ftem,*)_(fver,*)
    -->
					<element name="Ort" type="gml:PolygonPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Ort</label>
								<tooltip>geometriescher Ort</tooltip>
								<description>Geometrie des Objektes</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Geometry</label>
								<tooltip>Geometric position</tooltip>
								<description>Geometry of the object</description>
							</documentation>
						</annotation>
					</element>
					<element name="inum" type="integer">
						<annotation>
							<documentation xml:lang="de">
								<label>TG-Nummer</label>
								<tooltip>Teilgebietsnummer</tooltip>
								<description>Nummer des Teilgebietes</description>
							</documentation>
							<documentation xml:lang="en">
								<label>SC-Number</label>
								<tooltip>Subcatchment number</tooltip>
								<description>Number of the subcatchments</description>
							</documentation>
						</annotation>
					</element>
					<element name="entwaesserungsStrangMember" type="na:ChannelAssociationType">
						<annotation>
							<documentation xml:lang="de">
								<label>Zielstrang</label>
								<tooltip>Relation zum Entwässerungsstrang</tooltip>
								<description>Relation zum dem Strang, in welchen das Teilgebiet entwässert.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Aim strand</label>
								<tooltip>Relation to drainage line </tooltip>
								<description>Relation to the line, in  which the subcatchment drains.</description>
							</documentation>
						</annotation>
					</element>
					<element name="generateResult" type="boolean">
						<annotation>
							<documentation xml:lang="de">
								<label>berechnen</label>
								<tooltip>Berechnung bzw. Ergebnisse erzeugen</tooltip>
								<description>Auswahl bewirkt, dass bei einer Berechnung ein Ergebnis für dieses Gebiet erzeugt wird.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>calculating</label>
								<tooltip>Calculation, producing results </tooltip>
								<description>Selection secures, that by single calculation one result will be produced for this catchment</description>
							</documentation>
						</annotation>
					</element>
					<element name="flaech" type="long">
						<annotation>
							<documentation xml:lang="de">
								<label>TG-Flaeche</label>
								<tooltip>Flaeche des Teilgebietes [m^2]</tooltip>
								<description>Gesamtfläche des Teilgebietes [m^2]</description>
							</documentation>
							<documentation xml:lang="en">
								<label>SC-Area</label>
								<tooltip>Area of the subcatchments [m^2]</tooltip>
								<description>Total area of the subcatchments[m^2]</description>
							</documentation>
						</annotation>
						<!-- JH, Flaeche kann auch aus der Hydrotopdatei hierher übernommen werden (wird im Modell ohnehin aus Hydrotopdatei übernommen)-->
					</element>
					<element name="faktn" type="double">
						<annotation>
							<documentation xml:lang="de">
								<label>Faktor Niederschlag</label>
								<tooltip>Korrekturfaktor des Niederschlages, multiplikativ</tooltip>
								<description>Korrekturfaktor mit dem alle Werte der Niederschlagszeitreihe zur Berechnung multipliziert werden</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Precipitation coefficient</label>
								<tooltip>Correction coefficient of the precipitaition, multiplicative</tooltip>
								<description>Correction coefficient with all the values of the precipitation time series, which were multiplicated during the calculations</description>
							</documentation>
						</annotation>
					</element>
					<element name="snowtype">
						<annotation>
							<documentation xml:lang="de">
								<label>Schneetyp</label>
								<tooltip>Schneetyp Referenz DB</tooltip>
								<description>Schneetypen-Referenz auf Schneedatenbank</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Snowtype</label>
								<tooltip>Snowtype Reference DB </tooltip>
								<description>Snowtypes-Reference on snowdatabase</description>
							</documentation>
						</annotation>
						<simpleType>
                        <restriction base="string">
						  <maxLength value="30"/>
						  <minLength value="0"/>
						 </restriction>
						</simpleType>
						<!-- TODO standard oder snowdatei uebernehmen -->
					</element>
					<element name="ftem" type="double">
						<annotation>
							<documentation xml:lang="de">
								<label>Temperaturverschiebung</label>
								<tooltip>Korrekturwert Temperatur, additiv [°C]</tooltip>
								<description>Korrekturwert, der zu jedem Wert der Temperaturzeitreihe addiert wird</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Change in temperature</label>
								<tooltip>Correcting value for the Temperature, additive [°C]</tooltip>
