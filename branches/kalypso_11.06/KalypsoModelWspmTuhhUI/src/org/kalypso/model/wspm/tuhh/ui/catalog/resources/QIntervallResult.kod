  <!-- new chart configurations -->
  <Chart id="runoffChannelAndFloodplainChart">
    <Title horizontalTextAnchor="CENTER" verticalTextAnchor="TOP" horizontalPosition="CENTER" verticalPosition="TOP">Abfluss Flussschlauch und Vorland</Title>
    <Description>Abflusswerte innerhalb des Flussschlauchs und des Vorlands</Description>
    <Layers>

      <!-- grid and border -->
      <Layer visible="true" legend="false" id="runoffChannelAndFloodplainGridLayer">
        <Provider epid="org.kalypso.zml.ui.chart.layer.provider.ZmlGridLayerProvider">
          <Parameters>
          </Parameters>
        </Provider>
        <Title>Gitterlinien</Title>
        <Description />
        <MapperRefs>
          <DomainAxisRef ref="wspChannelAndFloodplain_Axis" />
          <TargetAxisRef ref="runoffChannelAndFloodplain_Axis" />
        </MapperRefs>
        <Styles>
          <LineStyle role="gridLineStyle">
            <Width>1</Width>
            <LineColor>CCCCCC</LineColor>
            <DashOffset>0</DashOffset>
            <DashArray>2 2 2</DashArray>
            <LineCap>ROUND</LineCap>
            <LineJoin>ROUND</LineJoin>
            <MiterLimit>1</MiterLimit>
          </LineStyle>
          <!-- THIS IS THE STLYE OF THE BORDER... (even if it is a point style) -->
          <PointStyle role="gridPointStyle">
            <Stroke>
              <Width>2</Width>
              <LineColor>000000</LineColor>
            </Stroke>
            <FillColor isVisible="false">
              <Color>000000</Color>
            </FillColor>
          </PointStyle>
        </Styles>
      </Layer>

      <!-- sum of channel and floodplain -->
      <Layer visible="true" id="runoffSumLayer">
        <Provider epid="org.kalypso.model.wspm.ui.featureview.TupleResultLineLayerProvider">
          <Parameters>
            <Parameter name="featureKey" value="theResultFeatureKey" />
            <Parameter name="propertyName" value="{org.kalypso.model.wspm.tuhh}pointsMember" />
            <Parameter name="showPoints" value="true" />
            <Parameter name="showLines" value="false" />
            <Parameter name="domainComponentId" value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#Waterlevel" />
            <Parameter name="valueComponentId" value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#Runoff" />
          </Parameters>
        </Provider>
        <Title>Summe</Title>
        <Description>Summe des Abflusses von Flussschlauch und Vorland</Description>
        <MapperRefs>
          <DomainAxisRef ref="wspChannelAndFloodplain_Axis" />
          <TargetAxisRef ref="runoffChannelAndFloodplain_Axis" />
        </MapperRefs>
        <Styles>
          <LineStyle role="line" isVisible="true">
            <Title>Summe</Title>
            <Width>1</Width>
            <LineColor>999999</LineColor>
          </LineStyle>
          <PointStyle role="point">
            <Title>Summe</Title>
            <Width>8</Width>
            <Height>8</Height>
            <Stroke>
              <Width>1</Width>
              <LineColor>000000</LineColor>
            </Stroke>
            <OvalMarker>
            </OvalMarker>
            <FillColor>
              <Color>ff0000</Color>
            </FillColor>
          </PointStyle>
        </Styles>
      </Layer>

      <!-- channel -->
      <Layer visible="true" id="runoffChannelLayer">
        <Provider epid="org.kalypso.model.wspm.ui.featureview.TupleResultLineLayerProvider">
          <Parameters>
            <Parameter name="featureKey" value="theResultFeatureKey" />
            <Parameter name="propertyName" value="{org.kalypso.model.wspm.tuhh}pointsMember" />
            <Parameter name="showPoints" value="true" />
            <Parameter name="showLines" value="false" />
            <Parameter name="domainComponentId" value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#Waterlevel" />
            <Parameter name="valueComponentId" value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#RunoffChannel" />
          </Parameters>
        </Provider>
        <Title>Abfluss Flussschlauch</Title>
        <Description>Abfluss im Flussschlauch abhängig vom Wasserspiegel</Description>
        <MapperRefs>
          <DomainAxisRef ref="wspChannelAndFloodplain_Axis" />
          <TargetAxisRef ref="runoffChannelAndFloodplain_Axis" />
        </MapperRefs>
        <Styles>
          <LineStyle role="line" isVisible="true">
            <Title>Abfluss Flussschlauch</Title>
            <Width>1</Width>
            <LineColor>999999</LineColor>
          </LineStyle>
          <PointStyle role="point">
            <Title>Abfluss Flussschlauch</Title>
            <Width>8</Width>
            <Height>8</Height>
            <Stroke>
              <Width>1</Width>
              <LineColor>000000</LineColor>
            </Stroke>
            <OvalMarker>
            </OvalMarker>
            <FillColor>
              <Color>0000ff</Color>
            </FillColor>
          </PointStyle>
        </Styles>
      </Layer>

      <!-- floodplain -->
      <Layer visible="true" id="runoffFloodplainLayer">
        <Provider epid="org.kalypso.model.wspm.ui.featureview.TupleResultLineLayerProvider">
          <Parameters>
            <Parameter name="featureKey" value="theResultFeatureKey" />
            <Parameter name="propertyName" value="{org.kalypso.model.wspm.tuhh}pointsMember" />
            <Parameter name="showPoints" value="true" />
            <Parameter name="showLines" value="false" />
            <Parameter name="domainComponentId" value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#Waterlevel" />
            <Parameter name="valueComponentId" value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#RunoffFloodplain" />
          </Parameters>
        </Provider>
        <Title>Abfluss Vorland</Title>
        <Description>Abfluss im Vorland abhängig vom Wasserspiegel</Description>
        <MapperRefs>
          <DomainAxisRef ref="wspChannelAndFloodplain_Axis" />
          <TargetAxisRef ref="runoffChannelAndFloodplain_Axis" />
        </MapperRefs>
        <Styles>
          <LineStyle role="line" isVisible="true">
            <Title>Abfluss Vorland</Title>
            <Width>1</Width>
            <LineColor>999999</LineColor>
          </LineStyle>
          <PointStyle role="point">
            <Title>Abfluss Vorland</Title>
            <Width>8</Width>
            <Height>8</Height>
            <Stroke>
              <Width>1</Width>
              <LineColor>000000</LineColor>
            </Stroke>
            <OvalMarker>
            </OvalMarker>
            <FillColor>
              <Color>00ff00</Color>
            </FillColor>
          </PointStyle>
        </Styles>
      </Layer>

      <!-- bankfull height -->
      <Layer visible="true" id="runoffBankfullHeightLayer">
        <Provider epid="org.kalypso.model.wspm.tuhh.ui.chart.VerticalLineLayerProvider">
          <Parameters>
            <Parameter name="featureKey" value="theResultFeatureKey" />
            <Parameter name="propertyName" value="{org.kalypso.model.wspm.tuhh}bankfullHeight" />
            <Parameter name="showPoints" value="true" />
            <Parameter name="showLines" value="true" />
          </Parameters>
        </Provider>
        <Title>Bordvollhöhe</Title>
        <Description>Bordvollhöhe</Description>
        <MapperRefs>
          <DomainAxisRef ref="wspChannelAndFloodplain_Axis" />
          <TargetAxisRef ref="runoffChannelAndFloodplain_Axis" />
        </MapperRefs>
        <Styles>
          <PointStyle role="point" isVisible="false">
            <Title></Title>
          </PointStyle>
          <LineStyle role="line" isVisible="true">
            <Title>Bordvollhöhe</Title>
            <Width>1</Width>
            <LineColor>AA0000</LineColor>
          </LineStyle>
        </Styles>
      </Layer>
    </Layers>

    <Mappers>
      <Axis id="wspChannelAndFloodplain_Axis">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Label>Wasserspiegel [mNN]</Label>
        <Direction>POSITIVE</Direction>
        <Position>BOTTOM</Position>
        <NumberRange>
          <MinValue>0</MinValue>
          <MaxValue>100</MaxValue>
        </NumberRange>
        <RendererRef ref="NumberAxisRenderer" />
      </Axis>
      <Axis id="runoffChannelAndFloodplain_Axis">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Label>Abfluss [m³/s]</Label>
        <Direction>POSITIVE</Direction>
        <Position>LEFT</Position>
        <NumberRange>
          <MinValue>5</MinValue>
          <MaxValue>10</MaxValue>
        </NumberRange>
        <RendererRef ref="NumberAxisRenderer4" />
