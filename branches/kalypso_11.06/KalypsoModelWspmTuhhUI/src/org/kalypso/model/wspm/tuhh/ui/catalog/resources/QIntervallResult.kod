<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ChartConfiguration xmlns="http://www.openali.de/odysseus/chartconfig/0.2.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.openali.de/odysseus/chartconfig/0.2.0/../../../../../../../../../../OdysseusChartFactory/etc/binding/schema/ChartConfiguration.xsd">
  <Chart id="runoffChart">
    <Title horizontalTextAnchor="CENTER" verticalTextAnchor="TOP" horizontalPosition="CENTER" verticalPosition="TOP">Abfluss</Title>
    <Description>Abflusswerte der Berechnung</Description>
    <Layers>

      <Layer visible="true" legend="false" id="runoffGridLayer">
        <Provider epid="org.kalypso.zml.ui.chart.layer.provider.ZmlGridLayerProvider">
          <Parameters>
          </Parameters>
        </Provider>
        <Title>Gitterlinien</Title>
        <Description />
        <MapperRefs>
          <DomainAxisRef ref="wsp_Axis" />
          <TargetAxisRef ref="runoff_Axis" />
        </MapperRefs>
        <Styles>
          <LineStyle role="gridLineStyle">
            <Width>1</Width>
            <LineColor>CCCCCC</LineColor>
            <DashOffset>0</DashOffset>
            <DashArray>2 2 2</DashArray>
            <LineCap>ROUND</LineCap>
            <LineJoin>ROUND</LineJoin>
            <MiterLimit>1</MiterLimit>
          </LineStyle>
          <!-- THIS IS THE STLYE OF THE BORDER... (even if it is a point style) -->
          <PointStyle role="gridPointStyle">
            <Stroke>
              <Width>2</Width>
              <LineColor>000000</LineColor>
            </Stroke>
            <FillColor isVisible="false">
              <Color>000000</Color>
            </FillColor>
          </PointStyle>
        </Styles>
      </Layer>

      <Layer visible="true" id="runoffLayer">
        <Provider epid="org.kalypso.model.wspm.ui.featureview.TupleResultLineLayerProvider">
          <Parameters>
            <Parameter name="featureKey" value="theResultFeatureKey" />
            <Parameter name="propertyName" value="{org.kalypso.model.wspm.tuhh}pointsMember" />
            <Parameter name="showPoints" value="true" />
            <Parameter name="showLines" value="false" />
            <Parameter name="domainComponentId" value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#Depth" />
            <Parameter name="valueComponentId" value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#Runoff" />
          </Parameters>
        </Provider>
        <Title>Abfluss</Title>
        <Description>Abfluss abhängig vom Wasserspiegel</Description>
        <MapperRefs>
          <DomainAxisRef ref="wsp_Axis" />
          <TargetAxisRef ref="runoff_Axis" />
        </MapperRefs>
        <Styles>
          <LineStyle role="line" isVisible="false">
            <Title></Title>
          </LineStyle>
          <PointStyle role="point">
            <Title>Abfluss</Title>
            <Width>8</Width>
            <Height>8</Height>
            <Stroke>
              <Width>1</Width>
              <LineColor>000000</LineColor>
            </Stroke>
            <OvalMarker>
            </OvalMarker>
            <FillColor>
              <Color>ff0000</Color>
            </FillColor>
          </PointStyle>
        </Styles>
      </Layer>

      <Layer visible="true" id="runoffPolynomeLayer">
        <Provider epid="org.kalypso.model.wspm.ui.featureview.PolynomeLayerProvider">
          <Parameters>
            <Parameter name="featureKey" value="theResultFeatureKey" />
            <Parameter name="propertyName" value="{org.kalypso.model.wspm.tuhh}polynomialMember" />
            <Parameter name="domainId" value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#phenomenonRunoff" />
          </Parameters>
        </Provider>
        <Title>Abflusspolynome</Title>
        <Description>Abflusspolynome abhängig vom Wasserspiegel</Description>
        <MapperRefs>
          <DomainAxisRef ref="wsp_Axis" />
          <TargetAxisRef ref="runoff_Axis" />
        </MapperRefs>
        <Styles>
          <PointStyle role="point" isVisible="false">
            <Title></Title>
          </PointStyle>
          <LineStyle role="line">
            <Title>Abflusspolynome</Title>
            <Width>3</Width>
            <LineColor>00aaff</LineColor>
          </LineStyle>
        </Styles>
      </Layer>
    </Layers>

    <Mappers>
      <Axis id="wsp_Axis">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Label>Wassertiefe [m]</Label>
        <Direction>POSITIVE</Direction>
        <Position>BOTTOM</Position>
        <NumberRange>
          <MinValue>0</MinValue>
          <MaxValue>100</MaxValue>
        </NumberRange>
        <RendererRef ref="NumberAxisRenderer" />
      </Axis>
      <Axis id="runoff_Axis">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Label>Abfluss [m³/s]</Label>
        <Direction>POSITIVE</Direction>
        <Position>LEFT</Position>
        <NumberRange>
          <MinValue>5</MinValue>
          <MaxValue>10</MaxValue>
        </NumberRange>
        <RendererRef ref="NumberAxisRenderer" />
      </Axis>
      <Axis id="runoffDelta_Axis">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Label>Abfluss-Delta [m³/s]</Label>
        <Direction>POSITIVE</Direction>
        <Position>LEFT</Position>
        <NumberRange>
          <MinValue>5</MinValue>
          <MaxValue>10</MaxValue>
        </NumberRange>
        <RendererRef ref="NumberAxisRenderer" />
      </Axis>
    </Mappers>
    <Renderers>
      <AxisRenderer id="NumberAxisRenderer">
        <Provider epid="de.openali.odysseus.chart.ext.base.axisrenderer.provider.GenericNumberAxisRendererProvider">
          <Parameters>
            <Parameter name="inset_tick" value="1" />
            <Parameter name="hide_cut" value="false" />
          </Parameters>
        </Provider>
      </AxisRenderer>
    </Renderers>
  </Chart>

