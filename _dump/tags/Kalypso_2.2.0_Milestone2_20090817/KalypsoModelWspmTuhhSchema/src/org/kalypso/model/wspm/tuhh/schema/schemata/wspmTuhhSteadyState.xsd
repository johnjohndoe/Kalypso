	<complexType name="CalculationWspmTuhhSteadyStateType">
		<complexContent>
			<extension base="wspm:AbstractCalculationType">
				<sequence>
					<element ref="tuhh:fliessgesetz" />
					<element name="mode" default="WATERLEVEL">
						<annotation>
							<documentation xml:lang="de">
								<label>Berechnungsart</label>
								<tooltip>
									<![CDATA[Auswahl der Berechnungsart (Wasserspiegellagenberechnung o. Bordvollberechnung)]]>
								</tooltip>
								<description>
									<![CDATA[Auswahl der Berechnungsart (Wasserspiegellagenberechnung o. Bordvollberechnung)]]>
								</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Type of calculation</label>
								<tooltip>
									<![CDATA[Choose type of calculation (water surface or bank-full)]]>
								</tooltip>
								<description>
									Choose type of calculation (water
									surface or
									bank-full)
								</description>
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<enumeration value="WATERLEVEL">
									<annotation>
										<documentation xml:lang="de">
											<label>
												Wasserspiegellagenberechnung
											</label>
											<tooltip>
												Wasserspiegellagenberechnung
											</tooltip>
											<description>
												Wasserspiegellagenberechnung
											</description>
										</documentation>
										<documentation xml:lang="en">
											<label>
												<![CDATA[Water surface calculation]]>
											</label>
											<tooltip>
												<![CDATA[Water surface calculation]]>
											</tooltip>
											<description>
												Water surface
												calculation
											</description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="BF_UNIFORM">
									<annotation>
										<documentation xml:lang="de">
											<label>
												<![CDATA[Bordvollberechnung - stationär-gleichförmige Abflussverhältnisse]]>
											</label>
											<tooltip>
