					<element name="useExtremeRoughness" type="boolean" default="true">
						<annotation>
							<documentation xml:lang="de">
								<label>
									Extreme Rauhigkeit berücksichtigen
								</label>
								<tooltip>
									<![CDATA[falls aktiv, wird bei geringen Wassertiefen der Ansatz für extreme Rauheiten verwendet.]]>
								</tooltip>
							</documentation>
							<documentation xml:lang="en">
								<label>
								<![CDATA[use extreme roughnes]]>
								</label>
								<tooltip>
									<![CDATA[if activated, the method of 'extreme roughness' will be used for small waterdepths]]>
								</tooltip>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
