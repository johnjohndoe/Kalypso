<schema xmlns:gml="http://www.opengis.net/gml" xmlns:om="http://www.opengis.net/om"
	xmlns="http://www.w3.org/2001/XMLSchema" xmlns:wspm="org.kalypso.model.wspm"
	xmlns:runoff="org.kalypso.model.wspmrunoff" xmlns:wspmcommon="org.kalypso.model.wspmcommon"
	xmlns:wspmprofile="org.kalypso.model.wspmprofile" targetNamespace="org.kalypso.model.wspm"
	elementFormDefault="qualified" version="0.1">
	<annotation>
		<appinfo xmlns:kapp="org.kalypso.appinfo">
			<kapp:gmlVersion>3.1.1</kapp:gmlVersion>
		</appinfo>
	</annotation>
	<import namespace="org.kalypso.model.wspmcommon" schemaLocation="wspmCommons.xsd" />
	<import namespace="org.kalypso.model.wspmrunoff" schemaLocation="runOff.xsd" />
	<import namespace="http://www.opengis.net/gml"
		schemaLocation="http://schemas.opengis.net/gml/3.1.1/base/gml.xsd" />
	<import namespace="http://www.opengis.net/om"
		schemaLocation="http://dev.bjoernsen.de/ogc/schema/om/1.0.30/om.xsd" />
	<import namespace="http://www.opengis.net/swe"
		schemaLocation="http://dev.bjoernsen.de/ogc/schema/sweCommon/1.0.30/swe.xsd" />
	<import namespace="org.kalypso.model.wspmprofile"
		schemaLocation="profile.xsd" />
	<!-- schema for WSPM = 'water surface profile model' -->
	<!--
		_________________________________________________________________________________________
		ELEMENTS
		_________________________________________________________________________________________
	-->
	<element name="calculationMember" type="wspm:CalculationPropertyType"
		substitutionGroup="gml:featureMember">
		<annotation>
			<documentation>WSPM calculations</documentation>
		</annotation>
	</element>
	<element name="AbstractCalculation" abstract="true"
		type="wspm:AbstractCalculationType" substitutionGroup="gml:_Feature" />
	<!--
	-->
	<element name="waterBodyMember" type="wspm:WaterBodyPropertyType">
		<annotation>
			<documentation>Water bodies</documentation>
		</annotation>
	</element>
	<element name="WaterBody" type="wspm:WaterBodyType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation>River</documentation>
		</annotation>
	</element>
	<!--
	-->
	<element name="reachMember" type="wspm:ReachPropertyType">
		<annotation>
			<documentation>River string</documentation>
		</annotation>
	</element>
	<element name="AbstractReach" type="wspm:AbstractReachType"
		abstract="true" substitutionGroup="gml:_Feature">
		<annotation>
			<documentation>River string</documentation>
		</annotation>
	</element>
	<element name="roughnessDefinitionCollectionMember" type="wspm:RoughnessDefinitionCollectionPropertyType"
		substitutionGroup="gml:featureMember">
		<annotation>
			<documentation>Roughness definition lists</documentation>
		</annotation>
	</element>
	<element name="RoughnessDefinitionCollection" type="wspm:RoughnessDefinitionCollectionType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation>Roughness definition list</documentation>
		</annotation>
	</element>
	<!--
	-->
	<element name="roughnessDefinitionMember" type="wspm:RoughnessDefinitionPropertyType"
		substitutionGroup="gml:featureMember">
		<annotation>
			<documentation>Roughness definitions</documentation>
		</annotation>
	</element>
	<element name="RoughnessDefinition" type="wspm:RoughnessDefinitionType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation>Roughness definition</documentation>
		</annotation>
	</element>
	<!--
	-->
	<element name="vegetationDefinitionCollectionMember" type="wspm:VegetationDefinitionCollectionPropertyType"
		substitutionGroup="gml:featureMember">
		<annotation>
			<documentation>Vegetation definition lists</documentation>
		</annotation>
	</element>
	<element name="VegetationDefinitionCollection" type="wspm:VegetationDefinitionCollectionType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation>Vegetation definition list</documentation>
		</annotation>
	</element>
	<!--
	-->
	<element name="vegetationDefinitionMember" type="wspm:VegetationDefinitionPropertyType"
		substitutionGroup="gml:featureMember">
		<annotation>
			<documentation>Vegetation definitions</documentation>
		</annotation>
	</element>
	<element name="VegetationDefinition" type="wspm:VegetationDefinitionType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation>Vegetation definition</documentation>
		</annotation>
	</element>
	<!--
	-->
	<element name="CalcCreation" type="wspm:calcCreationType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation>Calculation creation</documentation>
		</annotation>
	</element>
	<!--
		_________________________________________________________________________________________
		TYPES
		_________________________________________________________________________________________
	-->
	<complexType name="CalculationPropertyType">
		<complexContent>
			<restriction base="gml:FeaturePropertyType">
				<sequence>
					<element ref="wspm:AbstractCalculation" />
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<complexType name="AbstractCalculationType" abstract="true">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element name="calcCreationMember" type="wspm:calcCreationInlinePropertyType">
						<annotation>
							<documentation>Calculation creation</documentation>
						</annotation>
					</element>
					<!--
						element ref="wspm:resultMember" minOccurs="0"
						maxOccurs="unbounded"/
					-->
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!--
	-->
	<complexType name="ReachPropertyType">
		<sequence>
			<element ref="wspm:AbstractReach" />
		</sequence>
	</complexType>
	<complexType name="AbstractReachType" abstract="true">
		<complexContent>
			<extension base="gml:AbstractFeatureType" />
		</complexContent>
	</complexType>
	<complexType name="WaterBodyPropertyType">
		<sequence minOccurs="0">
			<element ref="wspm:WaterBody" />
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup" />
	</complexType>
	<complexType name="WaterBodyLinkPropertyType">
		<sequence minOccurs="0" maxOccurs="0">
			<element ref="wspm:WaterBody" />
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup" />
	</complexType>
	<complexType name="WaterBodyType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element name="refNr" type="string" nillable="true">
						<annotation>
							<documentation>River reference number (optional)</documentation>
						</annotation>
					</element>
					<element name="waterBodyCenterLine" type="gml:LineStringSegmentType">
						<annotation>
							<documentation>River center line</documentation>
						</annotation>
					</element>
					<element name="isDirectionUpstream" type="boolean" default="true">
						<annotation>
							<documentation><![CDATA[ Stationing direction.Selection means that stationing will be against flow direction. ]]></documentation>
						</annotation>
					</element>
					<element name="profileMember" minOccurs="0" maxOccurs="unbounded"
						type="wspm:ProfileInlinePropertyType">
						<annotation>
							<documentation>Profiles</documentation>
						</annotation>
					</element>
					<element ref="wspm:reachMember" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>River strings</documentation>
						</annotation>
					</element>
					<element name="runOffEventMember" type="runoff:RunOffEventInlinePropertyType"
						minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Discharge events</documentation>
						</annotation>
					</element>
					<element name="waterlevelFixationMember" type="runoff:WaterlevelFixationInlinePropertyType"
						minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Water surface fixings</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ProfileInlinePropertyType">
		<sequence minOccurs="1" maxOccurs="1">
			<element ref="wspmprofile:Profile" />
		</sequence>
	</complexType>
	<complexType name="ProfileLinkPropertyType">
		<sequence minOccurs="0" maxOccurs="0">
			<element ref="wspmprofile:Profile" />
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup" />
	</complexType>
