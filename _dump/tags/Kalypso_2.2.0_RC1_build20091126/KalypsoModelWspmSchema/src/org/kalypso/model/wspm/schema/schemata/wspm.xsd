		base="gml:FeaturePropertyType"> <sequence> <element
		ref="wspm:Result"/> </sequence> </restriction> </complexContent>
		</complexType> <complexType name="ResultType"> <complexContent>
		<extension base="wspmcommon:EmptyFeatureBaseType"> <sequence> <element
