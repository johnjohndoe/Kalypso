	<complexType name="CalculationWspmTuhhSteadyStateType">
		<complexContent>
			<extension base="wspm:AbstractCalculationType">
				<sequence>
					<element ref="tuhh:fliessgesetz" />
					<element name="mode" default="WATERLEVEL">
						<annotation>
							<documentation xml:lang="de">
								<label>Berechnungsart</label>
								<tooltip>
									Auswahl der Berechnungsart (Wasserspiegellagenberechnung o. Bordvollberechnung)
								</tooltip>
								<description>
									Auswahl der Berechnungsart (Wasserspiegellagenberechnung o. Bordvollberechnung)
								</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Type of calculation</label>
								<tooltip>Choose type of calculation (water surface or bank-full)</tooltip>
								<description>Choose type of calculation (water surface or bank-full)</description>
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<enumeration value="WATERLEVEL">
									<annotation>
										<documentation xml:lang="de">
											<label>Wasserspiegellagenberechnung</label>
											<tooltip>Wasserspiegellagenberechnung</tooltip>
											<description>Wasserspiegellagenberechnung</description>
										</documentation>
										<documentation xml:lang="en">
											<label>Water surface calculation</label>
											<tooltip>Water surface calculation</tooltip>
											<description>Water surface calculation</description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="BF_UNIFORM">
									<annotation>
										<documentation xml:lang="de">
											<label>
												<![CDATA[Bordvollberechnung - stationär-gleichförmige Abflussverhältnisse]]>
											</label>
											<tooltip>
												<![CDATA[ Bordvollberechnung - stationär-gleichförmige Abflussverhältnisse ]]>
											</tooltip>
											<description>
												<![CDATA[ Bordvollberechnung -	stationär-gleichförmige Abflussverhältnisse ]]>
											</description>
										</documentation>
										<documentation xml:lang="en">
											<label>
												<![CDATA[Bank-full - steady-uniform discharge]]>
											</label>
											<tooltip>
												<![CDATA[ Bank-full - steady-uniform discharge ]]>
											</tooltip>
											<description>
												<![CDATA[ Bank-full - steady-uniform discharge ]]>
											</description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="BF_NON_UNIFORM">
									<annotation>
										<documentation xml:lang="de">
											<label>
												<![CDATA[ Bordvollberechnung - stationär-ungleichförmige Abflussverhältnisse ]]>
											</label>
											<tooltip>
												<![CDATA[ Bordvollberechnung - stationär-ungleichförmige Abflussverhältnisse ]]>
											</tooltip>
											<description>
												<![CDATA[ Bordvollberechnung - stationär-ungleichförmige	Abflussverhältnisse ]]>
											</description>
										</documentation>
										<documentation xml:lang="en">
											<label>
												<![CDATA[ Bank-full - steady-non-uniform discharge ]]>
											</label>
											<tooltip>
												<![CDATA[ Bank-full - steady-non-uniform discharge ]]>
											</tooltip>
											<description>
												<![CDATA[ Bank-full - steady-non-uniform discharge ]]>
											</description>
										</documentation>
									</annotation>
								</enumeration>
							</restriction>
						</simpleType>
					</element>
					<element name="reachWspmTuhhSteadyStateMember" type="tuhh:ReachWspmTuhhSteadyStatePropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Gewässerstrang</label>
								<tooltip>
									Auswahl des Gewässerstrangs, für den die Berechnung durchgeführt wird.
								</tooltip>
								<description>
									Auswahl des Gewässerstrangs, für den die Berechnung durchgeführt wird.
								</description>
							</documentation>
							<documentation xml:lang="en">
								<label>River string</label>
								<tooltip>Selection of river string included in calculation.</tooltip>
								<description>Selection of river string included in calculation.</description>
							</documentation>
						</annotation>
					</element>
					<element name="subReachDefinitionMember" type="tuhh:subReachDefinitionPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Berechnungsabschnitt</label>
								<tooltip>Berechnungsabschnitt</tooltip>
								<description>Berechnungsabschnitt</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Calculation segment</label>
								<tooltip>Calculation segment</tooltip>
								<description>Calculation segment</description>
							</documentation>
						</annotation>
					</element>
					<!-- TODO: not used at this moment -->
					<!--element ref="wspm:roughnessDefinitionCollectionMember" minOccurs="0"/-->
					<!--element ref="wspm:vegetationDefinitionCollectionMember" minOccurs="0"/-->
					<element name="waterlevelParameterMember" type="tuhh:WaterlevelParameterPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Numerische Parameter</label>
								<tooltip>Numerische Parameter</tooltip>
								<description>Numerische Parameter</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Numerical parameter</label>
								<tooltip>Numerical parameter</tooltip>
								<description>Numerical parameter</description>
							</documentation>
						</annotation>
					</element>
					<element name="startConditionMember" type="tuhh:StartConditionPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Untere Randbedingung (1. Profil)</label>
								<tooltip>Untere Randbedingung (am 1. Profil)</tooltip>
								<description>Untere Randbedingung (am 1. Profil)</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Lower boundary condition (1. profile)</label>
								<tooltip>Lower boundary condition (1. profile)</tooltip>
								<description>Lower boundary condition (1. profile)</description>
							</documentation>
						</annotation>
					</element>
					<!-- TODO force it to be one of the water body's runOffEvents? -->
					<element name="runOffEventMember" type="runoff:RunOffEventLinkPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Abflussereignis</label>
								<tooltip>Auswahl des Abflussereignisses</tooltip>
								<description>Auswahl des Abflussereignisses</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Discharge event</label>
								<tooltip>Selection of discharge event</tooltip>
								<description>Selection of discharge event</description>
							</documentation>
						</annotation>
					</element>
					<element name="runOffIntervalMember" type="tuhh:RunOffIntervalPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Abflussintervall</label>
								<tooltip>
									Definition eines Abflussintervalls (nur bei Bordvollrechnung und Kalinin-Miljukov)
								</tooltip>
								<description>
									Definition eines Abflussintervalls (nur bei Bordvollrechnung und Kalinin-Miljukov)
								</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Discharge interval</label>
								<tooltip>
									Definition of discharge interval (only for bank-full and Kalinin-Miljukov
									calculations)
								</tooltip>
								<description>
									Definition of discharge interval (only for bank-full and Kalinin-Miljukov
									calculations)
								</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

