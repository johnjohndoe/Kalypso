	<complexType name="CalculationWspmTuhhSteadyStateType">
		<complexContent>
			<extension base="wspm:AbstractCalculationType">
				<sequence>
					<element name="fliessgesetz" default="DARCY_WEISBACH_MIT_FORMEINFLUSS">
						<annotation>
							<documentation xml:lang="de">
								<label>Fließgesetz</label>
								<tooltip>Auswahl des anzuwendenden Fließgesetzes (Darcy-Weißbach o. Manning-Strickler)</tooltip>
								<description>Auswahl des anzuwendenden Fließgesetzes (Darcy-Weißbach o. Manning-Strickler)</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Fluid model</label>
								<tooltip>Selection of used fluid model (Darcy-Weißbach or Manning-Strickler)</tooltip>
								<description>Selection of used fluid model (Darcy-Weißbach or Manning-Strickler)</description>
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<enumeration value="DARCY_WEISBACH_OHNE_FORMEINFLUSS">
									<annotation>
										<documentation xml:lang="de">
											<label><![CDATA[ Darcy-Weisbach (Rohrströmungen, äquivalente Sandrauheiten, ks) ]]></label>
											<tooltip><![CDATA[ Darcy-Weisbach (Rohrströmungen, äquivalente Sandrauheiten, ks) ]]></tooltip>
											<description><![CDATA[ Fließgesetz nach Darcy-Weisbach (Rohrströmungen, äquivalente Sandrauheiten, ks) ]]></description>
										</documentation>
										<documentation xml:lang="en">
											<label><![CDATA[ Darcy-Weisbach (pipe flows, equivalent sand roughnesses, ks) ]]></label>
											<tooltip><![CDATA[ Darcy-Weisbach (pipe flows, equivalent sand roughnesses, ks) ]]></tooltip>
											<description><![CDATA[ Darcy-Weisbach fluid model(pipe flows, equivalent sand roughnesses, ks) ]]></description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="DARCY_WEISBACH_MIT_FORMEINFLUSS">
									<annotation>
										<documentation xml:lang="de">
											<label><![CDATA[ Darcy-Weisbach (offene Gerinne mit Formeinfluss, äquivalente Sandrauheiten, ks) ]]></label>
											<tooltip><![CDATA[ Darcy-Weisbach (offene Gerinne mit Formeinfluss, äquivalente Sandrauheiten, ks) ]]></tooltip>
											<description><![CDATA[ Fließgesetz nach Darcy-Weisbach (offene Gerinne mit Formeinfluss, äquivalente Sandrauheiten, ks) ]]></description>
										</documentation>
										<documentation xml:lang="en">
											<label><![CDATA[ Darcy-Weisbach (open channels with profile impact, equivalent sand roughnesses, ks) ]]></label>
											<tooltip><![CDATA[ Darcy-Weisbach (open channels with profile impact,	equivalent sand roughnesses, ks) ]]></tooltip>
											<description><![CDATA[ Darcy-Weisbach fluid model (open channels with profile impact,	equivalent sand roughnesses, ks) ]]></description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="MANNING_STRICKLER">
									<annotation>
										<documentation xml:lang="de">
											<label><![CDATA[ Gauckler-Manning-Strickler (Rauheitsbeiwerte, kst) ]]></label>
											<tooltip><![CDATA[ Gauckler-Manning-Strickler (Rauheitsbeiwerte, kst) ]]></tooltip>
											<description><![CDATA[ Fließgesetz nach Gauckler-Manning-Strickler (Rauheitsbeiwerte, kst) ]]></description>
										</documentation>
										<documentation xml:lang="en">
											<label><![CDATA[ Gauckler-Manning-Strickler (roughness coefficients, kst) ]]></label>
											<tooltip><![CDATA[ Gauckler-Manning-Strickler (roughness coefficients, kst) ]]></tooltip>
											<description><![CDATA[ Gauckler-Manning-Strickler fluid model (roughness coefficients, kst) ]]></description>
										</documentation>
									</annotation>
								</enumeration>
							</restriction>
						</simpleType>
					</element>
					<element name="mode" default="WATERLEVEL">
						<annotation>
							<documentation xml:lang="de">
								<label>Berechnungsart</label>
								<tooltip>Auswahl der Berechnungsart (Wasserspiegellagenberechnung o. Bordvollberechnung)</tooltip>
								<description>Auswahl der Berechnungsart (Wasserspiegellagenberechnung o. Bordvollberechnung)</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Type of calculation</label>
								<tooltip>Choose type of calculation (water level or bank-full)</tooltip>
								<description>Choose type of calculation (water level or bank-full)</description>
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<enumeration value="WATERLEVEL">
									<annotation>
										<documentation xml:lang="de">
											<label>Wasserspiegellagenberechnung</label>
											<tooltip>Wasserspiegellagenberechnung</tooltip>
											<description>Wasserspiegellagenberechnung</description>
										</documentation>
										<documentation xml:lang="en">
											<label>Water level calculation</label>
											<tooltip>Water level calculation</tooltip>
											<description>Water level calculation</description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="BF_UNIFORM">
									<annotation>
										<documentation xml:lang="de">
											<label><![CDATA[Bordvollberechnung - stationär-gleichförmige Abflussverhältnisse]]></label>
											<tooltip><![CDATA[ Bordvollberechnung - stationär-gleichförmige Abflussverhältnisse ]]></tooltip>
											<description><![CDATA[ Bordvollberechnung -	stationär-gleichförmige Abflussverhältnisse ]]></description>
										</documentation>
										<documentation xml:lang="en">
											<label><![CDATA[Bank-full - steady-uniform discharge]]></label>
											<tooltip><![CDATA[ Bank-full - steady-uniform discharge ]]></tooltip>
											<description><![CDATA[ Bank-full - steady-uniform discharge ]]></description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="BF_NON_UNIFORM">
									<annotation>
										<documentation xml:lang="de">
											<label><![CDATA[ Bordvollberechnung - stationär-ungleichförmige Abflussverhältnisse ]]></label>
											<tooltip><![CDATA[ Bordvollberechnung - stationär-ungleichförmige Abflussverhältnisse ]]></tooltip>
											<description><![CDATA[ Bordvollberechnung - stationär-ungleichförmige	Abflussverhältnisse ]]></description>
										</documentation>
										<documentation xml:lang="en">
											<label><![CDATA[ Bank-full - steady-non-uniform discharge ]]></label>
											<tooltip><![CDATA[ Bank-full - steady-non-uniform discharge ]]></tooltip>
											<description><![CDATA[ Bank-full - steady-non-uniform discharge ]]></description>
										</documentation>
									</annotation>
								</enumeration>
							</restriction>
						</simpleType>
					</element>
					<element name="reachWspmTuhhSteadyStateMember"
						type="tuhh:ReachWspmTuhhSteadyStatePropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Gewässerstrang</label>
								<tooltip>Auswahl des Gewässerstrangs, für den die Berechnung durchgeführt wird.</tooltip>
								<description>Auswahl des Gewässerstrangs, für den die Berechnung durchgeführt wird.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Reach</label>
								<tooltip>Selection of reach included in calculation.</tooltip>
