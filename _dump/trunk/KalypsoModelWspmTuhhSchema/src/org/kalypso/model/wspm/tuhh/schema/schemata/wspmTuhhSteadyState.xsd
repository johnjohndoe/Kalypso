							</documentation>
						</annotation>
					</element>
					<element name="subReachDefinitionMember"
						type="tuhh:subReachDefinitionPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Berechnungsabschnitt</label>
								<tooltip>Berechnungsabschnitt</tooltip>
								<description>Berechnungsabschnitt</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Calculation segment</label>
								<tooltip>Calculation segment</tooltip>
								<description>Calculation segment</description>
							</documentation>
						</annotation>
					</element>
					<!-- TODO: not used at this moment -->
					<!--element ref="wspm:roughnessDefinitionCollectionMember" minOccurs="0"/-->
					<!--element ref="wspm:vegetationDefinitionCollectionMember" minOccurs="0"/-->
					<element name="waterlevelParameterMember"
						type="tuhh:WaterlevelParameterPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Numerische Parameter</label>
								<tooltip>Numerische Parameter</tooltip>
								<description>Numerische Parameter</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Numerical parameter</label>
								<tooltip>Numerical parameter</tooltip>
								<description>Numerical parameter</description>
							</documentation>
						</annotation>
					</element>
					<element name="startConditionMember" type="tuhh:StartConditionPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Untere Randbedingung (1. Profil)</label>
								<tooltip>Untere Randbedingung (am 1. Profil)</tooltip>
								<description>Untere Randbedingung (am 1. Profil)</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Lower boundary condition (1. profile)</label>
								<tooltip>Lower boundary condition (1. profile)</tooltip>
								<description>Lower boundary condition (1. profile)</description>
