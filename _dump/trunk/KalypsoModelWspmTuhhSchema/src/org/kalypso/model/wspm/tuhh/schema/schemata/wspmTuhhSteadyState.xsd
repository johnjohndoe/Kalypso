	<complexType name="WaterlevelParameterPropertyType">
		<complexContent>
			<restriction base="gml:FeaturePropertyType">
				<sequence>
					<element ref="tuhh:WaterlevelParameter"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<complexType name="WaterlevelParameterType">
		<complexContent>
			<extension base="wspmcommon:EmptyFeatureBaseType">
				<sequence>
					<element name="wspIteration" default="SIMPLE">
						<annotation>
							<documentation xml:lang="de">
								<label>Iterationsgenauigkeit</label>
								<tooltip>Iterationsgenauigkeit</tooltip>
								<description>Iterationsgenauigkeit (Berechnung des
								Wasserspiegels)</description>
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<enumeration value="SIMPLE">
									<annotation>
										<documentation xml:lang="de">
											<label>einfache Iteration</label>
											<tooltip>einfache Iteration</tooltip>
											<description>Iterationsgenauigkeit: einfache
											Iteration</description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="EXACT">
									<annotation>
										<documentation xml:lang="de">
											<label>genaue Berechnung durch Einschlussintervall</label>
											<tooltip>genaue Berechnung durch Einschlussintervall</tooltip>
											<description>Iterationsgenauigkeit: genaue Berechnung
												durch Einschlussintervall</description>
										</documentation>
									</annotation>
								</enumeration>
							</restriction>
						</simpleType>
					</element>
					<element name="verzoegerungsverlust" default="BJOERNSEN">
						<annotation>
							<documentation xml:lang="de">
								<label>Verzögerungsverlust</label>
								<tooltip>Verzögerungsverlust</tooltip>
								<description>Verzögerungsverlust</description>
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<enumeration value="DVWK">
									<annotation>
										<documentation xml:lang="de">
											<label>nach DVWK (beta = 0.67)</label>
											<tooltip>nach DVWK (beta = 0.67)</tooltip>
											<description>Verzögerungsverlust nach DVWK (beta =
											0.67)</description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="BJOERNSEN">
									<annotation>
										<documentation xml:lang="de">
											<label>nach Björnsen (beta = 0.5)</label>
											<tooltip>nach Björnsen (beta = 0.5)</tooltip>
											<description>Verzögerungsverlust nach Björnsen (beta =
												0.5)</description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="DFG">
									<annotation>
										<documentation xml:lang="de">
											<label>nach DFG (beta = 2 / (1+Ai + 1/Ai) )</label>
											<tooltip>nach DFG (beta = 2 / (1+Ai + 1/Ai) )</tooltip>
											<description>Verzögerungsverlust nach DFG (beta = 2 /
												(1+Ai + 1/Ai) )</description>
										</documentation>
									</annotation>
								</enumeration>
							</restriction>
						</simpleType>
					</element>
					<element name="reibungsverlust" default="TRAPEZ_FORMULA">
						<annotation>
							<documentation xml:lang="de">
								<label>Berechnung von Reibungsverlusten</label>
								<tooltip>Berechnung von Reibungsverlusten</tooltip>
								<description>Berechnung von Reibungsverlusten</description>
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<enumeration value="TRAPEZ_FORMULA">
									<annotation>
										<documentation xml:lang="de">
											<label>nach Trapezformel</label>
											<tooltip>nach Trapezformel</tooltip>
											<description>Berechnung von Reibungsverlusten nach
												Trapezformel</description>
										</documentation>
