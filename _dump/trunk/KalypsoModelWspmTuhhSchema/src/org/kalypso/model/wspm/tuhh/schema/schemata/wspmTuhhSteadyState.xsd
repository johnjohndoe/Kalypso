	<complexType name="CalculationWspmTuhhSteadyStateType">
		<complexContent>
			<extension base="wspm:AbstractCalculationType">
				<sequence>
					<element name="fliessgesetz" default="DARCY_WEISBACH_MIT_FORMEINFLUSS">
						<annotation>
							<documentation xml:lang="de">
								<label>Fließgesetz</label>
								<tooltip>Fließgesetz</tooltip>
								<description>verwendetes Fließgesetz</description>
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<enumeration value="DARCY_WEISBACH_OHNE_FORMEINFLUSS">
									<annotation>
										<documentation xml:lang="de">
											<label>Darcy-Weisbach (Rohrströmungen, äquivalente Sandrauheiten, ks)</label>
											<tooltip>Darcy-Weisbach (Rohrströmungen, äquivalente Sandrauheiten, ks)</tooltip>
											<description>Fließgesetz nach Darcy-Weisbach (Rohrströmungen, äquivalente Sandrauheiten, ks)</description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="DARCY_WEISBACH_MIT_FORMEINFLUSS">
									<annotation>
										<documentation xml:lang="de">
											<label>Darcy-Weisbach (offene Gerinne mit Formeinfluss, äquivalente Sandrauheiten, ks)</label>
											<tooltip>Darcy-Weisbach (offene Gerinne mit Formeinfluss, äquivalente Sandrauheiten, ks)</tooltip>
											<description>Fließgesetz nach Darcy-Weisbach (offene Gerinne mit Formeinfluss, äquivalente Sandrauheiten, ks)</description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="MANNING_STRICKLER">
									<annotation>
										<documentation xml:lang="de">
											<label>Gauckler-Manning-Strickler (Rauheitsbeiwerte, kst)</label>
											<tooltip>Gauckler-Manning-Strickler (Rauheitsbeiwerte, kst)</tooltip>
											<description>Fließgesetz nach Gauckler-Manning-Strickler (Rauheitsbeiwerte, kst)</description>
										</documentation>
									</annotation>
								</enumeration>
							</restriction>
						</simpleType>
					</element>
					<element name="mode" default="WATERLEVEL">
						<annotation>
							<documentation xml:lang="de">
								<label>Berechnungsart</label>
								<tooltip>Berechnungsart</tooltip>
								<description>Berechnungsart</description>
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<enumeration value="WATERLEVEL">
									<annotation>
										<documentation xml:lang="de">
											<label>Wasserspiegellagenberechnung</label>
											<tooltip>Wasserspiegellagenberechnung</tooltip>
											<description>Wasserspiegellagenberechnung</description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="BF_UNIFORM">
									<annotation>
										<documentation xml:lang="de">
											<label><![CDATA[Bordvollberechnung - stationär-gleichförmige Abflussverhältnisse]]></label>
											<tooltip><![CDATA[ Bordvollberechnung - stationär-gleichförmige Abflussverhältnisse ]]></tooltip>
											<description><![CDATA[ Bordvollberechnung -	stationär-gleichförmige Abflussverhältnisse ]]></description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="BF_NON_UNIFORM">
									<annotation>
										<documentation xml:lang="de">
											<label>Bordvollberechnung - stationär-ungleichförmige Abflussverhältnisse</label>
											<tooltip>Bordvollberechnung - stationär-ungleichförmige Abflussverhältnisse</tooltip>
											<description>Bordvollberechnung - stationär-ungleichförmige Abflussverhältnisse</description>
										</documentation>
									</annotation>
								</enumeration>
							</restriction>
						</simpleType>
					</element>
					<element ref="tuhh:reachWspmTuhhSteadyStateMember">
						<annotation>
							<documentation xml:lang="de">
								<label>Strang, siehe Tooltip</label>
								<tooltip>Beispiel für eine Annotation an einem referenzierenden Element.</tooltip>
								<description>Gewässerstrang</description>
							</documentation>
						</annotation>
					</element>
					<element ref="tuhh:subReachDefinitionMember"/>
					<element ref="wspm:roughnessDefinitionCollectionMember" minOccurs="0"/>
					<element ref="wspm:vegetationDefinitionCollectionMember" minOccurs="0"/>
					<element ref="tuhh:waterlevelParameterMember"/>
					<element ref="tuhh:startConditionMember"/>
					<!-- TODO force it to be one of the water body's runOffEvents? -->
					<element name="runOffEventMember" type="om:AbstractObservationPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Abflussereignis</label>
								<tooltip>Abflussereignis</tooltip>
								<description>Abflussereignis</description>
							</documentation>
						</annotation>
					</element>
					<element ref="tuhh:runOffIntervalMember"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
