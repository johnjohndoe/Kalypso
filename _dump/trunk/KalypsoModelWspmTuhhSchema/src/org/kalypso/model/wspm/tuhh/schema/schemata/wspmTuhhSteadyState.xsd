	<complexType name="StartConditionPropertyType">
		<sequence minOccurs="1" maxOccurs="1">
			<element ref="tuhh:StartCondition"/>
		</sequence>
	</complexType>
	<complexType name="StartConditionType">
		<complexContent>
			<extension base="wspmcommon:EmptyFeatureBaseType">
				<sequence>
					<element name="kind" default="CRITICAL_WATER_DEPTH">
						<annotation>
							<documentation xml:lang="de">
								<label>Art</label>
								<tooltip>Auswahl der Randbedingung am ersten Profil (Unterlauf)</tooltip>
								<description>Auswahl der Randbedingung am ersten Profil (Unterlauf)</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Type</label>
								<tooltip>Selection of boundary condition for first profile (lower reaches)</tooltip>
								<description>Selection of boundary condition for first profile (lower reaches)</description>
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<enumeration value="CRITICAL_WATER_DEPTH">
									<annotation>
										<documentation xml:lang="de">
											<label>Grenztiefe</label>
											<tooltip>Grenztiefe</tooltip>
											<description>Untere Randbedingung (1.Profil):
											Grenztiefe</description>
										</documentation>
										<documentation xml:lang="en">
											<label>Critical water depth</label>
											<tooltip>Critical water depth</tooltip>
											<description><![CDATA[ Lower boundary condition (1. profile):	critical water depth ]]></description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="WATERLEVEL">
									<annotation>
										<documentation xml:lang="de">
											<label>Anfangswasserspiegel</label>
											<tooltip>Anfangswasserspiegel</tooltip>
											<description><![CDATA[ Untere Randbedingung (1.Profil): Anfangswasserspiegel ]]></description>
										</documentation>
										<documentation xml:lang="en">
											<label>Initial water level</label>
											<tooltip>Initial water level</tooltip>
											<description><![CDATA[ Lower boundary condition (1. profile):	initial water level ]]></description>
										</documentation>
									</annotation>
								</enumeration>
								<enumeration value="UNIFORM_BOTTOM_SLOPE">
									<annotation>
										<documentation xml:lang="de">
											<label>stationär-gleichförmiges Gefälle</label>
											<tooltip>stationär-gleichförmiges Gefälle</tooltip>
