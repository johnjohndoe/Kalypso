<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            class="org.kalypso.ui.model.wspm.importwizard.WspWinImportWizard"
            icon="icons/obj16/wspwin.gif"
            id="org.kalypso.ui.model.wspm.importwizard.wspwin"
            name="WspWin Daten...">
         <description>
            Importiert Daten aus einem bestehenden WspWin Projekt
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.exportWizards">
      <wizard
            class="org.kalypso.ui.model.wspm.exportwizard.WspWinExportWizard"
            icon="icons/obj16/wspwin.gif"
            id="org.kalypso.ui.model.wspm.exportwizard.wspwin"
            name="WspWin-Format">
         <description>
            Exportiert WSP-Modell (TU-HH) als WspWin-Projekt
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="org.kalypso.ui.model.wspm.objectContribution.calcTuhh"
            objectClass="org.kalypsodeegree.model.feature.Feature">
         <filter
               name="featureType"
               value="org.kalypso.model.wspm.tuhh#CalculationWspmTuhhSteadyState"/>
         <action
               class="org.kalypso.ui.model.wspm.tuhh.actions.CalcTuhhAction"
               icon="icons/obj16/startCalulation.gif"
               id="org.kalypso.ui.model.wspm.calcTuhhAction"
               label="Berechnung starten"
               style="push"
               tooltip="Startet die selektierten Berechnungen"/>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.kalypso.ui.model.wspm.ui.objectContribution.showProfile"
            objectClass="org.kalypsodeegree.model.feature.Feature">
         <filter
               name="featureType"
               value="org.kalypso.model.wspm#ProfileReachSegment"/>
         <action
               class="org.kalypso.ui.model.wspm.ui.action.ShowProfileAction"
               id="org.kalypso.ui.model.wspm.ui.action.ShowProfileAction"
               label="Profil anzeigen"/>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.kalypso.ui.editor.mapeditor.GisMapEditor"
            class="org.kalypso.ui.model.wspm.ui.adapter.ProfileProviderAdapterFactory">
         <adapter type="com.bce.profil.ui.view.IProfilProvider2"/>
      </factory>
      <factory
            adaptableType="org.kalypso.ui.editor.gistableeditor.GisTableEditor"
            class="org.kalypso.ui.model.wspm.ui.adapter.ProfileProviderAdapterFactory">
         <adapter type="com.bce.profil.ui.view.IProfilProvider2"/>
      </factory>
      <factory
            adaptableType="org.kalypso.ui.editor.gmleditor.ui.GmlEditor"
            class="org.kalypso.ui.model.wspm.ui.adapter.ProfileProviderAdapterFactory">
         <adapter type="com.bce.profil.ui.view.IProfilProvider2"/>
      </factory>
   </extension>

</plugin>
