	<!--
		abstrakte Node-Typen allgemein
	-->
	<complexType name="AbstractSbkStructureNodeType" abstract="true">
		<complexContent>
			<extension base="wspmSobek:AbstractNetworkNodeType">
				<sequence/>
			</extension>
		</complexContent>
	</complexType>
	<!--
		abstrakter Knoten-Typ für StructureNodes, die in einer Compoundable Structure verwendet werden können
	-->
	<complexType name="AbstractSbkCompoundableStructureNodePropertyType">
		<sequence>
			<element ref="wspmSobekStructures:AbstractSbkCompoundableStructureNode"/>
		</sequence>
	</complexType>
	<complexType name="AbstractSbkCompoundableStructureNodeType" abstract="true">
		<complexContent>
			<extension base="wspmSobekStructures:AbstractSbkStructureNodeType"/>
		</complexContent>
	</complexType>
	<!--
		Structure Nodes
	-->
	<complexType name="SbkCompoundStructureNodeType">
		<complexContent>
			<extension base="wspmSobekStructures:AbstractSbkStructureNodeType">
				<sequence>
					<element ref="wspmSobekStructures:abstractSbkCompoundStructureNodeMember" minOccurs="2" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="SbkRiverWeirType">
		<complexContent>
