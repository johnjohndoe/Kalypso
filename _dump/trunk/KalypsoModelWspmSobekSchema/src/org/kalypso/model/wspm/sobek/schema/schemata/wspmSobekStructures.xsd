			<extension base="wspmSobekStructures:AbstractSbkStructureNodeType">
				
				<sequence> <element name="crestWidth" type="wspmSobekCommon:notNegDoubleType" nillable="false">
					<annotation>
						<documentation xml:lang="de">
							<label>Überfallbreite [m]</label>
							<tooltip>Überfallbreite [m]</tooltip>
							<description>Überfallbreite [m]</description>
						</documentation>
						<documentation xml:lang="en">
							<label>Crest Width [m]</label>
							<tooltip>Crest Width [m]</tooltip>
							<description>Crest Width [m]</description>
						</documentation>
					</annotation>
				</element>
					<element name="crestHeight" type="double" nillable="false">
						<annotation>
							<documentation xml:lang="de">
								<label>Überfallhöhe [m NN]</label>
								<tooltip>Überfallhöhe [m NN]</tooltip>
								<description>Überfallhöhe [m NN]</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Crest Height [m asl]</label>
								<tooltip>Crest Height [m asl]</tooltip>
								<description>Crest Height [m asl]</description>
							</documentation>
						</annotation>
					</element>
					<element name="dischargeCoeffCE" type="wspmSobekCommon:notNegDoubleType" default="1.0" nillable="false">
						<annotation>
							<documentation xml:lang="de">
								<label>Abflussbeiwert []</label>
								<tooltip>Abflussbeiwert []</tooltip>
								<description>Abflussbeiwert []</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Discharge Coefficient Ce []</label>
								<tooltip>Discharge Coefficient Ce []</tooltip>
								<description>Discharge Coefficient Ce []</description>
							</documentation>
						</annotation>
					</element>
					<element name="lateralContractionCoeffCW" type="wspmSobekCommon:notNegDoubleType" default="1.0" nillable="false">
						<annotation>
							<documentation xml:lang="de">
								<label>Einengungsverlustbeiwert []</label>
								<tooltip>Einengungsverlustbeiwert []</tooltip>
								<description>Einengungsverlustbeiwert []</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Lateral Contraction Coefficient Cw []</label>
								<tooltip>Lateral Contraction Coefficient Cw []</tooltip>
								<description>Lateral Contraction Coefficient Cw []</description>
							</documentation>
						</annotation>
					</element>
					<element name="flowDirection" type="wspmSobekCommon:sbkFlowDirectionType" default="Direction.Both" nillable="false">
						<annotation>
							<documentation xml:lang="de">
								<label>Mögliche Fließrichtung</label>
								<tooltip>Mögliche Fließrichtung</tooltip>
								<description>Mögliche Fließrichtung</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Possible Flow Direction</label>
								<tooltip>Possible Flow Direction</tooltip>
								<description>Possible Flow Direction</description>
							</documentation>
						</annotation>
					</element></sequence>
			</extension>
