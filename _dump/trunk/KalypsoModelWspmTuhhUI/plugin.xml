<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="org.kalypso.model.wspm.objectContribution.calcTuhh"
            objectClass="org.kalypsodeegree.model.feature.Feature">
         <filter
               name="featureType"
               value="org.kalypso.model.wspm.tuhh#CalculationWspmTuhhSteadyState"/>
         <action
               class="org.kalypso.model.wspm.tuhh.ui.actions.CalcTuhhAction"
               icon="icons/obj16/startCalulation.gif"
               id="org.kalypso.model.wspm.calcTuhhAction"
               label="Berechnung starten"
               style="push"
               tooltip="Startet die selektierten Berechnungen"/>
      </objectContribution>
   </extension>

   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            class="org.kalypso.model.wspm.tuhh.ui.wizards.WspWinImportWizard"
            icon="icons/obj16/wspwin.gif"
            id="org.kalypso.model.wspm.importwizard.wspwin"
            name="WspWin Daten...">
         <description>
            Importiert Daten aus einem bestehenden WspWin Projekt
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.exportWizards">
      <wizard
            class="org.kalypso.model.wspm.tuhh.ui.wizards.WspWinExportWizard"
            icon="icons/obj16/wspwin.gif"
            id="org.kalypso.model.wspm.exportwizard.wspwin"
            name="WspWin-Format">
         <description>
            Exportiert WSP-Modell (TU-HH) als WspWin-Projekt
         </description>
      </wizard>
   </extension>

     <extension
           point="org.kalypso.model.wspm.core.validatortype">
        <validatortype
              description="Pasche"
              id="pasche"/>
     </extension>
   <extension
         point="org.kalypso.model.wspm.core.validatorrule">
      <rule
            class="org.kalypso.model.wspm.tuhh.ui.rules.RuecksprungRule"
            description="Rücksprung"
            id="pasche.ruecksprung"
            type="Pasche"/>
        <rule
              class="org.kalypso.model.wspm.tuhh.ui.rules.TrennerRule"
              description="Trennflächen"
              id="pasche.trenner"
              type="Pasche"/>
        <rule
              class="org.kalypso.model.wspm.tuhh.ui.rules.BrueckeRule"
              description="Brücke"
              id="pasche.bruecke"
              type="Pasche"/>
        <rule
              class="org.kalypso.model.wspm.tuhh.ui.rules.WehrRule"
              description="Wehr"
              id="pasche.wehr"
              type="Pasche"/>
        <rule
              class="org.kalypso.model.wspm.tuhh.ui.rules.DoppelterPunktRule"
              description="DoppelterPunkt"
              id="pasche.doppelterpunkt"
              type="Pasche"/>
        <rule
              class="org.kalypso.model.wspm.tuhh.ui.rules.BewuchsRule"
              description="Bewuchs"
              id="pasche.bewuchs"
              type="Pasche"/>
   </extension>
   <extension
         point="org.kalypso.core.catalogContribution">
      <catalogContribution
            class="org.kalypso.model.wspm.tuhh.ui.sld.WspmTuhhFeatureTypeStyleCatalogContribution"
            name="Wspm Tuhh Style Catalog"/>
   </extension>
</plugin>
