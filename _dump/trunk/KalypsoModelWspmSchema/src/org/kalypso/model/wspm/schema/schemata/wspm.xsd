	<element name="waterBodyMember" type="wspm:WaterBodyPropertyType">
		<annotation>
			<documentation xml:lang="de">
				<label>Gewässer</label>
				<tooltip>Gewässer</tooltip>
				<description>Gewässer</description>
			</documentation>
		</annotation>
	</element>
	<!-- same as waterBodyMember, but supports only linked waterBodies -->
	<element name="waterBodyLinkMember" type="wspm:WaterBodyLinkPropertyType">
		<annotation>
			<documentation xml:lang="de">
				<label>Gewässer</label>
				<tooltip>Gewässer</tooltip>
				<description>Gewässer</description>
			</documentation>
		</annotation>
	</element>
	<element name="WaterBody" type="wspm:WaterBodyType" substitutionGroup="gml:_Feature">
		<annotation>
			<documentation xml:lang="de">
				<name>Gewässer</name>
				<label>${listProperty:http://www.opengis.net/gml#name;0;-Name nicht gesetzt-}</label>
				<tooltip>Gewässer</tooltip>
				<description><![CDATA[ ${property:http://www.opengis.net/gml#description;-keine Beschreibung vorhanden-}]]></description>
			</documentation>
		</annotation>
	</element>
	<!-- 
	-->
	<element name="reachMember" type="wspm:ReachPropertyType" substitutionGroup="gml:featureMember">
		<annotation>
			<documentation xml:lang="de">
				<label>Gewässerstrang</label>
				<tooltip>Gewässerstrang</tooltip>
				<description>Gewässerstrang</description>
			</documentation>
		</annotation>
	</element>
	<element name="Reach" type="wspm:ReachType" substitutionGroup="gml:_Feature">
		<annotation>
			<documentation xml:lang="de">
				<name>Gewässerstrang</name>
				<label>${property:http://www.opengis.net/gml#name}</label>
				<tooltip>${property:http://www.opengis.net/gml#name}</tooltip>
				<description>${property:http://www.opengis.net/gml#name}</description>
			</documentation>
		</annotation>
	</element>
	<!-- 
	-->
	<element name="reachSegmentMember" type="wspm:ReachSegmentPropertyType"
		substitutionGroup="gml:featureMember">
		<annotation>
			<documentation xml:lang="de">
				<label>Gewässerstrangsegmente</label>
				<tooltip>Gewässerstrangsegmente</tooltip>
				<description>Gewässerstrangsegmente</description>
			</documentation>
		</annotation>
	</element>
	<element name="AbstractReachSegment" type="wspm:AbstractReachSegmentType" abstract="true"
		substitutionGroup="gml:_Feature"/>
	<element name="InfluentReachSegment" type="wspm:InfluentReachSegmentType"
		substitutionGroup="wspm:AbstractReachSegment">
		<annotation>
			<documentation xml:lang="de">
				<name>Zufluss-Strangsegment</name>
				<label>Zufluss-Strangsegment</label>
				<tooltip>Zufluss-Strangsegment</tooltip>
				<description>Zufluss-Strangsegment</description>
			</documentation>
		</annotation>
	</element>
	<element name="ProfileReachSegment" type="wspm:ProfileReachSegmentType"
		substitutionGroup="wspm:AbstractReachSegment">
		<annotation>
			<documentation xml:lang="de">
				<name>Profil-Strangsegment</name>
				<label>Station km ${property:org.kalypso.model.wspm#station;-nicht gesetzt-}</label>
				<tooltip>Station km ${property:org.kalypso.model.wspm#station;-nicht gesetzt-}</tooltip>
				<description>Station km ${property:org.kalypso.model.wspm#station;-nicht
				gesetzt-}</description>
			</documentation>
		</annotation>
	</element>
	<!-- 
	-->
	<element name="roughnessDefinitionCollectionMember"
		type="wspm:RoughnessDefinitionCollectionPropertyType" substitutionGroup="gml:featureMember">
		<annotation>
			<documentation xml:lang="de">
				<label>Rauheitsklassen-Listen</label>
				<tooltip>Rauheitsklassen-Listen</tooltip>
				<description>Rauheitsklassen-Listen</description>
			</documentation>
		</annotation>
	</element>
	<element name="RoughnessDefinitionCollection" type="wspm:RoughnessDefinitionCollectionType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation xml:lang="de">
				<name>Rauheitsklassen-Liste</name>
				<label>${property:http://www.opengis.net/gml#name}</label>
				<tooltip>Rauheitsklassen-Liste ${property:http://www.opengis.net/gml#name}</tooltip>
				<description>Rauheitsklassen-Liste
				${property:http://www.opengis.net/gml#name}</description>
			</documentation>
		</annotation>
	</element>
	<!-- 
	-->
	<element name="roughnessDefinitionMember" type="wspm:RoughnessDefinitionPropertyType"
		substitutionGroup="gml:featureMember">
		<annotation>
			<documentation xml:lang="de">
				<label>Rauheitsklassen</label>
				<tooltip>Rauheitsklassen</tooltip>
				<description>Rauheitsklassen</description>
			</documentation>
		</annotation>
	</element>
	<element name="RoughnessDefinition" type="wspm:RoughnessDefinitionType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation xml:lang="de">
				<name>Rauheitsklasse</name>
				<label>${property:http://www.opengis.net/gml#name}</label>
				<tooltip>Rauheitsklasse ${property:http://www.opengis.net/gml#name}</tooltip>
				<description>Rauheitsklasse
				${property:http://www.opengis.net/gml#name}</description>
			</documentation>
		</annotation>
	</element>
	<!-- 
	-->
	<element name="vegetationDefinitionCollectionMember"
		type="wspm:VegetationDefinitionCollectionPropertyType" substitutionGroup="gml:featureMember">
		<annotation>
			<documentation xml:lang="de">
				<label>Bewuchsklassen-Listen</label>
				<tooltip>Bewuchsklassen-Listen</tooltip>
				<description>Bewuchsklassen-Listen</description>
			</documentation>
		</annotation>
	</element>
	<element name="VegetationDefinitionCollection" type="wspm:VegetationDefinitionCollectionType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation xml:lang="de">
				<name>Bewuchsklassen-Liste</name>
				<label>${property:http://www.opengis.net/gml#name}</label>
				<tooltip>Bewuchsklassen-Liste ${property:http://www.opengis.net/gml#name}</tooltip>
				<description>Bewuchsklassen-Liste
				${property:http://www.opengis.net/gml#name}</description>
			</documentation>
		</annotation>
	</element>
	<!-- 
	-->
	<element name="vegetationDefinitionMember" type="wspm:VegetationDefinitionPropertyType"
		substitutionGroup="gml:featureMember">
		<annotation>
			<documentation xml:lang="de">
				<label>Bewuchsklassen</label>
				<tooltip>Bewuchsklassen</tooltip>
				<description>Bewuchsklassen</description>
			</documentation>
		</annotation>
	</element>
	<element name="VegetationDefinition" type="wspm:VegetationDefinitionType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation xml:lang="de">
				<name>Bewuchsklasse</name>
				<label>${property:http://www.opengis.net/gml#name}</label>
				<tooltip>Bewuchsklasse ${property:http://www.opengis.net/gml#name}</tooltip>
				<description>Bewuchsklasse ${property:http://www.opengis.net/gml#name}</description>
			</documentation>
		</annotation>
	</element>
	<!-- 
	-->
	<element name="CalcCreation" type="wspm:calcCreationType" substitutionGroup="gml:_Feature">
		<annotation>
			<documentation xml:lang="de">
				<name>Info zur Erstellung der Berechnung</name>
				<label>Erstellung</label>
				<tooltip>Erstellung der Berechnung</tooltip>
				<description>Erstellung der Berechnung</description>
			</documentation>
		</annotation>
	</element>
	<!-- 
	_________________________________________________________________________________________
	TYPES
	_________________________________________________________________________________________
	-->
	<complexType name="CalculationPropertyType">
		<complexContent>
			<restriction base="gml:FeaturePropertyType">
				<sequence>
					<element ref="wspm:AbstractCalculation"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<complexType name="AbstractCalculationType" abstract="true">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element name="calcCreationMember" type="wspm:calcCreationInlinePropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Erstellung</label>
								<tooltip>Erstellung der Berechnung</tooltip>
								<description>Erstellung der Berechnung</description>
							</documentation>
						</annotation>
					</element>
					<!--element ref="wspm:resultMember" minOccurs="0" maxOccurs="unbounded"/-->
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- 
	-->
	<complexType name="ReachPropertyType">
		<complexContent>
			<restriction base="gml:FeaturePropertyType">
				<sequence>
					<element ref="wspm:Reach"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<complexType name="ReachType" abstract="false">
		<complexContent>
			<extension base="wspmcommon:NamedFeatureBaseType">
				<sequence>
					<element ref="wspm:waterBodyLinkMember">
						<annotation>
							<documentation xml:lang="de">
								<label>Gewässer</label>
								<tooltip>Gewässer</tooltip>
								<description>Gewässer</description>
							</documentation>
						</annotation>
					</element>
					<element name="waterAxis" type="gml:LineStringPropertyType">
						<annotation>
							<appinfo>This property is backuped by the observation (profileMember)
								itself.</appinfo>
							<documentation xml:lang="de">
								<label>Gewässerachse</label>
								<tooltip>(generierte) Gewässerachse</tooltip>
								<description>(generierte) Gewässerachse</description>
							</documentation>
						</annotation>
					</element>
					<element ref="wspm:reachSegmentMember" minOccurs="0" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- 
	-->
	<complexType name="ReachSegmentPropertyType">
		<complexContent>
			<restriction base="gml:FeaturePropertyType">
				<sequence>
					<element ref="wspm:AbstractReachSegment"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<complexType name="AbstractReachSegmentType" abstract="true">
		<complexContent>
			<annotation>
				<appinfo>We have two properties (line, markers) here, which are backuped by another
					property (profileMember). The time of last synchronization is stored in
					'cacheDate'</appinfo>
			</annotation>
			<extension base="wspmcommon:EmptyFeatureBaseType">
				<sequence>
					<element name="station" type="wspmcommon:stationDouble">
						<annotation>
							<appinfo>This property is backuped by the observation (profileMember)
								itself.</appinfo>
							<documentation xml:lang="de">
								<label>Station km</label>
								<tooltip>Station km</tooltip>
								<description>Station km</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="InfluentReachSegmentType">
		<complexContent>
			<annotation>
				<appinfo>We have two properties (line, markers) here, which are backuped by another
					property (profileMember). The time of last synchronization is stored in
					'cacheDate'</appinfo>
			</annotation>
			<extension base="wspm:AbstractReachSegmentType">
				<sequence>
					<element name="location" type="gml:PointType">
						<annotation>
							<documentation xml:lang="de">
								<label>Position des Zulaufs</label>
								<tooltip>Position des Zulaufs</tooltip>
								<description>Position des Zulaufs</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ProfileReachSegmentType">
		<complexContent>
			<annotation>
				<appinfo>We have two properties (line, markers) here, which are backuped by another
					property (profileMember). The time of last synchronization is stored in
					'cacheDate'</appinfo>
			</annotation>
			<extension base="wspm:AbstractReachSegmentType">
				<sequence>
					<!-- TODO hier kommt noch das neue Profil rein -->
					<element name="profileMember" type="wspm:ProfileLinkPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Profil</label>
								<tooltip>Referenz auf Profil</tooltip>
								<description>Referenz auf Profil</description>
							</documentation>
						</annotation>
					</element>
					<element name="profileLocation" type="gml:LineStringPropertyType">
						<annotation>
							<appinfo>This property is backed by profileMember, more explicit by the
								referenced profile-observation and can be a line or a point</appinfo>
							<documentation xml:lang="de">
								<label>Profillinie</label>
								<tooltip>Profillinie</tooltip>
								<description>Profillinie</description>
							</documentation>
						</annotation>
					</element>
					<element name="marker" type="gml:PointType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<appinfo>This property is backed by profileMember, more explicit by the
								referenced profile-observation. This is more difficult than the line
								property bceause, it depends on the profile, how many points we
								have.</appinfo>
							<documentation xml:lang="de">
								<label>???marker</label>
								<tooltip>???tooltip</tooltip>
								<description>???description</description>
							</documentation>
						</annotation>
					</element>
					<element name="cacheDate" type="dateTime">
						<annotation>
							<appinfo>This property represent the lastTime, when the cached
								proeprties (line, markers) have been synchronized against the
								profileMember</appinfo>
							<documentation xml:lang="de">
								<label>Cache Datum</label>
								<tooltip>Cache Datum</tooltip>
								<description>Cache Datum</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- 
	-->
	<complexType name="WaterBodyPropertyType">
		<sequence minOccurs="0">
			<element ref="wspm:WaterBody"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<complexType name="WaterBodyLinkPropertyType">
		<sequence minOccurs="0" maxOccurs="0">
			<element ref="wspm:WaterBody"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
	<complexType name="WaterBodyType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element name="refNr" type="string">
						<annotation>
							<documentation xml:lang="de">
								<label>Gewässerkennziffer</label>
								<tooltip>Gewässerkennziffer</tooltip>
								<description>Gewässerkennziffer</description>
							</documentation>
						</annotation>
					</element>
					<element name="waterBodyCenterLine" type="gml:LineStringSegmentType">
						<annotation>
							<documentation xml:lang="de">
								<label>Gewässerachse</label>
								<tooltip>Gewässerachse</tooltip>
								<description>Gewässerachse</description>
							</documentation>
						</annotation>
					</element>
					<element name="isDirectionUpstream" type="boolean" default="true">
						<annotation>
							<documentation xml:lang="de">
								<label>Stationierung beginnt an der Mündung</label>
								<tooltip>Gewässerstationierung beginnt an der Mündung</tooltip>
								<description>Beginnt die Gewässerstationierung an der
								Mündung?</description>
							</documentation>
						</annotation>
					</element>
					<!-- TODO hier kommt noch das neue Profil rein -->
					<element name="profileMember" minOccurs="0" maxOccurs="unbounded"
						type="wspm:ProfileInlinePropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Profile</label>
								<tooltip>Profile</tooltip>
								<description>Profile</description>
							</documentation>
						</annotation>
					</element>
					<element ref="wspm:reachMember" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation xml:lang="de">
								<label>Gewässerstränge</label>
								<tooltip>Gewässerstränge</tooltip>
								<description>Gewässerstränge</description>
							</documentation>
						</annotation>
					</element>
					<element name="runOffEventMember" type="runoff:RunOffEventInlinePropertyType"
						minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation xml:lang="de">
								<label>Abflussereignisse</label>
								<tooltip>Abflussereignisse</tooltip>
								<description>Abflussereignisse</description>
							</documentation>
						</annotation>
					</element>
					<element name="waterlevelFixationMember"
						type="runoff:WaterlevelFixationInlinePropertyType" minOccurs="0"
						maxOccurs="unbounded">
						<annotation>
							<documentation xml:lang="de">
								<label>Wasserspiegelfixierungen</label>
								<tooltip>Wasserspiegelfixierungen</tooltip>
								<description>Wasserspiegelfixierungen</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<complexType name="ProfileInlinePropertyType">
		<sequence minOccurs="1" maxOccurs="1">
			<element ref="wspmprofile:Profile"/>
		</sequence>
	</complexType>

	<complexType name="ProfileLinkPropertyType">
		<sequence minOccurs="0" maxOccurs="0">
			<element ref="wspmprofile:Profile"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>

