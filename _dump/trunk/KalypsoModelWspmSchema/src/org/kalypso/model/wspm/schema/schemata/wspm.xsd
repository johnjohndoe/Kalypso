	<!-- 
	_________________________________________________________________________________________
	TYPES
	_________________________________________________________________________________________
	-->
	<complexType name="CalculationPropertyType">
		<complexContent>
			<restriction base="gml:FeaturePropertyType">
				<sequence>
					<element ref="wspm:AbstractCalculation"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<complexType name="AbstractCalculationType" abstract="true">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element name="calcCreationMember" type="wspm:calcCreationInlinePropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Erstellung</label>
								<tooltip>Erstellung der Berechnung</tooltip>
								<description>Erstellung der Berechnung</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Creation</label>
								<tooltip>Calculation creation</tooltip>
								<description>Calculation creation</description>
							</documentation>
						</annotation>
					</element>
					<!--element ref="wspm:resultMember" minOccurs="0" maxOccurs="unbounded"/-->
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- 
	-->
	<complexType name="ReachPropertyType">
		<complexContent>
			<restriction base="gml:FeaturePropertyType">
				<sequence>
					<element ref="wspm:Reach"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<complexType name="ReachType" abstract="false">
		<complexContent>
			<extension base="wspmcommon:NamedFeatureBaseType">
				<sequence>
					<element ref="wspm:waterBodyLinkMember">
						<annotation>
							<documentation xml:lang="de">
								<label>Gewässer</label>
								<tooltip>Übergeordnetes Gewässer</tooltip>
								<description>Übergeordnetes Gewässer</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Water body</label>
								<tooltip>Superior water body</tooltip>
								<description>Superior water body</description>
							</documentation>
						</annotation>
					</element>
					<element name="waterAxis" type="gml:LineStringPropertyType">
						<annotation>
							<appinfo>This property is backuped by the observation (profileMember)
								itself.</appinfo>
							<documentation xml:lang="de">
								<label>Gewässerachse</label>
								<tooltip>(generierte) Gewässerachse</tooltip>
								<description>(generierte) Gewässerachse</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Water axis</label>
								<tooltip>(Generated) water axis</tooltip>
								<description>(Generated) water axis</description>
							</documentation>
						</annotation>
					</element>
					<element ref="wspm:reachSegmentMember" minOccurs="0" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- 
	-->
	<complexType name="ReachSegmentPropertyType">
		<complexContent>
			<restriction base="gml:FeaturePropertyType">
				<sequence>
					<element ref="wspm:AbstractReachSegment"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<complexType name="AbstractReachSegmentType" abstract="true">
		<complexContent>
			<annotation>
				<appinfo>We have two properties (line, markers) here, which are backuped by another
					property (profileMember). The time of last synchronization is stored in
					'cacheDate'</appinfo>
			</annotation>
			<extension base="wspmcommon:EmptyFeatureBaseType">
				<sequence>
					<element name="station" type="wspmcommon:stationDouble">
						<annotation>
							<appinfo>This property is backuped by the observation (profileMember)
								itself.</appinfo>
							<documentation xml:lang="de">
								<label>Station km</label>
								<tooltip>Stelle im Gewässerstrang, an dem das unten selektierte Profil platziert wird.</tooltip>
								<description>Stelle im Gewässerstrang, an dem das unten selektierte Profil platziert wird.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Station km</label>
								<tooltip>Station in reach, where the profile selected below will be placed.</tooltip>
								<description>Station in reach, where the profile selected below will be placed.</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="InfluentReachSegmentType">
		<complexContent>
			<annotation>
				<appinfo>We have two properties (line, markers) here, which are backuped by another
					property (profileMember). The time of last synchronization is stored in
					'cacheDate'</appinfo>
			</annotation>
			<extension base="wspm:AbstractReachSegmentType">
				<sequence>
					<element name="location" type="gml:PointType">
						<annotation>
							<documentation xml:lang="de">
								<label>Position des Zulaufs</label>
								<tooltip>Position des Zulaufs</tooltip>
								<description>Position des Zulaufs</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Inflow location</label>
								<tooltip>Inflow location</tooltip>
								<description>Inflow location</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ProfileReachSegmentType">
		<annotation>
			<appinfo xmlns:kapp="org.kalypso.appinfo">
				<kapp:functionProperty
					kapp:functionId="org.kalypso.model.wspm.functionProperty.profileGeometry"
					kapp:property="wspm:profileLocation"/>
			</appinfo>
			<documentation>The profileLocation is in reality retrieved from the underlying
			profile.</documentation>
		</annotation>
		<complexContent>
			<extension base="wspm:AbstractReachSegmentType">
				<sequence>
					<element name="profileMember" type="wspm:ProfileLinkPropertyType">
						<annotation>
							<documentation xml:lang="de">
								<label>Profil</label>
								<tooltip>Für Strangsegment verwendetes Profil</tooltip>
								<description>Für Strangsegment verwendetes Profil</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Profile</label>
								<tooltip>Reach segment profile</tooltip>
								<description>Reach segment profile</description>
							</documentation>
						</annotation>
					</element>
					<element name="profileLocation" type="gml:LineStringPropertyType">
						<annotation>
							<appinfo>This property is backed by profileMember, more explicit by the
								referenced profile-observation and can be a line or a point</appinfo>
							<documentation xml:lang="de">
								<label>Profillinie</label>
								<tooltip>Profillinie</tooltip>
								<description>Profillinie</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Profile location</label>
								<tooltip>Profile location</tooltip>
								<description>Profile location</description>
							</documentation>
						</annotation>
					</element>
					<element name="marker" type="gml:PointType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<appinfo>This property is backed by profileMember, more explicit by the
								referenced profile-observation. This is more difficult than the line
								property bceause, it depends on the profile, how many points we
								have.</appinfo>
							<documentation xml:lang="de">
								<label>???marker</label>
								<tooltip>???tooltip</tooltip>
								<description>???description</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- 
	-->
	<complexType name="WaterBodyPropertyType">
		<sequence minOccurs="0">
			<element ref="wspm:WaterBody"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
