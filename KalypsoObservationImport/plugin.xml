<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.ui.observationimport"
   name="ObservationImport"
   version="0.0.5"
   provider-name="org.kalypso"
   >

   <runtime>
      <library name="KalypsoNA_UI.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.kalypso.ui"/>
   </requires>

   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            icon="icons/sample.gif"
            class="org.kalypso.ui.wizard.sensor.ImportObservationWizard"
            name="Zeitreihen aus externen Dateien"
            id="KalypsoNA_UI.wizard1"/>
   </extension>

</plugin>
