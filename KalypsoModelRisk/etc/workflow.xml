<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Workflow URI="http___www.tu-harburg.de_wb_kalypso_risk__WF_KalypsoRisk"
	xmlns="http://www.renew.de/workflow/base"
	xmlns:con="http://www.renew.de/workflow/contexts"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	defaultTask="org.kalypso.risk.model.handlers.WaterdepthCollectionsWidgetHandler">
	<Help>%KalypsoRisk_help</Help>
	<Name>%KalypsoRisk</Name>
	
	<!-- TASK DEFINITIONS -->
	<TaskGroup context="MainMap"
		URI="http___www.tu-harburg.de_wb_kalypso_risk__Preprocessing">
		<Help>%KalypsoRisk_Preprocessing_help</Help>
		<Name>%KalypsoRisk_Preprocessing</Name>
		<TaskGroup context="MainMap"
			URI="http___www.tu-harburg.de_wb_kalypso_risk__DataLoad">
			<Help>%KalypsoRisk_Preprocessing_DataLoad_help</Help>
			<Name>%KalypsoRisk_Preprocessing_DataLoad</Name>
			<Task context="ImportBaseMap"
				URI="http___www.tu-harburg.de_wb_kalypso_kb_workflow_test__MapLoad">
				<Help>%KalypsoRisk_Preprocessing_DataLoad_MapLoad_help</Help>
				<Name>%KalypsoRisk_Preprocessing_DataLoad_MapLoad</Name>
			</Task>
			<Task context="ImportLanduseShapeDataWizard"
				URI="http___www.tu-harburg.de_wb_kalypso_risk__ImportLanduseShapeData">
				<Help>%KalypsoRisk_Preprocessing_DataLoad_ImportLanduseShapeData_help</Help>
				<Name>%KalypsoRisk_Preprocessing_DataLoad_ImportLanduseShapeData</Name>
			</Task>
			<Task context="ImportWaterdepthRasterWizard"
				URI="http___www.tu-harburg.de_wb_kalypso_risk__ImportWaterdepthRaster">
				<Help>%KalypsoRisk_Preprocessing_DataLoad_ImportWaterdepthRaster_help</Help>
				<Name>%KalypsoRisk_Preprocessing_DataLoad_ImportWaterdepthRaster</Name>
			</Task>
			<Task context="MainMap"
				URI="org.kalypso.risk.model.handlers.WaterdepthCollectionsWidgetHandler">
				<Help>%KalypsoRisk_Preprocessing_DataLoad_WaterdepthCollectionsWidgetHandler_help</Help>
				<Name>%KalypsoRisk_Preprocessing_DataLoad_WaterdepthCollectionsWidgetHandler</Name>
			</Task>
			<Task context="MainMap"
				URI="org.kalypso.risk.model.handlers.WaterdepthCoveragesWidgetHandler">
				<Help>%KalypsoRisk_Preprocessing_DataLoad_WaterdepthCoveragesWidgetHandler_help</Help>
				<Name>%KalypsoRisk_Preprocessing_DataLoad_WaterdepthCoveragesWidgetHandler</Name>
			</Task>
		</TaskGroup>
		<TaskGroup context="MainMap"
			URI="http___www.tu-harburg.de_wb_kalypso_risk__Databases">
			<Help>%KalypsoRisk_Preprocessing_Databases_help</Help>
			<Name>%KalypsoRisk_Preprocessing_Databases</Name>
			<Task context="CalculationControlPanel"
				URI="http___www.tu-harburg.de_wb_kalypso_risk__CalculationControlPanel">
				<Help>%KalypsoRisk_Preprocessing_Databases_CalculationControlPanel_help</Help>
				<Name>%KalypsoRisk_Preprocessing_Databases_CalculationControlPanel</Name>
			</Task>
			<Task context="RiskZonesPanel"
				URI="http___www.tu-harburg.de_wb_kalypso_risk__RiskZonesPanel">
				<Help>%KalypsoRisk_Preprocessing_Databases_RiskZonesPanel_help</Help>
				<Name>%KalypsoRisk_Preprocessing_Databases_RiskZonesPanel</Name>
			</Task>
		</TaskGroup>
	</TaskGroup>
	<TaskGroup context="MainMap"
		URI="http___www.tu-harburg.de_wb_kalypso_risk__Processing">
		<Help>%KalypsoRisk_Processing_help</Help>
		<Name>%KalypsoRisk_Processing</Name>
		<!-- 
		<Task type="ASYNCHRONOUS"
			context="ImportWaterdepthRasterContext"
			URI="http___www.tu-harburg.de_wb_kalypso_risk__LanduseRasterizationHandler">
			<Help>%KalypsoRisk_Processing_LanduseRasterizationHandler_help</Help>
			<Name>%KalypsoRisk_Processing_LanduseRasterizationHandler</Name>
		</Task>
		 -->
		<Task type="SYNCHRONOUS"
			context="ImportWaterdepthRasterContext"
			URI="http___www.tu-harburg.de_wb_kalypso_risk__DamagePotentialCalculationHandler">
			<Help>%KalypsoRisk_Processing_DamagePotentialCalculationHandler_help</Help>
			<Name>%KalypsoRisk_Processing_DamagePotentialCalculationHandler</Name>
		</Task>
		<Task type="SYNCHRONOUS"
			context="ImportWaterdepthRasterContext"
			URI="http___www.tu-harburg.de_wb_kalypso_risk__RiskZonesCalculationHandler">
			<Help>%KalypsoRisk_Processing_RiskZonesCalculationHandler_help</Help>
			<Name>%KalypsoRisk_Processing_RiskZonesCalculationHandler_help</Name>
		</Task>
	</TaskGroup>
	<TaskGroup context="MainMap"
		URI="http___www.tu-harburg.de_wb_kalypso_risk__Postprocessing">
		<Help>%KalypsoRisk_Postprocessing_help</Help>
		<Name>%KalypsoRisk_Postprocessing</Name>
		<Task context="RiskStatisticsView"
			URI="org.kalypso.risk.model.handlers.RiskShowResultStatisticHandler">
			<Help>%KalypsoRisk_Postprocessing_RiskShowResultStatisticHandler_help</Help>
			<Name>%KalypsoRisk_Postprocessing_RiskShowResultStatisticHandler</Name>
		</Task>
		<Task context="MainMap"
			URI="org.kalypso.risk.model.handlers.ExportSpecificDamageCoveragesWidgetHandler">
			<Help>%KalypsoRisk_Postprocessing_ExportSpecificDamageCoveragesWidgetHandler_help</Help>
			<Name>%KalypsoRisk_Postprocessing_ExportSpecificDamageCoveragesWidgetHandler</Name>
		</Task>
		<Task context="MainMap"
			URI="org.kalypso.risk.model.handlers.ExportRiskZoneCoveragesWidgetHandler">
			<Help>%KalypsoRisk_Postprocessing_ExportRiskZoneCoveragesWidgetHandler_help</Help>
			<Name>%KalypsoRisk_Postprocessing_ExportRiskZoneCoveragesWidgetHandler</Name>
		</Task>
	</TaskGroup>
	
	
	
	<!-- PERSPECTIVE DEFINITIONS -->
	
	<Perspectives>
		<con:PerspectiveContext id="ContextRiskModelling"
			perspectiveId="org.kalypso.kalypso1d2d.pjt.perspective.Perspective">
			<!-- perspectiveId="org.kalypso.risk.perspective.Perspective"> -->
			<con:ViewContext id="FeatureView"
				partId="org.kalypso.ui.views.featuretemplateview"
				parent="ContextRiskModelling">
				<con:ExtensionContext id="CalculationControlPanel"
					parent="FeatureView"
					handlerFactoryId="kalypsoContextHandlerFactory">
					<con:Parameter name="type"
						value="featureViewInputContext" />
					<con:Parameter name="input"
						value="templates/CalculationControlPanel.gft" />
				</con:ExtensionContext>
				<con:ExtensionContext id="RiskZonesPanel"
					parent="FeatureView"
					handlerFactoryId="kalypsoContextHandlerFactory">
					<con:Parameter name="type"
						value="featureViewInputContext" />
					<con:Parameter name="input"
						value="templates/RiskZonesPanel.gft" />
				</con:ExtensionContext>
			</con:ViewContext>
			
			<con:ViewContext id="RiskStatisticsView"
				partId="org.kalypso.risk.views.RiskStatisticsResultView"
				parent="ContextRiskModelling">
			</con:ViewContext>	
					
			<con:WorkbenchSiteContext id="MapContext"
				parent="ContextRiskModelling">
				<con:ViewContext id="MapOutlineView"
					partId="org.kalypso.ui.views.outline" parent="MapContext" />
				<con:ViewContext id="MapView"
					partId="org.kalypso.ui.views.mapView" parent="MapContext">
					<con:ExtensionContext id="BaseMap"
						parent="MapContext"
						handlerFactoryId="kalypsoContextHandlerFactory">
						<con:WizardContext id="ImportBaseMap"
							wizardId="org.kalypso.ui.wizards.imports.baseMap.ImportBaseMapWizard"
							wizardType="NEW_WIZARD" parent="BaseMap" />
						<con:Parameter name="type"
							value="mapViewInputContext" />
						<con:Parameter name="input"
							value="maps/BackroundMap.gmt" />
					</con:ExtensionContext>
					<con:ExtensionContext
						id="ImportLanduseShapeDataContext" parent="MapContext"
						handlerFactoryId="kalypsoContextHandlerFactory">
						<con:WizardContext
							id="ImportLanduseShapeDataWizard"
							wizardId="org.kalypso.risk.model.actions.dataImport.landuse.ImportLanduseWizard"
							wizardType="NEW_WIZARD" parent="ImportLanduseShapeDataContext" />
						<con:Parameter name="type"
							value="mapViewInputContext" />
						<con:Parameter name="input"
							value="maps/MainMap.gmt" />
					</con:ExtensionContext>
					<con:ExtensionContext
						id="ImportWaterdepthRasterContext" parent="MapContext"
						handlerFactoryId="kalypsoContextHandlerFactory">
						<con:WizardContext
							id="ImportWaterdepthRasterWizard"
							wizardId="org.kalypso.risk.model.actions.dataImport.waterdepth.ImportWaterdepthWizard"
							wizardType="NEW_WIZARD" parent="ImportWaterdepthRasterContext" />
						<con:Parameter name="type"
							value="mapViewInputContext" />
						<con:Parameter name="input"
							value="maps/MainMap.gmt" />
					</con:ExtensionContext>
					<con:ExtensionContext id="MainMap"
						parent="MapContext"
						handlerFactoryId="kalypsoContextHandlerFactory">
						<con:Parameter name="type"
							value="mapViewInputContext" />
						<con:Parameter name="input"
							value="maps/MainMap.gmt" />
						<!-- 
						<con:ExtensionContext
							id="CoveragesWidgetContext" parent="MainMap"
							handlerFactoryId="kalypsoContextHandlerFactory">
							<con:Parameter name="type"
								value="themeContext" />
							<con:Parameter name="input"
								value="{http://www.opengis.net/gml}_Coverage" />
						</con:ExtensionContext>
						 -->
					</con:ExtensionContext>
				</con:ViewContext>
			</con:WorkbenchSiteContext>
			
			<con:ViewContext id="FeatureSelectionView"
				partId="org.kalypso.featureview.views.FeatureView"
				parent="ContextRiskModelling" />
		</con:PerspectiveContext>
	</Perspectives>
</Workflow>