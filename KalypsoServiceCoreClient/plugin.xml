<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.services.core.client"
   name="KalypsoServiceClientCore Plug-in"
   version="1.0.0"
   provider-name="kalypso.org"
   class="org.kalypso.core.client.KalypsoServiceCoreClientPlugin">

   <runtime>
      <library name="KalypsoServiceClientCore.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.kalypso.jwsdp"/>
      <import plugin="org.kalypso.contribs.java"/>
   </requires>
   <extension-point id="serviceproxy" name="Kalypso Service Proxy" schema="schema/serviceproxy.exsd"/>

</plugin>
