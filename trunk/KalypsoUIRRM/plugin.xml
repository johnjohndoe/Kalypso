      <handler class="org.kalypso.ui.rrm.internal.simulations.RefreshSimulationsTaskHandler" commandId="org.kalypso.model.rrm.simulationManagement.refreshSimulations">
         <enabledWhen>
         	<and>
	            <with variable="activeGmlTable">
	                <!-- -reference not null -->
	                <instanceof value="java.lang.Object"/>
	            </with>
	             <with variable="selection">
	                <iterate ifEmpty="false" operator="and">
	                   <or>
	                	 <test property="org.kalypso.gml.qname" value="{org.kalypso.na.control_v2}NAControl"/>
	                   </or>
	                </iterate>
	             </with>
         	</and>
         </enabledWhen>
      </handler>
