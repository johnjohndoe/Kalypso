      </LineStyle>
      <PointStyle role="w_singlePoint_0">
        <Width>10</Width>
        <Height>10</Height>
        <Stroke isVisible="true">
          <Width>1</Width>
          <LineColor>8FADDF</LineColor>
        </Stroke>
        <FillColor>
          <Color>8FADDF</Color>
        </FillColor>
      </PointStyle>
      <LineStyle role="w_line_1">
        <Width>2</Width>
