		<appinfo xmlns:kapp="org.kalypso.appinfo">
			<kapp:gmlVersion>3.1.1</kapp:gmlVersion>
		</appinfo>
	</annotation>
	<import namespace="org.kalypso.model.wspm"
		schemaLocation="../../../../../../../../../KalypsoModelWspmSchema/src/org/kalypso/model/wspm/schema/schemata/wspm.xsd" />
	<import namespace="org.kalypso.model.wspmcommon"
		schemaLocation="../../../../../../../../../KalypsoModelWspmSchema/src/org/kalypso/model/wspm/schema/schemata/wspmCommons.xsd" />
	<import namespace="org.kalypso.model.wspmrunoff"
		schemaLocation="../../../../../../../../../KalypsoModelWspmSchema/src/org/kalypso/model/wspm/schema/schemata/runOff.xsd" />
	<import namespace="http://www.opengis.net/gml"
		schemaLocation="http://schemas.opengis.net/gml/3.1.1/base/gml.xsd" />
	<import namespace="http://www.opengis.net/om"
		schemaLocation="http://dev.bjoernsen.de/ogc/schema/om/1.0.30/om.xsd" />
	<!-- Schema-Erweiterung für das stationäre WSPM der TU-HH -->
	<include schemaLocation="wspmTuhhQintervallResults.xsd" />
	<!--
		_________________________________________________________________________________________
		ELEMENTS
		_________________________________________________________________________________________
	-->
	<element name="CalculationWspmTuhhSteadyState" type="tuhh:CalculationWspmTuhhSteadyStateType"
		substitutionGroup="wspm:AbstractCalculation">
		<annotation>
			<documentation>Calculation case - steady (TU-HH)</documentation>
		</annotation>
	</element>
	<element name="CalculationReibConstWspmTuhhSteadyState"
		type="tuhh:CalculationReibConstWspmTuhhSteadyStateType"
		substitutionGroup="wspm:AbstractCalculation">
		<annotation>
			<documentation><![CDATA[Calculation case - steady (TU-HH)- constant XXXX???]]></documentation>
		</annotation>
	</element>
	<!-- 
	-->
	<element name="StartCondition" type="tuhh:StartConditionType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation><![CDATA[Lower boundary condition (at 1. profile)]]></documentation>
		</annotation>
	</element>
	<!-- 
	-->
	<element name="RunOffInterval" type="tuhh:RunOffIntervalType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation>discharge interval - steady (TU-HH)</documentation>
		</annotation>
	</element>
	<!--
	-->
	<element name="WaterlevelParameter" type="tuhh:WaterlevelParameterType"
		substitutionGroup="gml:_Feature">
		<annotation>
			<documentation>Numerical parameters - steady (TU-HH)</documentation>
		</annotation>
	</element>
	<!-- 
	-->
	<element name="ReachWspmTuhhSteadyState" type="tuhh:ReachWspmTuhhSteadyStateType"
		substitutionGroup="wspm:AbstractReach">
		<annotation>
			<documentation>
					<![CDATA[ River string - steady (TU-HH) ]]>
			</documentation>
		</annotation>
	</element>
	<!-- 
	-->
