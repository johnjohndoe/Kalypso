<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.ui"
   name="KalypsoUI"
   version="1.0.0"
   provider-name="kalypso.org"
   class="org.kalypso.ui.KalypsoGisPlugin">

   <runtime>
      <library name="KalypsoUi.jar">
         <export name="*"/>
      </library>
      <library name="lib/kalypsoservice_observation.jar">
         <export name="*"/>
      </library>
      <library name="lib/kalypsoservice_calculation.jar">
         <export name="*"/>
      </library>
      <library name="lib/kalypsoservice_metadoc.jar">
         <export name="*"/>
      </library>
      <library name="lib/kalypsoservice_user.jar">
         <export name="*"/>
      </library>
      <library name="lib/KalypsoServiceCore.jar">
         <export name="*"/>
      </library>
      <library name="lib/swtcalendar.jar">
         <export name="*"/>
      </library>
      <library name="lib/jcommon-0.9.6.jar">
         <export name="*"/>
      </library>
      <library name="lib/jfreechart-0.9.21.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.kalypso.core" export="true"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.resources" export="true"/>
      <import plugin="org.eclipse.ui.ide" export="true"/>
      <import plugin="org.eclipse.ui.views" export="true"/>
      <import plugin="org.eclipse.compare" export="true"/>
      <import plugin="org.eclipse.core.runtime" export="true"/>
      <import plugin="org.eclipse.ui.workbench" export="true"/>    
      <import plugin="org.eclipse.debug.ui" export="true"/>
      <import plugin="org.kalypso.jwsdp" export="true"/>
      <import plugin="org.kalypso.contribs.eclipseplatform"/>
   </requires>
   <extension-point id="mapviewaction" name="mapviewaction" schema="schema/org.kalypso.ui.mapview.action.exsd"/>
   <extension-point id="dataimportwizard" name="Data Import Wizard" schema="schema/org.kalypso.ui.wizard.dataImportWizard.exsd"/>
   <extension-point id="calculationService" name="calculationService" schema="schema/org.kalypso.services.calculation.exsd"/>

   <extension
         id="preferences"
         name="preferences"
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.kalypso.ui.preferences.KalypsoPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         id="preferencePages"
         name="preferencePages"
         point="org.eclipse.ui.preferencePages">
      <page
            name="Kalypso"
            class="org.kalypso.ui.preferences.KalypsoPreferencePage"
            id="org.kalypso.ui.preferences.KalypsoPreferencePage">
      </page>
   </extension>
   <extension
         id="editors"
         name="editors"
         point="org.eclipse.ui.editors">
      <editor
            default="true"
            name="Karten Editor"
            icon="icons/kalypso_gmt.gif"
            extensions="gmt"
            class="org.kalypso.ui.editor.mapeditor.GisMapEditor"
            id="org.kalypso.ui.editor.mapeditor.GisMapEditor">
      </editor>
      <editor
            default="true"
            name="Tabellen Editor"
            icon="icons/kalypso_gtt.gif"
            extensions="gtt"
            class="org.kalypso.ui.editor.gistableeditor.GisTableEditor"
            contributorClass="org.kalypso.ui.editor.gistableeditor.GisTableEditorActionBarContributor"
            id="org.kalypso.ui.editor.gistableeditor.GisTableEditor">
      </editor>
      <editor
            default="true"
            name="Feature Editor"
            icon="icons/kalypso_gtt.gif"
            extensions="gft"
            class="org.kalypso.ui.editor.featureeditor.FeatureEditor"
            id="org.kalypso.ui.editor.FeatureEditor">
      </editor>
      <editor
            name="GMLEditor"
            default="true"
            extensions="gmv"
            icon="icons/kalypso_gtt.gif"
            class="org.kalypso.ui.editor.gmleditor.ui.GMLEditor"
            id="org.kalypso.ui.editor.GMLEditor">
      </editor>
      <editor
            default="true"
            name="Diagramm Editor"
            icon="icons/kalypso_odt.gif"
            extensions="odt"
            class="org.kalypso.ui.editor.diagrameditor.ObservationDiagramEditor"
            id="org.kalypso.ui.editor.diagrameditor.ObservationDiagramEditor">
      </editor>
      <editor
            default="true"
            name="Zeitreihen Editor"
            icon="icons/kalypso_ott.gif"
            extensions="ott"
            class="org.kalypso.ui.editor.obstableeditor.ObservationTableEditor"
            id="org.kalypso.ui.editor.obstableeditor.ObservationTableEditor">
      </editor>
      <editor
            launcher="org.kalypso.ui.editorLauncher.GisTemplateLauncher"
            icon="icons/sample.gif"
            default="true"
            name="Gis Vorlagenauswahl"
            id="org.kalypso.ui.editor.GisEditor"
            extensions="gml,shp">
      </editor>
      <editor
            launcher="org.kalypso.ui.editorLauncher.ObsTemplateLauncher"
            icon="icons/sample.gif"
            default="false"
            name="Zeitreihen Vorlagenauswahl"
            id="org.kalypso.ui.editor.ObsEditor"
            extensions="zml">
      </editor>
      <editor
            launcher="org.kalypso.ui.editorLauncher.GrafikEditorLauncher"
            icon="icons/observation/grafik-icon.gif"
            default="false"
            name="Grafik Programm"
            id="org.kalypso.zml.grafik"
            extensions="zml,tpl,odt"/>
   </extension>
   <extension
         id="views"
         name="views"
         point="org.eclipse.ui.views">
      <category
            name="Kalypso"
            id="org.kalypso.ui.kalypso">
      </category>
      <view
            name="Zeitreihenbrowser"
            icon="icons/repository/repository.gif"
            category="org.kalypso.ui.kalypso"
            class="org.kalypso.ui.repository.view.RepositoryExplorerPart"
            id="org.kalypso.ui.repository.view.RepositoryExplorerPart">
      </view>
      <view
            name="Diagram QuickView"
            icon="icons/kalypso_odt.gif"
            category="org.kalypso.ui.kalypso"
            class="org.kalypso.ogc.sensor.view.DiagramViewPart"
            id="org.kalypso.ogc.sensor.view.DiagramViewPart">
      </view>
      <view
            name="Table QuickView"
            icon="icons/kalypso_ott.gif"
            category="org.kalypso.ui.kalypso"
            class="org.kalypso.ogc.sensor.view.TableViewPart"
            id="org.kalypso.ogc.sensor.view.TableViewPart">
      </view>
      <view
            name="Style Editor"
            icon="icons/full/elcl16/kde_filter.gif"
            category="org.kalypso.ui.kalypso"
            class="org.kalypso.ui.editor.mapeditor.views.StyleEditorViewPart"
            id="org.kalypso.ui.editor.mapeditor.views.styleeditor">
      </view>
      <view
            allowMultiple="false"
            icon="icons/kalypso16.gif"
            class="org.kalypso.ui.editor.mapeditor.views.ActionOptionsView"
            category="org.kalypso.ui.kalypso"
            name="Edit Optionen"
            id="org.kalypso.ui.editor.mapeditor.views.ActionOptionsView"/>
   </extension>
   <extension
         id="editorActions"
         name="editorActions"
         point="org.eclipse.ui.editorActions">
      <editorContribution
            targetID="org.kalypso.ui.editor.gistableeditor.GisTableEditor"
            id="org.kalypso.ui.editor.gistableeditor.GisTableActionDelegate">
         <menu
               label="Tabelle"
               path="additions"
               id="org.kalypso.ui.editors.tableeditor.menu">
            <groupMarker
                  name="tabelle">
            </groupMarker>
            <separator
                  name="spalten">
            </separator>
            <separator
                  name="edit">
            </separator>
         </menu>
         <menu
               label="Spalten"
               path="org.kalypso.ui.editors.tableeditor.menu/spalten"
               id="org.kalypso.ui.editors.tableeditor.columnmenu">
            <separator
                  name="spaltenMenu">
            </separator>
         </menu>
         <action
               label="Berichtsablage..."
               icon="icons/util/bericht.gif"
               style="push"
               tooltip="Exportiert die Tabelle in das Berichtswesen."
               class="org.kalypso.ui.editor.gistableeditor.actions.ExportBerichtActionDelegate"
               menubarPath="org.kalypso.ui.editors.tableeditor.menu/tabelle"
               toolbarPath="tabelle"
               id="org.kalypso.ui.editor.gistableeditor.ExportAction">
         </action>
         <action
               label="Exportieren..."
               icon="icons/gistable/export_wiz.gif"
               style="push"
               tooltip="Die Tabelle exportieren"
               class="org.kalypso.ui.editor.gistableeditor.actions.ExportActionDelegate"
               menubarPath="org.kalypso.ui.editors.tableeditor.menu/tabelle"
               toolbarPath="tabelle"
               id="org.kalypso.ui.editor.gistableeditor.ExportAction">
         </action>
         <action
               disabledIcon="icons/feature/save_GML_gray.gif"
               enablesFor="+"
               toolbarPath="edit"
               label="Daten speichern"
               tooltip="Speichert die Daten der Tabelle"
               icon="icons/feature/save_GML.gif"
               class="org.kalypso.ui.editor.gistableeditor.actions.SaveThemeDelegate"
               style="push"
               menubarPath="org.kalypso.ui.editors.tableeditor.menu/edit"
               id="org.kalypso.ui.editor.gistableeditor.SaveDataAction">
         </action>
         <action
               label="Rückgängig"
               icon="icons/gistable/undo_edit.gif"
               style="push"
               tooltip="Die letzte Änderung der Daten Rückgängig machen"
               class="org.kalypso.ui.editor.gistableeditor.actions.UndoDelegate"
               menubarPath="org.kalypso.ui.editors.tableeditor.menu/edit"
               toolbarPath="edit"
               id="org.kalypso.ui.editor.gistableeditor.UndoActionDelegate">
         </action>
         <action
               label="Wiederherstellen"
               icon="icons/gistable/redo_edit.gif"
               style="push"
               tooltip="Die letzte Korrektur wiederherstellen"
               class="org.kalypso.ui.editor.gistableeditor.actions.RedoDelegate"
               menubarPath="org.kalypso.ui.editors.tableeditor.menu/edit"
               toolbarPath="edit"
               id="org.kalypso.ui.editor.gistableeditor.RedoActionDelegate">
         </action>
      </editorContribution>
      <editorContribution
            targetID="org.kalypso.ui.editor.GMLEditor"
            id="org.kalypso.ui.editor.gistreeeditor.GisTreeActionDelegate">
         <menu
               label="Baum"
               path="additions"
               id="org.kalypso.ui.editors.treeeditor.menu">
            <groupMarker name="edit"/>
         </menu>
         <action
               disabledIcon="icons/map/save_edit_disabled.gif"
               toolbarPath="Baum/edit"
               label="Daten speichern"
               tooltip="Speichert die Daten des Baums"
               icon="icons/feature/save_GML.gif"
               class="org.kalypso.ui.editor.gmleditor.util.actions.SaveGmlDelagate"
               style="push"
               menubarPath="org.kalypso.ui.editors.treeeditor.menu/edit"
               id="org.kalypso.ui.editor.gistreeeditor.SaveDataAction">
         </action>
      </editorContribution>
      <editorContribution
            targetID="org.kalypso.ui.editor.mapeditor.GisMapEditor"
            id="org.kalypso.ogc.widgets.GisMapEditorActionDelegate">
         <menu
               label="Karte"
               path="additions"
               id="org.kalypso.ui.editors.mapeditor.menu">
            <separator name="sep1"/>
            <groupMarker name="export"/>
            <groupMarker
                  name="tools">
            </groupMarker>
            <separator
                  name="sep2">
            </separator>
            <groupMarker
                  name="actions">
            </groupMarker>
            <separator
                  name="sep3">
            </separator>
            <groupMarker
                  name="edit">
            </groupMarker>
         </menu>
         <action
               label="Berichtsablage..."
               icon="icons/util/bericht.gif"
               style="push"
               tooltip="Exportiert die Karte in das Berichtswesen."
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.ExportBerichtActionDelegate"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/export"
               toolbarPath="karte/export"
               id="org.kalypso.ui.editor.mapeditor.ExportAction">
         </action>
         <action
               label="Exportieren..."
               icon="icons/gistable/export_wiz.gif"
               style="push"
               tooltip="Die Karte exportieren"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.ExportActionDelegate"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/export"
               toolbarPath="karte/export"
               id="org.kalypso.ui.editor.mapeditor.ExportAction">
         </action>
         <action
               label="Ausschnitt verkleinern"
               icon="icons/map/zoomout.gif"
               style="push"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.ZoomOutDelegate"
               tooltip="verkleinert den Kartenausschnitt"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/actions"
               toolbarPath="karte/actions"
               id="org.kalypso.ui.editor.mapeditor.action.ZoomOutAction">
         </action>
         <action
               label="Ausschnitt maximieren"
               icon="icons/full/elcl16/kde_window_fullscreen.gif"
               style="push"
               tooltip="maximiert den Kartenausschnitt"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.FullExtentDelegate"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/actions"
               toolbarPath="karte/actions"
               id="org.kalypso.ui.editor.mapeditor.action.FullExtentAction">
         </action>
         <action
               state="true"
               style="radio"
               id="org.kalypso.ui.editor.mapeditor.action.PanAction"
               toolbarPath="karte/tools"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.PanToWidgetDelegate"
               icon="icons/map/pan.gif"
               label="Ausschnitt verschieben"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/tools"
               tooltip="verschiebt den Kartenausschnitt">
         </action>
         <action
               label="Objekt editieren"
               icon="icons/full/elcl16/kde_contexthelp.gif"
               style="radio"
               tooltip="die Daten eines Objekts anzeigen/editieren"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.EditFeatureWidgetDelegate"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/tools"
               toolbarPath="karte/tools"
               id="org.kalypso.ui.editor.mapeditor.action.EditFeatureAction">
         </action>
         <action
               label="Ausschnitt vergrößern"
               icon="icons/map/zoomin.gif"
               style="radio"
               tooltip="vergrößert den Kartenausschnitt"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.ZoomInByRectWidgetDelegate"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/tools"
               toolbarPath="karte/tools"
               id="org.kalypso.ui.editor.mapeditor.action.ZoomInAction">
         </action>
         <action
               disabledIcon="icons/map/edit_relation_gray.gif"
               toolbarPath="karte/tools"
               label="Relationen setzen"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.EditRelationsWidgetDelegate"
               tooltip="setzen von Relationen"
               icon="icons/map/edit_relation.gif"
               style="radio"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/tools"
               id="org.kalypso.ui.editor.mapeditor.action.NewRelationAction">
         </action>
             <action
                   disabledIcon="icons/map/geoInfo_gray.gif"
                   toolbarPath="karte/tools"
                   label="Koordinaten Information"
                   class="org.kalypso.ui.editor.mapeditor.actiondelegates.CoordinateInfoWidgetDelegate"
                   tooltip="Koordinaten Information"
                   icon="icons/map/geoInfo.gif"
                   style="radio"
                   menubarPath="org.kalypso.ui.editors.mapeditor.menu/tools"
                   id="org.kalypso.ui.editor.mapeditor.action.CoordinateInfoAction">
         </action>
         <action
               disabledIcon="icons/map/save_edit_disabled.gif"
               toolbarPath="karte/edit"
               label="Thema speichern"
               tooltip="Speichert die Daten des aktiven Themas"
               icon="icons/feature/save_GML.gif"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.SaveThemeDelegate"
               style="push"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/edit"
               id="org.kalypso.ui.editor.mapeditor.action.SaveDataAction">
         </action>

         <action
               label="Rückgängig"
               icon="icons/map/undo_edit.gif"
               style="push"
               tooltip="letzte Änderung rückgängig machen"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.UndoDelegate"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/edit"
               toolbarPath="karte/edit"
               id="org.kalypso.ui.editor.mapeditor.action.SaveDataAction">
         </action>
         <action
               label="Wiederherstellen"
               icon="icons/map/redo_edit.gif"
               style="push"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.RedoDelegate"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/edit"
               toolbarPath="karte/edit"
               id="org.kalypso.ui.editor.mapeditor.action.SaveDataAction">
         </action>
      </editorContribution>
      <editorContribution
            targetID="org.kalypso.ui.editor.obstableeditor.ObservationTableEditor"
            id="org.kalypso.ui.editor.obstableeditor.EditorContribution">
         <menu
               label="Tabelle"
               path="additions"
               id="org.kalypso.ui.editor.obstableeditor.menu">
            <separator name="separator1"/>
            <groupMarker name="edit"/>
            <separator name="separator2"/>
            <groupMarker name="tools"/>
         </menu>
         <action
               disabledIcon="icons/feature/save_GML_gray.gif"
               toolbarPath="tabelle/edit"
               label="Daten speichern"
               icon="icons/feature/save_GML.gif"
               tooltip="Speichert die grundliegende Daten"
               class="org.kalypso.ui.editor.obstableeditor.actions.SaveDataAction"
               style="push"
               menubarPath="org.kalypso.ui.editor.obstableeditor.menu/edit"
               id="org.kalypso.ui.editor.obstableeditor.actions.SaveDataAction">
         </action>
         <action
               toolbarPath="tabelle/tools"
               label="Tabelle exportieren"
               tooltip="Exportiert die Tabelle in das Berichtswesen"
               class="org.kalypso.ui.editor.obstableeditor.actions.ExportBerichtActionDelegate"
               icon="icons/util/bericht.gif"
               style="push"
               menubarPath="org.kalypso.ui.editor.obstableeditor.menu/tools"
               id="org.kalypso.ui.editor.obstableeditor.actions.ExportBerichtActionDelegate">
         </action>
      </editorContribution>
      <editorContribution
            targetID="org.kalypso.ui.editor.diagrameditor.ObservationDiagramEditor"
            id="org.kalypso.ui.editor.diagrameditor.EditorContribution">
         <menu
               label="Diagramm"
               path="additions"
               id="org.kalypso.ui.editor.obsdiageditor.menu">
            <separator name="separator1"/>
            <groupMarker name="tools"/>
         </menu>
         <action
               toolbarPath="diagramm"
               label="Diagramm Eigenschaften"
               tooltip="Öffnet das Dialog um die Diagramm-Eigenschaften setzen zu können"
               icon="icons/util/settings.gif"
               class="org.kalypso.ui.editor.diagrameditor.actions.DiagramPropertiesAction"
               style="push"
               menubarPath="org.kalypso.ui.editor.obsdiageditor.menu/tools"
               id="org.kalypso.ui.editor.diagrameditor.actions.DiagramPropertiesAction"/>
         <action
               toolbarPath="diagramm"
               label="Im Grafikprogramm öffnen"
               icon="icons/observation/grafik-icon.gif"
               tooltip="Öffnet das aktuelle Diagramm im Grafikprogramm"
               class="org.kalypso.ui.editor.diagrameditor.actions.OpenGrafikAction"
               style="push"
               menubarPath="org.kalypso.ui.editor.obsdiageditor.menu/tools"
               id="org.kalypso.ui.editor.diagrameditor.actions.OpenGrafikAction">
         </action>
         <action
               toolbarPath="diagramm"
               label="Diagramm exportieren"
               tooltip="Exportiert das Diagramm in das Berichtswesen."
               class="org.kalypso.ui.editor.diagrameditor.actions.ExportBerichtActionDelegate"
               icon="icons/util/bericht.gif"
               style="push"
               menubarPath="org.kalypso.ui.editor.obsdiageditor.menu/tools"
               id="org.kalypso.ui.editor.diagrameditor.actions.ExportBerichtActionDelegate">
         </action>
      </editorContribution>
   </extension>
   <extension
         id="popupMenus"
         name="popupMenus"
         point="org.eclipse.ui.popupMenus">
       <objectContribution
            objectClass="org.kalypsodeegree.model.feature.Feature"
            adaptable="true"
            id="org.kalypso.objectclass.feature">
         <action
               enablesFor="1"
               label="Feature löschen"
               tooltip="löscht ein feature"
               class="org.kalypso.ui.editor.actions.FeatureRemoveActionDelegate"
               icon="icons/gistable/rem_row.gif"
               style="push"
               id="org.kalypso.objectclass.feature.action.remove">
         </action>
         <action
               enablesFor="0"
               label="Feature neu"
               tooltip="erstellt neues feature"
               class="org.kalypso.ui.editor.actions.FeatureAddActionDelegate"
               icon="icons/gistable/add_row.gif"
               style="push"
               id="org.kalypso.objectclass.feature.action.remove">
         </action>
         <action
               enablesFor="1"
               label="Wert übertragen "
               tooltip="überträgt Parameter auf Auswahl"
               icon="icons/gistable/applyValues2.gif"
               class="org.kalypso.ui.editor.actions.FeatureSetPropertyActionDelegate"
               style="push"
               id="org.kalypso.objectclass.feature.action.remove"/>
         <action
               enablesFor="1"
               label="Feature hervorheben"
               tooltip="Feature hervorheben um es z.B. in der zu finden"
               icon="icons/feature/feature_highlight.gif"
               class="org.kalypso.ui.editor.actions.FeatureHighlightActionDelegate"
               style="push"
               id="org.kalypso.objectclass.feature.action.highlight"/>
      </objectContribution>
 
   </extension>
   <extension
         id="resourceFilters"
         name="resourceFilters"
         point="org.eclipse.ui.ide.resourceFilters">
      <filter
            selected="false"
            pattern="*.gml">
      </filter>
   </extension>
   <extension
         id="perspectives"
         name="perspectives"
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Kalypso Zeitreihenbrowser"
            icon="icons/perspective/zml_browser.gif"
            class="org.kalypso.ui.perspectives.ObservationRepositoryPerspectiveFactory"
            id="org.kalypso.ui.perspectives.ObservationRepositoryPerspectiveFactory">
      </perspective>
      <perspective
            name="Kalypso Modeler"
            icon="icons/perspective/expert.gif"
            fixed="false"
            class="org.kalypso.ui.perspectives.ModelerPerspectiveFactory"
            id="org.kalypso.ui.perspectives.ModelerPerspectiveFactory">
      </perspective>
   </extension>
   <extension
         id="exportWizards"
         name="exportWizards"
         point="org.eclipse.ui.exportWizards">
      <wizard
            name="Export über Kalypso Zeitreihendienst"
            icon="icons/repository/zml_upload.gif"
            class="org.kalypso.ui.wizard.ocs.OcsExportWizard"
            id="org.kalypso.ui.wizard.ocs.OcsExportWizard">
      </wizard>
   </extension>
   <extension
         id="repository.factories"
         name="repository.factories"
         point="org.kalypso.repository.factories">
         <!-- TODO: Marc: Remove 'class' Attribut because of compilation-problems under linux
         check if still everything works -->
      <repository
            factory="org.kalypso.ui.repository.factory.SWTZmlRepositoryFactory"
