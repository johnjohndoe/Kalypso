      </editorContribution>
   </extension>
   <extension
         id="popupMenus"
         name="popupMenus"
         point="org.eclipse.ui.popupMenus">
       <objectContribution
            objectClass="org.kalypsodeegree.model.feature.Feature"
            adaptable="true"
            id="org.kalypso.objectclass.feature">
         <action
               enablesFor="1"
               label="%Extension.popupMenus.feature.delete.label"
               tooltip="%Extension.popupMenus.feature.delete.tooltip"
               class="org.kalypso.ui.editor.actions.FeatureRemoveActionDelegate"
               icon="icons/feature/gis_feature_delete.gif"
               style="push"
               id="org.kalypso.objectclass.feature.action.remove">
         </action>
         <action
               enablesFor="0"
               label="%Extension.popupMenus.feature.new.label"
               tooltip="%Extension.popupMenus.feature.new.tooltip"
               class="org.kalypso.ui.editor.actions.FeatureAddActionDelegate"
               icon="icons/feature/gis_feature_neu.gif"
               style="push"
               id="org.kalypso.objectclass.feature.action.remove">
         </action>
         <action
               enablesFor="1"
               label="%Extension.popupMenus.feature.setValue.label"
               tooltip="%Extension.popupMenus.feature.setValue.tooltip"
               icon="icons/gistable/applyValues2.gif"
               class="org.kalypso.ui.editor.actions.FeatureSetPropertyActionDelegate"
               style="push"
               id="org.kalypso.objectclass.feature.action.remove"/>
         <action
               label="%Extension.popupMenus.feature.edit"
               icon="icons/feature/gis_feature_edit.gif"
               class="org.kalypso.ui.editor.gmleditor.util.actions.EditFeature"
               id="org.kalypso.objectclass.feature.editfeature"/>
      </objectContribution>
       <objectContribution
             adaptable="false"
             objectClass="org.kalypsodeegree.filterencoding.Filter"
             id="org.kalypso.objectclass.fiterencoding.filter">
          <menu
                label="Neuer Filter..."
                id="org.kalypso.objectclass.filterencoding.filter.menu">
             <separator name="filter"/>
             <separator name="operations"/>
          </menu>
          <menu
                label="%Extension.popupMenus.filterencoding.newOperation"
                id="org.kalypso.objectclass.filterencoding.filter.operation.menu">
             <separator name="operations"/>
          </menu>
