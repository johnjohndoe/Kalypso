               tooltip="%Extension.EditorActions.GisMapEditor.maximize.tooltip"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.ZoomInByRectWidgetDelegate"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/tools"
               toolbarPath="karte/tools"
               id="org.kalypso.ui.editor.mapeditor.action.ZoomInAction">
         </action>
         <action
               disabledIcon="icons/map/edit_relation_gray.gif"
               toolbarPath="karte/tools"
               label="%Extension.EditorActions.GisMapEditor.setRelation.label"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.EditRelationsWidgetDelegate"
               tooltip="%Extension.EditorActions.GisMapEditor.setRelation.tooltip"
               icon="icons/map/edit_relation.gif"
               style="radio"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/tools"
               id="org.kalypso.ui.editor.mapeditor.action.NewRelationAction">
         </action>
         <action
                   disabledIcon="icons/map/geoInfo_gray.gif"
                   toolbarPath="karte/tools"
                   label="%Extension.EditorActions.GisMapEditor.coordInfo.label"
                   class="org.kalypso.ui.editor.mapeditor.actiondelegates.CoordinateInfoWidgetDelegate"
                   tooltip="%Extension.EditorActions.GisMapEditor.coordInfo.tooltip"
                   icon="icons/map/geoInfo.gif"
                   style="radio"
                   menubarPath="org.kalypso.ui.editors.mapeditor.menu/tools"
                   id="org.kalypso.ui.editor.mapeditor.action.CoordinateInfoAction">
         </action>
         <action
               disabledIcon="icons/map/wmsInfo_gray.gif"
               toolbarPath="karte/tools"
               label="%Extension.EditorActions.GisMapEditor.wmsInfo.label"
               icon="icons/map/wmsInfo.gif"
               tooltip="%Extension.EditorActions.GisMapEditor.wmsInfo.tooltip"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.WMSGetfeatureInfoWidgetDelegate"
               style="radio"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/tools"
               id="org.kalypso.ui.editor.mapeditor.action.CoordinateInfoAction">
         </action>
         <action
               disabledIcon="icons/map/save_edit_disabled.gif"
               toolbarPath="karte/edit"
               label="%Extension.EditorActions.saveData.label"
               tooltip="%Extension.EditorActions.saveData.tooltip"
               icon="icons/feature/save_GML.gif"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.SaveThemeDelegate"
               style="push"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/edit"
               id="org.kalypso.ui.editor.mapeditor.action.SaveDataAction">
         </action>

         <action
               label="%Extension.EditorActions.undo.label"
               icon="icons/map/undo_edit.gif"
               style="push"
               tooltip="%Extension.EditorActions.undo.tooltip"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.UndoDelegate"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/edit"
               toolbarPath="karte/edit"
               id="org.kalypso.ui.editor.mapeditor.action.SaveDataAction">
         </action>
         <action
               toolbarPath="karte/edit"
               label="%Extension.EditorActions.redo.label"
               tooltip="%Extension.EditorActions.redo.tooltip"
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.RedoDelegate"
               icon="icons/map/redo_edit.gif"
               style="push"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/edit"
               id="org.kalypso.ui.editor.mapeditor.action.SaveDataAction">
         </action>
         <action
               toolbarPath="karte/edit"
               label="Polygon zeichnen"
               tooltip="Neues Polygon in einer Karte auf das Skizzier-Thema zeichen "
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.CreatePolygonWidgetDelegate"
               icon="icons/feature/geom_polygon.gif"
               style="push"
               state="false"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/edit"
               id="org.kalypso.ui.editor.mapeditor.action.CreatePolygon"/>
         <action
               toolbarPath="karte/edit"
               label="Polylinie zeichnen"
               tooltip="Neue Polylinie in einer Karte auf das Skizzier-Thema zeichen "
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.CreateLineStringWidgetDelegate"
               icon="icons/feature/geom_line.gif"
               style="push"
               state="false"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/edit"
               id="org.kalypso.ui.editor.mapeditor.action.CreatePolyline"/>
         <action
               toolbarPath="karte/edit"
               label="Punkt zeichnen"
               tooltip="Neuer Punkt in einer Karte auf das Skizzier-Thema zeichen "
               class="org.kalypso.ui.editor.mapeditor.actiondelegates.CreatePointWidgetDelegate"
               icon="icons/feature/geom_point.gif"
               style="push"
               state="false"
               menubarPath="org.kalypso.ui.editors.mapeditor.menu/edit"
               id="org.kalypso.ui.editor.mapeditor.action.CreatePoint"/>
         <action
               label="Neues Feature erzeugen"
               icon="icons/feature/gis_feature_neu.gif"
               class="org.kalypso.ui.editor.actions.FeatureAddActionDelegate"
               style="push"
               id="org.kalypso.ui.editor.mapeditor.action.newfeature"/>
      </editorContribution>
      <editorContribution
            targetID="org.kalypso.ui.editor.obstableeditor.ObservationTableEditor"
            id="org.kalypso.ui.editor.obstableeditor.EditorContribution">
