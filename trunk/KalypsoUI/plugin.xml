      </repository>
      <repository
            factory="org.kalypso.services.ocs.repository.ObservationServiceRepositoryFactory"
            name="Kalypso Zeitreihendienst">
      </repository>
   </extension>
   <extension
         id="importWizards"
         name="importWizards"
