<?xml version="1.0" encoding="Cp1252" standalone="yes"?>
<ChartConfiguration xmlns="http://www.openali.de/odysseus/chartconfig/0.2.0/"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.openali.de/odysseus/chartconfig/0.2.0/ ../../../../../../../../OdysseusChartFactory/etc/binding/schema/ChartConfiguration.xsd">

	<Chart id="runoffChart">
		<Title>Ganglinie</Title>
		<Description>Berechnungsergebnisse</Description>
		<Layers>
			<Layer visible="true" id="gridLayer">
				<Provider epid="de.openali.odysseus.chart.ext.layer.provider.Grid" >
					<Parameters>
						<Parameter name="orientation" value="VERTICAL"/>
					</Parameters>
				</Provider>
				<Title>Grid Layer</Title>
				<Description>Grid Layer</Description>
				
				<MapperRefs>
					<DomainAxisRef ref="timeAxis" />
					<TargetAxisRef ref="nnAxis" />
				</MapperRefs>
				<Styles>
					<LineStyle role="line">
						<Title></Title>
						<Width>1</Width>
						<LineColor>dddddd</LineColor>
						<DashArray>3 3</DashArray>
					</LineStyle>
				</Styles>
			</Layer>
			
			<Layer visible="true" id="wspLayer">
				<Provider epid="org.kalypso.model.wspm.ui.featureview.TupleResultLineLayerProvider">
					<Parameters>
						<Parameter name="featureKey" value="hydrographFeatureKey" />
						<Parameter name="domainComponentId" value="urn:ogc:gml:dict:kalypso:model:1d2d:timeserie:components#Time" />
						<Parameter name="valueComponentId" value="urn:ogc:gml:dict:kalypso:model:1d2d:timeserie:components#Waterlevel" />
					</Parameters>
				</Provider>
				<Title>Wasserstand</Title>
				<Description>Wasserstandslinie</Description>
				<MapperRefs>
					<DomainAxisRef ref="timeAxis" />
					<TargetAxisRef ref="nnAxis" />
				</MapperRefs>
				<Styles>
					<LineStyle role="line">
						<Title>wsp line title</Title>
						<Width>2</Width>
						<LineColor>0000aa</LineColor>
					</LineStyle>
					<PointStyle role="point">
						<Title>wsp point title</Title>
						<Width>5</Width>
						<Height>5</Height>
						<Stroke>
							<Width>1</Width>
							<LineColor>0000aa</LineColor>
						</Stroke>
						<FillColor>3333aa</FillColor>
					</PointStyle>
				</Styles>
			</Layer>
			
			<Layer visible="true" id="dischargeLayer">
				<Provider epid="org.kalypso.model.wspm.ui.featureview.TupleResultLineLayerProvider">
					<Parameters>
						<Parameter name="featureKey" value="hydrographFeatureKey" />
						<Parameter name="domainComponentId" value="urn:ogc:gml:dict:kalypso:model:1d2d:timeserie:components#Time" />
						<Parameter name="valueComponentId" value="urn:ogc:gml:dict:kalypso:model:1d2d:timeserie:components#Discharge" />
					</Parameters>
				</Provider>
				<Title>Abfluss</Title>
				<Description>Abflussganglinie</Description>
				<MapperRefs>
					<DomainAxisRef ref="timeAxis" />
					<TargetAxisRef ref="m3sAxis" />
				</MapperRefs>
				<Styles>
					<LineStyle role="line">
						<Title>discharge line title</Title>
						<Width>2</Width>
						<LineColor>00aa00</LineColor>
					</LineStyle>
					
					<PointStyle role="point">
						<Title>discharge point title</Title>
						<Width>5</Width>
						<Height>5</Height>
						<Stroke>
							<Width>1</Width>
							<LineColor>00aa00</LineColor>
						</Stroke>
						<FillColor>33aa33</FillColor>
					</PointStyle>
				</Styles>
			</Layer>
			
			<Layer visible="true" id="velocityLayer">
				<Provider epid="org.kalypso.model.wspm.ui.featureview.TupleResultLineLayerProvider">
					<Parameters>
						<Parameter name="featureKey" value="hydrographFeatureKey" />
						<Parameter name="domainComponentId" value="urn:ogc:gml:dict:kalypso:model:1d2d:timeserie:components#Time" />
						<Parameter name="valueComponentId" value="urn:ogc:gml:dict:kalypso:model:1d2d:timeserie:components#Velocity" />
					</Parameters>
				</Provider>
				<Title>Geschwindigkeit</Title>
				<Description>Geschwindigkeitsganglinie</Description>
				<MapperRefs>
					<DomainAxisRef ref="timeAxis" />
					<TargetAxisRef ref="msAxis" />
				</MapperRefs>
				<Styles>
					<LineStyle role="line">
						<Title>velocity line title</Title>
						<Width>2</Width>
						<LineColor>aa0000</LineColor>
					</LineStyle>
					
					<PointStyle role="point">
						<Title>velocity point title</Title>
						<Width>5</Width>
						<Height>5</Height>
						<Stroke>
							<Width>1</Width>
							<LineColor>aa0000</LineColor>
						</Stroke>
						<FillColor>aa3333</FillColor>
					</PointStyle>
				</Styles>
			</Layer>

		</Layers>
		<Mappers>
			<Axis id="nnAxis">
				<Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
				<Label>Wasserspiegel [NN+m]</Label>
				<Direction>POSITIVE</Direction>
				<Position>LEFT</Position>
				<NumberRange>
					<MinValue>5.0</MinValue>
					<MaxValue>10.0</MaxValue>
				</NumberRange>
				<PreferredAdjustment before="1" range="20" after="1"/>			
				<RendererRef ref="NumberWspAxisRenderer" />
			</Axis>
			<Axis id="m3sAxis">
				<Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
				<Label>Abfluss [m³/s]</Label>
				<Direction>POSITIVE</Direction>
				<Position>RIGHT</Position>
				<NumberRange>
					<MinValue>5.0</MinValue>
					<MaxValue>10.0</MaxValue>
				</NumberRange>
				<PreferredAdjustment before="1" range="20" after="1"/>			
				<RendererRef ref="NumberDisAxisRenderer" />
			</Axis>
			<Axis id="msAxis">
				<Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
				<Label>Geschwindigkeit [m/s]</Label>
				<Direction>POSITIVE</Direction>
				<Position>RIGHT</Position>
				<NumberRange>
					<MinValue>5.0</MinValue>
					<MaxValue>10.0</MaxValue>
				</NumberRange>
				<PreferredAdjustment before="1" range="20" after="1"/>			
				<RendererRef ref="NumberVelAxisRenderer" />
			</Axis>
			<Axis id="timeAxis">
				<Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
				<Label>Datum</Label>
				<Direction>POSITIVE</Direction>
				<Position>BOTTOM</Position>
				<DurationRange>
					<MinValue>-P14D</MinValue>
					<MaxValue>P1D</MaxValue>
				</DurationRange>
				<RendererRef ref="CalendarAxisRenderer" />
			</Axis>
			<!-- NOT USED
			<Axis id="wsp_Axis">
				<Provider epid="org.kalypso.chart.ext.axis.provider.Number" />
				<Label>Wassertiefe [m]</Label>
				<Direction>POSITIVE</Direction>
				<Position>BOTTOM</Position>
				<NumberRange>
					<MinValue>5.0</MinValue>
					<MaxValue>10.0</MaxValue>
				</NumberRange>
				<RendererRef ref="NumberAxisRenderer" />
			</Axis>
			-->
		</Mappers>
		<Renderers>
			<AxisRenderer id="NumberWspAxisRenderer">
				<Provider epid="de.openali.odysseus.chart.ext.base.axisrenderer.provider.GenericNumberAxisRendererProvider">
					<Parameters>
						<Parameter name="color" value="#0000aa" />
						<Parameter name="inset_tick" value="1" />
					</Parameters>
				</Provider>
			</AxisRenderer>
			<AxisRenderer id="NumberDisAxisRenderer">
				<Provider epid="de.openali.odysseus.chart.ext.base.axisrenderer.provider.GenericNumberAxisRendererProvider">
					<Parameters>
						<Parameter name="color" value="#00aa00" />
						<Parameter name="inset_tick" value="1" />
					</Parameters>
				</Provider>
			</AxisRenderer>
			<AxisRenderer id="NumberVelAxisRenderer">
				<Provider epid="de.openali.odysseus.chart.ext.base.axisrenderer.provider.GenericNumberAxisRendererProvider">
					<Parameters>
						<Parameter name="color" value="#aa0000" />
						<Parameter name="inset_tick" value="1" />
					</Parameters>
				</Provider>
			</AxisRenderer>
			<AxisRenderer id="CalendarAxisRenderer">
				<Provider epid="de.openali.odysseus.chart.ext.base.axisrenderer.provider.GenericCalendarAxisRendererProvider">
					<Parameters>
						<Parameter name="color" value="#000000" />
						<Parameter name="date-format" value="dd.MM.\nHH:mm"/>
						<Parameter name="inset_tick" value="1" />
					</Parameters>
				</Provider>
			</AxisRenderer>
		</Renderers>
		
	</Chart>


	<!-- TODO: depth layer -->
	<!-- TODO: sohlschubspannung layer -->
	<!-- TODO: other layers ? -->




</ChartConfiguration>
