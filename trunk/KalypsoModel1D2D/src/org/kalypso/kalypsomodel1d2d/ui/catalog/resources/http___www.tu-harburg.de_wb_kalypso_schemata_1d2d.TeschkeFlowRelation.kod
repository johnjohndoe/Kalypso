<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ChartConfiguration xmlns="http://www.openali.de/odysseus/chartconfig/0.2.0/"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.openali.de/odysseus/chartconfig/0.2.0/ ../../../../../../../../OdysseusChartFactory/etc/binding/schema/ChartConfiguration.xsd">

	


	<Chart id="runoffChart">
		<Title horizontalTextAnchor="CENTER">Abfluss</Title>
		<Description>Abflusswerte der Berechnung</Description>
		<Layers>

			<Layer visible="true" id="runoffLayer">
				<Provider epid="org.kalypso.model.wspm.ui.featureview.TupleResultLineLayerProvider" >
					<Parameters>
						<Parameter name="featureKey" value="theResultFeatureKey" />
						<Parameter name="propertyName" value="{http://www.tu-harburg.de/wb/kalypso/schemata/1d2d}pointsMember" />
						<Parameter name="showPoints" value="true" />
						<Parameter name="showLines" value="false" />
						<Parameter name="domainComponentId"
							value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#Depth" />
						<Parameter name="valueComponentId"
							value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#Runoff" />
					</Parameters>
				</Provider>
				<Title>Abfluss</Title>
				<Description>Abfluss abhängig vom Wasserspiegel</Description>
				<MapperRefs>
					<DomainAxisRef ref="wsp_Axis" />
					<TargetAxisRef ref="runoff_Axis" />
				</MapperRefs>
				<Styles>
					<PointStyle role="point">
						<Title>Runoff</Title>
						<Width>8</Width>
						<Height>8</Height>
						<Stroke>
							<Width>1</Width>
							<LineColor>000000</LineColor>
						</Stroke>
						<FillColor>ff0000</FillColor>
					</PointStyle>
					<LineStyle role="line" isVisible="false">
						<Title></Title>
					</LineStyle>
					
					<!-- war: runoffStyle" -->
				</Styles>
			</Layer>
			
			<Layer visible="true" id="runoffPolynomeLayer">
				<Provider epid="org.kalypso.model.wspm.ui.featureview.PolynomeLayerProvider" >
					<Parameters>
						<Parameter name="featureKey" value="theResultFeatureKey" />
						<Parameter name="propertyName" value="{http://www.tu-harburg.de/wb/kalypso/schemata/1d2d}polynomialMember" />
						<Parameter name="domainId"
							value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#phenomenonRunoff" />
					</Parameters>
				</Provider>
				<Title>Abflusspolynome</Title>
				<Description>Abflusspolynome abhängig vom Wasserspiegel</Description>
				<MapperRefs>
					<DomainAxisRef ref="wsp_Axis" />
					<TargetAxisRef ref="runoff_Axis" />
				</MapperRefs>
				<Styles>
					<LineStyle role="line">
						<!-- war: "polynomeStyle" -->
						<Title>Polynome</Title>
						<Width>3</Width>
						<LineColor>00aaff</LineColor>
					</LineStyle>
				</Styles>
			</Layer>
			
			<!-- TODO: show Histogramm instead 
			<Layer visible="true" id="runoffDeltaLayer">
				
				<Provider epid="org.kalypso.model.wspm.ui.featureview.TupleResultLineLayerProvider" >
					<Parameters>
						<Parameter name="featureKey" value="theResultFeatureKey" />
						<Parameter name="propertyName" value="{http://www.tu-harburg.de/wb/kalypso/schemata/1d2d}pointsMember" />
						<Parameter name="showPoints" value="true" />
						<Parameter name="showLines" value="false" />
						<Parameter name="domainComponentId"
							value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#Depth" />
						<Parameter name="valueComponentId"
							value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#DeltaRunoff" />
					</Parameters>
				</Provider>
				<Title>Abfluss-Delta</Title>
				<Description>Abfluss Differenzen bei der Polynomerstellung</Description>
				<Mapper>
					<DomainAxisRef ref="wsp_Axis" />
					<TargetAxisRef ref="runoffDelta_Axis" />
				</Mapper>
				<Style>
					<StyleRef ref="runoffDeltaStyle" />
				</Style>
			</Layer>
			-->
			
		</Layers>
		<Mappers>
			<Axis id="wsp_Axis">
				<Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
				<Label>Wassertiefe [m]</Label>
				<Direction>POSITIVE</Direction>
				<Position>BOTTOM</Position>
				<NumberRange>
					<MinValue>0.0</MinValue>
					<MaxValue>100.0</MaxValue>
				</NumberRange>
				<RendererRef ref="NumberAxisRendererRunoff"/>
			</Axis>
			<Axis id="runoff_Axis">
				<Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
				<Label>Abfluss [m³/s]</Label>
				<Direction>POSITIVE</Direction>
				<Position>LEFT</Position>
				<NumberRange>
					<MinValue>5.0</MinValue>
					<MaxValue>10.0</MaxValue>
				</NumberRange>
				<RendererRef ref="NumberAxisRendererRunoff"/>
			</Axis>
		</Mappers>
		<Renderers>
			<AxisRenderer id="NumberAxisRendererRunoff">
				<Provider epid="de.openali.odysseus.chart.ext.base.axisrenderer.provider.GenericNumberAxisRendererProvider">
					<Parameters>
						<Parameter name="inset_tick" value="1"/>
					</Parameters>
				</Provider>
			</AxisRenderer>
		</Renderers>
		
	</Chart>
	
	
	
	
	
	


	


