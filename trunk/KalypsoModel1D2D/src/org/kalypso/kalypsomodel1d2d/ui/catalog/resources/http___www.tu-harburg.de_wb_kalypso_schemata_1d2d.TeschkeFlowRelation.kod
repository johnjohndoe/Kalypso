							value="urn:ogc:gml:dict:kalypso:model:wspmtuhh:qIntervallPointsComponents#Alpha" />
					</Parameters>
				</Provider>
				<Title>Alpha</Title>
				<Description>Alpha abh㭧ig vom Wasserspiegel</Description>
				
				<MapperRefs>
					<DomainAxisRef ref="wsp_Axis" />
					<TargetAxisRef ref="alpha_Axis" />
				</MapperRefs>
				<Styles>
					<PointStyle role="point">
						<!-- war: "alphaStyle" -->
						<Title>Alpha</Title>
						<Width>8</Width>
						<Height>8</Height>
						<Stroke>
							<Width>1</Width>
							<LineColor>000000</LineColor>
						</Stroke>
						<FillColor>00a000</FillColor>
					</PointStyle>
					<LineStyle role="line" isVisible="false">
						<Title></Title>
					</LineStyle>
				</Styles>
			</Layer>
			
			
			<Layer visible="true" id="alphaPolynomeLayer">
