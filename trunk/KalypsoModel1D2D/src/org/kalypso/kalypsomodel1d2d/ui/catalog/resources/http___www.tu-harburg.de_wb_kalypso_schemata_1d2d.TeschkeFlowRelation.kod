				</MapperRefs>
				<Styles>
					<LineStyle role="line">
						<!-- war: "polynomeStyle" -->
						<Title>Polynome</Title>
						<Width>3</Width>
						<LineColor>00aaff</LineColor>
					</LineStyle>
				</Styles>
				
			</Layer>

			
			<!-- TODO: show Histogramm instead 
