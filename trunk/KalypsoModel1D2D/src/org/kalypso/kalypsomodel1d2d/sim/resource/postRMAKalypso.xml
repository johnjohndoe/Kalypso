<?xml version="1.0" encoding="UTF-8"?>
<modelspec xmlns="simspec.core.simulation.kalypso.org"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	typeID="org.kalypso.simulation.rma.postRMAKalypso">

	<input id="results" type="xs:anyURI" description="2d-result directory" />
	<input id="control" type="xs:anyURI" description="control gml-file" />
	<input id="mesh" type="xs:anyURI" description="mesh gml-file" />
	<input id="flowRelationships" type="xs:anyURI" description="flow relationships gml-file" />
	<input id="scenarioResultMeta" type="xs:anyURI"
		description="scenario results metadata gml-file" />
	<input id="singleResultDate" type="xs:string" description="single result to be processed"
		optional="true" />

	<output id="scenarioResultMeta" type="xs:anyURI"
		description="updated scenario results metadata gml-file" />
	<output id="results" type="xs:anyURI" description="calculation unit results directory" />

</modelspec>