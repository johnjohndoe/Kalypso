<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="KalypsoGridInterpolation"
   name="KalypsoGridInterpolation"
   version="1.0.0"
   provider-name="kalypso.org"
   class="org.kalypso.interpolation.KalypsoInterpolationPlugin">
   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.kalypso.core"/>
      <import plugin="org.kalypso.deegree"/>
      <import plugin="org.kalypso.ui"/>
   </requires>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="KalypsoGridInterpolation.actionSet1"
            description="Gis &amp;Tools"
            visible="true"
            id="KalypsoGridInterpolation.actionSet1">
         <action
               toolbarPath="grid"
               label="Netz auf Raster &amp;interpolieren"
               icon="icons/sample.gif"
               class="org.kalypso.interpolation.action.GridInterpolationAction"
               tooltip="Netz auf Raster interpolieren"
               style="push"
               menubarPath="org.kalypso.gistools/gird"
               id="org.kalypso.interpolate.action"/>
         <menu
               label="Gis &amp;Tools"
               id="org.kalypso.gistools">
            <separator name="grid"/>
         </menu>
      </actionSet>
   </extension>

</plugin>
