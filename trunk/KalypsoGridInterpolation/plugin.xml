<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.grid.interpolation"
   name="KalypsoGridInterpolation"
   version="1.0.0"
   provider-name="kalypso.org"
   class="org.kalypso.interpolation.KalypsoInterpolationPlugin">
   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.kalypso.core"/>
      <import plugin="org.kalypso.deegree"/>
      <import plugin="org.kalypso.ui"/>
   </requires>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="org.kalypso.grid.interpolation.actionSet1"
            description="Gis &amp;Tools"
            visible="true"
            id="org.kalypso.grid.interpolation.actionSet1">
         <action
               toolbarPath="grid"
               label="%org.kalypso.interpolate.action.label"
               tooltip="Netz auf Raster interpolieren"
               class="org.kalypso.interpolation.action.GridInterpolationAction"
               icon="icons/sample.gif"
               style="push"
               menubarPath="org.kalypso.gistools/gird"
               id="org.kalypso.interpolate.action"/>
         <menu
               label="%org.kalypso.gistools.label"
               id="org.kalypso.gistools">
            <separator name="grid"/>
         </menu>
      </actionSet>
   </extension>

</plugin>
