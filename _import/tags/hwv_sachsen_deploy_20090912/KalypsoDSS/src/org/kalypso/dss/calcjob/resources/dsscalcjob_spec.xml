<?xml version="1.0" encoding="UTF-8"?>
<modelspec xmlns="simspec.core.simulation.kalypso.org"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	typeID="KalypsoDSS">

	<input id="metadata"
		description="Metadaten - Zur Steuerung des Calcjob" />
	<input id="measureRHB"
		description="Modelldaten - Massnahme Rueckhaltebecken"
		optional="true" />
	<input id="measureMRS"
		description="Modelldaten - Massnahme Mulden-Rigolen" optional="true" />
	<input id="measureSealing"
		description="Modelldaten - Massnahme Entsiegelung" optional="true" />
	<input id="designArea" description="Modelldaten - Plangebiet" />
	<input id="SceConf" description="Modelldaten - Optimierung"
		optional="true" />
	<input id="xplanung" description="Modelldaten - BPlan Massnahmen"
		optional="true" />

	<input id="lastResults" description="letzte Ergebnisse"
		optional="true" />
	<input id="doMeasures"
		description="Datei mit yes, no ob planung oder planung+massnahmen gerechnet werden sollen"
		optional="true" />

	<!-- Ergebnis-Dateien/Verzeichnisse -->
	<output id="OUT_ZML"
		description="Verzeichnis der Berechnungsergebnisse" />
</modelspec>
