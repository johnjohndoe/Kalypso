<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="org.kalypso.zwg.control" version="0.1" 
    xmlns:zwg="org.kalypso.zwg.control" 
    xmlns:gml="http://www.opengis.net/gml" 
    xmlns="http://www.w3.org/2001/XMLSchema" 
    elementFormDefault="qualified">
    
    <import namespace="http://www.opengis.net/gml" schemaLocation="feature.xsd"/>
    <element name="ZwgControl" type="zwg:_ZwgControlType"/>
    <complexType name="_ZwgControlBaseType">
        <complexContent>
            <restriction base="gml:AbstractFeatureType"/>
        </complexContent>
    </complexType>
    <complexType name="_ZwgControlType">
        <complexContent>
            <extension base="zwg:_ZwgControlBaseType">
                <sequence>
                    <element name="scenarioId" type="string" minOccurs="0" maxOccurs="1">
                        <annotation>
                            <documentation xml:lang="de">
                                <label>Szenario Id</label>
                                <tooltip>Id des Szenario in welchem die Rechnung durchgeführt wird</tooltip>
                                <description></description>
                            </documentation>
                        </annotation>
                    </element>
                    <element name="scenarioName" type="string" minOccurs="0" maxOccurs="1">
                        <annotation>
                            <documentation xml:lang="de">
                                <label>Szenario</label>
                                <tooltip>Name des Szenario in welchem die Rechnung durchgeführt wird</tooltip>
                                <description></description>
                            </documentation>
                        </annotation>
                    </element>
                    <element name="scenarioDescription" type="string" minOccurs="0" maxOccurs="1">
                        <annotation>
                            <documentation xml:lang="de">
                                <label>Szenario Beschreibung</label>
                                <tooltip>Beschreibung des Szenario in welchem die Rechnung durchgeführt wird</tooltip>
                                <description></description>
                            </documentation>
                        </annotation>
                    </element>
                    <element name="startsimulation" type="dateTime">
                        <annotation>
                            <documentation xml:lang="de">
                                <label>Start der Simulation</label>
                                <tooltip>Startzeitpunkt der Modellrechung</tooltip>
                                <description></description>
                            </documentation>
                        </annotation>
                    </element>
                    <element name="startforecast" type="dateTime">
                        <annotation>
                            <documentation xml:lang="de">
                                <label>Start der Vorhersage</label>
                                <tooltip>Startzeitpunkt der Vorhersagerechnung</tooltip>
                                <description></description>
                            </documentation>
                        </annotation>
                    </element>
                    <element name="editor" type="string">
                        <annotation>
                            <documentation xml:lang="de">
                                <label>Ersteller</label>
                                <tooltip>Name des Erstellers</tooltip>
                                <description></description>
                            </documentation>
                        </annotation>
                    </element>
                    <element name="description" type="string">
                        <annotation>
                            <documentation xml:lang="de">
                                <label>Beschreibung</label>
                                <tooltip>Kurzbeschreibung dieses Rechenfalls</tooltip>
                                <description></description>
                            </documentation>
                        </annotation>
                    </element>
                    <element name="comment" type="string">
                        <annotation>
                            <documentation xml:lang="de">
                                <label>Kommentar</label>
                                <tooltip>Langbeschreibung dieses Rechenfalls</tooltip>
                                <description></description>
                            </documentation>
                        </annotation>
                    </element>
                    <element name="calctime" type="dateTime">
                        <annotation>
                            <documentation xml:lang="de">
                                <label>Erstellungszeitpunkt</label>
                                <tooltip>Zeitpunkt der Erstellung des Rechenfalls</tooltip>
                                <description></description>
                            </documentation>
                        </annotation>
                    </element>
                    <element name="mergeCasePath" type="string">
                       	<annotation>
							<documentation xml:lang="de">
								<label>Fortgesetzte Rechenvariante</label>
								<tooltip>Pfad auf fortgesetzte Rechenvariante</tooltip>
								<description></description>
							</documentation>
						</annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
</schema>
