<?xml version="1.0" encoding="UTF-8"?>
<?NLS TYPE="org.eclipse.help.contexts"?>
<contexts>

	<!-- ********************************** -->
 	<!-- Wizard Pages 						-->
 	<!-- ********************************** -->
 	
 	<context id="share_project_page_context">
      <description>此页面帮助您选择您希望将项目连接到的目标站点的类型。</description>
   	</context>
   	
   	<context id="import_project_set_page_context">
      <description>此页面帮助您从文件系统上的文件中导入项目集。项目集是对一组共享项目的轻量级描述。它可用于重新构造一组您在工作空间中拥有的共享项目。</description>
   	</context>
	
	<context id="export_project_set_page_context">
      <description>此页面帮助您将项目集导出至可以与其他小组成员共享的文件。项目集是对一组共享项目的轻量级描述。它可用于重新构造一组您在工作空间中拥有的共享项目。</description>
   	</context>
	
	<context id="target_new_site_page_context">
      <description>此页面帮助您选择您的项目应当连接到的站点。您可以从现有站点的列表中选择，或者创建一个新站点。</description>
   	</context>
	
	<context id="target_mapping_selection_page_context">
      <description>此页面帮助您在选择的站点上（您的项目应当连接到此站点）选择文件夹。您可以选择现有的文件夹，或者创建一个新的文件夹。</description>
   	</context>
   	
    <context id="sync_resource_selection_page_context">
      <description>此页面允许您选择要同步的资源。</description>
   	</context>
	
	
 	<!-- ********************************** -->
 	<!-- Preference Pages 					-->
 	<!-- ********************************** -->
 	
 	<context id="ignore_preference_page_context" >
      <description>您可能想从版本控制中排除某些资源。将不会发布其名称与任何启用模式相匹配的资源。允许使用通配符‘*’和‘?’。这些设置将不会影响已经处于版本控制下的资源。
</description>
      <topic label="创建全局忽略模式" href="tasks/tasks-100d1.htm"/>
   	</context>

 	<context id="team_preference_page_context" >
      <description>此页面允许您设置“同步”视图的打开方式。</description>
      <topic label="CVS 同步视图" href="reference/ref-33.htm"/>
   	</context>

 	<context id="file_type_preference_page_context" >
      <description>此页面允许您使文件扩展名与其内容类型（ASCII 或二进制）相关联。这些设置由资源库提供程序（如 CVS）使用</description>
      <topic label="设置文件扩展名的内容类型" href="tasks/tasks-team-1.htm"/>
   	</context>
   	   	  	
   	<!-- ********************************** -->
 	<!-- TargetMenuActions 					-->
 	<!-- ********************************** -->
	
	<context id="target_get_action_context">
   		<description>从站点将所选资源下载到工作空间。</description>
   	</context>

 	<context id="target_put_action_context">
   		<description>从工作空间将所选资源上载到站点。</description>
   	</context>
   	
   	<context id="target_synchronize_action_context">
   		<description>在“同步”视图中显示所选资源的本地和远程站点状态之间的差别。</description>
   	</context>
   	
   	<context id="target_getas_action_context">
   		<description>从站点将所选资源下载到工作空间。您可以指定资源的本地名称。</description>
   	</context>
   	
   	<context id="target_getasproject_action_context">
   		<description>从站点将所选资源下载到工作空间。可以在工作空间中为每个选择的项创建一个项目。</description>
   	</context>
   	
   	<context id="target_disconnect_action_context">
   		<description>从记住的站点列表中废弃选择的目标站点。</description>
   	</context>
   	
   	<!-- ********************************** -->
 	<!-- TargetCatchupReleaseViewer			-->
 	<!-- ********************************** -->
	
	<context id="target_catchup_release_viewer_context">
   		<description>此视图显示传入、传出和冲突的更改。</description>
   	</context>
   	
   	<!-- ********************************** -->
 	<!-- TargetActions						-->
 	<!-- ********************************** -->
   	
   	<context id="sync_get_action_context">
   		<description>从站点将所选资源下载到工作空间。</description>
   	</context>
   	
   	<context id="sync_put_action_context">
   		<description>从工作空间将所选资源上载到站点。</description>
   	</context>
   	
   	<!-- ********************************** -->
 	<!-- Views					            -->
 	<!-- ********************************** -->
 	
 	<context id="site_explorer_view_context">
      <description>“站点浏览器”视图允许您浏览已知的目标站点的内容并将远程资源下载到本地工作空间。</description>
   	</context>

 	<context id="sync_view_context">
      <description>“同步”视图允许工作空间的本地内容与远程资源库或目标同步。</description>
      <topic label="CVS 同步视图" href="reference/ref-33.htm"/>
   	</context>
   	   	
   	<!-- ********************************** -->
 	<!-- SiteExplorerViewActions		    -->
 	<!-- ********************************** -->
 	
 	<context id="add_site_action_context">
      <description>创建一个新的目标站点，您可以将资源上载或下载到该站点。</description>
   	</context>
   	
   	<context id="new_folder_action_context">
      <description>在目标站点上创建一个新的文件夹。</description>
   	</context>
   	
   	<!-- ********************************** -->
 	<!-- SyncViewActions				    -->
 	<!-- ********************************** -->
 	
 	<context id="refresh_action_context">
      <description>刷新视图的内容。</description>
   	</context>

   	<context id="open_action_context">
      <description>在比较查看器中显示文件的本地和远程内容的比较。</description>
   	</context>
   	 	
   	<context id="expandall_action_context">
      <description>完全展开所有选择的项。</description>
   	</context>
   	
   	<context  id="remove_action_context">
      <description>从“同步”视图除去选择的项。</description>
   	</context>
   	
   	<context  id="navigator_show_action_context">
      <description>在“导航器”视图中选择和显示所选的资源。</description>
   	</context>
 
</contexts>
