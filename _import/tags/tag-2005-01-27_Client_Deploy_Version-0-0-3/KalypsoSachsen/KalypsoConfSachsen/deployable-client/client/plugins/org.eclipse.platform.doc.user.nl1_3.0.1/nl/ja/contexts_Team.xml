<?xml version="1.0" encoding="UTF-8"?>
<?NLS TYPE="org.eclipse.help.contexts"?>
<contexts>

	<!-- ********************************** -->
 	<!-- Wizard Pages 						-->
 	<!-- ********************************** -->
 	
 	<context id="share_project_page_context">
      <description>このページは、プロジェクトを接続したいターゲット・サイトのタイプを選択するために役立ちます。</description>
   	</context>
   	
   	<context id="import_project_set_page_context">
      <description>このページは、ファイル・システム上のファイルからプロジェクト・セットをインポートするために役立ちます。プロジェクト・セットとは、共用プロジェクト・セットの簡単な説明です。
これは、ワークスペースにある共用プロジェクトのラインナップを再構成するために使用できます。</description>
   	</context>
	
	<context id="export_project_set_page_context">
      <description>このページは、他のチーム・メンバーと共用できるファイルにプロジェクト・セットをエクスポートするために役立ちます。プロジェクト・セットとは、共用プロジェクト・セットの簡単な説明です。
これは、ワークスペースにある共用プロジェクトのラインナップを再構成するために使用できます。</description>
   	</context>
	
	<context id="target_new_site_page_context">
      <description>このページは、プロジェクトを接続するサイトを選択するために役立ちます。既存のサイトのリストから選択するか、または新しいサイトを作成することができます。</description>
   	</context>
	
	<context id="target_mapping_selection_page_context">
      <description>このページは、プロジェクトを接続する選択済みサイト上でフォルダーを選択するために役立ちます。既存のフォルダーを選択するか、または新規フォルダーを作成することができます。</description>
   	</context>
   	
    <context id="sync_resource_selection_page_context">
      <description>このページを使用すると、同期するリソースを選択できます。</description>
   	</context>
	
	
 	<!-- ********************************** -->
 	<!-- Preference Pages 					-->
 	<!-- ********************************** -->
 	
 	<context id="ignore_preference_page_context" >
      <description>バージョン管理から特定のリソースを除外することができます。使用可能パターンに一致する名前を持つリソースはリリースされません。ワイルドカード文字 '*' および '?' が許可されています。これらの設定は、既にバージョン管理されているリソースには影響を与えません。</description>
      <topic label="グローバルな無視パターンの作成" href="tasks/tasks-100d1.htm"/>
   	</context>

 	<context id="team_preference_page_context" >
      <description>このページでは、「同期化」ビューが開くときのモードを設定することができます。</description>
      <topic label="「CVS 同期化」ビュー" href="reference/ref-33.htm"/>
   	</context>

 	<context id="file_type_preference_page_context" >
      <description>このページでは、ファイル拡張子とそのコンテンツ型 (ASCII またはバイナリー) を関連付けることができます。これらの設定は、CVS などのリポジトリー・プロバイダーによって使用されます。</description>
      <topic label="ファイル拡張子のコンテンツ・タイプの設定" href="tasks/tasks-team-1.htm"/>
   	</context>
   	   	  	
   	<!-- ********************************** -->
 	<!-- TargetMenuActions 					-->
 	<!-- ********************************** -->
	
	<context id="target_get_action_context">
   		<description>選択されたリソースをサイトからワークスペースにダウンロードします。</description>
   	</context>

 	<context id="target_put_action_context">
   		<description>選択されたリソースをワークスペースからサイトにアップロードします。</description>
   	</context>
   	
   	<context id="target_synchronize_action_context">
   		<description>「同期化」ビューに、選択されたリソースのローカルおよびリモート・サイトの状態の違いを表示します。</description>
   	</context>
   	
   	<context id="target_getas_action_context">
   		<description>選択されたリソースをサイトからワークスペースにダウンロードします。リソースのローカル名を指定できます。</description>
   	</context>
   	
   	<context id="target_getasproject_action_context">
   		<description>選択されたリソースをサイトからワークスペースにダウンロードします。選択された項目それぞれについて、ワークスペースにプロジェクトが作成されます。</description>
   	</context>
   	
   	<context id="target_disconnect_action_context">
   		<description>記憶されたサイトのリストから、選択されたターゲット・サイトを廃棄します。</description>
   	</context>
   	
   	<!-- ********************************** -->
 	<!-- TargetCatchupReleaseViewer			-->
 	<!-- ********************************** -->
	
	<context id="target_catchup_release_viewer_context">
   		<description>このビューは着信、発信、および競合変更を表示します。</description>
   	</context>
   	
   	<!-- ********************************** -->
 	<!-- TargetActions						-->
 	<!-- ********************************** -->
   	
   	<context id="sync_get_action_context">
   		<description>選択されたリソースをサイトからワークスペースにダウンロードします。</description>
   	</context>
   	
   	<context id="sync_put_action_context">
   		<description>選択されたリソースをワークスペースからサイトにアップロードします。</description>
   	</context>
   	
   	<!-- ********************************** -->
 	<!-- Views					            -->
 	<!-- ********************************** -->
 	
 	<context id="site_explorer_view_context">
      <description>「サイト・エクスプローラー」ビューでは、既知のターゲット・サイトのコンテンツをブラウズし、リモート・リソースをローカル・ワークスペースにダウンロードすることができます。</description>
   	</context>

 	<context id="sync_view_context">
      <description>「同期化」ビューでは、ワークスペースのローカル・コンテンツとリモート・リポジトリーまたはターゲットを同期化することができます。</description>
      <topic label="「CVS 同期化」ビュー" href="reference/ref-33.htm"/>
   	</context>
   	   	
   	<!-- ********************************** -->
 	<!-- SiteExplorerViewActions		    -->
 	<!-- ********************************** -->
 	
 	<context id="add_site_action_context">
      <description>リソースをアップロードまたはダウンロードできる新しいターゲット・サイトを作成します。</description>
   	</context>
   	
   	<context id="new_folder_action_context">
      <description>ターゲット・サイトに新規フォルダーを作成します。</description>
   	</context>
   	
   	<!-- ********************************** -->
 	<!-- SyncViewActions				    -->
 	<!-- ********************************** -->
 	
 	<context id="refresh_action_context">
      <description>ビューの内容を最新表示します。</description>
   	</context>

   	<context id="open_action_context">
      <description>ファイルのローカル・コンテンツおよびリモート・コンテンツの比較を比較ビューアー内に表示します。</description>
   	</context>
   	 	
   	<context id="expandall_action_context">
      <description>選択された項目すべてを完全に展開します。</description>
   	</context>
   	
   	<context  id="remove_action_context">
      <description>選択された項目を「同期化」ビューから除去します。</description>
   	</context>
   	
   	<context  id="navigator_show_action_context">
      <description>「ナビゲーター」ビューで、項目を選択および表示します。</description>
   	</context>
 
</contexts>
