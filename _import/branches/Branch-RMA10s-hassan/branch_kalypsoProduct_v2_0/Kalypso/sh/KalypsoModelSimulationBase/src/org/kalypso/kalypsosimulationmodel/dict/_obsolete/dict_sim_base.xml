     <!-- ================================================================
     	Water Level
     ==================================================================-->
     <gml:dictionaryEntry>
          <swe:ItemDefinition gml:id="WaterLevel">
               <gml:name>Station</gml:name>
               <swe:property>
                    <swe:Phenomenon gml:id="phen_waterlevel">
                         <gml:description>Water Level</gml:description>
                         <gml:name>Water Level</gml:name>
                    </swe:Phenomenon>
               </swe:property>
               <swe:representation>
                    <swe:Number>
						<gml:unitOfMeasure uom="m"/>	
					</swe:Number>
                    <!-- swe:SimpleType>
                         <st:restriction base="decimal">
                              <st:fractionDigits value="4"/>
                         </st:restriction>
					 <gml:unitOfMeasure uom="dict_uom.xml#m"/>			
                    </swe:SimpleType-->
               </swe:representation>
          </swe:ItemDefinition>
     </gml:dictionaryEntry>

