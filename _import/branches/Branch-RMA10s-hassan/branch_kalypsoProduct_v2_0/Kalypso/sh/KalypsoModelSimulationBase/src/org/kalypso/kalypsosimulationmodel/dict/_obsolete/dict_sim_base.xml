		Time
	================================================================== -->	
     <gml:dictionaryEntry>
          <swe:ItemDefinition gml:id="Time">
               <gml:name>Time</gml:name>
               <swe:property>
                    <swe:Phenomenon gml:id="phen_time">
                         <gml:name>Sohlhöhe</gml:name>
                    </swe:Phenomenon>
               </swe:property>
               <swe:representation>                
               		<swe:SimpleType>
	               		<st:restriction base="time">                            
                    	</st:restriction>
                    	<swe:noScale/>
                    </swe:SimpleType>
               </swe:representation>
          </swe:ItemDefinition>
     </gml:dictionaryEntry>
     
	<swe:ItemDefinition gml:id="TimeX">
			<gml:name>time</gml:name>
			<!--  swe:property xlink:href="http://sweet.jpl.nasa.gov/ontology/time.owl#Instant"/-->
			<swe:property>
                    <swe:Phenomenon gml:id="phen_time">
                         <gml:name>Time</gml:name>
                    </swe:Phenomenon>
               </swe:property>
			<swe:representation>
				<swe:General>
					<xst:restriction base="xs:dateTime"/>
					<swe:frame xlink:href="#ISO-8601"/>
				</swe:General>
			</swe:representation>
	</swe:ItemDefinition>
