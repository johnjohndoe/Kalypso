<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.model.spree"
   name="KalypsoModelSpree"
   version="1.0.0"
   provider-name="kalypso.org">
   <runtime>
      <library name="kalypsoModelSpree.jar"/>
   </runtime>
   <requires>
      <import plugin="org.kalypso.commons"/>
      <import plugin="org.kalypso.core"/>
      <import plugin="org.kalypso.jwsdp"/>
      <import plugin="org.kalypso.services.calculation.local"/>
      <import plugin="org.kalypso.dwd"/>
   </requires>
   <extension
         point="org.kalypso.services.calculation.local.calcJob">
      <calcJob
            typeID="spree"
            catalogClass="org.kalypso.lhwzsachsen.spree.SpreeUrlCatalog"
            jobClass="org.kalypso.lhwzsachsen.spree.SpreeCalcJob"/>
      <calcJob
            typeID="schwarzeelster"
            catalogClass="org.kalypso.lhwzsachsen.spree.SpreeUrlCatalog"
            jobClass="org.kalypso.lhwzsachsen.spree.SchwarzeElsterCalcJob"/>
   </extension>

</plugin>
