        <arg name="ignoreType1" value="Q"/>
		<arg name="ignoreType2" value="W"/>
        <arg name="ignoreLabel1" value="Wasserstand"/>
        <arg name="ignoreLabel2" value="Abfluss"/>
		
		<arg name="pegelNameProperty" value="Name"/>
		<arg name="resultProperty" value="Wasserstand_vorhersage"/>

		<arg name="pageID" value="Berichtsablage"/>
		<arg name="htmlFilePath" value=".model/wizardhtml/Berichtsablage.html"/>
 		<arg name="helpId" value="&hid;sec_Assi_schwElster_05_Berichtsablage"/>

		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="25"/>

		<arg name="exporter1">
			<arg name="id" value="featureWithTemplateExporter"/>
			<arg name="name" value="Vorhersagepegel"/>
			<arg name="nameproperty" value="Name"/>
			<arg name="width" value="1024"/>
			<arg name="height" value="768"/>
			<arg name="imageFormat" value ="png"/>
			<arg name="tokens">
				<arg name="title" value="Name"/>
				<arg name="href1" value="Wasserstand_gemessen"/>
				<arg name="href2" value="Wasserstand_gerechnet"/>
				<arg name="href3" value="Wasserstand_vorhersage"/>
				<arg name="href4" value="Niederschlag_rechnung"/>
				<arg name="href5" value="Wasserstand_vorhersage_unten"/>
				<arg name="href6" value="Wasserstand_vorhersage_oben"/>
			</arg>
			<arg name="templateDiagQ">
				<arg name="label" value="Abflussgrafik"/>
				<arg name="href" value="project:/.templates/calcCase/berichtDiagQ.odt"/>
			</arg>
			<arg name="templateDiagW">
				<arg name="label" value="Wasserstandsgrafik"/>
				<arg name="href" value="project:/.templates/calcCase/berichtDiagW.odt"/>
			</arg>
			<arg name="templateTable">
				<arg name="label" value="Tabelle"/>
				<arg name="href" value="project:/.templates/calcCase/berichtTabelle.ott"/>
			</arg>
		</arg>
		<!-- Scheitelwert-Tabellen W / Q -->
		<arg name="exporter3">
			<arg name="id" value="multiExporter"/>
			<arg name="name" value="Scheitelwerte"/>
			<arg name="description" value="Exportiert Vorhersageeergebnisse in Form von Scheitelwerttabellen."/>
			<arg name="exporter3a">
				<arg name="id" value="sumUpTableExporter"/>
				<arg name="name" value="Wasserstand"/>
				<arg name="documentName" value="ScheitelwerteW.csv"/>
				<arg name="separator" value=";"/>
				<!--arg name="charset" value="cp1252"/-->
				<arg name="delta" value="0.0001"/>
				<arg name="dateFormat" value="dd.MM.yyyy HH:mm:ss"/>
				<arg name="column1">
					<arg name="position" value="1"/>
					<arg name="label" value="Pegel"/>
					<arg name="content" value="arg:label"/>
				</arg>
				<arg name="axisType" value="W"/>
				<arg name="timeUnit" value="HOUR"/>
				<arg name="timeStep" value="6"/>
				<arg name="obs1">
					<arg name="label" value="Trado 1"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/QV_TRADO.zml"/>
				</arg>
				<arg name="obs2">
					<arg name="label" value="Schönau"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/QV_SCHOEN.zml"/>
				</arg>
				<arg name="obs3">
					<arg name="label" value="Zescha"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/QV_ZESCHA.zml"/>
				</arg>
				<arg name="obs4">
					<arg name="label" value="Neuwiese"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/QV_NEUWIE.zml"/>
				</arg>
			</arg>
			<arg name="exporter4">
				<arg name="id" value="sumUpTableExporter"/>
				<arg name="name" value="Abfluss"/>
				<arg name="documentName" value="ScheitelwerteQ.csv"/>
				<arg name="separator" value=";"/>
				<!--arg name="charset" value="cp1252"/-->
				<arg name="delta" value="0.0001"/>
				<arg name="dateFormat" value="dd.MM.yyyy HH:mm:ss"/>
				<arg name="column1">
					<arg name="position" value="1"/>
					<arg name="label" value="Pegel"/>
					<arg name="content" value="arg:label"/>
				</arg>
				<arg name="axisType" value="Q"/>
				<arg name="timeUnit" value="HOUR"/>
				<arg name="timeStep" value="6"/>
				<arg name="obs1">
					<arg name="label" value="Trado 1"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/QV_TRADO.zml"/>
				</arg>
				<arg name="obs2">
					<arg name="label" value="Schönau"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/QV_SCHOEN.zml"/>
				</arg>
				<arg name="obs3">
					<arg name="label" value="Zescha"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/QV_ZESCHA.zml"/>
				</arg>
				<arg name="obs4">
					<arg name="label" value="Neuwiese"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/QV_NEUWIE.zml"/>
				</arg>
			</arg>
		</arg>
