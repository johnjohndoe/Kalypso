<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entities
[
	<!ENTITY hid "org.kalypso.manual.">
]>
<calcwizard xmlns="xml.model.kalypso.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" browserInitialSize="17" helpId="&hid;SA_Assi_allg_00_Start">
	<!-- 
ToDo: Help-Ids setzen!!! -->
	<!-- 
		Phase 1: KONTROLLE DER ZEITREIHEN
	-->
	<!-- Kontrolle Zwischengebietspegel -->
	<page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ObservationMapTableDiagWizardPage" pageTitle="Kontrolle Zwischengebietszuflüsse (Pegel) "
		imageLocation="icons/calcwizard/kalypso32.gif">
		<arg name="pageID" value="Zwischengebietspegel"/>
		<arg name="htmlFilePath" value=".model/wizardhtml/01_Zwischengebietspegel.html"/>
		<arg name="helpId" value="&hid;sec_Assi_elbeZwge_02_Zwischengebietspegel"/>
		<!--  -->
		<arg name="mapTemplate" value=".templates/calcCase/wiz01_zwischengebietspegel.gmt"/>
		<arg name="maximizeMap" value="true"/>
		<arg name="mapWidgetID" value="SELECT"/>
		<!--  -->
		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="50"/>
		<!--  -->
		<arg name="zmlTableShow" value="all"/>
		<arg name="timeserie1" value="nameColumn=name#nameString=%featureprop% - %axisname%#linkColumn=ganglinie_messwerte"/>
		<!--  -->
		<arg name="ignoreType1" value="Q"/>
		<arg name="ignoreType2" value="W"/>
		<arg name="ignoreLabel1" value="Wasserstand"/>
		<arg name="ignoreLabel2" value="Abfluss"/>
	</page>
	<!-- Berechnung Zwischengebiet Usti - Dresden -->
	<page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.MapAndFeatureWizardPage" pageTitle="Berechnung Zwischengebietszufluss Usti/Dresden"
		imageLocation="icons/calcwizard/kalypso32.gif">
		<arg name="pageID" value="UstiDresden"/>
		<arg name="htmlFilePath" value=".model/wizardhtml/02_ZWG_UstiDresden.html"/>
		<arg name="helpId" value="&hid;sec_Assi_elbeZwge_03_ZwgUstiDresden"/>
		<!--  -->
		<arg name="mapTemplate" value=".templates/calcCase/wiz02_ZwgUstiDresden.gmt"/>
		<arg name="maximizeMap" value="true"/>
		<arg name="mapWidgetID" value="SELECT"/>
		<!--  -->
		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="50"/>
		<!--
			FeatureView -->
		<arg name="featureTemplate" value=".templates/calcCase/wiz02_ZwgUstiDresden.gft"/>
		<arg name="selectFeatureID" value="root"/>
		<arg name="featureControlStyle" value="SWT.NONE"/>
		<!-- 
			Diagramm -->
		<arg name="timeserie1" value="nameColumn=Name#nameString=Messwerte#linkColumn=ganglinie_messwerte#color=0x0000ff"/>
		<arg name="timeserie2" value="nameColumn=Name#nameString=Modellwerte#linkColumn=ganglinie_modellwerte#color=0xff0000"/>
		<!--  -->
		<arg name="ignoreType1" value="Q"/>
		<arg name="ignoreType2" value="W"/>
		<arg name="ignoreLabel1" value="Wasserstand"/>
		<arg name="ignoreLabel2" value="Abfluss"/>
	</page>
	<!-- Berechnung Zwischengebiet Dresden - Torgau -->
	<page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.MapAndFeatureWizardPage" pageTitle="Berechnung Zwischengebietszufluss Dresden/Torgau"
		imageLocation="icons/calcwizard/kalypso32.gif">
		<arg name="pageID" value="DresdenTorgau"/>
		<arg name="htmlFilePath" value=".model/wizardhtml/03_ZWG_DresdenTorgau.html"/>
		<arg name="helpId" value="&hid;sec_Assi_elbeZwge_04_ZwgDresdenTorgau"/>
		<!--  -->
		<arg name="mapTemplate" value=".templates/calcCase/wiz03_ZwgDresdenTorgau.gmt"/>
		<arg name="maximizeMap" value="true"/>
		<arg name="mapWidgetID" value="SELECT"/>
		<!--  -->
		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="50"/>
		<!--
			FeatureView -->
		<arg name="featureTemplate" value=".templates/calcCase/wiz03_ZwgDresdenTorgau.gft"/>
		<arg name="selectFeatureID" value="root"/>
		<arg name="featureControlStyle" value="SWT.NONE"/>
		<!-- 
			Diagramm -->
		<arg name="timeserie1" value="nameColumn=name#nameString=%featureprop% - %axisname%#linkColumn=ganglinie_gesamt"/>
		<arg name="timeserie2" value="nameColumn=Name#nameString=Modellwerte#linkColumn=ganglinie_messwerte"/>
		<!--  -->
		<arg name="ignoreType1" value="Q"/>
		<arg name="ignoreType2" value="W"/>
		<arg name="ignoreLabel1" value="Wasserstand"/>
		<arg name="ignoreLabel2" value="Abfluss"/>
	</page>
	<!-- Kontrolle Abfluss - Zwischengebiete -->
	<page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ObservationMapTableDiagWizardPage" pageTitle="Kontrolle Abfluss - Zwischengebiete"
		imageLocation="icons/calcwizard/kalypso32.gif">
		<!--  
			HTML-Navigation und pageID-->
		<arg name="pageID" value="Zwischengebiete"/>
		<arg name="htmlFilePath" value=".model/wizardhtml/04_ZwischengebietsErgebnisse.html"/>
		<arg name="helpId" value="&hid;sec_Assi_elbeZwge_05_AbflussZwge"/>
		<!-- 
			Page-Layout -->
		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="30"/>
		<arg name="hideCalcButton" value="true"/>
		<!-- 
			Karte -->
		<arg name="mapTemplate" value=".templates/calcCase/wiz04_ZwischengebietsErgebnisse.gmt"/>
		<arg name="mapTemplateContextIsCalcCase" value="true"/>
		<arg name="maximizeMap" value="true"/>
		<arg name="mapWidgetID" value="SELECT"/>
		<!--  -->
		<arg name="zmlTableShow" value="all"/>
		<!-- 
			Diagramm -->
		<arg name="timeserie1" value="nameColumn=name#nameString=%featureprop% - %axisname%#linkColumn=ganglinie_gesamt"/>
		<arg name="timeserie2" value="nameColumn=Name#nameString=Modellwerte#linkColumn=ganglinie_messwerte"/>
		<!--  -->
		<arg name="ignoreType1" value="W"/>
	</page>
	<!-- Phase 3: Ergebnisdarstellung und Ablage -->
	<page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ExportResultsWizardPage" pageTitle="Ergebnisbearbeitung und Veröffentlichung"
		imageLocation="icons/calcwizard/kalypso32.gif">
		<!--  
			HTML-Navigation und pageID-->
		<arg name="pageID" value="Berichtsablage"/>
		<arg name="htmlFilePath" value=".model/wizardhtml/05_Berichtsablage.html"/>
		<arg name="helpId" value="&hid;sec_Assi_elbeZwge_06_Berichtsablage"/>
		<!-- 
			Page-Layout -->
		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="30"/>
		<!-- 
			Karte -->
		<arg name="mapTemplate" value=".templates/calcCase/wiz05_berichtsablage.gmt"/>
		<arg name="mapTemplateContextIsCalcCase" value="true"/>
		<arg name="maximizeMap" value="true"/>
		<arg name="mapWidgetID" value="SELECT"/>
		<!-- 
			Diagramm -->
		<!--  -->
		<arg name="ignoreType1" value="W"/>
		<!--  -->
		<arg name="pegelNameProperty" value="Name"/>
		<arg name="resultProperty" value="ganglinie_modellwerte"/>
		<!-- tschechische Vorhersage auch mit anzeigen -->
		<arg name="timeserie1" value="nameString=%axisname%#linkColumn=Niederschlag_rechnung"/>
		<arg name="timeserie2" value="nameString=Beobachtung#linkColumn=Wasserstand_gemessen#color=0x0000ff"/>
		<arg name="timeserie3" value="nameString=Rechnung#linkColumn=Wasserstand_gerechnet#color=0xff0000"/>
		<arg name="timeserie4" value="nameString=Vorhersage#linkColumn=Wasserstand_vorhersage#color=0x008040"/>
		<arg name="timeserie5" value="nameString=untere Umhüllende#linkColumn=Wasserstand_vorhersage_unten#lineDash=4f#color=0x008040"/>
		<arg name="timeserie6" value="nameString=obere Umhüllende#linkColumn=Wasserstand_vorhersage_oben#lineDash=4f#color=0x008040"/>
	</page>
</calcwizard>
