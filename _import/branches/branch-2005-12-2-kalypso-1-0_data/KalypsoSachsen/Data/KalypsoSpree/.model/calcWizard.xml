<?xml version="1.0" encoding="UTF-8"?>
<calcwizard xmlns="xml.model.kalypso.org" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<!-- Allgemeine Argumente -->
	<arg name="showSaveButton" value="false"/>
	<arg name="askForSaveOnFinish" value="false"/>

	<!-- Phase 1: KONTROLLE DER ZEITREIHEN -->

	<!-- Kontrolle Ombrometer -->
    <page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ObservationMapTableDiagWizardPage" pageTitle="Kontrolle Niederschlag - Ombrometer " imageLocation="icons/calcwizard/kalypso32.gif" >
        <arg name="mapTemplate" value=".templates/calcCase/ombrometer.gmt"/>
        <arg name="mainSash" value="50"/>
        <arg name="rightSash" value="50"/>
        <arg name="timeserie1" value="nameColumn=name#nameString=%featureprop% - %axisname%#linkColumn=precipitation"/>
        <arg name="maximizeMap" value="true"/>
        <arg name="mapWidgetID" value="SELECT"/>
       	<arg name="selectFeatureID" value="Ombrometer-Sohland"/>
    </page>

	<!-- Kontrolle Niederschlag -->
    <page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ObservationMapTableDiagWizardPage" pageTitle="Kontrolle Niederschlag - Eingabepegel (NA-Modelle)" imageLocation="icons/calcwizard/kalypso32.gif" >
        <arg name="mapTemplate" value=".templates/calcCase/ezg.gmt"/>
       	<arg name="maximizeMap" value="true"/>
       	<arg name="selectFeatureID" value="EZ_1"/>

        <arg name="mainSash" value="40"/>
        <arg name="rightSash" value="50"/>

        <arg name="zmlTableShow" value="all"/>

        <arg name="timeserie1" value="nameColumn=Name#nameString=%featureprop% - %axisname%#linkColumn=Niederschlag_rechnung"/>
        
        <arg name="buttonText" value="Neuberechnung Gebietsniederschlag"/>
		<arg name="buttonTooltip" value="Ombrometermesswerte mittels Gebietsniederschlagsmodell übernehmen"/>
		<arg name="buttonLaunch" value="gebietsniederschlag"/>
    </page>

	<!-- Kontrolle Talsperrenabgaben -->
    <page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ObservationMapTableDiagWizardPage" pageTitle="Kontrolle Talsperrenabgaben" imageLocation="icons/calcwizard/kalypso32.gif" >
        <arg name="mapTemplate" value=".templates/calcCase/talsperren.gmt"/>
       	<arg name="maximizeMap" value="true"/>
       	<arg name="selectFeatureID" value="TS_BAUTZEN"/>

        <arg name="mainSash" value="40"/>
        <arg name="rightSash" value="50"/>

        <arg name="zmlTableShow" value="all"/>
        
        <arg name="timeserie1" value="nameColumn=Name#nameString=%featureprop% - Abgabe#linkColumn=Abgabe#color=0x0000ff"/>

        <arg name="ignoreType1" value="W"/>
        <!--arg name="ignoreType2" value="Q"/-->
        <!--arg name="ignoreLabel1" value="Abfluss"/-->
        <!--arg name="ignoreLabel2" value="Wasserstand"/-->
    </page>

	<!-- Kontrolle W-Eingabepegel -->
    <page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ObservationMapTableDiagWizardPage" pageTitle="Kontrolle Wasserstände - Eingabepegel" imageLocation="icons/calcwizard/kalypso32.gif" >
        <arg name="mapTemplate" value=".templates/calcCase/ezg.gmt"/>
       	<arg name="maximizeMap" value="true"/>
       	<arg name="selectFeatureID" value="EZ_1"/>

        <arg name="mainSash" value="40"/>
        <arg name="rightSash" value="50"/>
        <arg name="zmlTableShow" value="all"/>

        <arg name="timeserie1" value="nameColumn=Name#nameString=%featureprop% - %axisname% - Beobachtung#linkColumn=Wasserstand_gemessen#color=0x0000ff"/>
        
        <arg name="ignoreType1" value="Q"/>
        <arg name="ignoreType2" value="W"/>
        <arg name="ignoreLabel1" value="Wasserstand"/>
        <arg name="ignoreLabel2" value="Abfluss"/>
        
    </page>

	<!-- Kontrolle W-Eingabepegel -->
    <page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ObservationMapTableDiagWizardPage" pageTitle="Kontrolle Wasserstände - Eingabepegel" imageLocation="icons/calcwizard/kalypso32.gif" >
        <arg name="mapTemplate" value=".templates/calcCase/flusslauf.gmt"/>
       	<arg name="maximizeMap" value="true"/>
       	<arg name="selectFeatureID" value="FM_1"/>

        <arg name="mainSash" value="40"/>
        <arg name="rightSash" value="50"/>
        <arg name="zmlTableShow" value="all"/>
		
        <arg name="ignoreType1" value="Q"/>
		<arg name="ignoreType2" value="W"/>
        <arg name="ignoreLabel1" value="Wasserstand"/>
        <arg name="ignoreLabel2" value="Abfluss"/>
		
        <arg name="timeserie1" value="nameColumn=Name#nameString=%featureprop% - %axisname% - Beobachtung#linkColumn=Wasserstand_gemessen#color=0x0000ff"/>
        
    </page>

	<!-- Kontrolle Flutungen und Zuflüsse -->
    <page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ObservationMapTableDiagWizardPage" pageTitle="Kontrolle Flutungen und Zuflüsse" imageLocation="icons/calcwizard/kalypso32.gif" >
        <arg name="mapTemplate" value=".templates/calcCase/vorgabepegel.gmt"/>
       	<arg name="maximizeMap" value="true"/>
       	<arg name="selectFeatureID" value="F_4"/>

        <arg name="mainSash" value="40"/>
        <arg name="rightSash" value="50"/>

        <arg name="zmlTableShow" value="all"/>

		<arg name="ignoreType1" value="W"/>
        <!--arg name="ignoreType2" value="Q"/-->
        <!--arg name="ignoreLabel1" value="Abfluss"/-->
        <!--arg name="ignoreLabel2" value="Wasserstand"/-->
        
        <arg name="timeserie1" value="nameColumn=Name#nameString=%featureprop% - %axisname%#linkColumn=Abfluss#color=0x0000ff"/>
    </page>


	<!-- Phase 2: Berechnung und Anpassen der Anfangsbedingungen -->

	<!-- Eingabe Anfangsbodenfeuchten der N-A Modelle -->
    <page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.MapAndTableWizardPage" pageTitle="Eingabe NA-Modelle" imageLocation="icons/calcwizard/kalypso32.gif" >
        <arg name="mapTemplate" value=".templates/calcCase/ezg.gmt"/>
       	<arg name="maximizeMap" value="true"/>
       	<arg name="selectFeatureID" value="EZ_1"/>

        <arg name="tableTemplate" value=".templates/calcCase/ezg.gtt"/>

        <arg name="mainSash" value="40"/>
        <arg name="rightSash" value="30"/>
        
        <arg name="ignoreType1" value="Q"/>
        <arg name="ignoreType2" value="W"/>
        <arg name="ignoreLabel1" value="Wasserstand"/>
        <arg name="ignoreLabel2" value="Abfluss"/>

        <arg name="timeserie1" value="nameString=%axisname%#linkColumn=Niederschlag_rechnung"/>
        <arg name="timeserie2" value="nameString=Beobachtung#linkColumn=Wasserstand_gemessen#color=0x0000ff"/>
        <arg name="timeserie3" value="nameString=Rechnung#linkColumn=Wasserstand_gerechnet#color=0xff0000"/>
        <arg name="timeserie4" value="nameString=Vorhersage#linkColumn=Wasserstand_vorhersage#color=0x008040"/>
        <arg name="timeserie5" value="nameString=untere Umhüllende#linkColumn=Wasserstand_vorhersage_unten#lineDash=4f#color=0x008040"/>
        <arg name="timeserie6" value="nameString=obere Umhüllende#linkColumn=Wasserstand_vorhersage_oben#lineDash=4f#color=0x008040"/>
        
       	<arg name="buttonText" value="Berechnung durchführen"/>
       	<arg name="buttonLaunch" value="runCalculation"/>
       	<arg name="buttonTooltip" value="Führt die Modellrechnung durch"/>
       	<arg name="buttonProperties"/>
    </page>

	<!-- Eingabe Anfangsvolumina der Talsperren -->
    <page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.MapAndTableWizardPage" pageTitle="Eingabe Talsperren" imageLocation="icons/calcwizard/kalypso32.gif" >
        <arg name="mapTemplate" value=".templates/calcCase/talsperren.gmt"/>

        <arg name="tableTemplate" value=".templates/calcCase/talsperren.gtt"/>
       	<arg name="maximizeMap" value="true"/>
       	<arg name="selectFeatureID" value="TS_BAUTZEN"/>

        <arg name="mainSash" value="40"/>
        <arg name="rightSash" value="30"/>

        <arg name="ignoreType1" value="V;W"/>
        <arg name="ignoreType2" value="Q;W"/>
        <arg name="ignoreLabel1" value="Abfluss"/>
        <arg name="ignoreLabel2" value="Volumen"/>

        <arg name="timeserie1" value="nameString=Eingabe Abgabe#linkColumn=Abgabe#color=0x0000ff"/>
        <arg name="timeserie2" value="nameString=Plausibilisierung Abgabe#linkColumn=Abgabe_plausibilisiert#color=0xff0000"/>
        <arg name="timeserie3" value="nameString=Berechnung Zufluss #linkColumn=Zufluss_berechnet#color=0x008040"/>
        <arg name="timeserie4" value="nameString=Stauinhalt#linkColumn=Stauinhalt"/>
        
       	
        <arg name="buttonText" value="Berechnung durchführen"/>
       	<arg name="buttonLaunch" value="runCalculation"/>
       	<arg name="buttonTooltip" value="Führt die Modellrechnung durch"/>
       	<arg name="buttonProperties"/>
    </page>

	<!-- Eingabe Parameter der Flusslaufmodelle -->
    <page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.MapAndTableWizardPage" pageTitle="Eingabe Flusslaufmodelle" imageLocation="icons/calcwizard/kalypso32.gif" >
        <arg name="mapTemplate" value=".templates/calcCase/flusslauf.gmt"/>
       	<arg name="maximizeMap" value="true"/>
       	<arg name="selectFeatureID" value="FM_1"/>
        
        <arg name="tableTemplate" value=".templates/calcCase/flusslauf.gtt"/>
        
        <arg name="ignoreType1" value="Q"/>
        <arg name="ignoreType2" value="W"/>
        <arg name="ignoreLabel1" value="Wasserstand"/>
        <arg name="ignoreLabel2" value="Abfluss"/>

        <arg name="timeserie1" value="nameString=Beobachtung#linkColumn=Wasserstand_gemessen#color=0x0000ff"/>
        <arg name="timeserie2" value="nameString=Rechnung#linkColumn=Wasserstand_gerechnet#color=0xff0000"/>
        <arg name="timeserie3" value="nameString=Vorhersage#linkColumn=Wasserstand_vorhersage#color=0x008040"/>
        <arg name="timeserie4" value="nameString=untere Umhüllende#linkColumn=Wasserstand_vorhersage_unten#lineDash=4f#color=0x008040"/>
        <arg name="timeserie5" value="nameString=obere Umhüllende#linkColumn=Wasserstand_vorhersage_oben#lineDash=4f#color=0x008040"/>

        <arg name="mainSash" value="40"/>
        <arg name="rightSash" value="30"/>
       	
        <arg name="buttonText" value="Berechnung durchführen"/>
       	<arg name="buttonLaunch" value="runCalculation"/>
       	<arg name="buttonTooltip" value="Führt die Modellrechnung durch"/>
       	<arg name="buttonProperties"/>
    </page>

	<!-- Phase 3: Ergebnisdarstellung und Ablage -->
	<page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ExportResultsWizardPage"
	   pageTitle="Ergebnisbearbeitung und Veröffentlichung"
	   imageLocation="icons/calcwizard/kalypso32.gif" >
	   
		<arg name="mapTemplate" value=".templates/calcCase/pegel.gmt"/>
		<arg name="selectFeatureID" value="EZ_1"/>
		<arg name="maximizeMap" value="true"/>
		
        <arg name="timeserie1" value="nameString=%axisname%#linkColumn=Niederschlag_rechnung"/>
        <arg name="timeserie2" value="nameString=Beobachtung#linkColumn=Wasserstand_gemessen#color=0x0000ff"/>
        <arg name="timeserie3" value="nameString=Rechnung#linkColumn=Wasserstand_gerechnet#color=0xff0000"/>
        <arg name="timeserie4" value="nameString=Vorhersage#linkColumn=Wasserstand_vorhersage#color=0x008040"/>
        <arg name="timeserie5" value="nameString=untere Umhüllende#linkColumn=Wasserstand_vorhersage_unten#lineDash=4f#color=0x008040"/>
        <arg name="timeserie6" value="nameString=obere Umhüllende#linkColumn=Wasserstand_vorhersage_oben#lineDash=4f#color=0x008040"/>
		
        <arg name="ignoreType1" value="Q"/>
		<arg name="ignoreType2" value="W"/>
        <arg name="ignoreLabel1" value="Wasserstand"/>
        <arg name="ignoreLabel2" value="Abfluss"/>
		
		<arg name="pegelNameProperty" value="Name"/>
		<arg name="resultProperty" value="Wasserstand_vorhersage"/>

		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="25"/>

		<arg name="exporter1">
			<arg name="id" value="featureWithTemplateExporterTree"/>
			<arg name="name" value="Vorhersagepegel"/>
			<arg name="nameproperty" value="Name"/>
			<arg name="width" value="1024"/>
			<arg name="height" value="768"/>
			<arg name="imageFormat" value ="png"/>
			<arg name="tokens">
				<arg name="title" value="Name"/>
				<arg name="href1" value="Wasserstand_gemessen"/>
				<arg name="href2" value="Wasserstand_gerechnet"/>
				<arg name="href3" value="Wasserstand_vorhersage"/>
				<arg name="href4" value="Niederschlag_rechnung"/>
				<arg name="href5" value="Wasserstand_vorhersage_unten"/>
				<arg name="href6" value="Wasserstand_vorhersage_oben"/>
			</arg>
			<arg name="templateDiagQ">
				<arg name="label" value="Abflußgrafik"/>
				<arg name="href" value="project:/.templates/calcCase/berichtDiagQ.odt"/>
			</arg>
			<arg name="templateDiagW">
				<arg name="label" value="Wasserstandsgrafik"/>
				<arg name="href" value="project:/.templates/calcCase/berichtDiagW.odt"/>
			</arg>
			<arg name="templateTable">
				<arg name="label" value="Tabelle"/>
				<arg name="href" value="project:/.templates/calcCase/berichtTabelle.ott"/>
			</arg>
		</arg>
      </page>
	
</calcwizard>