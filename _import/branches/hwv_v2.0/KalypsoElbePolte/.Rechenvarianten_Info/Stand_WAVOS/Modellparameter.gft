            </text>
            <label style="SWT.NONE" visible="false">
                <gridData grabExcessHorizontalSpace="false" horizontalAlignment="GridData.BEGINNING"/>
            </label>
            <label text="Nachführung RIDO" style="SWT.NONE" tooltip="Nachführung RIDO" visible="true">
                <gridData grabExcessHorizontalSpace="false" horizontalAlignment="GridData.BEGINNING"/>
            </label>
            <checkbox editable="true" property="rido" style="SWT.NONE" tooltip="Nachführung RIDO">
                <gridData horizontalAlignment="GridData.BEGINNING" horizontalSpan="1" widthHint="200"/>
            </checkbox>
            <label style="SWT.NONE" visible="false">
                <gridData grabExcessHorizontalSpace="false" horizontalAlignment="GridData.BEGINNING"/>
            </label>
        </group>
    </view>
</featuretemplate>
