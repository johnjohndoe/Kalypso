<?xml version="1.0" encoding="UTF-8"?>
<modeldata xmlns="xml.model.kalypso.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" typeID="BodeV1.0">
	<!-- Dateien relativ zum Projektverzeichnis (.../input/base/) -->
	<input id="MODELL_XSD" path=".model/schema/bodemodell.xsd" relativeToCalcCase="false"/>
	<input id="CONTROL_XSD" path=".model/schema/control.xsd" relativeToCalcCase="false"/>
	
	<!-- Verzeichnisse relativ zum Projektverzeichnis (.../input/base/) -->
	<input id="SchemaDir" path=".model/schema" relativeToCalcCase="false"/>
	
	<!-- Dateien relativ zur Rechenvariante (.../input/calc/) -->
	<input id="MODELL_GML" path="bodemodell.gml" relativeToCalcCase="true"/>
	<input id="CONTROL_GML" path=".calculation" relativeToCalcCase="true"/>

	
	<!-- Verzeichnisse relativ zur Rechenvariante (.../input/calc/) -->
	<!-- Unterverzeichnisse Messung und Vorhersage! -->
	<input id="QPegelDirIn" path="Zeitreihen/Pegel" relativeToCalcCase="true"/>
	<input id="QSpeicherDirIn" path="Zeitreihen/Speicherabgabe" relativeToCalcCase="true"/>
	<!-- Unterverzeichnisse Messung und Vorhersage! -->
	<input id="VSpeicherDirIn" path="Zeitreihen/Speicherinhalt" relativeToCalcCase="true"/>
	<input id="NsGebietDirIn" path="Zeitreihen/Gebietsniederschlag" relativeToCalcCase="true"/>
	

	<output id="Logs" path="Log-Dateien" relativeToCalcCase="true"/>
	<output id="Ergebnisse" path="Log-Dateien" relativeToCalcCase="true"/>
<!--
	<output id="QPegelDirOut" path="Zeitreihen/Pegel/Vorhersage" relativeToCalcCase="true"/>
	<output id="QSpeicherDirOut" path="Zeitreihen/Speicherabgabe" relativeToCalcCase="true"/>
	<output id="VSpeicherDirOut" path="Zeitreihen/Speicherinhalt/Vorhersage" relativeToCalcCase="true"/>
	<output id="ZflSpeicherDirOut" path="Zeitreihen/Speicherzufluss" relativeToCalcCase="true"/>
-->
	
	<!-- Verzeichnis wird geleert, bevor die neuen Ergebnisse geholt werden -->
	<clearAfterCalc path="Ergebnisse" relativeToCalcCase="true"/>
	<clearAfterCalc path="DivErgebnisse" relativeToCalcCase="true"/>
	
</modeldata>
