<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<featuretemplate xmlns="featureview.template.kalypso.org" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<layer xlink:href=".calculation" featurePath="" linktype="gml" id="blubb"/>
	<view typename="TubigControl" style="SWT.NONE">
		<gridData grabExcessHorizontalSpace="false"/>
		<gridLayout numColumns="1" horizontalSpacing="10"/>
			<subcomposite property="ControlAssociation" style="SWT.NONE">
				<gridData horizontalAlignment="4" grabExcessHorizontalSpace="true"/>
			</subcomposite>
	</view>
	<view typename="Control" style="SWT.NONE">
		<gridLayout numColumns="2" horizontalSpacing="10"/>

		<label text="Batches" style="SWT.NONE">
			<gridData verticalAlignment="GridData.BEGINNING" horizontalAlignment="GridData.BEGINNING"/>
		</label>
		<subcomposite property="BatchIDCollectionAssociation" style="SWT.BORDER"/>
	</view>
<view typename = "BatchIDCollection" style = "SWT.NONE" >

	        <radiobutton text="Abgabe = Zufluss" style="SWT.NONE" tooltip="" property="BatchID" valueToSet="1">
	            <gridData grabExcessHorizontalSpace="false" horizontalAlignment="GridData.BEGINNING"/>
	        </radiobutton>
	        <label style="SWT.NONE" visible="false">
	            <gridData grabExcessHorizontalSpace="false" horizontalAlignment="GridData.BEGINNING"/>
	        </label>
</view>
</featuretemplate>
