<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="org.kalypso.saale.pegel" version="0.1" xmlns:obslink="obslink.zml.kalypso.org" xmlns:pegel="org.kalypso.saale.pegel" xmlns:common="org.kalypso.saale.common" xmlns:gml="http://www.opengis.net/gml" xmlns="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<import namespace="http://www.opengis.net/gml" schemaLocation="basis/feature.xsd"/>
	<import namespace="obslink.zml.kalypso.org" schemaLocation="basis/obslink.xsd"/>
	<import namespace="org.kalypso.saale.common" schemaLocation="common.xsd"/>
	<element name="PegelCollectionAssociation" type="pegel:PegelCollectionAssociationType"/>
	<complexType name="PegelCollectionAssociationType">
		<complexContent>
			<restriction base="gml:FeatureAssociationType">
				<sequence>
					<element ref="pegel:PegelCollection"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<element name="PegelCollection" type="pegel:PegelCollectionType" substitutionGroup="gml:_FeatureCollection"/>
	<complexType name="PegelCollectionType">
		<complexContent>
			<restriction base="gml:AbstractFeatureCollectionType">
				<sequence>
					<element ref="gml:boundedBy"/>
					<element ref="pegel:PegelMember" minOccurs="0" maxOccurs="unbounded"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<element name="PegelMember" type="pegel:PegelAssociationType" substitutionGroup="gml:featureMember"/>
	<complexType name="PegelAssociationType">
		<complexContent>
			<restriction base="gml:FeatureAssociationType">
				<sequence>
					<element ref="pegel:Pegel"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<element name="Pegel" abstract="false" type="pegel:PegelType" substitutionGroup="gml:_Feature"/>
	<complexType name="PegelType">
		<complexContent>
			<extension base="common:EmptyFeatureBaseType">
				<sequence>
					<element name="PNR" type="string">
						<annotation>
							<documentation xml:lang="de">
								<label>Messstellennummer</label>
								<description>Im Saale-Modell intern verwendete Pegelnummer</description>
							</documentation>
						</annotation>
					</element>
					<element name="Name" type="string"/>
					<element name="Gewässer" type="string">
						<annotation>
							<documentation xml:lang="de">
								<label>Lage</label>
								<tooltip>Lage des Pegels</tooltip>
								<description>Die geografische Lage des Pegels.</description>
							</documentation>
						</annotation>
					</element>
					<element ref="gml:location"/>
					<element name="Durchfluss_eingabe" type="obslink:TimeseriesLinkType"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<element name="EZG" type="pegel:EZGType" substitutionGroup="pegel:Pegel">
		<annotation>
			<documentation xml:lang="de">
				<label>Einzugsgebiet</label>
				<tooltip>Einzugsgebiet</tooltip>
				<description>Einzugsgebiet</description>
			</documentation>
		</annotation>
	</element>
	<complexType name="EZGType">
		<complexContent>
			<extension base="pegel:PegelType">
				<sequence>
					<element name="L" type="int">
						<annotation>
							<documentation xml:lang="de">
								<label>L</label>
								<tooltip>effektive Länge der Anstiegsfunktion h(t)</tooltip>
								<description>effektive Länge der Anstiegsfunktion h(t)</description>
							</documentation>
						</annotation>
					</element>
					<element name="N" type="int">
						<annotation>
							<documentation xml:lang="de">
								<label>N</label>
								<tooltip>INDEX für den Direktabfluss - Rückgangsbeginn</tooltip>
								<description>INDEX für den Direktabfluss - Rückgangsbeginn</description>
							</documentation>
						</annotation>
					</element>
					<element name="M" type="int">
						<annotation>
							<documentation xml:lang="de">
								<label>M</label>
								<tooltip>INDEX für den Direktabfluss - Rückgangsende</tooltip>
								<description>INDEX für den Direktabfluss - Rückgangsende</description>
							</documentation>
						</annotation>
					</element>


