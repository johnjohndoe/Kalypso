	</xs:complexType>
	<xs:complexType name="viscosityAssociationType">
		<xs:complexContent>
			<xs:restriction base="gml:FeaturePropertyType">
				<xs:sequence>
					<xs:element ref="bc2d:viscosity" />
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
