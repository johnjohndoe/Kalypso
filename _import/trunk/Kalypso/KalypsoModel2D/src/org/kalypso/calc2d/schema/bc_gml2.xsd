					<xs:element name="geomCollectionMember" type="wb:geomCollectionAssociationType"/>
					<xs:element name="iterationCollectionMember" type="wb:iterationCollectionAssociationType"/>
					<xs:element name="viscosityCollectionMember" type="wb:viscosityCollectionAssociationType"/>
					<xs:element name="lineCollectionMember" type="wb:lineCollectionAssociationType"/>
					<xs:element name="dischargeCollectionMember" type="wb:dischargeCollectionAssociationType"/>
					<xs:element name="dynamicBC_CollectionMember" type="wb:dynamicBC_CollectionAssociationType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ==============================================================
				general parameters for the simulation (FirstParamGroup)
	============================================================== -->
	<xs:complexType name="generalCollectionAssociationType">
		<xs:complexContent>
			<xs:restriction base="gml:FeatureAssociationType">
				<xs:sequence>
					<xs:element ref="wb:generalCollection"/>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="generalCollection" type="wb:generalCollectionType" substitutionGroup="gml:_Feature"/>
