						minOccurs="0" />
					<xs:element name="ITURB" type="xs:integer"
						minOccurs="0" />
					<xs:element name="P_BOTTOM" type="xs:double"
						minOccurs="0" />
					<xs:element name="P_PRANDTL" type="xs:double"
						minOccurs="0" />
					<xs:element name="MINEDDY" type="xs:double"
						minOccurs="0" />
					<xs:element name="Param" type="bc2d:paramType"
						minOccurs="0" maxOccurs="unbounded" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
