					<xs:element name="ITSI" type="xs:double" />
					<xs:element name="ISPLPT" type="xs:double" />
					<xs:element name="JSPLPT" type="xs:double" />
					<xs:element name="IHOE" type="xs:double" />
					<xs:element name="IDEN" type="xs:double" />
					<xs:element name="ICONVG" type="xs:double" />
					<xs:element name="IGRAV" type="xs:double" />
