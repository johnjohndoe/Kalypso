					<xs:element name="IRSLP" type="xs:integer"/>
					<xs:element name="IQGEN" type="xs:integer"/>
					<xs:element name="IHGEN" type="xs:integer"/>
					<xs:element name="ISTGEN" type="xs:integer"/>
