					<xs:element name="AUSSTEU" type="xs:integer" minOccurs="0"/>
					<xs:element name="MORPH" type="xs:integer" minOccurs="0"/>
					<xs:element name="ITURB" type="xs:integer" minOccurs="0"/>
					<xs:element name="P_BOTTOM" type="xs:integer" minOccurs="0"/>
					<xs:element name="P_PRANDTL" type="xs:integer" minOccurs="0"/>
					<xs:element name="MINEDDY" type="xs:integer" minOccurs="0"/>
					<xs:element name="Param" type="wb:paramType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ==============================================================
				general addtional parameters for the simulation (FirstParamGroup)
	============================================================== -->
	<xs:complexType name="genCollectionAssociationType">
		<xs:complexContent>
			<xs:restriction base="gml:FeatureAssociationType">
				<xs:sequence>
					<xs:element ref="wb:genCollection"/>
