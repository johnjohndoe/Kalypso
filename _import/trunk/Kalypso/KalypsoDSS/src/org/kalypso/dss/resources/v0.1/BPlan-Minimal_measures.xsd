			<xs:documentation>
Flächen mit wasserrechtlichen Festsetzungen (§9 Abs. 6 BauGB)
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xplan:BPlanFlaechenobjektType">
				<xs:sequence>
					<xs:element name="zweckbestimmung" type="xs:string" minOccurs="0"/>
					<xs:element name="sonstigeZweckbestimmung" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- Paket Naturschutz -->
	<xs:element name="NebenanlagenStellplaetzeGarabenflaechen" type="xplan:NebenanlagenStellplaetzeGarabenflaechenType" substitutionGroup="gml:_Feature"/>
	<xs:complexType name="NebenanlagenStellplaetzeGarabenflaechenType">
		<xs:annotation>
			<xs:documentation>

