<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:wfs="http://www.opengis.net/wfs" xmlns:gml="http://www.opengis.net/gml" xmlns:adv="http://www.adv-online.de/namespaces/adv/gid/3.1" xmlns:xplan="http://www.xplanung.de/bplangml" targetNamespace="http://www.xplanung.de/bplangml" elementFormDefault="qualified" version="3.1">
	<xs:import namespace="http://www.opengis.net/gml" schemaLocation="feature.xsd"/>
	<xs:element name="BPlanObjekt" type="xplan:BPlanObjektType" abstract="true" substitutionGroup="gml:_Feature"/>
	<xs:complexType name="BPlanObjektType" abstract="false">
		<xs:annotation>
			<xs:documentation>
Basisklasse aller Bebauungsplanobjekte.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element name="text" type="xs:string" minOccurs="0"/>
					<xs:element name="textSchluessel" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BPlanFlaechenobjekt" type="xplan:BPlanFlaechenobjektType" abstract="true" substitutionGroup="gml:_Feature"/>
	<xs:complexType name="BPlanFlaechenobjektType" abstract="true">
		<xs:annotation>
			<xs:documentation>
Basisklasse für alle flächenhaften Objekte, die zum Flächenschluss beitragen oder andere Flächen überlagern.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xplan:BPlanObjektType">
				<xs:sequence>
					<xs:element name="position" type="gml:PolygonPropertyType"/>
					<xs:element name="flaechenschluss" type="xs:boolean"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BPlanFlaechenschlussobjekt" type="xplan:BPlanFlaechenschlussobjektType" abstract="true" substitutionGroup="xplan:BPlanFlaechenobjekt"/>
	<xs:complexType name="BPlanFlaechenschlussobjektType" abstract="true">
		<xs:annotation>
			<xs:documentation>
Basisklasse für alle flächenhaften Objekte, die zusammen mit anderen Flächenschluss-Objekten das Planungsgebiet überschneidungsfrei überdecken.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xplan:BPlanFlaechenobjektType">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BPlanLinienobjekt" type="xplan:BPlanLinienobjektType" abstract="true" substitutionGroup="xplan:BPlanObjekt"/>
	<xs:complexType name="BPlanLinienobjektType" abstract="true">
		<xs:annotation>
			<xs:documentation>
Basisklasse für alle linienförmigen Objekte.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xplan:BPlanObjektType">
				<xs:sequence>
					<xs:element name="position" type="gml:LineStringPropertyType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BPlanPunktobjekt" type="xplan:BPlanPunktobjektType" abstract="true" substitutionGroup="xplan:BPlanObjekt"/>
	<xs:complexType name="BPlanPunktobjektType" abstract="true">
		<xs:annotation>
			<xs:documentation>
Basisklasse für alle punktförmigen Objekte.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xplan:BPlanObjektType">
				<xs:sequence>
					<xs:element name="position" type="gml:PointPropertyType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BPlanUeberlagerungsobjekt" type="xplan:BPlanUeberlagerungsobjektType" abstract="true" substitutionGroup="xplan:BPlanFlaechenobjekt"/>
	<xs:complexType name="BPlanUeberlagerungsobjektType" abstract="true">
		<xs:annotation>
			<xs:documentation>
Basisklasse für alle flächenhaften Objekte, die andere Flächen überlagern.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xplan:BPlanFlaechenobjektType">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- Paket Baugebiet, Baugebietsteil -->
	<xs:complexType name="festsetzungenAssociation">
		<xs:complexContent>
			<xs:restriction base="gml:FeatureAssociationType">
				<xs:sequence>
					<xs:element ref="xplan:FestsetzungenBaugebiet"/>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="FestsetzungenBaugebiet" type="xplan:FestsetzungenBaugebietType" substitutionGroup="gml:_Feature"/>
	<xs:element name="BaugebietsFlaechenTeil" type="xplan:BaugebietsFlaechenTeilType" substitutionGroup="xplan:BPlanFlaechenschlussobjekt"/>
