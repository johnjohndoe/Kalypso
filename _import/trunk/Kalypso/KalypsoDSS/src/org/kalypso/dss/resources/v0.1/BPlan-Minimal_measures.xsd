		<xs:annotation>
			<xs:documentation>
Teil eines Baugebiets mit einheitlicher Art und Maß der baulichen Nutzung.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xplan:BPlanFlaechenschlussobjektType">
				<xs:sequence>
					<xs:element name="festsetzungen" type="xplan:festsetzungenAssociation" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="FlaecheMitBesonderemNutzungszweck" type="xplan:FlaecheMitBesonderemNutzungszweckType" substitutionGroup="xplan:BPlanFlaechenobjekt"/>
	<xs:complexType name="FlaecheMitBesonderemNutzungszweckType">
		<xs:annotation>
			<xs:documentation>
Fläche gemäß §9 Abs. 1 Nr. 9 BauGB.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xplan:BPlanFlaechenobjektType">
				<xs:sequence>
					<xs:element name="zweckbestimmung" type="xs:string" minOccurs="0"/>
					<xs:element name="festsetzungen" type="xplan:festsetzungenAssociation" minOccurs="0"/>
					<xs:element name="gestaltung" type="xplan:GestaltungBaugebietType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GestaltungBaugebietType">
		<xs:annotation>
			<xs:documentation>

			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DNmin" type="xs:decimal" minOccurs="0"/>
			<xs:element name="DNmax" type="xs:decimal" minOccurs="0"/>
			<xs:element name="FR" type="xs:decimal" minOccurs="0"/>
			<xs:element name="dachform" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FestsetzungenBaugebietType">
		<xs:annotation>
			<xs:documentation>

			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:restriction base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element name="maxZahlWohnungen" type="xs:integer" minOccurs="0"/>
					<xs:element name="Fmin" type="xs:integer" minOccurs="0"/>
					<xs:element name="Fmax" type="xs:integer" minOccurs="0"/>
					<xs:element name="GFZmin" type="xs:decimal" minOccurs="0"/>
					<xs:element name="GFZmax" type="xs:decimal" minOccurs="0"/>
					<xs:element name="GFmin" type="xs:decimal" minOccurs="0"/>
					<xs:element name="GFmax" type="xs:decimal" minOccurs="0"/>
					<xs:element name="BMZ" type="xs:decimal" minOccurs="0"/>
					<xs:element name="BM" type="xs:decimal" minOccurs="0"/>
					<xs:element name="GRZ" type="xs:decimal" minOccurs="0"/>
					<xs:element name="GR" type="xs:decimal" minOccurs="0"/>
					<xs:element name="Zmin" type="xs:decimal" minOccurs="0"/>
					<xs:element name="Zmax" type="xs:decimal" minOccurs="0"/>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Baugrenze" type="xplan:BaugrenzeType" substitutionGroup="xplan:BPlanLinienobjekt"/>
	<xs:complexType name="BaugrenzeType">
		<xs:annotation>
			<xs:documentation>
Baugrenze (§9 Abs. 1 Nr. 2 BauGB, §22 und 23 BauNVO)
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xplan:BPlanLinienobjektType">
				<xs:sequence>
					<xs:element name="bautiefe" type="xs:double" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Baulinie" type="xplan:BaulinieType" substitutionGroup="xplan:BPlanLinienobjekt"/>
	<xs:complexType name="BaulinieType">
		<xs:annotation>
			<xs:documentation>
Baulinie (§9 Abs. 1 Nr. 2 BauGB, §22 und 23 BauNVO)
			</xs:documentation>
