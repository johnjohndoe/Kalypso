				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="VerkehrsflaecheBesondererZweckbestimmung" type="xplan:VerkehrsflaecheBesondererZweckbestimmungType" substitutionGroup="xplan:BPlanFlaechenschlussobjekt"/>
	<xs:complexType name="VerkehrsflaecheBesondererZweckbestimmungType">
		<xs:annotation>
			<xs:documentation>
Verkehrsfläche (§9 Abs. 1 Nr. 11 und Abs. 6 BauGB)
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xplan:BPlanFlaechenschlussobjektType">
				<xs:sequence>
					<xs:element name="zweckbestimmung" type="xs:string" minOccurs="0"/>
					<xs:element name="festsetzungen" type="xplan:festsetzungenAssociation" minOccurs="0"/>
					<xs:element name="sonstigeZweckbestimmung" type="xs:string" minOccurs="0"/>
					<!-- <xs:element name="begrenzungslinie" type="gml:FeatureAssociationType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:appinfo>
								<adv:referenziertesElement>xplan:Strassenbegrenzungslinie</adv:referenziertesElement>
							</xs:appinfo>
						</xs:annotation>
					</xs:element> -->
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- Paket Wasser -->
	<xs:element name="WasserFlaeche" type="xplan:WasserFlaecheType" substitutionGroup="xplan:BPlanFlaechenschlussobjekt"/>
	<xs:complexType name="WasserFlaecheType">
		<xs:annotation>
			<xs:documentation>
Wasserfläche (§9 Abs. 1 Nr. 16 und Abs. 6 BauGB)
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xplan:BPlanFlaechenschlussobjektType">
				<xs:sequence>
					<xs:element name="zweckbestimmung" type="xs:string" minOccurs="0"/>
					<xs:element name="sonstigeZweckbestimmung" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="WasserwirtschaftsFlaeche" type="xplan:WasserwirtschaftsFlaecheType" substitutionGroup="xplan:BPlanFlaechenobjekt"/>
	<xs:complexType name="WasserwirtschaftsFlaecheType">
		<xs:annotation>
			<xs:documentation>
Flächen für die Wasserwirtschaft, den Hochwasserschutz  und die Regelungen des Wasserabflusses (§9 Abs. 1 Nr. 16 und Abs. 6 BauGB)
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xplan:BPlanFlaechenobjektType">
				<xs:sequence>
					<xs:element name="zweckbestimmung" type="xs:string" minOccurs="0"/>
					<xs:element name="sonstigeZweckbestimmung" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="WasserrechtlicheFestsetzungsFlaeche" type="xplan:WasserrechtlicheFestsetzungsFlaecheType" substitutionGroup="xplan:BPlanFlaechenobjekt"/>
	<xs:complexType name="WasserrechtlicheFestsetzungsFlaecheType">
