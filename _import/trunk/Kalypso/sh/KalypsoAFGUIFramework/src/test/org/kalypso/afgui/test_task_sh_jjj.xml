<!--program defintion, that contains the workflow Kalypso-1D2D-->
	<wbs:Workflow
		rdf:about="&k1d2dwf;WF_Kalypso1D2D">
		<wbs:hasName>Kalypso 1D-2D Workflow</wbs:hasName>
		<wbs:hasHelp>??? mir ist das eine Ebene zu viel!!!</wbs:hasHelp>
		<wbs:hasPhases rdf:resource="&k1d2dwf;PhaseOrder"/>
	</wbs:Workflow>

<!--definition of order of the phases for building up a 1D-2D-model-->
	<wbs:PhaseSeq
		rdf:about="&k1d2dwf;PhaseOrder">
		<rdf:_1 rdf:resource="&k1d2dwf;Preprocessing"/>
		<rdf:_2 rdf:resource="&k1d2dwf;Processing"/>
		<rdf:_3 rdf:resource="&k1d2dwf;Postprocessing"/>
	</wbs:PhaseSeq>

<!--first hierarchical level are the Phases (Processings)-->
	<wbs:Phase
		rdf:about="&k1d2dwf;Preprocessing">
		<wbs:hasName>Preprocessing: Modellaufbau</wbs:hasName>
		<wbs:hasHelp>Im Modellaufbau werden alle notwendigen Daten für die Berechnung mit
		Kalypso-1D-2D zusammengestellt und aufgebaut. Es lässt sich zwischen den verschiedenen Arbeitsphasen
		für die unterschiedlichen Netzarten (1D oder 2D) unterscheiden. Wechseln Sie durch Aktivieren der entsprechenden
		Taskgruppe den Bearbeitungsmodus des gewünschten Datensatzes.</wbs:hasHelp>
		<wbs:hasTaskGroups rdf:resource="&k1d2dwf;ModBuild"/>
	</wbs:Phase>
	
	<wbs:Phase
		rdf:about="&k1d2dwf;Processing">
		<wbs:hasName>Processing: Kalibrieren und Rechnen</wbs:hasName>
		<wbs:hasHelp>Nach Aufbau der Modelldaten können Berechnungen mit dem Ziel der Kalibrierung und/oder 
		Szenarienrechnung vorgenommen werden.</wbs:hasHelp>
		<wbs:hasTaskGroups rdf:resource="&k1d2dwf;CalSim"/>
	</wbs:Phase>

	<wbs:Phase
		rdf:about="&k1d2dwf;Postprocessing">
		<wbs:hasName>Postprocessing: Ergebnisauswertungen</wbs:hasName>
		<wbs:hasHelp>Nach Aufbau der Modelldaten können Berechnungen mit dem Ziel der Kalibrierung und/oder 
		Szenarienrechnung vorgenommen werden.</wbs:hasHelp>
		<wbs:hasTaskGroups rdf:resource="&k1d2dwf;ResVis"/>
	</wbs:Phase>

<!--Within the phases there are ordered TaskGroups-->
	<wbs:TaskGroupSeq 
			rdf:about="&k1d2dwf;ModBuild">
		<rdf:_1 rdf:resource="&k1d2dwf;DataLoad"/>
		<rdf:_2 rdf:resource="&k1d2dwf;NetCstr"/>
		<rdf:_3 rdf:resource="&k1d2dwf;RoughMod"/>
		<rdf:_4 rdf:resource="&k1d2dwf;Cond"/>
	</wbs:TaskGroupSeq> 

	<wbs:TaskGroupSeq 
			rdf:about="&k1d2dwf;CalSim">
