<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
	<!ENTITY wbs 'http://www.tu-harburg.de/wb/kalypso/schema/workflow#'>
	<!ENTITY kb 'http://www.tu-harburg.de/wb/kalypso/kb/workflow#'>
	<!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
]>
<rdf:RDF 
	xmlns:kb="http://www.tu-harburg.de/wb/kalypso/kb/workflow#" 
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
	xmlns:wbs="http://www.tu-harburg.de/wb/kalypso/schema/workflow#">
	
	<wbs:Workflow 
			rdf:about="&kb;SimulationWorkflow">
		<rdfs:label>Wimulation Workflow</rdfs:label>
		<wbs:isRoot>true</wbs:isRoot>
		<wbs:hasPhase rdf:resource="&kb;PreProcessing"/>
		<wbs:hasPhase rdf:resource="&kb;Processing"/>
		<wbs:hasPhase rdf:resource="&kb;PostProcessing"/>
	</wbs:Workflow>
	
	<wbs:Phase
		rdf:about="&kb;Preprocessing">
		<rdfs:label>Preprocessing</rdfs:label>
		<wbs:hasActivity>
			<rdf:Seq>
				<rdf:_1 rdf:resource="&kb;TerrainModelBuilding"/>
				<rdf:_1 rdf:resource="&kb;Discretisierung"/>
			</rdf:Seq>
		</wbs:hasActivity>
		<wbs:nextPhase rdf:resource="&kb;Processing"/>;
	</wbs:Phase>
	
	<wbs:Phase rdf:about="&kb;Processing">
		<rdfs:label>Processing</rdfs:label>
		<wbs:dependsOn rdf:resource="&kb;Preprocessing"/>
		<wbs:nextPhase rdf:resource="&kb;PostProcessing"/>;
	</wbs:Phase>
	
	<wbs:Phase rdf:about="&kb;Postprocessing">
		<rdfs:label>Post-Processing</rdfs:label>
	</wbs:Phase>
	
	
	<wbs:Activity rdf:about="&kb;Projectmanagement">
		<rdfs:label>Projectmanagement</rdfs:label>
		<wbs:isRoot>true</wbs:isRoot>
	</wbs:Activity>	
	
	
	<wbs:Activity rdf:about="&kb;BuildStaticModel" >
		<rdfs:label>Build Static Model</rdfs:label>
		<wbs:partOf rdf:resource="&kb;Preprocessing"/>
		<wbs:hasA  rdf:resource="&kb;Discretisierung"/>	
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>
	
	<wbs:Activity rdf:about="&kb;BasisKartenl">
		<rdfs:label>Basis Karten laden</rdfs:label>
		<wbs:partOf rdf:resource="&kb;Preprocessing"/>	
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>
	
	<wbs:Activity rdf:about="&kb;Discretisierung">
		<rdfs:label>Discretisierung des Ueberschemmungsgebiet</rdfs:label>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>
	
	<wbs:Activity rdf:about="&kb;FlussSchlauModellierren">
		<rdfs:label>Flussschlau modellieren</rdfs:label>
		<wbs:partOf rdf:resource="&kb;BuildStaticModel"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>

	<wbs:Activity rdf:about="&kb;TerrainModelBuilding">
		<rdfs:label>build a terrain model</rdfs:label>
		<wbs:partOf rdf:resource="&kb;BuildStaticModel"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>


	<wbs:Activity rdf:about="&kb;ModelFlowResistance">
		<rdfs:label>Model flow resistance</rdfs:label>
		<wbs:partOf rdf:resource="&kb;BuildStaticModel"/>
		<wbs:isRoot>false</wbs:isRoot>
		<wbs:hasHelp>Defines the flow resistance Model</wbs:hasHelp>
	</wbs:Activity>


	<wbs:Activity rdf:about="&kb;ModelFlowRelationship">
		<rdfs:label>Model flow resistance</rdfs:label>
		<wbs:partOf rdf:resource="&kb;StaticModel"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>

	<wbs:Activity rdf:about="&kb;RoughnessLayerModelling">
		<rdfs:label>Define Rougness Sectors</rdfs:label>
		<wbs:partOf rdf:resource="&kb;TerrainModelBuilding"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>
	
	<wbs:Activity rdf:about="&kb;RiverProfileNetwork">
		<rdfs:label>Define River Profile Network</rdfs:label>
		<wbs:partOf rdf:resource="&kb;TerrainModelBuilding"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>
	<wbs:Activity rdf:about="&kb;DefinedDigitalTerrainModel">
		<rdfs:label>Define the digital Terrain model</rdfs:label>
		<wbs:partOf rdf:resource="&kb;TerraimModelBuilding"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>
		
	<wbs:Activity rdf:about="&kb;StartSimulation">
		<rdfs:label>start a simulation</rdfs:label>
		<wbs:partOf rdf:resource="&kb;Processing"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>


	<wbs:Activity rdf:about="&kb;StopSimulation">
		<rdfs:label>Stop a running simulaton</rdfs:label>
		<wbs:partOf rdf:resource="&kb;Processing"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>

	
	<wbs:Activity rdf:about="&kb;MonitorSimulation">
		<rdfs:label>Monitor Simulation</rdfs:label>
		<wbs:partOf rdf:resource="&kb;Processing"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>
	
	
	<wbs:Activity rdf:about="&kb;ComposeScenario">
		<rdfs:label>Compose scenarios</rdfs:label>
		<wbs:partOf rdf:resource="&kb;Processing"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>


	<wbs:Activity rdf:about="&kb;FloodModelling">
		<rdfs:label>start a simulation</rdfs:label>
		<wbs:partOf rdf:resource="&kb;Postprocessing"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>

	
	<wbs:Activity rdf:about="&kb;RiskModelling">
		<rdfs:label>Risk modelling</rdfs:label>
		<wbs:partOf rdf:resource="&kb;Postprocessing"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>

	
	<wbs:Activity rdf:about="&kb;HydrographViz">
		<rdfs:label>Show Simmulation results as hydrograph</rdfs:label>
		<wbs:partOf rdf:resource="&kb;Processing"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>	
	
	<wbs:Activity rdf:about="&kb;FlowVectorViz">
		<rdfs:label>Show Simulation result as flow vector</rdfs:label>
		<wbs:partOf rdf:resource="&kb;Postprocessing"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>

	<wbs:Activity rdf:about="&kb;IsolineViz">
		<rdfs:label>Show simulation result as isolines</rdfs:label>
		<wbs:partOf rdf:resource="&kb;Postprocessing"/>
		<wbs:isRoot>false</wbs:isRoot>
	</wbs:Activity>
	
	<wbs:WorkflowStatus rdf:resource="&wbs;simulationModel1_WF">
		<wbs:associatedModelResource rdf:resource="&wbs;resourceURI"/>
		<wbs:executedWorkflow rdf:resource="&wbs;SimulationModelWF"/>
		<wbs:activityStates>
			<rdf:Seq>
				
			</rdf:Seq>
		</wbs:activityStates>
		<wbs:currentActivity rdf:resource="&wbs;Activity"/>
		<wbs:currentTask rdf:resource="&wbs;Task"/>
	</wbs:WorkflowStatus>
	
	<wbs:ActivityStatus rdf:resource="&wbs;simulationModel1_AS">
		<wbs:associatedActivity 
			rdf:resource="&wbs;HydrographViz"/>
		<wbs:hasState
			rdf:resource=""/>
	</wbs:ActivityStatus>
	
</rdf:RDF>
