		           <parameter
		                 name="org.kalypso.ogc.gml.map.widgets.SelectWidgetCommand.widget"
		                 value="org.kalypso.kalypsomodel1d2d.ui.map.junction1d2d.CreateJunctionContext1dToCLineFromSelectedEdges"/>
