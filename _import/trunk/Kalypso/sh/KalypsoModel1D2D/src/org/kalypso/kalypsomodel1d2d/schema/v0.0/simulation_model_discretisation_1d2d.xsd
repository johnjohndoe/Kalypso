				</xs:sequence>
				<xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
			<!--/xs:restriction>
		</xs:complexContent-->
	</xs:complexType>
	
	
	<!-- =================================================================
			FE1D2DTriElement
	================================================================== -->
	<xs:element 
		name="FE1D2DTriElement"
		type="wb1d2d:FE1D2DTriElementType"
		substitutionGroup="wb1d2d:FE1D2D_2DElement"/>
	
	<xs:complexType name="FE1D2DTriElementType">
		<xs:annotation>
			<xs:documentation>
				An 2D fe element is defined by 3 edges.
				singular edges cannot be used.
				the edges must build a close kurve.				
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension 
				base="wb1d2d:FE1D2D_2DElementType">
					<xs:sequence>
						<xs:element 
							ref="wb1d2d:fe1d2dDirectedEdge" minOccurs="3" maxOccurs="3"/>
					</xs:sequence>
				
			</xs:extension>
		</xs:complexContent>	
	</xs:complexType>
	
	<!-- =================================================================
		FE1D2DQuadriElement
	================================================================== -->
	<xs:element 
		name="FE1D2DQuadriElement"
		type="wb1d2d:FE1D2DQuadriElementType"
		substitutionGroup="wb1d2d:FE1D2D_2DElement"/>
	
	<xs:complexType name="FE1D2DQuadriElementType">
		<xs:annotation>
			<xs:documentation>
				An 2D fe element is defined by 4 edges.
				singular edges cannot be used.
				the edges must build a close kurve.				
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension 
				base="wb1d2d:FE1D2D_2DElementType">
					<xs:sequence>
					<xs:element
						ref="wb1d2d:fe1d2dDirectedEdge"
						minOccurs="4" maxOccurs="4"/>
					</xs:sequence>
				
			</xs:extension>
		</xs:complexContent>	
	</xs:complexType>
	
	<!-- ================================================================ 
			FE1D2D_2DElement
	==================================================================-->
	<xs:element
		name="FE1D2D_2DElement"
		type="wb1d2d:FE1D2D_2DElementType"
		substitutionGroup="wb1d2d:FE1D2DElement"/>
	
	<xs:complexType name="FE1D2D_2DElementType">
		<xs:annotation>
			<xs:documentation>
				An 2D fe element is defined by 4 edges.
				singular edges cannot be used.
				the edges must build a close kurve.				
			</xs:documentation>
			<xs:appinfo xmlns:kapp="org.kalypso.appinfo">
				<kapp:functionProperty
					kapp:functionId="org.kalypso.kalypsomodel1d2d.schema.functions.FE1D2D_2DElementTypeGeometry"
					kapp:property="wb1d2d:geometry"/>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension 
				base="wb1d2d:FE1D2DElementType">
				<xs:sequence>
				<xs:element name="geometry" type="gml:PolygonPropertyType"></xs:element>
				</xs:sequence>
				</xs:extension>
		</xs:complexContent>	
	</xs:complexType>
	
