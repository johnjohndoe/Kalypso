	<!-- =================================================================
		2d element property 
	================================================================== -->
	<xs:element 
		name="fe1d2d_2DElement"
		type="wb1d2d:FE1D2D_2DElementPropertyType"/>
		
	<xs:complexType name="FE1D2D_2DElementPropertyType">
		<!-- xs:complexContent>
			<xs:restriction 
				base="simBase:FEComplexElementPropertyType"-->
				<xs:sequence>
					<xs:element 
						ref="wb1d2d:FE1D2D_2DElement"
