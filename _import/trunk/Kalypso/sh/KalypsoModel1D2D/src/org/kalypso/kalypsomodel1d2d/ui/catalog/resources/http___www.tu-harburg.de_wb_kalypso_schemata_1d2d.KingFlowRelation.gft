        <gft:validatorlabel property="wb1d2d:width" visible="true" style="SWT.NONE">
            <gft:gridData horizontalAlignment="GridData.BEGINNING" grabExcessHorizontalSpace="false"/>
        </gft:validatorlabel>
        <gft:label text="Böschungsneigung rechts" visible="true" tooltip="Böschungsneigung rechts" style="SWT.NONE">
            <gft:gridData verticalAlignment="GridData.CENTER" horizontalAlignment="GridData.BEGINNING" grabExcessHorizontalSpace="false"/>
        </gft:label>
        <gft:text editable="true" property="wb1d2d:ss1" tooltip="Böschungsneigung rechts" style="SWT.BORDER">
