        <gft:validatorlabel property="wb1d2d:ss1" visible="true" style="SWT.NONE">
            <gft:gridData horizontalAlignment="GridData.BEGINNING" grabExcessHorizontalSpace="false"/>
        </gft:validatorlabel>
        <gft:label text="Böschungsneigung links" visible="true" tooltip="Böschungsneigung links" style="SWT.NONE">
            <gft:gridData verticalAlignment="GridData.CENTER" horizontalAlignment="GridData.BEGINNING" grabExcessHorizontalSpace="false"/>
        </gft:label>
        <gft:text editable="true" property="wb1d2d:ss2" tooltip="Böschungsneigung links" style="SWT.BORDER">
            <gft:gridData horizontalSpan="2" horizontalAlignment="GridData.FILL" grabExcessHorizontalSpace="true"/>
        </gft:text>
        <gft:validatorlabel property="wb1d2d:ss2" visible="true" style="SWT.NONE">
            <gft:gridData horizontalAlignment="GridData.BEGINNING" grabExcessHorizontalSpace="false"/>
        </gft:validatorlabel>
        <gft:label text="Speicherbreite" visible="true" tooltip="Breite des Speichers bei Übertreten des Ufers (Summe beider Vorländer)" style="SWT.NONE">
            <gft:gridData verticalAlignment="GridData.CENTER" horizontalAlignment="GridData.BEGINNING" grabExcessHorizontalSpace="false"/>
        </gft:label>
        <gft:text editable="true" property="wb1d2d:wids" tooltip="Breite des Speichers bei Übertreten des Ufers (Summe beider Vorländer)" style="SWT.BORDER">
            <gft:gridData horizontalSpan="2" horizontalAlignment="GridData.FILL" grabExcessHorizontalSpace="true"/>
        </gft:text>
        <gft:validatorlabel property="wb1d2d:wids" visible="true" style="SWT.NONE">
            <gft:gridData horizontalAlignment="GridData.BEGINNING" grabExcessHorizontalSpace="false"/>
        </gft:validatorlabel>
        <gft:label text="Übertretungshöhe" visible="true" tooltip="Übertretungshöhe des Ufers - entspricht geringster Höhe des Wasserstands im Hochwasserspeicher" style="SWT.NONE">
            <gft:gridData verticalAlignment="GridData.CENTER" horizontalAlignment="GridData.BEGINNING" grabExcessHorizontalSpace="false"/>
        </gft:label>
        <gft:text editable="true" property="wb1d2d:widbs" tooltip="Übertretungshöhe des Ufers - entspricht geringster Höhe des Wasserstands im Hochwasserspeicher" style="SWT.BORDER">
            <gft:gridData horizontalSpan="2" horizontalAlignment="GridData.FILL" grabExcessHorizontalSpace="true"/>
        </gft:text>
        <gft:validatorlabel property="wb1d2d:widbs" visible="true" style="SWT.NONE">
            <gft:gridData horizontalAlignment="GridData.BEGINNING" grabExcessHorizontalSpace="false"/>
        </gft:validatorlabel>
        <gft:label text="Böschungsneigung Speicher" visible="true" tooltip="Neigung der Böschung des Hochwasserspeichers" style="SWT.NONE">
            <gft:gridData verticalAlignment="GridData.CENTER" horizontalAlignment="GridData.BEGINNING" grabExcessHorizontalSpace="false"/>
        </gft:label>
        <gft:text editable="true" property="wb1d2d:wss" tooltip="Neigung der Böschung des Hochwasserspeichers" style="SWT.BORDER">
            <gft:gridData horizontalSpan="2" horizontalAlignment="GridData.FILL" grabExcessHorizontalSpace="true"/>
        </gft:text>
        <gft:validatorlabel property="wb1d2d:wss" visible="true" style="SWT.NONE">
            <gft:gridData horizontalAlignment="GridData.BEGINNING" grabExcessHorizontalSpace="false"/>
        </gft:validatorlabel>
        
</gft:featureview>

