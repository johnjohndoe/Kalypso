<?xml version="1.0" encoding="WINDOWS-1252" standalone="yes"?>
<gft:featuretemplate xmlns:gft="featureview.template.kalypso.org"
	xmlns:ns1="gistableview.template.kalypso.org"
	xmlns:xlink="http://www.w3.org/1999/xlink"
	xmlns:gml="http://www.opengis.net/gml" 
	name="Berechnungseinstellungen">
	<gft:layer id="layer_1" featurePath="#fid#root" linktype="gml"
		xlink:href="../models/control.gml" />


	<!-- Timeseries Table -->
	<gft:view typename="c1d2d:TimestepsObservation" style="SWT.NONE"
		xmlns:c1d2d="http://www.tu-harburg.de/wb/kalypso/schemata/1d2dControl">
		<gft:gridData verticalAlignment="GridData.FILL"
			horizontalAlignment="GridData.FILL" 
			grabExcessVerticalSpace="false"
			grabExcessHorizontalSpace="true" />
		<gft:gridLayout numColumns="1" 
			horizontalSpacing="10" />
		<!-- Button -->
		<gft:extensioncontrol property="om:result"
			extension-id="org.kalypso.kalypsomodel1d2d.ui.featurecontrols.TimestepFillerControlFactory"
			style="SWT.NONE">
			<gft:gridData horizontalSpan="1"
				horizontalAlignment="GridData.END" 
				grabExcessHorizontalSpace="true"
				verticalAlignment="GridData.BEGINNING"
				grabExcessVerticalSpace="false" />
		</gft:extensioncontrol>
		<!-- Table -->
		<gft:tupleResult property="om:result"
			tooltip="Zeitschrittdefinition"
			style="SWT.BORDER | SWT.MULTI | SWT.FULL_SELECTION"
			xmlns:om="http://www.opengis.net/om">
			<gft:gridData verticalAlignment="GridData.FILL"
				horizontalAlignment="GridData.FILL"
				grabExcessVerticalSpace="false"
				heightHint="270"
				grabExcessHorizontalSpace="true" />
			<gft:columnDescriptor
				component="urn:ogc:gml:dict:kalypso:model:1d2d:timeserie:components#OrdinalNumber"
				alignment="SWT.LEFT" displayFormat="%d" editable="false" width="75" />
			<gft:columnDescriptor
				component="urn:ogc:gml:dict:kalypso:model:1d2d:timeserie:components#Time"
				alignment="SWT.LEFT" displayFormat="%1$td.%1$tm.%1$tY %1$tH:%1$tM"
				parseFormat="dd.MM.yyyy HH:mm" width="150"/>
			<gft:columnDescriptor
				component="urn:ogc:gml:dict:kalypso:model:1d2d:timeserie:components#UnderRelaxationFactor"
				alignment="SWT.LEFT" displayFormat="%.1f" width="150"/>
			<gft:toolbar/>
		</gft:tupleResult>
	</gft:view>

	<gft:view typename="c1d2d:ControlModel" style="SWT.NONE"
		xmlns:c1d2d="http://www.tu-harburg.de/wb/kalypso/schemata/1d2dControl">
		<gft:gridData verticalAlignment="GridData.FILL"
			horizontalAlignment="GridData.FILL" 
			grabExcessVerticalSpace="false"
			grabExcessHorizontalSpace="true" />
		<gft:gridLayout numColumns="1" 
			horizontalSpacing="10" />
		
		<gft:tabFolder style="SWT.NONE" >
			<gft:gridData verticalAlignment="GridData.FILL" 
			horizontalAlignment="GridData.FILL" 
			grabExcessHorizontalSpace="true" 
			grabExcessVerticalSpace="false"/>

			<!-- Allgemeines-->
			<gft:tabItem>
				<gft:tabLabel>Allgemeines</gft:tabLabel>
				<gft:composite style="SWT.NONE">
					<gft:gridData 
						horizontalAlignment="GridData.FILL"
						verticalAlignment="GridData.FILL" 
						grabExcessHorizontalSpace="true"
						grabExcessVerticalSpace="true" />
					<gft:gridLayout numColumns="1" makeColumnsEqualWidth="true" horizontalSpacing="10" marginHeight="10" />
					
					<gft:group text="Modellinformationen" style="0">
						<gft:gridData  
							horizontalAlignment="GridData.FILL"
							verticalAlignment="GridData.FILL" 
							grabExcessHorizontalSpace="true"
							grabExcessVerticalSpace="false" />
						<gft:gridLayout numColumns="2" horizontalSpacing="10" />
						<gft:label text="Modellname" visible="true"
							style="SWT.NONE">
							<gft:gridData verticalAlignment="GridData.CENTER"
								horizontalAlignment="GridData.BEGINNING"
								grabExcessHorizontalSpace="false" />
						</gft:label>
						<gft:text editable="true" property="c1d2d:title" tooltip=""
							style="SWT.BORDER">
							<gft:gridData horizontalAlignment="GridData.FILL"
								grabExcessHorizontalSpace="true" />
						</gft:text>
						<gft:label text="Beschreibung" visible="true"
							style="SWT.NONE">
							<gft:gridData verticalAlignment="GridData.CENTER"
								horizontalAlignment="GridData.BEGINNING"
								grabExcessHorizontalSpace="false" />
						</gft:label>
						<gft:text editable="true" property="gml:description"
							tooltip="" style="SWT.BORDER">
							<gft:gridData horizontalAlignment="GridData.FILL"
								grabExcessHorizontalSpace="true" />
						</gft:text>
					</gft:group>

					<gft:group text="zu verwendender Rechenkern" style="SWT.NONE">
						<gft:gridData  
							horizontalAlignment="GridData.FILL"
							verticalAlignment="GridData.FILL" 
							grabExcessHorizontalSpace="true"
							grabExcessVerticalSpace="false" />
						<gft:gridLayout numColumns="3" horizontalSpacing="10" />
						<gft:label text="Version" visible="true"
							tooltip="Version des Rechenkerns RMA 10S" style="SWT.NONE">
							<gft:gridData verticalAlignment="GridData.CENTER"
								horizontalAlignment="GridData.BEGINNING"
								grabExcessHorizontalSpace="false" />
						</gft:label>
						<gft:combo property="c1d2d:Version"
							tooltip="Version des Rechenkerns RMA 10S"
							style="SWT.DROP_DOWN | SWT.READ_ONLY">
							<gft:gridData 
								horizontalAlignment="GridData.FILL"
								grabExcessHorizontalSpace="true" />
						</gft:combo>
						<gft:validatorlabel property="c1d2d:Version" visible="true"
							style="SWT.NONE">
							<gft:gridData horizontalAlignment="GridData.BEGINNING"
								grabExcessHorizontalSpace="false" />
						</gft:validatorlabel>
					</gft:group>
					
				</gft:composite>
			</gft:tabItem>

			<!-- Berechnungssteuerung-->
			<gft:tabItem>		
				<gft:tabLabel>Berechnungssteuerung</gft:tabLabel>
				<gft:composite style="SWT.NONE">
					<gft:gridData 
						horizontalAlignment="GridData.FILL"
						verticalAlignment="GridData.FILL" 
						grabExcessHorizontalSpace="true"
						grabExcessVerticalSpace="true" />
					<gft:gridLayout numColumns="2" horizontalSpacing="10" makeColumnsEqualWidth="false" />
					
					<!-- Composite, that holds the three groups stationäre Berechung, instationäre Berechung and Anfangswerte -->
					<gft:composite style="SWT.NONE">
						<gft:gridData 
							horizontalAlignment="GridData.FILL"
							marginLeft="0"
							marginTop="0"
							marginRight="0"
							marginBottom="0"
							verticalAlignment="GridData.FILL" 
							grabExcessHorizontalSpace="true"
							grabExcessVerticalSpace="true"/>
						<gft:gridLayout numColumns="1" numRows="3" horizontalSpacing="10" />
					
						<gft:group text="Stationäre Berechnung" style="0">
							<gft:gridData 
								horizontalAlignment="GridData.FILL"
								verticalAlignment="GridData.FILL" 
								grabExcessHorizontalSpace="true"
								grabExcessVerticalSpace="false" />
							<gft:gridLayout numColumns="3" horizontalSpacing="10" />
							<gft:checkbox 
								editable="true" property="c1d2d:_steady"
								style="SWT.NONE">
								<gft:gridData horizontalSpan="3"
									horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="true" />
								<gft:text>aktivieren</gft:text>
							</gft:checkbox>
							<gft:label
								text="Max. Anzahl Iterationen" visible="true"
								tooltip="Maximale Anzahl der Iterationen für stationäre Berechnung"
								style="SWT.NONE">
								<gft:gridData verticalAlignment="GridData.CENTER"
									horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="false" />
								<gft:enabledOperation>
									<ogc:PropertyIsLike wildCard="*" singleChar="?"
										escape="\" xmlns:ogc="http://www.opengis.net/ogc">
										<ogc:PropertyName>
											c1d2d:_steady
										</ogc:PropertyName>
										<ogc:Literal>true</ogc:Literal>
									</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:label>
							<gft:text editable="true" property="c1d2d:NITI"
								tooltip="Maximale Anzahl der Iterationen für stationäre Berechnung"
								style="SWT.BORDER">
								<gft:gridData 
									horizontalAlignment="GridData.FILL"
									grabExcessHorizontalSpace="true" />
								<gft:enabledOperation>
									<ogc:PropertyIsLike wildCard="*" singleChar="?"
										escape="\" xmlns:ogc="http://www.opengis.net/ogc">
										<ogc:PropertyName>
											c1d2d:_steady
										</ogc:PropertyName>
										<ogc:Literal>true</ogc:Literal>
									</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:text>
							<gft:validatorlabel property="c1d2d:NITI" visible="true"
								style="SWT.NONE">
								<gft:gridData horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="false" />
								<gft:enabledOperation>
									<ogc:PropertyIsLike wildCard="*" singleChar="?"
										escape="\" xmlns:ogc="http://www.opengis.net/ogc">
										<ogc:PropertyName>
											c1d2d:_steady
										</ogc:PropertyName>
										<ogc:Literal>true</ogc:Literal>
									</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:validatorlabel>
				
							<gft:label
								text="Relaxationsfaktor" visible="true"
								tooltip="Der Relaxationsfaktor für stationäre Berechnung"
								style="SWT.NONE">
								<gft:gridData verticalAlignment="GridData.CENTER"
									horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="false" />
								<gft:enabledOperation>
									<ogc:PropertyIsLike wildCard="*" singleChar="?"
										escape="\" xmlns:ogc="http://www.opengis.net/ogc">
										<ogc:PropertyName>
											c1d2d:_steady
										</ogc:PropertyName>
										<ogc:Literal>true</ogc:Literal>
									</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:label>
							<gft:combo property="c1d2d:_steadyBC"
								tooltip="Der Relaxationsfaktor für stationäre Berechnung"
								style="SWT.BORDER">
								<gft:gridData 
									horizontalAlignment="GridData.FILL"
									grabExcessHorizontalSpace="true" />
								<gft:enabledOperation>
									<ogc:PropertyIsLike wildCard="*" singleChar="?"
										escape="\" xmlns:ogc="http://www.opengis.net/ogc">
										<ogc:PropertyName>
											c1d2d:_steady
										</ogc:PropertyName>
										<ogc:Literal>true</ogc:Literal>
									</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:combo>
							<gft:validatorlabel property="c1d2d:_steadyBC" visible="true"
								style="SWT.NONE">
								<gft:gridData horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="false" />
								<gft:enabledOperation>
									<ogc:PropertyIsLike wildCard="*" singleChar="?"
										escape="\" xmlns:ogc="http://www.opengis.net/ogc">
										<ogc:PropertyName>
											c1d2d:_steady
										</ogc:PropertyName>
										<ogc:Literal>true</ogc:Literal>
									</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:validatorlabel>
						</gft:group>
						
						<gft:group text="Instationäre Berechnung" style="0">
							<gft:gridData 
								horizontalAlignment="GridData.FILL"
								verticalAlignment="GridData.FILL" 
								grabExcessHorizontalSpace="true"
								grabExcessVerticalSpace="false" />
							<gft:gridLayout numColumns="3" horizontalSpacing="10" />
							<gft:checkbox editable="true" property="c1d2d:_unsteady"
								style="SWT.NONE">
								<gft:gridData horizontalSpan="3"
									horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="true" />
								<gft:text>aktivieren</gft:text>
							</gft:checkbox>							
							<gft:label
								text="Max. Anzahl Iterationen" visible="true"
								tooltip="Maximale Anzahl der Iterationen für instationäre Berechnung."
								style="SWT.NONE">
								<gft:gridData verticalAlignment="GridData.CENTER"
									horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="false" />
								<gft:enabledOperation>
										<ogc:PropertyIsLike wildCard="*" singleChar="?"
											escape="\" xmlns:ogc="http://www.opengis.net/ogc">
											<ogc:PropertyName>
												c1d2d:_unsteady
											</ogc:PropertyName>
											<ogc:Literal>true</ogc:Literal>
										</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:label>
							<gft:text editable="true" property="c1d2d:NITN"
								tooltip="Maximale Anzahl der Iterationen für instationäre Berechnung."
								style="SWT.BORDER">
								<gft:gridData 
									horizontalAlignment="GridData.FILL"
									grabExcessHorizontalSpace="true" />
								<gft:enabledOperation>
										<ogc:PropertyIsLike wildCard="*" singleChar="?"
											escape="\" xmlns:ogc="http://www.opengis.net/ogc">
											<ogc:PropertyName>
												c1d2d:_unsteady
											</ogc:PropertyName>
											<ogc:Literal>true</ogc:Literal>
										</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:text>
							<gft:validatorlabel property="c1d2d:NITN" visible="true"
								style="SWT.NONE">
								<gft:gridData horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="false" />
								<gft:enabledOperation>
									<ogc:PropertyIsLike wildCard="*" singleChar="?"
										escape="\" xmlns:ogc="http://www.opengis.net/ogc">
										<ogc:PropertyName>
											c1d2d:_unsteady
										</ogc:PropertyName>
										<ogc:Literal>true</ogc:Literal>
									</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:validatorlabel>
							<gft:label text="Beginn Restart" visible="true"
								tooltip="Angabe der Restart-Zeitschrittnummer der aktuellen Zeitreihe ab der gerechnet werden soll." style="SWT.NONE">
								<gft:gridData verticalAlignment="GridData.CENTER"
									horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="false" />
								<gft:enabledOperation>
									<and>
										<ogc:PropertyIsLike wildCard="*"
											singleChar="?" escape="\"
											xmlns:ogc="http://www.opengis.net/ogc">
											<ogc:PropertyName>
												c1d2d:_steady
											</ogc:PropertyName>
											<ogc:Literal>false</ogc:Literal>
										</ogc:PropertyIsLike>
										<ogc:PropertyIsLike wildCard="*" singleChar="?"
											escape="\" xmlns:ogc="http://www.opengis.net/ogc">
											<ogc:PropertyName>
												c1d2d:_unsteady
											</ogc:PropertyName>
											<ogc:Literal>true</ogc:Literal>
										</ogc:PropertyIsLike>
										<ogc:PropertyIsLike wildCard="*"
											singleChar="?" escape="\"
											xmlns:ogc="http://www.opengis.net/ogc">
											<ogc:PropertyName>
												c1d2d:_restart
											</ogc:PropertyName>
											<ogc:Literal>true</ogc:Literal>
										</ogc:PropertyIsLike>
									</and>
								</gft:enabledOperation>
							</gft:label>
							<gft:text editable="true" property="c1d2d:IACCYC"
								tooltip="Angabe der Restart-Zeitschrittnummer der aktuellen Zeitreihe ab der gerechnet werden soll." style="SWT.BORDER">
								<gft:gridData 
									horizontalAlignment="GridData.FILL"
									grabExcessHorizontalSpace="true" />
								<gft:enabledOperation>
									<and>
										<ogc:PropertyIsLike wildCard="*"
											singleChar="?" escape="\"
											xmlns:ogc="http://www.opengis.net/ogc">
											<ogc:PropertyName>
												c1d2d:_steady
											</ogc:PropertyName>
											<ogc:Literal>false</ogc:Literal>
										</ogc:PropertyIsLike>
										<ogc:PropertyIsLike wildCard="*" singleChar="?"
											escape="\" xmlns:ogc="http://www.opengis.net/ogc">
											<ogc:PropertyName>
												c1d2d:_unsteady
											</ogc:PropertyName>
											<ogc:Literal>true</ogc:Literal>
										</ogc:PropertyIsLike>
										<ogc:PropertyIsLike wildCard="*"
											singleChar="?" escape="\"
											xmlns:ogc="http://www.opengis.net/ogc">
											<ogc:PropertyName>
												c1d2d:_restart
											</ogc:PropertyName>
											<ogc:Literal>true</ogc:Literal>
										</ogc:PropertyIsLike>
									</and>
								</gft:enabledOperation>
							</gft:text>
							<gft:validatorlabel property="c1d2d:IACCYC" visible="true"
								style="SWT.NONE">
								<gft:gridData horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="false" />
								<gft:enabledOperation>
									<and>
										<ogc:PropertyIsLike wildCard="*"
											singleChar="?" escape="\"
											xmlns:ogc="http://www.opengis.net/ogc">
											<ogc:PropertyName>
												c1d2d:_steady
											</ogc:PropertyName>
											<ogc:Literal>false</ogc:Literal>
										</ogc:PropertyIsLike>
										<ogc:PropertyIsLike wildCard="*" singleChar="?"
											escape="\" xmlns:ogc="http://www.opengis.net/ogc">
											<ogc:PropertyName>
												c1d2d:_unsteady
											</ogc:PropertyName>
											<ogc:Literal>true</ogc:Literal>
										</ogc:PropertyIsLike>
										<ogc:PropertyIsLike wildCard="*"
											singleChar="?" escape="\"
											xmlns:ogc="http://www.opengis.net/ogc">
											<ogc:PropertyName>
												c1d2d:_restart
											</ogc:PropertyName>
											<ogc:Literal>true</ogc:Literal>
										</ogc:PropertyIsLike>
									</and>
								</gft:enabledOperation>
							</gft:validatorlabel>
						</gft:group>
						
						<!-- Anfangswerte-->
						<gft:group text="Anfangswerte" style="0">
							<gft:gridData 
								horizontalAlignment="GridData.FILL"
								verticalAlignment="GridData.FILL" grabExcessHorizontalSpace="true"
								grabExcessVerticalSpace="false" />
							<gft:gridLayout numColumns="3" horizontalSpacing="10" />
							<gft:checkbox editable="true" property="c1d2d:_restart"
								tooltip="Angabe des Restart Zeitschrittes" style="SWT.NONE">
								<gft:gridData 
									horizontalAlignment="GridData.FILL"
									grabExcessHorizontalSpace="true" />
								<gft:text>aktivieren</gft:text>
							</gft:checkbox>
							<gft:extensioncontrol property="c1d2d:Version"
								extension-id="org.kalypso.kalypso1d2d.pjt.wizards.RestartSelectorControlFactory"
								style="SWT.NONE">
								<gft:gridData 
									horizontalAlignment="GridData.FILL"
									grabExcessHorizontalSpace="true" 
									grabExcessVerticalSpace="false" />
								<gft:enabledOperation>
									<ogc:PropertyIsLike wildCard="*" singleChar="?"
										escape="\" xmlns:ogc="http://www.opengis.net/ogc">
										<ogc:PropertyName>
											c1d2d:_restart
										</ogc:PropertyName>
										<ogc:Literal>true</ogc:Literal>
									</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:extensioncontrol>
							<gft:label visible="false" style="SWT.NONE">
								<gft:gridData horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="false" />
							</gft:label>
							<gft:label text="Anfangswasserspiegel [m]" visible="true"
								tooltip="Anfangswert des Wasserstandes über jeden Knoten [m]" 
								style="SWT.NONE">
								<gft:gridData verticalAlignment="GridData.CENTER"
									horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="false" />
								<gft:enabledOperation>
									<ogc:PropertyIsLike wildCard="*" singleChar="?"
										escape="\" xmlns:ogc="http://www.opengis.net/ogc">
										<ogc:PropertyName>
											c1d2d:_restart
										</ogc:PropertyName>
										<ogc:Literal>false</ogc:Literal>
									</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:label>
							<gft:text editable="true" property="c1d2d:ELEV"
								tooltip="Anfangswert des Wasserstandes über jeden Knoten [m]"
								style="SWT.BORDER">
								<gft:gridData 
									horizontalAlignment="GridData.FILL"
									grabExcessHorizontalSpace="true" />
								<gft:enabledOperation>
									<ogc:PropertyIsLike wildCard="*" singleChar="?"
										escape="\" xmlns:ogc="http://www.opengis.net/ogc">
										<ogc:PropertyName>
											c1d2d:_restart
										</ogc:PropertyName>
										<ogc:Literal>false</ogc:Literal>
									</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:text>
							<gft:validatorlabel property="c1d2d:ELEV" visible="true"
								style="SWT.NONE">
								<gft:gridData horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="false" />
								<gft:enabledOperation>
									<ogc:PropertyIsLike wildCard="*" singleChar="?"
										escape="\" xmlns:ogc="http://www.opengis.net/ogc">
										<ogc:PropertyName>
											c1d2d:_restart
										</ogc:PropertyName>
										<ogc:Literal>false</ogc:Literal>
									</ogc:PropertyIsLike>
								</gft:enabledOperation>
							</gft:validatorlabel>
							<gft:label text="1D-Anfangsgeschwindigkeit [m/s]"
								visible="true" tooltip="1D-Anfangsgeschwindigkeit [m/s]"
								style="SWT.NONE">
								<gft:gridData verticalAlignment="GridData.CENTER"
									horizontalAlignment="GridData.BEGINNING"
									grabExcessHorizontalSpace="false" />
								<gft:enabledOperation>
									<ogc:PropertyIsLike wildCard="*" singleChar="?"
										escape="\" xmlns:ogc="http://www.opengis.net/ogc">
										<ogc:PropertyName>
											c1d2d:_restart
