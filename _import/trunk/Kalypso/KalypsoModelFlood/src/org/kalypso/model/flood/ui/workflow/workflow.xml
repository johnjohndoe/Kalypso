<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Workflow
	URI="http___www.tu-harburg.de_wb_kalypso_flood__WF_KalypsoFlood"
	xmlns="http://www.renew.de/workflow/base"
	xmlns:con="http://www.renew.de/workflow/contexts"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Help>NONE</Help>
	<Name>KalypsoFlood-Workflow</Name>
	
	<TaskGroup
		URI="http___www.tu-harburg.de_wb_kalypso_flood__DataLoad">
		<Help><![CDATA[Grunddaten importieren]]></Help>
		<Name>Grunddaten importieren</Name>
		<Task context="ImportProjectDataSetWizard"
			URI="http___www.tu-harburg.de_wb_kalypso_flood__ImportBackgroundData">
			<Help><![CDATA[Grunddaten anderer Projekte in die Karte importieren]]></Help>
			<Name>
				Grunddaten anderer Projekte in die Karte importieren
			</Name>
		</Task>
		<Task context="ImportBackgroundDataWizard"
			URI="http___www.tu-harburg.de_wb_kalypso_flood__ImportBackgroundData">
			<Help><![CDATA[importiert Hintergrunddaten.]]></Help>
			<Name>Hintergrunddaten importieren</Name>
		</Task>
		<Task context="ImportWaterlevelData"
			URI="http___www.tu-harburg.de_wb_kalypso_flood__ImportWaterlevelData">
			<Help><![CDATA[Importiert Wasserspiegeldaten.]]></Help>
			<Name>Wasserspiegeldaten importieren</Name>
		</Task>
		<Task context="ImportDTMData"
			URI="http___www.tu-harburg.de_wb_kalypso_flood__ImportDTMData">
			<Help><![CDATA[Importiert Gelände-Rasterdaten]]></Help>
			<Name>Gelände-Raster importieren</Name>
		</Task>
	</TaskGroup>

	<TaskGroup
		URI="http___www.tu-harburg.de_wb_kalypso_flood__Editing">
		<Help><![CDATA[Manuelle Bearbeitung]]></Help>
		<Name>Manuelle Bearbeitung</Name>
		<Task type="ASYNCHRONOUS" context="PolygonEditing"
			URI="http___www.tu-harburg.de_wb_kalypso_flood__PolygonEditing">
			<Help><![CDATA[Ermöglicht das Anlegen von Korrekturpolygonen.]]></Help>
			<Name>Korrekturpolygone</Name>
		</Task>
	</TaskGroup>

	<TaskGroup
		URI="http___www.tu-harburg.de_wb_kalypso_flood__Processing">
		<Help><![CDATA[Fliesstiefen generieren]]></Help>
		<Name>Fliesstiefen generieren</Name>
		<Task context="ProcessFloodModel"
			URI="http___www.tu-harburg.de_wb_kalypso_flood__ProcessFloodModel">
			<Help><![CDATA[Startet Generierung des Fliesstiefen-Rasters.]]></Help>
			<Name>Start</Name>
		</Task>
	</TaskGroup>

	<TaskGroup
		URI="http___www.tu-harburg.de_wb_kalypso_flood__Postprocessing">
		<Help><![CDATA[Ergebnisauswertung]]></Help>
		<Name>Ergebnisauswertung</Name>
		<Task context="ExportRasterData"
			URI="http___www.tu-harburg.de_wb_kalypso_flood__ExportRasterData">
			<Help><![CDATA[Exportiert die Fliesstiefen-Rasterdaten in externe Dateien.]]></Help>
			<Name>Rasterdatenexport</Name>
		</Task>
	</TaskGroup>
	
	<Perspectives>
		<con:PerspectiveContext id="ContextFloodModelling"
			perspectiveId="org.kalypso.kalypso1d2d.pjt.perspective.Perspective">
			<!-- perspectiveId="org.kalypso.flood.perspective.Perspective"> -->
			
			<con:ViewContext id="FeatureView"
				partId="org.kalypso.ui.views.featuretemplateview"
				parent="ContextFloodModelling">
				<con:ExtensionContext id="LanduseDatabase"
					parent="FeatureView"
					handlerFactoryId="kalypsoContextHandlerFactory">
					<con:Parameter name="type"
						value="featureViewInputContext" />
					<con:Parameter name="input"
						value="templates/landuse.gft" />
				</con:ExtensionContext>
				<con:ExtensionContext id="AssetValuesDatabase"
					parent="FeatureView"
					handlerFactoryId="kalypsoContextHandlerFactory">
					<con:Parameter name="type"
						value="featureViewInputContext" />
					<con:Parameter name="input"
						value="templates/assetValues.gft" />
				</con:ExtensionContext>
				<con:ExtensionContext id="DamageFunctionsDefinition"
					parent="FeatureView"
					handlerFactoryId="kalypsoContextHandlerFactory">
					<con:Parameter name="type"
						value="featureViewInputContext" />
					<con:Parameter name="input"
						value="templates/damageFunctions.gft" />
				</con:ExtensionContext>
			</con:ViewContext>
			
			<con:WorkbenchSiteContext id="MapContext"
				parent="ContextFloodModelling">
				<con:ViewContext id="MapOutlineView"
					partId="org.kalypso.ui.views.outline" parent="MapContext" />
				<con:ViewContext id="MapView"
					partId="org.kalypso.ui.views.mapView" parent="MapContext">
					<con:ExtensionContext
						id="ImportLanduseShapeDataContext" parent="MapContext"
						handlerFactoryId="kalypsoContextHandlerFactory">
						<con:WizardContext
							id="ImportLanduseShapeDataWizard"
							wizardId="org.kalypso.flood.model.wizards.importlanduse.ImportLanduseWizard"
							wizardType="NEW_WIZARD" parent="ImportLanduseShapeDataContext" />
						<con:Parameter name="type"
							value="mapViewInputContext" />
						<con:Parameter name="input"
							value="maps/basemap.gmt" />
					</con:ExtensionContext>
				</con:ViewContext>
			</con:WorkbenchSiteContext>
			
			<con:ViewContext id="FeatureSelectionView"
				partId="org.kalypso.featureview.views.FeatureView"
				parent="ContextFloodModelling" />
				
		</con:PerspectiveContext>
	</Perspectives>
</Workflow>