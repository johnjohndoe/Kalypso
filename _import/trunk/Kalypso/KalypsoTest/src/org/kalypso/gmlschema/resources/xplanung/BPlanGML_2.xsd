	<xs:element name="DenkmalschutzEinzelanlage" type="bplan:DenkmalschutzEinzelanlageType" substitutionGroup="bplan:BPlanPunktobjekt"/>
	<xs:complexType name="DenkmalschutzEinzelanlageType">
