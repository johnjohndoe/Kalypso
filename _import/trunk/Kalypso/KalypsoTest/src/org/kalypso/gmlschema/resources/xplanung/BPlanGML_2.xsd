					<xs:element name="sonstigeZweckbestimmung" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="AnpflanzungBindungErhaltungsGegenstandType">
		<xs:annotation>
			<xs:documentation>
Gegenstand der nach §9, Abs. 1, Nr. 25 festgesetzten Maßnahme
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Baeume"/>
			<xs:enumeration value="Straeucher"/>
			<xs:enumeration value="SonstBepflanzung"/>
			<xs:enumeration value="Gewaesser"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ZweckbestimmungSchutzgebietType">
		<xs:annotation>
			<xs:documentation>

			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Naturschutzgebiet"/>
			<xs:enumeration value="Nationalpark"/>
			<xs:enumeration value="Landschaftsschutzgebiet"/>
			<xs:enumeration value="Naturpark"/>
			<xs:enumeration value="Naturdenkmal"/>
			<xs:enumeration value="GeschuetzterLandschaftsBestandteil"/>
			<xs:enumeration value="Sonstiges"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="AnpflanzungBindungErhaltungsMassnahme" type="bplan:AnpflanzungBindungErhaltungsMassnahmeType" substitutionGroup="bplan:BPlanPunktobjekt"/>
	<xs:complexType name="AnpflanzungBindungErhaltungsMassnahmeType">
		<xs:annotation>
			<xs:documentation>
a) Punktförmige Festsetzung des Anpflanzens von Bäumen, Sträuchern und sonstigen Bepflanzungen;
b) Punktförmige Festsetzung von Bindungen für Bepflanzungen und für die Erhaltung von Bäumen, Sträuchern und sonstigen Bepflanzungen sowie von Gewässern;  (§9 Abs. 1 Nr. 25 und Abs. 6 BauGB)
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="bplan:BPlanPunktobjektType">
				<xs:sequence>
					<xs:element name="massnahme" type="bplan:MassnahmeType"/>
					<xs:element name="gegenstand" type="bplan:AnpflanzungBindungErhaltungsGegenstandType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="AnpflanzungBindungErhaltungsLinie" type="bplan:AnpflanzungBindungErhaltungsLinieType" substitutionGroup="bplan:BPlanLinienobjekt"/>
	<xs:complexType name="AnpflanzungBindungErhaltungsLinieType">
		<xs:annotation>
			<xs:documentation>
a) Linienhafte Festsetzung des Anpflanzens von Bäumen, Sträuchern und sonstigen Bepflanzungen;
b) Linienhafte Festsetzung von Bindungen für Bepflanzungen und für die Erhaltung von Bäumen, Sträuchern und sonstigen Bepflanzungen sowie von Gewässern;  (§9 Abs. 1 Nr. 25a und Abs. 6 BauGB)
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="bplan:BPlanLinienobjektType">
				<xs:sequence>
					<xs:element name="massnahme" type="bplan:MassnahmeType"/>
					<xs:element name="gegenstand" type="bplan:AnpflanzungBindungErhaltungsGegenstandType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="MassnahmeType">
		<xs:annotation>
			<xs:documentation>
Liste der nach §9, Abs.1, Nr. 25 möglichen Maßnahmen
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Erhaltung"/>
			<xs:enumeration value="Anpflanzung"/>
			<xs:enumeration value="Bindung"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="AusgleichsFlaeche" type="bplan:AusgleichsFlaecheType" substitutionGroup="bplan:BPlanFlaechenobjekt"/>
	<xs:complexType name="AusgleichsFlaecheType">
		<xs:annotation>
			<xs:documentation>
Festsetzung einer Fläche zum Ausgleich im Sinne des § 1a Abs.3 und §9 Abs. 1a BauGB.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="bplan:BPlanFlaechenobjektType">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="AusgleichsMassnahme" type="bplan:AusgleichsMassnahmeType" substitutionGroup="bplan:BPlanPunktobjekt"/>
	<xs:complexType name="AusgleichsMassnahmeType">
		<xs:annotation>
			<xs:documentation>
Festsetzung einer Maßnahme zum Ausgleich im Sinne des § 1a Abs.3 und §9 Abs. 1a BauGB.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="bplan:BPlanPunktobjektType">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- Paket Landwirtschaft, Wald- und Grünflächen -->
	<xs:element name="LandwirtschaftsFlaeche" type="bplan:LandwirtschaftsFlaecheType" substitutionGroup="bplan:BPlanFlaechenschlussobjekt"/>
	<xs:complexType name="LandwirtschaftsFlaecheType">
		<xs:annotation>
			<xs:documentation>
Festsetzungen von Flächen für die Landwirtschaft  (§9, Abs. 1, Nr. 18a BauGB)
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="bplan:BPlanFlaechenschlussobjektType">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="WaldFlaeche" type="bplan:WaldFlaecheType" substitutionGroup="bplan:BPlanFlaechenschlussobjekt"/>
	<xs:complexType name="WaldFlaecheType">
		<xs:annotation>
			<xs:documentation>
Festsetzung von Waldflächen  (§9, Abs. 1, Nr. 18b BauGB)
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="bplan:BPlanFlaechenschlussobjektType">
				<xs:sequence>
					<xs:element name="zweckbestimmung" type="bplan:ZweckbestimmungWaldflaecheType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="sonstigeZweckbestimmung" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="ZweckbestimmungWaldflaecheType">
		<xs:annotation>
			<xs:documentation>

			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Erholungswald"/>
			<xs:enumeration value="Sonstiges"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="GruenFlaeche" type="bplan:GruenFlaecheType" substitutionGroup="bplan:BPlanFlaechenschlussobjekt"/>
	<xs:complexType name="GruenFlaecheType">
		<xs:annotation>
			<xs:documentation>
Festsetzungen von öffentlichen und privaten Grünflächen(§9, Abs. 1, Nr. 15 BauGB)  und von Flächen für die Kleintierhaltung (§9, Abs. 1, Nr. 19 BauGB) ,
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="bplan:BPlanFlaechenschlussobjektType">
				<xs:sequence>
					<xs:element name="maxZahlWohnungen" type="xs:integer" minOccurs="0"/>
					<xs:element name="Fmin" type="xs:integer" minOccurs="0"/>
					<xs:element name="Fmax" type="xs:integer" minOccurs="0"/>
					<xs:element name="GFZmin" type="xs:decimal" minOccurs="0"/>
					<xs:element name="GFZmax" type="xs:decimal" minOccurs="0"/>
					<xs:element name="GFmin" type="xs:decimal" minOccurs="0"/>
					<xs:element name="GFmax" type="xs:decimal" minOccurs="0"/>
					<xs:element name="BMZ" type="xs:decimal" minOccurs="0"/>
					<xs:element name="BM" type="xs:decimal" minOccurs="0"/>
					<xs:element name="GRZ" type="xs:decimal" minOccurs="0"/>
					<xs:element name="GR" type="xs:decimal" minOccurs="0"/>
					<xs:element name="Zmin" type="xs:decimal" minOccurs="0"/>
					<xs:element name="Zmax" type="xs:decimal" minOccurs="0"/>
					<xs:element name="zweckbestimmung" type="bplan:ZweckbestimmungGruenflaecheType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="nutzungsForm" type="bplan:NutzungsformType" minOccurs="0"/>
					<xs:element name="sonstigeZweckbestimmung" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="ZweckbestimmungGruenflaecheType">
		<xs:annotation>
			<xs:documentation>

			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Parkanlage"/>
			<xs:enumeration value="Dauerkleingarten"/>
			<xs:enumeration value="Sportplatz"/>
			<xs:enumeration value="Spielplatz"/>
			<xs:enumeration value="Zeltplatz"/>
			<xs:enumeration value="Badeplatz"/>
			<xs:enumeration value="Friedhof"/>
			<xs:enumeration value="Kleintierhaltung"/>
			<xs:enumeration value="Sonstiges"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- Paket Sonstige Festsetzungen -->
	<xs:element name="RaeumlicherGeltungsbereich" type="bplan:RaeumlicherGeltungsbereichType" substitutionGroup="bplan:BPlanUeberlagerungsobjekt"/>
	<xs:complexType name="RaeumlicherGeltungsbereichType">
		<xs:annotation>
			<xs:documentation>
Grenze des räumlichen Geltungsbereichs des Bebauungsplans (§9 Abs. 7 BauGB)
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="bplan:BPlanUeberlagerungsobjektType">
				<xs:sequence>
					<xs:element name="aufstellungsbeschluss" type="xs:date" minOccurs="0"/>
					<xs:element name="veraenderungssperre" type="xs:date" minOccurs="0"/>
					<xs:element name="satzungsbeschluss" type="xs:date" minOccurs="0"/>
					<xs:element name="rechtsverordnung" type="xs:date" minOccurs="0"/>
					<xs:element name="datumDesInkrafttretens" type="xs:date" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
