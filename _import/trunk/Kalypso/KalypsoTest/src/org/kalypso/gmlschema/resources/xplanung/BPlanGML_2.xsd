	<xs:element name="DenkmalschutzBereich" type="bplan:DenkmalschutzBereichType" substitutionGroup="bplan:BPlanUeberlagerungsobjekt"/>
	<xs:complexType name="DenkmalschutzBereichType">
		<xs:annotation>
			<xs:documentation>
Festsetzung einer Fläche zur Erhaltung baulicher Anlagen und der Eigenart von Gebieten (§172 BauGB)
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="bplan:BPlanUeberlagerungsobjektType">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DenkmalschutzEnsemble" type="bplan:DenkmalschutzEnsembleType" substitutionGroup="bplan:BPlanUeberlagerungsobjekt"/>
	<xs:complexType name="DenkmalschutzEnsembleType">
		<xs:annotation>
			<xs:documentation>
Umgrenzung eines Denkmalgeschützten Ensembles
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="bplan:BPlanUeberlagerungsobjektType">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
