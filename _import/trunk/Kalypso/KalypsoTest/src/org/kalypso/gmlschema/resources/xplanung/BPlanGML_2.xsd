								<adv:referenziertesElement>bplan:AnpflanzungBindungErhaltungsLinie</adv:referenziertesElement>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="einzelObjekt" type="gml:FeatureAssociationType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:appinfo>
								<adv:referenziertesElement>bplan:AnpflanzungBindungErhaltungsMassnahme</adv:referenziertesElement>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
