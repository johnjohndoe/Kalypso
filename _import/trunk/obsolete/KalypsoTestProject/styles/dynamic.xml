<?xml version="1.0" encoding="UTF-8"?>
<StyledLayerDescriptor version="String" xmlns="http://www.opengis.net/sld" xmlns:gml="http://www.opengis.net/gml" xmlns:wfs="http://www.opengis.net/wfs" xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <NamedLayer>
    <Name>hamburg:bezirke</Name>
    <UserStyle>
      <Name>default:hamburg:bezirke</Name>
      <Title>default:hamburg:bezirke</Title>
      <IsDefault>1</IsDefault>
      <FeatureTypeStyle>
        <Name>hamburg:bezirke</Name>
        <Rule>
          <MinScaleDenominator>100000</MinScaleDenominator>
          <MaxScaleDenominator>999999999</MaxScaleDenominator>
          <PolygonSymbolizer>
            <Geometry>
              <ogc:PropertyName>GEOM</ogc:PropertyName>
            </Geometry>
            <Fill>
              <CssParameter name="fill">#3333BB</CssParameter>
              <CssParameter name="fill-opacity">0.2</CssParameter>
            </Fill>
            <Stroke>
              <CssParameter name="stroke">#000000</CssParameter>
              <CssParameter name="stroke-opacity">1.0</CssParameter>
              <CssParameter name="stroke-width">3.0</CssParameter>
              <CssParameter name="stroke-dasharray">1</CssParameter>
            </Stroke>
          </PolygonSymbolizer>
        </Rule>
        <Rule>
          <Name>Bezirke_Text</Name>
          <MinScaleDenominator>0</MinScaleDenominator>
          <MaxScaleDenominator>99999999</MaxScaleDenominator>
          <TextSymbolizer>
            <Geometry>
              <ogc:PropertyName>GEOM</ogc:PropertyName>
            </Geometry>
            <Label>
              <ogc:PropertyName>BEZIRK</ogc:PropertyName>
            </Label>
            <Font>
              <CssParameter name="font-family">Arial</CssParameter>
              <CssParameter name="font-family">Sans-Serif</CssParameter>
              <CssParameter name="font-style">italic</CssParameter>
              <CssParameter name="font-size">11</CssParameter>
              <CssParameter name="font-color">#222222</CssParameter>
            </Font>
            <LabelPlacement>
              <PointPlacement>
                <AnchorPoint>
                  <AnchorPointX>1.0</AnchorPointX>
                  <AnchorPointY>0.2</AnchorPointY>
                </AnchorPoint>
                <Displacement>
                  <DisplacementX>8.0</DisplacementX>
                  <DisplacementY>2.0</DisplacementY>
                </Displacement>
              </PointPlacement>
            </LabelPlacement>
            <Halo>
              <Fill>
                <CssParameter name="fill">#5555EE</CssParameter>
                <CssParameter name="fill-opacity">1.0</CssParameter>
              </Fill>
            </Halo>
          </TextSymbolizer>
        </Rule>
      </FeatureTypeStyle>
    </UserStyle>
  </NamedLayer>
  <NamedLayer>
    <Name>naturschutzgebiete</Name>
    <UserStyle>
      <Name>default:naturschutzgebiete</Name>
      <Title>default:naturschutzgebiete</Title>
      <IsDefault>1</IsDefault>
      <FeatureTypeStyle>
        <Name>naturschutzgebiete</Name>
        <Rule>
          <Name>NSG</Name>
          <MinScaleDenominator>0</MinScaleDenominator>
          <MaxScaleDenominator>999999999</MaxScaleDenominator>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo wildCard="*" singleChar="?" escape="\">
              <ogc:PropertyName>SCHUTZSTAT</ogc:PropertyName>
              <ogc:Literal>NSG</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <PolygonSymbolizer>
            <Geometry>
              <ogc:PropertyName>GEOM</ogc:PropertyName>
            </Geometry>
            <Fill>
              <CssParameter name="fill">#33BB33</CssParameter>
              <CssParameter name="fill-opacity">0.8</CssParameter>
            </Fill>
            <Stroke>
              <CssParameter name="stroke">#000000</CssParameter>
              <CssParameter name="stroke-opacity">1.0</CssParameter>
              <CssParameter name="stroke-width">1.0</CssParameter>
              <CssParameter name="stroke-dasharray">1</CssParameter>
            </Stroke>
          </PolygonSymbolizer>
        </Rule>
        <Rule>
          <Name>LSG</Name>
          <MinScaleDenominator>0</MinScaleDenominator>
          <MaxScaleDenominator>999999999</MaxScaleDenominator>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo wildCard="*" singleChar="?" escape="\">
              <ogc:PropertyName>SCHUTZSTAT</ogc:PropertyName>
              <ogc:Literal>LSG</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <PolygonSymbolizer>
            <Geometry>
              <ogc:PropertyName>GEOM</ogc:PropertyName>
            </Geometry>
            <Fill>
              <CssParameter name="fill">#77DD77</CssParameter>
              <CssParameter name="fill-opacity">0.8</CssParameter>
            </Fill>
            <Stroke>
              <CssParameter name="stroke">#000000</CssParameter>
              <CssParameter name="stroke-opacity">1.0</CssParameter>
              <CssParameter name="stroke-width">1.0</CssParameter>
              <CssParameter name="stroke-dasharray">1</CssParameter>
            </Stroke>
          </PolygonSymbolizer>
        </Rule>
        <Rule>
          <Name>ND</Name>
          <MinScaleDenominator>0</MinScaleDenominator>
          <MaxScaleDenominator>999999999</MaxScaleDenominator>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo wildCard="*" singleChar="?" escape="\">
              <ogc:PropertyName>SCHUTZSTAT</ogc:PropertyName>
              <ogc:Literal>ND</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <PolygonSymbolizer>
            <Geometry>
              <ogc:PropertyName>GEOM</ogc:PropertyName>
            </Geometry>
            <Fill>
              <CssParameter name="fill">#AAEEAA</CssParameter>
              <CssParameter name="fill-opacity">0.8</CssParameter>
            </Fill>
            <Stroke>
              <CssParameter name="stroke">#000000</CssParameter>
              <CssParameter name="stroke-opacity">1.0</CssParameter>
              <CssParameter name="stroke-width">1.0</CssParameter>
              <CssParameter name="stroke-dasharray">1</CssParameter>
            </Stroke>
          </PolygonSymbolizer>
        </Rule>
      </FeatureTypeStyle>
    </UserStyle>
  </NamedLayer>
  
  <NamedLayer>
