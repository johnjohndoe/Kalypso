<?xml version="1.0" encoding="UTF-8"?>
<modelspec xmlns="simspec.core.simulation.kalypso.org"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	typeID="Gaja3d_createGrid">

	<!-- Input -->
	<input id="Boundary" type="gaja3d:Boundaries" description="The boundary for each tile." />
	<input id="DemPoints" type="xs:anyURI"
		description="The elevation points to create grids from." />
	<input id="gridx" type="xs:double" description="Grid resolution in x-direction." />
	<input id="gridy" type="xs:double" description="Grid resolution in y-direction." />

	<!-- Output -->
	<output id="DemGrid" type="gaja3d:DemGrids" description="The created grids." />

	<!-- Logging -->
	<output id="stdout" type="xs:anyURI" description="Standard out" />
	<output id="stderr" type="xs:anyURI" description="Standard err" />
</modelspec>