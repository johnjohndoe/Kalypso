<?xml version="1.0" encoding="UTF-8"?>
<modelspec xmlns="simspec.core.simulation.kalypso.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" typeID="Gaja3d_createGrid">

  <!-- Input -->
  <input id="Boundary" type="xs:anyURI" description="The boundary for restricting the points." />
  <input id="DemPoints" type="xs:anyURI" description="The elevation points to create a grid for." />
  <input id="gridx" type="double" description="Grid resolution in x-direction." />
  <input id="gridy" type="double" description="Grid resolution in y-direction." />

  <!-- Output -->
  <input id="_DemGrid" type="string" description="File name for DemGrid output."/>
  <output id="DemGrid" type="xs:anyURI" description="The created grid." />
  
  <!-- Logging -->
  <output id="stdout" type="xs:anyURI" description="Standard out" />
  <output id="stderr" type="xs:anyURI" description="Standard err" />
</modelspec>