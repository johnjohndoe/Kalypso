<?xml version="1.0" encoding="UTF-8"?>
<modelspec xmlns="simspec.core.simulation.kalypso.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" typeID="Gaja3d_createTin">

  <!-- Input -->
  <input id="Boundary" type="gaja3d:Boundaries" description="The boundary for restricting the points." />
  <input id="Breaklines" type="gaja3d:Breaklines" description="The breaklines for the model tin." />
  <input id="MaxArea" type="xs:double" optional="true" description="The maximum area to enforce for each triangle." />
  <input id="MinAngle" type="xs:double" optional="true" description="The minimum internal angle to enforce for each triangle." />
  <input id="DemGrid" type="gaja3d:DemGrids" optional="true" description="The digital elevation grid to assign elevations from." />  

  <!-- Output -->
  <output id="ModelTin" type="xs:anyURI" description="The created model tin." />
  
  <!-- Logging -->
  <output id="stdout" type="xs:anyURI" description="Standard out" />
  <output id="stderr" type="xs:anyURI" description="Standard err" />
</modelspec>