<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
  <display-name>Kalypso</display-name>
  <description>NA-Modell</description>
  <enterprise-beans>
    <session>
      <ejb-name>Kalypso.VersionSession</ejb-name>
      <home>de.tuhh.wb.javagis.model.na.VersionSessionHome</home>
      <remote>de.tuhh.wb.javagis.model.na.VersionSession</remote>
      <ejb-class>de.tuhh.wb.javagis.model.na.VersionSessionBean</ejb-class>
      <session-type>Stateful</session-type>
      <transaction-type>Container</transaction-type>
      <ejb-local-ref>
        <description>access to VersionManager</description>
        <ejb-ref-name>ejb/versions</ejb-ref-name>
        <ejb-ref-type>Entity</ejb-ref-type>
        <local-home>de.tuhh.wb.javagis.model.na.VersionLocalHome</local-home>
        <local>de.tuhh.wb.javagis.model.na.VersionLocal</local>
        <ejb-link>Kalypso.Version</ejb-link>
      </ejb-local-ref>
      <ejb-local-ref>
        <description>access to the IdManager</description>
        <ejb-ref-name>ejb/idManager</ejb-ref-name>
        <ejb-ref-type>Entity</ejb-ref-type>
        <local-home>de.tuhh.wb.javagis.model.IdManagerLocalHome</local-home>
        <local>de.tuhh.wb.javagis.model.IdManagerLocal</local>
        <ejb-link>de.tuhh.wb.javagis.model.IdManager</ejb-link>
      </ejb-local-ref>
    </session>
    <session>
      <ejb-name>Kalypso.ChannelSession</ejb-name>
      <home>de.tuhh.wb.javagis.model.na.ChannelSessionHome</home>
      <remote>de.tuhh.wb.javagis.model.na.ChannelSession</remote>
      <ejb-class>de.tuhh.wb.javagis.model.na.ChannelSessionBean</ejb-class>
      <session-type>Stateful</session-type>
      <transaction-type>Container</transaction-type>
      <ejb-local-ref>
        <description>access to versionManager</description>
        <ejb-ref-name>ejb/versions</ejb-ref-name>
        <ejb-ref-type>Entity</ejb-ref-type>
        <local-home>de.tuhh.wb.javagis.model.na.VersionLocalHome</local-home>
        <local>de.tuhh.wb.javagis.model.na.VersionLocal</local>
        <ejb-link>Kalypso.Version</ejb-link>
      </ejb-local-ref>
      <ejb-local-ref>
        <description>access to my EntityBeans</description>
        <ejb-ref-name>ejb/MyEntityBeans</ejb-ref-name>
        <ejb-ref-type>Entity</ejb-ref-type>
        <local-home>de.tuhh.wb.javagis.model.na.ChannelLocalHome</local-home>
        <local>de.tuhh.wb.javagis.model.na.ChannelLocal</local>
        <ejb-link>Kalypso.Channel</ejb-link>
      </ejb-local-ref>
      <ejb-local-ref>
        <description>access to the IdManager</description>
        <ejb-ref-name>ejb/idManager</ejb-ref-name>
        <ejb-ref-type>Entity</ejb-ref-type>
        <local-home>de.tuhh.wb.javagis.model.IdManagerLocalHome</local-home>
        <local>de.tuhh.wb.javagis.model.IdManagerLocal</local>
        <ejb-link>de.tuhh.wb.javagis.model.IdManager</ejb-link>
      </ejb-local-ref>
    </session>
    <entity>
      <ejb-name>Kalypso.Channel</ejb-name>
      <local-home>de.tuhh.wb.javagis.model.na.ChannelLocalHome</local-home>
      <local>de.tuhh.wb.javagis.model.na.ChannelLocal</local>
      <ejb-class>de.tuhh.wb.javagis.model.na.ChannelBean</ejb-class>
      <persistence-type>Container</persistence-type>
      <prim-key-class>java.lang.Integer</prim-key-class>
      <reentrant>False</reentrant>
      <cmp-version>2.x</cmp-version>
      <abstract-schema-name>ChannelBean</abstract-schema-name>
      <cmp-field>
        <description>Der PrimaryKey</description>
        <field-name>objectId</field-name>
      </cmp-field>
      <cmp-field>
        <description>Strand-Number:String</description>
        <field-name>m_strandNr</field-name>
      </cmp-field>
      <cmp-field>
        <description>WaterCourseIndex:String</description>
        <field-name>m_wcIndex</field-name>
      </cmp-field>
      <cmp-field>
        <description>boardfull Discharchge:String</description>
        <field-name>m_qBoardfull</field-name>
      </cmp-field>
      <cmp-field>
        <description>DistributionFactor:String</description>
        <field-name>m_distribFact</field-name>
      </cmp-field>
      <cmp-field>
        <description>ChannelData:</description>
        <field-name>M_channelData</field-name>
      </cmp-field>
      <primkey-field>objectId</primkey-field>
    </entity>
    <entity>
      <ejb-name>Kalypso.Version</ejb-name>
      <local-home>de.tuhh.wb.javagis.model.na.VersionLocalHome</local-home>
      <local>de.tuhh.wb.javagis.model.na.VersionLocal</local>
      <ejb-class>de.tuhh.wb.javagis.model.na.VersionBean</ejb-class>
      <persistence-type>Container</persistence-type>
      <prim-key-class>java.lang.Integer</prim-key-class>
      <reentrant>False</reentrant>
      <cmp-version>2.x</cmp-version>
      <abstract-schema-name>VersionBean</abstract-schema-name>
      <cmp-field>
        <description>Der PrimaryKey</description>
        <field-name>objectId</field-name>
      </cmp-field>
      <cmp-field>
        <description>Name of Version</description>
        <field-name>versionName</field-name>
      </cmp-field>
      <cmp-field>
        <description>State of Version</description>
        <field-name>versionState</field-name>
      </cmp-field>
      <cmp-field>
        <description>Comment</description>
        <field-name>versionComment</field-name>
      </cmp-field>
      <primkey-field>objectId</primkey-field>
      <query>
        <query-method>
          <method-name>findAllVersions</method-name>
          <method-params/>
        </query-method>
        <ejb-ql>SELECT OBJECT(a) FROM VersionBean AS a</ejb-ql>
      </query>
    </entity>
    <entity>
      <ejb-name>de.tuhh.wb.javagis.model.IdManager</ejb-name>
      <local-home>de.tuhh.wb.javagis.model.IdManagerLocalHome</local-home>
      <local>de.tuhh.wb.javagis.model.IdManagerLocal</local>
      <ejb-class>de.tuhh.wb.javagis.model.IdManagerBean</ejb-class>
      <persistence-type>Container</persistence-type>
      <prim-key-class>java.lang.String</prim-key-class>
      <reentrant>False</reentrant>
      <cmp-version>2.x</cmp-version>
      <abstract-schema-name>IdManagerBean</abstract-schema-name>
      <cmp-field>
        <description>PrimKeyIsTableName</description>
        <field-name>tableName</field-name>
      </cmp-field>
      <cmp-field>
        <description>nextFreeId</description>
        <field-name>maxId</field-name>
      </cmp-field>
      <primkey-field>tableName</primkey-field>
    </entity>
  </enterprise-beans>
  <relationships>
    <ejb-relation>
      <ejb-relation-name>Versioning-of-Channel</ejb-relation-name>
      <ejb-relationship-role>
        <ejb-relationship-role-name>Version-has-Channel</ejb-relationship-role-name>
        <multiplicity>One</multiplicity>
        <relationship-role-source>
          <ejb-name>Kalypso.Version</ejb-name>
        </relationship-role-source>
        <cmr-field>
          <cmr-field-name>channel</cmr-field-name>
          <cmr-field-type>java.util.Collection</cmr-field-type>
        </cmr-field>
      </ejb-relationship-role>
      <ejb-relationship-role>
        <ejb-relationship-role-name>Channel-has-Version</ejb-relationship-role-name>
        <multiplicity>Many</multiplicity>
        <relationship-role-source>
          <ejb-name>Kalypso.Channel</ejb-name>
        </relationship-role-source>

        <cmr-field>
          <cmr-field-name>version</cmr-field-name>
<!--            <cmr-field-type>java.util.Collection</cmr-field-type> -->
        </cmr-field>

      </ejb-relationship-role>
    </ejb-relation>
  </relationships>
  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>Kalypso.Version</ejb-name>
        <method-name>getChannel</method-name>
      </method>
      <method>
        <ejb-name>Kalypso.Channel</ejb-name>
        <method-name>getObjectId</method-name>
      </method>
      <method>
        <ejb-name>Kalypso.Channel</ejb-name>
        <method-name>setObjectId</method-name>
      </method>
      <method>
        <ejb-name>Kalypso.Channel</ejb-name>
        <method-name>getM_strandNr</method-name>
      </method>
      <method>
        <ejb-name>Kalypso.Channel</ejb-name>
        <method-name>setM_strandNr</method-name>
      </method>
      <method>
        <ejb-name>Kalypso.Channel</ejb-name>
        <method-name>getM_wcIndex</method-name>
      </method>
      <method>
        <ejb-name>Kalypso.Channel</ejb-name>
        <method-name>setM_wcIndex</method-name>
      </method>
      <method>
        <ejb-name>Kalypso.Channel</ejb-name>
        <method-name>getM_qBoardfull</method-name>
      </method>
      <method>
        <ejb-name>Kalypso.Channel</ejb-name>
        <method-name>setM_qBoardfull</method-name>
      </method>
      <method>
        <ejb-name>Kalypso.Channel</ejb-name>
        <method-name>getM_distribFact</method-name>
      </method>
      <method>
        <ejb-name>Kalypso.Channel</ejb-name>
        <method-name>setM_distribFact</method-name>
      </method>
      <method>
        <ejb-name>Kalypso.Channel</ejb-name>
        <method-name>getM_channelData</method-name>
      </method>
      <method>
        <ejb-name>Kalypso.Channel</ejb-name>
        <method-name>setM_channelData</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
