<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.manual.sachsenanhalt"
   name="KalypsoManualSachsenAnhalt Plug-in"
   version="1.0.0"
   provider-name="kalypso.org">
   <requires>
      <import plugin="org.eclipse.help"/>
   </requires>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml"
            plugin="org.kalypso.manual"/>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            primary="true"
            file="toc.xml"/>
   </extension>

</plugin>
