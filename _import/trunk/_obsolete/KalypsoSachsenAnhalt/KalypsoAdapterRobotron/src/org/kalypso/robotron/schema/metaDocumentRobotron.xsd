<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="org.kalypso.hwv.services.metadoc.robotron" xmlns="org.kalypso.hwv.services.metadoc.robotron" xmlns:doc="org.kalypso.hwv.services.metadoc" xmlns:gml="http://www.opengis.net/gml" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
