    </xs:annotation>
  
    <xs:import namespace="org.kalypso.hwv.services.metadoc" />
    <xs:import namespace="http://www.opengis.net/gml" />
  
    <xs:element name="DocumentServiceRobotronFeature" substitutionGroup="doc:AbstractDocumentServiceFeature">
