<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         id="flows-catalogs"
         name="Catalogs for Flows"
         point="org.kalypso.contribs.java.urlCatalog">
      <catalog
            class="org.kalypso.flows.URLCatalogFlows"
            id="org.kalypso.catalog.flows"/>
   </extension>
   <extension
         id="flowsXMLCatalog"
         name="catalogs for flows"
         point="org.kalypso.core.catalogContribution">
      <catalogContribution class="org.kalypso.flows.xplanung.FeatureTypeStyleCatalogContribution"/>
      <catalogContribution
            class="org.kalypso.flows.measures.MeasuresFeatureTypeStyleCatalogContribution"
            name="Measure sytles catalog"/>
   </extension>
   <extension
         id="org.kalypso.model.flows"
         name="URLActionCommands"
         point="org.kalypso.workflow.urlCommandActionContribution">
      <urlaction
            class="org.kalypso.flows.URLActionAnalizeConflicts"
            command="analizeConflicts"
            name="Analize conflicts"/>
      <urlaction
            class="org.kalypso.flows.URLActionGenerateHTMLConflictAnalysis"
            command="generateFilterHTML"
            name="Generates HTML for filter"/>
      <urlaction
            class="org.kalypso.flows.URLActionFLOWSAddFilterToGMT"
            command="openFLOWSFilterDialog"
            name="Simple FLOWS filter dialog"/>
	  <urlaction
            class="org.kalypso.flows.URLActionImportBPlanGML"
            command="importBPlanGML"
            name="Import XPlanung GML"/>            
   </extension>

</plugin>
