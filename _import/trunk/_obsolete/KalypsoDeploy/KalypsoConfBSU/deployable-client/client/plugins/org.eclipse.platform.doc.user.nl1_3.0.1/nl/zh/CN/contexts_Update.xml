<?xml version="1.0" encoding="UTF-8"?>
<?NLS TYPE="org.eclipse.help.contexts"?>

<contexts>
   
   <!-- Context IDs added in the  code, ID is class name, where context ID is set -->
   
   <context  id="ConfigurationView" >
      <description>此对话框用于处理已安装的功能部件。缺省情况下，它只显示当前启用的功能部件。在对话框工具栏上打开“显示禁用的功能部件”过滤器以便可同时看到禁用的功能部件。</description>
      <topic label="检查配置" href="tasks/tasks-33.htm"/>
      <topic label="启用、禁用或卸载功能部件" href="tasks/tasks-35.htm"/>
   </context>
     
   <context  id="ReviewPage" >
      <description>功能部件搜索的结果显示在此页面上。选择想要安装的功能部件。</description>
   </context>
   
   
   <context  id="CofigurationView_showUnconfFeaturesAction" >
      <description>显示禁用的功能部件。</description>
   </context>
   
   <context  id="CofigurationView_revertAction" >
      <description>恢复配置以匹配已选择的历史记录。</description>
   </context>
   
   <context  id="CofigurationView_showStatusAction" >
      <description>显示选择的功能部件的当前配置状态。</description>
   </context>
   
   <context  id="CofigurationView_preserveAction" >
      <description>将选择的配置保存在<b>已保存的配置</b>文件夹中。</description>
   </context>
   
   <context  id="CofigurationView_removePreservedAction" >
      <description>从<b>已保存的配置</b>文件夹中删除选择的配置。</description>
   </context>
   
   <context  id="CofigurationView_unlinkAction" >
      <description>从配置中将选择的站点解链。</description>
   </context>
   
   <context  id="CofigurationView_propertiesAction" >
      <description>显示选择的属性。</description>
   </context>
   
   <context  id="UpdatesView_newAction" >
      <description>创建新的站点书签。</description>
   </context>
   
   <context  id="UpdatesView_newFolderAction" >
      <description>在“功能部件更新”视图中创建新的文件夹。文件夹可用于组织站点书签和搜索查询。</description>
   </context>
   
   <context  id="UpdatesView_newSearchAction" >
      <description>创建新的搜索查询。</description>
   </context>
   
   <context  id="UpdatesView_newLocalAction" >
      <description>为选择的本地站点创建站点书签。</description>
   </context>
   
   <context  id="UpdatesView_deleteAction" >
      <description>删除选择的项。</description>
   </context>
   
   <context  id="UpdatesView_refreshAction" >
      <description>刷新选择的项。</description>
   </context>
   
   <context  id="UpdatesView_fileFilterAction" >
      <description>选择以在<b>我的电脑</b>中显示各个文件。取消选择以仅显示本地站点和目录。</description>
   </context>
   
   <context  id="UpdatesView_filterEnvironmentAction" >
      <description>选择以显示仅与当前环境匹配的功能部件。取消选择以显示所有可用的功能部件（不能安装与当前环境不匹配的功能部件）。</description>
   </context>
   
   <context  id="UpdatesView_showCategoriesAction" >
      <description>选择以查看组织成站点类别的功能部件。取消选择以将所有功能部件作为一个列表来查看。</description>
   </context>
   
   <context  id="UpdatesView_linkExtensionAction" >
      <description>将选择的站点链接至配置。</description>
   </context>
   
   <context  id="DetailsView_homeAction" >
      <description>显示“更新管理器”欢迎页面。</description>
   </context>
   
   <context  id="DetailsView_backAction" >
      <description>后退至先前的详细信息页面。</description>
   </context>
   
   <context  id="DetailsView_forwardAction" >
      <description>前进至下一个详细信息页面。</description>
   </context>
   
   <context  id="MainPreferencePage" >
      <description>设置更新管理器首选项 － 在删除之前要维护的<b>历史记录数</b>；将<b>新的更新</b>搜索的作用域仅限为服务更新或可兼容的较小的发行版更新；备用更新站点的<b>更新策略</b>；<b>代理</b>设置</description>
      <topic label="更新策略控制"  href="tasks/tasks-37.htm"/>
   </context>

      
   <context  id="LicensePage2" >
      <description>必须查看显示的许可协议，或者接受，或者拒绝。</description>
   </context>
   
   <context  id="ModeSelectionPage" >
      <description>选择是想要搜索对已安装功能部件的可用更新，还是搜索存在于所选更新站点上的任何功能部件</description>   
      <topic label="使用更新管理器安装功能部件" href="tasks/tasks-34.htm"/>
      <topic label="使用更新管理器更新功能部件" href="tasks/tasks-36.htm"/>   
   </context>
   
   <context  id="SitePage" >
      <description>添加、除去或编辑更新站点书签。选择要在其中搜索功能部件和补丁的站点。
<b>远程站点</b>：包含功能部件的 Web 站点的 url
<b>本地站点</b>：在当前机器上的站点
<b>归档大小</b>：打包为 ZIP 或 JAR 文件的本地站点

可以打开或关闭对与环境（操作系统和语言环境等等）不匹配的功能部件的搜索
	</description>
   </context>
   
   <context  id="NewUpdatesWizardPage" >
      <description>选择一个或多个可用的更新来安装。</description>
   </context>
   
   <context  id="MultiTargetPage2" >
      <description>对于要安装的每个功能部件，从现有的选项中选择安装位置，或添加新的安装位置。由于驻留在同一位置的约束，可能不能为某些功能部件定义新位置。</description>
   </context>
   
   <context id="OptionalFeaturesPage">
   	  <description>选择将安装包括在主功能部件中的哪些可选功能部件。</description>
   </context>
   
   <context id="MultiOptionalFeaturesPage2">
   	  <description>选择将安装包括在主功能部件中的哪些可选功能部件。</description>
   </context>
   
   <context  id="JarVerificationPage" >
      <description>必须接受或拒绝 JAR 文件的证书</description>
   </context>
   
   <context  id="UserValidationDialog" >
      <description>输入用户信息。</description>
   </context>
   
   <context id="MultiReviewPage2">
      <description>选择想要安装的功能部件。如果存在错误，则按<b>错误详细信息</b>并通过选择或取消选择适当的功能部件来解决问题。</description>
     <topic label="使用更新管理器安装功能部件" href="tasks/tasks-34.htm"/>
   </context>

   
   <context  id="InstallableSiteForm" >
      <description>显示有关成为配置一部分的本地站点的详细信息。</description>          
   </context>
   
   
   <context  id="SiteForm" >
      <description>显示当前安装配置详细信息。</description> 
      <topic label="检查配置" href="tasks/tasks-33.htm"/>
      <topic label="启用、禁用或卸载功能部件" href="tasks/tasks-35.htm"/>       
   </context>
   
   <context  id="UnknownObjectForm" >
      <description>显示何时在活动部件中选择了未知对象或未选择任何对象。</description>
   </context>
   
   
   <context id ="SiteBookmarkPropertyPage">
      <description>更改书签对象的属性。</description>
   </context>
   
   <context id ="NamedObjectPropertyPage">
      <description>更改所选择的对象的名称。</description>
   </context>
   
   <context id ="ConfigurationPropertyPage">
      <description>更改所选择的配置的名称。</description>
   </context>
   
</contexts>
