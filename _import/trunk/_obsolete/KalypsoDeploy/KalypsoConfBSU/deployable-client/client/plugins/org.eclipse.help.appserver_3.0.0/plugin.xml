<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.help.appserver"
   name="%appserver_plugin_name"
   version="3.0.0"
   provider-name="%providerName"
   class="org.eclipse.help.internal.appserver.AppserverPlugin">

   <runtime>
      <library name="appserver.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.core.runtime"/>
   </requires>


   <extension-point id="server" name="%server" schema="schema/server.exsd"/>

</plugin>
