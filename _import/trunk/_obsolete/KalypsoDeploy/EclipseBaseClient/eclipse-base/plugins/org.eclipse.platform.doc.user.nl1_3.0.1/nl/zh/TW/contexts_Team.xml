<?xml version="1.0" encoding="UTF-8"?>
<?NLS TYPE="org.eclipse.help.contexts"?>
<contexts>

	<!-- ********************************** -->
 	<!-- Wizard Pages 						-->
 	<!-- ********************************** -->
 	
 	<context id="share_project_page_context">
      <description>這個頁面協助您選取想要連接專案的目標網站類型。</description>
   	</context>
   	
   	<context id="import_project_set_page_context">
      <description>這個頁面協助您從檔案系統中的檔案匯入專案集。專案集是共用專案集的簡易種類。可用來重新建構您在工作區上的共用專案。</description>
   	</context>
	
	<context id="export_project_set_page_context">
      <description>這個頁面協助您匯出專案集至檔案，檔案可以與其他團隊成員共用。專案集是共用專案集的簡易種類。可用來重新建構您在工作區上的共用專案。</description>
   	</context>
	
	<context id="target_new_site_page_context">
      <description>這個頁面協助您選取專案應該連接的網站。您可以從現有的網站清單中選取或建立新網站。</description>
   	</context>
	
	<context id="target_mapping_selection_page_context">
      <description>這個頁面協助您在已選取的網站選擇專案應該連接的資料夾。您可以選擇現有的資料夾或建立新資料夾。</description>
   	</context>
   	
    <context id="sync_resource_selection_page_context">
      <description>這個頁面可讓您選擇要同步化的資源。
</description>
   	</context>
	
	
 	<!-- ********************************** -->
 	<!-- Preference Pages 					-->
 	<!-- ********************************** -->
 	
 	<context id="ignore_preference_page_context" >
      <description>您可能想要從版本控制排除某些資源。
名稱符合啟用的型樣的資源將不會釋放出來。
允許使用萬用字元 '*' 及 '?'。
這些設定將不會影響已在版本控制下的資源。
</description>
      <topic label="建立廣域忽略型樣" href="tasks/tasks-100d1.htm"/>
   	</context>

 	<context id="team_preference_page_context" >
      <description>這個頁面可讓您設定「同步化」視圖將在何種模式下開啟。</description>
      <topic label="CVS 同步化視圖" href="reference/ref-33.htm"/>
   	</context>

 	<context id="file_type_preference_page_context" >
      <description>這個頁面可讓您建立副檔名與其內容類型（ASCII 或二進位）的關聯性。這些設定是由儲存庫提供者（例如 CVS）所使用</description>
      <topic label="設定副檔名的內容類型" href="tasks/tasks-team-1.htm"/>
   	</context>
   	   	  	
   	<!-- ********************************** -->
 	<!-- TargetMenuActions 					-->
 	<!-- ********************************** -->
	
	<context id="target_get_action_context">
   		<description>從網站下載選取的資源至您的工作區。</description>
   	</context>

 	<context id="target_put_action_context">
   		<description>從您的工作區上傳選取的資源至網站。</description>
   	</context>
   	
   	<context id="target_synchronize_action_context">
   		<description>在同步化視圖顯示本端與遠端網站已選取資源狀態的任何差異。</description>
   	</context>
   	
   	<context id="target_getas_action_context">
   		<description>從網站下載選取的資源至您的工作區。您可以指定資源的本端名稱。</description>
   	</context>
   	
   	<context id="target_getasproject_action_context">
   		<description>從網站下載選取的資源至您的工作區。已為每一個選取的項目在工作區中建立專案。</description>
   	</context>
   	
   	<context id="target_disconnect_action_context">
   		<description>從記得的網站清單中捨棄選取的目標網站。</description>
   	</context>
   	
   	<!-- ********************************** -->
 	<!-- TargetCatchupReleaseViewer			-->
 	<!-- ********************************** -->
	
	<context id="target_catchup_release_viewer_context">
   		<description>這個視圖顯示收到、送出及衝突的變更。</description>
   	</context>
   	
   	<!-- ********************************** -->
 	<!-- TargetActions						-->
 	<!-- ********************************** -->
   	
   	<context id="sync_get_action_context">
   		<description>從網站下載選取的資源至您的工作區。</description>
   	</context>
   	
   	<context id="sync_put_action_context">
   		<description>從您的工作區上傳選取的資源至網站。</description>
   	</context>
   	
   	<!-- ********************************** -->
 	<!-- Views					            -->
 	<!-- ********************************** -->
 	
 	<context id="site_explorer_view_context">
      <description>「網站瀏覽器」視圖可讓您瀏覽已知目標網站的內容並可下載遠端資源至本端工作區。</description>
   	</context>

 	<context id="sync_view_context">
      <description>「同步化」視圖可讓您將工作區的本端內容與遠端儲存庫或目標同步化。</description>
      <topic label="CVS 同步化視圖" href="reference/ref-33.htm"/>
   	</context>
   	   	
   	<!-- ********************************** -->
 	<!-- SiteExplorerViewActions		    -->
 	<!-- ********************************** -->
 	
 	<context id="add_site_action_context">
      <description>建立可以上傳或下載資源的新目標網站。</description>
   	</context>
   	
   	<context id="new_folder_action_context">
      <description>在目標網站建立新資料夾。</description>
   	</context>
   	
   	<!-- ********************************** -->
 	<!-- SyncViewActions				    -->
 	<!-- ********************************** -->
 	
 	<context id="refresh_action_context">
      <description>重新整理視圖內容。</description>
   	</context>

   	<context id="open_action_context">
      <description>在比較檢視器中顯示本端和遠端的檔案內容的比較。</description>
   	</context>
   	 	
   	<context id="expandall_action_context">
      <description>展開全部項目。</description>
   	</context>
   	
   	<context  id="remove_action_context">
      <description>從同步化視圖移除選取項目。</description>
   	</context>
   	
   	<context  id="navigator_show_action_context">
      <description>在「導覽器」視圖中選取並顯示已選取的項目。</description>
   	</context>
 
</contexts>
