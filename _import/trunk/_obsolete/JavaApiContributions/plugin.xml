<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.contribs.java"
   name="JavaApiContributions Plug-in"
   version="0.0.1"
   provider-name="kalypso.org"
   class="org.kalypso.contribs.java.JavaApiContributionsPlugin">

   <runtime>
      <library name="JavaApiContributions.jar">
         <export name="*"/>
      </library>
      <library name="lib/junit.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.junit"/>
   </requires>
   <extension-point id="urlCatalog" name="urlCatalog" schema="schema/urlCatalog.exsd"/>

</plugin>
