  <package>de.tuhh.wb.javagis.model.control</package>
  <objectClass ID="controlData">
    <name>Control Data</name>
    <description>Data to control simulation</description>
    <simpleProperty key="m_startDate">
      <name>Start Date</name>
      <description>Start date of simulation </description>
<unit>[d.M.y H:m]</unit>
      <format>d.M.y H:m</format>
      <type>java.util.Date</type>
    </simpleProperty>
    <simpleProperty key="m_endDate">
      <name>End Date</name>
      <description>End date of simulation </description>
<unit>[d.M.y H:m]</unit>
      <format>d.M.y H:m</format>
      <type>java.util.Date</type>
    </simpleProperty>

    <simpleProperty key="m_timeStep">
      <name>Timestep</name>
      <description>Timestep for simulation</description>  
<unit>[h]</unit>
          <type>java.lang.Float</type>
    </simpleProperty>

    <simpleProperty key="m_rootNode">
      <name>Root Node</name>
      <description>Downstream node in model network to define part of simulation, node numer given in model data</description>
      <type>java.lang.Integer</type>
    </simpleProperty>

    <simpleProperty key="lzpath">
      <name>Pfad Anfangswerte</name>
      <description>Pfadname, um vorbereitete Anfangswerte zu benutzen. Beispiel: Dateien liegen in c:\\data\\lzsim, dann hier c:\\data angeben. Ohne Angabe werden Anfangswerte aus dem Vorlageverzeichnis verwendet.</description>
      <type>java.lang.String</type>
    </simpleProperty>

    <simpleProperty key="m_tmp">
      <name>Output Temperature</name>
       <description>Time-series of temperature, catchment </description>
<unit>[degree]</unit>
      <type>java.lang.Boolean</type>
    </simpleProperty>

    <simpleProperty key="m_pre">
      <name>Output Precipitation</name>
      <description>Time-series of precipitation, catchment</description>
      <unit>[mm/h]</unit>
      <type>java.lang.Boolean</type>
    </simpleProperty>
    
    <simpleProperty key="m_sch">
      <name>Output Snow Height</name>
      <description>Simulated time-series of snow height, catchment</description>
      <unit>[mm]</unit>
      <type>java.lang.Boolean</type>
    </simpleProperty>
    
