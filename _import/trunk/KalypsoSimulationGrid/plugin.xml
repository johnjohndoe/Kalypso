<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.kalypso.commons.process">
      <factory
            class="org.kalypso.simulation.grid.GridProcessFactory"
            id="org.kalypso.simulation.gridprocess"
            label="Grid-Job">
      </factory>
   </extension>
   <extension
         point="org.apache.commons.vfs.provider">         
      <provider
            class="org.kalypso.simulation.grid.GsiFtpProviderExtension"
            scheme="gsiftp">
      </provider>
      <provider
            class="org.kalypso.simulation.grid.GsiFtpProviderExtension"
            scheme="gridftp">
      </provider>
   </extension>
</plugin>
