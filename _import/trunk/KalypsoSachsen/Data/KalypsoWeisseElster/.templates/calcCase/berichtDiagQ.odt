<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<?xml-stylesheet type="text/xsl" href="xsl/grafik-vorlage.xsl"?>
<obsdiagview xmlns="obsdiagview.template.kalypso.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="obsdiagview.template.kalypso.org
.\obsdiagview.xsd" xmlns:xlink="http://www.w3.org/1999/xlink">
	<title>%title%</title>
	<legend title="" visible="true"/>

	<axis id="D" datatype="date" label="Datum" unit="" direction="horizontal" position="bottom" />
	<axis id="Q" datatype="double" label="Abfluss" unit="mÂ³" direction="vertical" position="left" />
   	
   	<!-- Kodierung color:
   	red;green;blue[;alpha]
   	
   	jeweils in [0-255]
   		<arg name="tokens">
			<arg name="title" value="name"/>
			<arg name="href1" value="local1"/> Pegel Messung
			<arg name="href2" value="local2"/> Pegel Berechnung
			<arg name="href3" value="local3"/> SpurM
			<arg name="href4" value="local4"/> SpurU
			<arg name="href5" value="local5"/> SpurO		
		</arg>
   	-->


	<!-- Abfluss_gemessen -->
   	<observation linktype="zml" xlink:href="%href1%">
		<curve id="C1" name="Abfluss - Beobachtet" color="0;0;255" >
			<mapping observation-axis="Datum" diagram-axis="D" />
			<mapping observation-axis="Abfluss" diagram-axis="Q" />
	   	</curve>
	</observation>

	<!-- Abfluss_gerechnet -->
   	<observation linktype="zml" xlink:href="%href2%">
		<curve id="C2" name="Abfluss - Rechnung" color="255;0;0" >
			<mapping observation-axis="Datum" diagram-axis="D" />
			<mapping observation-axis="Abfluss" diagram-axis="Q" />
	   	</curve>
	</observation>
</obsdiagview>
