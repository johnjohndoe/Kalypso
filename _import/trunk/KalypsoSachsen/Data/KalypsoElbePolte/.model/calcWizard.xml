<calcwizard xmlns="xml.model.kalypso.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" browserInitialSize="17" helpId="&hid;SA_Assi_allg_00_Start">
	<!-- Allgemeine Argumente -->
	<arg name="showSaveButton" value="false"/>
	<arg name="askForSaveOnFinish" value="false"/>

	<!-- 
ToDo: Help-Ids setzen!!! -->
	<!-- 
		Phase 1: KONTROLLE DER ZEITREIHEN
	-->
	<!-- Kontrolle Startpegel -->
	<page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ObservationMapTableDiagWizardPage" pageTitle="Kontrolle Wasserstand - Startpegel "
		imageLocation="icons/calcwizard/kalypso32.gif">
		<arg name="pageID" value="Startpegel"/>
		<arg name="htmlFilePath" value=".model/wizardhtml/01_Startpegel.html"/>
		<arg name="helpId" value="&hid;sec_Assi_elbePolte_02_Eingangsdaten"/>
		<!--  -->
		<arg name="mapTemplate" value=".templates/calcCase/wiz01_startpegel.gmt"/>
		<arg name="maximizeMap" value="true"/>
		<arg name="mapWidgetID" value="SELECT"/>
		<!--  -->
		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="50"/>
		<!-- sortierte Pegel-Tabelle  -->
		<arg name="zmlTableShow" value="template"/>
		<arg name="zmlTableAlphaSort" value="false"/>
		<arg name="zmlTableTemplate" value=".templates/calcCase/wiz01_sortierteStartPegel.ott"/>
		<!--  -->
		<arg name="timeserie1" value="nameColumn=name#nameString=%featureprop% [%axisunit%]#linkColumn=ganglinie_gesamt"/>
		<!--  -->
		<arg name="ignoreType1" value="W"/>
	</page>
	<!-- Kontrolle Elbepegel -->
	<page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ObservationMapTableDiagWizardPage" pageTitle="Kontrolle Wasserstand - Elbepegel"
		imageLocation="icons/calcwizard/kalypso32.gif">
		<arg name="pageID" value="Elbepegel"/>
		<arg name="htmlFilePath" value=".model/wizardhtml/02_Elbepegel.html"/>
		<arg name="helpId" value="&hid;sec_Assi_elbePolte_02_Eingangsdaten"/>
		<!--  -->
		<arg name="mapTemplate" value=".templates/calcCase/wiz02_elbepegel.gmt"/>
		<arg name="maximizeMap" value="true"/>
		<!--  -->
		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="50"/>
		<!--  -->
		<arg name="timeserie1" value="nameColumn=name#nameString=%featureprop% [%axisunit%] - Rechnung#linkColumn=ganglinie_modellwerte#color=0xff0000"/>
		<arg name="timeserie2" value="nameColumn=name#nameString=%featureprop% [%axisunit%] - Beobachtung#linkColumn=ganglinie_messwerte#color=0x0000ff"/>
		<arg name="timeserie3" value="nameColumn=name#nameString=nur zur Information: %featureprop% [%axisunit%] - Beobachtung#linkColumn=ganglinie_messwerte_w#color=0xff8040"/>
		<!--  -->
		<arg name="ignoreType1" value="Q"/>
		<arg name="ignoreType2" value="W"/>
		<arg name="ignoreLabel1" value="Wasserstand"/>
		<arg name="ignoreLabel2" value="Abfluss"/>
	</page>
	<!-- Kontrolle Zwischengebiete -->
	<page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ObservationMapTableDiagWizardPage" pageTitle="Kontrolle Abfluss - Zwischengebiete"
		imageLocation="icons/calcwizard/kalypso32.gif">
		<arg name="pageID" value="Zwischengebiete"/>
		<arg name="htmlFilePath" value=".model/wizardhtml/03_Zwischengebiete.html"/>
		<arg name="helpId" value="&hid;sec_Assi_elbePolte_02_Eingangsdaten"/>
		<!--  -->
		<arg name="mapTemplate" value=".templates/calcCase/wiz03_zwischengebiete.gmt"/>
		<arg name="maximizeMap" value="true"/>
		<arg name="mapWidgetID" value="SELECT"/>
		<!--  -->
		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="50"/>
		<!-- sortierte Pegel-Tabelle  -->
		<arg name="zmlTableShow" value="template"/>
		<arg name="zmlTableAlphaSort" value="false"/>
		<arg name="zmlTableTemplate" value=".templates/calcCase/wiz03_sortierteZwischengebietsPegel.ott"/>
		<!--  -->
		<arg name="timeserie1" value="nameColumn=name#nameString=%featureprop% [%axisunit%]#linkColumn=ganglinie_gesamt"/>
		<!--  -->
		<arg name="ignoreType1" value="W"/>
	</page>
	<!-- Modellparameter -->
	<page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.MapAndFeatureWizardPage" pageTitle="Allgemeine Modellparameter" imageLocation="icons/calcwizard/kalypso32.gif">
		<!--  
			HTML-Navigation und pageID-->
		<arg name="pageID" value="Modellparameter"/>
		<arg name="htmlFilePath" value=".model/wizardhtml/04_Modellparameter.html"/>
		<arg name="helpId" value="&hid;sec_Assi_elbePolte_03_ModellParameter"/>
		<!-- 
			Page-Layout -->
		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="30"/>
		<arg name="hideCalcButton" value="true"/>
		<!-- 
			Karte -->
		<arg name="mapTemplate" value=".templates/calcCase/wiz04_modellparameter.gmt"/>
		<arg name="mapTemplateContextIsCalcCase" value="true"/>
		<arg name="maximizeMap" value="true"/>
		
		<!--
			FeatureView -->
		<arg name="featureTemplate" value=".templates/calcCase/wiz04_modellparameter.gft"/>
		<arg name="reselectFeatureview" value="false"/>
		<arg name="selectionFromFeatureview" value="false"/>
		<arg name="featureControlStyle" value="SWT.NONE"/>
		<!-- 
			Diagramm -->
		<arg name="timeserie1" value="nameColumn=name#nameString=%featureprop% [%axisunit%]#linkColumn=ganglinie_gesamt"/>
		<arg name="timeserie2" value="nameColumn=name#nameString=%featureprop% [%axisunit%]#linkColumn=ganglinie_messwerte"/>
		<!--  -->
		<arg name="ignoreType1" value="Q"/>
		<arg name="ignoreType2" value="W"/>
		<arg name="ignoreLabel1" value="Wasserstand"/>
		<arg name="ignoreLabel2" value="Abfluss"/>
	</page>
	<!-- Phase 2: Eingabe streckenbezogene Parameter und Berechnung -->
	<!-- streckenbezogene Parameter -->
	<page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.MapAndTableWizardPage" pageTitle="Streckenparameter und Berechnung" imageLocation="icons/calcwizard/kalypso32.gif">
		<!--  
			HTML-Navigation und pageID-->
		<arg name="pageID" value="Streckenparameter"/>
		<arg name="htmlFilePath" value=".model/wizardhtml/05_Streckenparameter_Berechnung.html"/>
		<arg name="helpId" value="&hid;sec_Assi_elbePolte_04_Vorhersage_Berechnung"/>
		<!-- 
			Page-Layout -->
		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="30"/>
		<arg name="hideCalcButton" value="true"/>
		<!-- 
			Karte -->
		<arg name="mapTemplate" value=".templates/calcCase/wiz05_streckenparameter.gmt"/>
		<arg name="mapTemplateContextIsCalcCase" value="true"/>
		<arg name="maximizeMap" value="true"/>
		<!--
			TableView -->
		<arg name="tableTemplate" value=".templates/calcCase/wiz05_streckenparameter.gtt"/>
		<!-- 
			Diagramm -->
		<arg name="timeserie1" value="nameColumn=name#nameString=Beobachtung und Vorgabe#linkColumn=ganglinie_gesamt#color=0x0000ff"/>
		<arg name="timeserie2" value="nameColumn=name#nameString=Beobachtung#linkColumn=ganglinie_messwerte#color=0x0000ff"/>
		<arg name="timeserie3" value="nameColumn=name#nameString=Vorhersage CZ#linkColumn=ganglinie_vorhersage_cz#color=0xff8040"/>
		<arg name="timeserie4" value="nameString=Rechnung#linkColumn=ganglinie_modellwerte#color=0x008040"/>
		<arg name="timeserie5" value="nameString=untere Umhüllende#linkColumn=ganglinie_modellwerte_unten#lineDash=4f#color=0x008040"/>
		<arg name="timeserie6" value="nameString=obere Umhüllende#linkColumn=ganglinie_modellwerte_oben#lineDash=4f#color=0x008040"/>
		<!--  -->
		<arg name="ignoreType1" value="Q"/>
		<arg name="ignoreType2" value="W"/>
		<arg name="ignoreLabel1" value="Wasserstand"/>
		<arg name="ignoreLabel2" value="Abfluss"/>
		<arg name="buttonText" value="Berechnung durchführen"/>
		<arg name="buttonLaunch" value="runCalculation"/>
		<arg name="buttonTooltip" value="Führt die Modellrechnung durch"/>
		<arg name="buttonProperties"/>
	</page>
	<!-- Phase 3: Ergebnisdarstellung und Ablage -->
	<page className="org.kalypso.simulation.ui.wizards.calculation.modelpages.ExportResultsWizardPage" pageTitle="Ergebnisbearbeitung und Veröffentlichung"
		imageLocation="icons/calcwizard/kalypso32.gif">
		<!--  
			HTML-Navigation und pageID-->
		<arg name="pageID" value="Berichtsablage"/>
		<arg name="htmlFilePath" value=".model/wizardhtml/06_Berichtsablage.html"/>
		<arg name="helpId" value="&hid;sec_Assi_elbePolte_05_Berichtsablage"/>
		<!-- 
			Page-Layout -->
		<arg name="mainSash" value="40"/>
		<arg name="rightSash" value="30"/>
		<!-- 
			Karte -->
		<arg name="mapTemplate" value=".templates/calcCase/wiz06_berichtsablage.gmt"/>
		<arg name="mapTemplateContextIsCalcCase" value="true"/>
		<arg name="maximizeMap" value="true"/>
		<!-- 
			Diagramm -->
		<!--  -->
		<arg name="ignoreType1" value="Q"/>
		<arg name="ignoreType2" value="W"/>
		<arg name="ignoreLabel1" value="Wasserstand"/>
		<arg name="ignoreLabel2" value="Abfluss"/>
		<arg name="timeserie1" value="nameString=Beobachtung#linkColumn=ganglinie_messwerte#color=0x0000ff"/>
		<arg name="timeserie2" value="nameString=Rechnung#linkColumn=ganglinie_modellwerte#color=0x008040"/>
		<arg name="timeserie3" value="nameString=untere Umhüllende#linkColumn=ganglinie_modellwerte_unten#lineDash=4f#color=0x008040"/>
		<arg name="timeserie4" value="nameString=obere Umhüllende#linkColumn=ganglinie_modellwerte_oben#lineDash=4f#color=0x008040"/>
		<!--  -->
		<arg name="pegelNameProperty" value="name"/>
		<arg name="resultProperty" value="ganglinie_modellwerte"/>
		<!-- Exporter fangen hier an... -->
		<arg name="exporter1">
			<arg name="id" value="featureWithTemplateExporter"/>
			<arg name="name" value="Vorhersagepegel"/>
			<arg name="nameproperty" value="Name"/>
			<arg name="width" value="1024"/>
			<arg name="height" value="768"/>
			<arg name="imageFormat" value="png"/>
			<arg name="tokens">
				<arg name="title" value="name"/>
				<arg name="href1" value="ganglinie_messwerte"/>
				<arg name="href3" value="ganglinie_modellwerte"/>
				<arg name="href5" value="ganglinie_modellwerte_unten"/>
				<arg name="href6" value="ganglinie_modellwerte_oben"/>
			</arg>
			<arg name="templateDiagQ">
				<arg name="label" value="Abflussgrafik"/>
				<arg name="href" value="project:/.templates/calcCase/berichtDiagQ.odt"/>
			</arg>
			<arg name="templateDiagW">
				<arg name="label" value="Wasserstandsgrafik"/>
				<arg name="href" value="project:/.templates/calcCase/berichtDiagW.odt"/>
			</arg>
			<arg name="templateTable">
				<arg name="label" value="Tabelle"/>
				<arg name="href" value="project:/.templates/calcCase/berichtTabelle.ott"/>
			</arg>
		</arg>
		<!-- Scheitelwert-Tabellen W / Q -->
		<arg name="exporter3">
			<arg name="id" value="multiExporter"/>
			<arg name="name" value="Scheitelwerte"/>
			<arg name="description" value="Exportiert Vorhersageeergebnisse in Form von Scheitelwerttabellen."/>
			<arg name="exporter3a">
				<arg name="id" value="sumUpTableExporter"/>
				<arg name="name" value="Wasserstand"/>
				<arg name="documentName" value="ScheitelwerteW.csv"/>
				<arg name="separator" value=";"/>
				<!--arg name="charset" value="cp1252"/-->
				<arg name="delta" value="0.0001"/>
				<arg name="dateFormat" value="dd.MM.yyyy HH:mm:ss"/>
				<arg name="column1">
					<arg name="position" value="1"/>
					<arg name="label" value="Pegel"/>
					<arg name="content" value="arg:label"/>
				</arg>
				<arg name="axisType" value="W"/>
				<arg name="timeUnit" value="HOUR"/>
				<arg name="timeStep" value="6"/>
				<!-- Usti hat keine W/Q-Beziehung
				<arg name="obs1">
					<arg name="label" value="Usti"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Usti.zml"/>
				</arg>
				-->
				<arg name="obs2">
					<arg name="label" value="Schöna"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Schöna.zml"/>
				</arg>
				<arg name="obs3">
					<arg name="label" value="Dresden"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Dresden.zml"/>
				</arg>
				<arg name="obs4">
					<arg name="label" value="Riesa"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Riesa.zml"/>
				</arg>
				<arg name="obs5">
					<arg name="label" value="Torgau"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Torgau.zml"/>
				</arg>
				<arg name="obs6">
					<arg name="label" value="Wittenberg"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Wittenberg.zml"/>
				</arg>
			</arg>
			<arg name="exporter4">
				<arg name="id" value="sumUpTableExporter"/>
				<arg name="name" value="Abfluss"/>
				<arg name="documentName" value="ScheitelwerteQ.csv"/>
				<arg name="separator" value=";"/>
				<!--arg name="charset" value="cp1252"/-->
				<arg name="delta" value="0.0001"/>
				<arg name="dateFormat" value="dd.MM.yyyy HH:mm:ss"/>
				<arg name="column1">
					<arg name="position" value="1"/>
					<arg name="label" value="Pegel"/>
					<arg name="content" value="arg:label"/>
				</arg>
				<arg name="axisType" value="Q"/>
				<arg name="timeUnit" value="HOUR"/>
				<arg name="timeStep" value="6"/>
				<arg name="obs1">
					<arg name="label" value="Usti"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Usti.zml"/>
				</arg>
				<arg name="obs2">
					<arg name="label" value="Schöna"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Schöna.zml"/>
				</arg>
				<arg name="obs3">
					<arg name="label" value="Dresden"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Dresden.zml"/>
				</arg>
				<arg name="obs4">
					<arg name="label" value="Riesa"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Riesa.zml"/>
				</arg>
				<arg name="obs5">
					<arg name="label" value="Torgau"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Torgau.zml"/>
				</arg>
				<arg name="obs6">
					<arg name="label" value="Wittenberg"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Wittenberg.zml"/>
				</arg>
			</arg>
		</arg>
		<arg name="exporter5">
			<arg name="id" value="simpleTemplateExporter"/>
			<arg name="name" value="Längsschnitte"/>
			<arg name="templateDiagLS_Q">
				<arg name="documentName" value="LängsschnittQ"/>
				<arg name="label" value="Längsschnitt - Abfluss"/>
				<arg name="category" value="Längsschnitt Pegel;Elbe Abfluss"/>
				<arg name="href" value="project:/.templates/calcCase/berichtLängsschnitt_Q.odt"/>
			</arg>
			<arg name="templateDiagLS_W">
				<arg name="documentName" value="LängsschnittW"/>
				<arg name="label" value="Längsschnitt - Wasserstand"/>
				<arg name="category" value="Längsschnitt Pegel;Elbe Wasserstand"/>
				<arg name="href" value="project:/.templates/calcCase/berichtLängsschnitt_W.odt"/>
			</arg>
		</arg>
	</page>
</calcwizard>
