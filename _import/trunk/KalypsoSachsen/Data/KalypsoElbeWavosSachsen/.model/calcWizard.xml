				<arg name="href" value="project:/.templates/calcCase/berichtTabelle.ott"/>
			</arg>
		</arg>
		<!-- Scheitelwert-Tabellen W / Q -->
		<arg name="exporter3">
			<arg name="id" value="multiExporter"/>
			<arg name="name" value="Scheitelwerte"/>
			<arg name="description" value="Exportiert Vorhersageeergebnisse in Form von Scheitelwerttabellen."/>
			<arg name="exporter3a">
				<arg name="id" value="sumUpTableExporter"/>
				<arg name="name" value="Wasserstand"/>
				<arg name="documentName" value="ScheitelwerteW.csv"/>
				<arg name="separator" value=";"/>
				<!--arg name="charset" value="cp1252"/-->
				<arg name="delta" value="0.0001"/>
				<arg name="dateFormat" value="dd.MM.yyyy HH:mm:ss"/>
				<arg name="column1">
					<arg name="position" value="1"/>
					<arg name="label" value="Pegel"/>
					<arg name="content" value="arg:label"/>
				</arg>
				<arg name="axisType" value="W"/>
				<arg name="timeUnit" value="HOUR"/>
				<arg name="timeStep" value="6"/>
				<!-- Usti hat keine W/Q-Beziehung
				<arg name="obs1">
					<arg name="label" value="Usti"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Usti.zml"/>
				</arg>
				-->
				<arg name="obs2">
					<arg name="label" value="Schöna"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Schöna.zml"/>
				</arg>
				<arg name="obs3">
					<arg name="label" value="Dresden"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Dresden.zml"/>
				</arg>
				<arg name="obs4">
					<arg name="label" value="Riesa"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Riesa.zml"/>
				</arg>
				<arg name="obs5">
					<arg name="label" value="Torgau"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Torgau.zml"/>
				</arg>
				<arg name="obs6">
					<arg name="label" value="Wittenberg"/>
					<arg name="href" value="./Ergebnisse/Zeitreihen/Pegel/Wittenberg.zml"/>
				</arg>
			</arg>
			<arg name="exporter4">
				<arg name="id" value="sumUpTableExporter"/>
