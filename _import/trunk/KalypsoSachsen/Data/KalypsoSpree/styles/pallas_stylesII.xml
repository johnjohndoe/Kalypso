						<Fill>
							<CssParameter name="fill">#666666</CssParameter>
							<CssParameter name="fill-opacity">0.3</CssParameter>
						</Fill>
						<Stroke>
							<CssParameter name="stroke">#000000</CssParameter>
						</Stroke>
					</PolygonSymbolizer>
				</Rule>
			</FeatureTypeStyle>
		</UserStyle>		
	</NamedLayer>
</StyledLayerDescriptor>
