					<MaxScaleDenominator>99999999</MaxScaleDenominator>
					<PolygonSymbolizer>
						<Geometry>
							<ogc:PropertyName>GEOM</ogc:PropertyName>
						</Geometry>
						<Fill>
							<CssParameter name="fill">#009999</CssParameter>
							<CssParameter name="fill-opacity">0.3</CssParameter>
						</Fill>
						<Stroke>
							<CssParameter name="stroke">#006666</CssParameter>
						</Stroke>
					</PolygonSymbolizer>
				</Rule>
			</FeatureTypeStyle>
		</UserStyle>
		<UserStyle>
			<Name>default:islands</Name>
			<Title>unigis:islands</Title>
			<IsDefault>1</IsDefault>
			<FeatureTypeStyle>
				<Name>unigis:islands</Name>
				<Rule>
					<Name>unigis:islands</Name>
					<MinScaleDenominator>0</MinScaleDenominator>
					<MaxScaleDenominator>99999999</MaxScaleDenominator>
					<PolygonSymbolizer>
						<Geometry>
							<ogc:PropertyName>GEOM</ogc:PropertyName>
						</Geometry>
						<Fill>
							<CssParameter name="fill">#999900</CssParameter>
							<CssParameter name="fill-opacity">0.3</CssParameter>
						</Fill>
						<Stroke>
							<CssParameter name="stroke">#666600</CssParameter>
						</Stroke>
					</PolygonSymbolizer>
				</Rule>
