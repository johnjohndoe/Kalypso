<schema targetNamespace="http://www.tuhh.de/kalypsoSpree" version="0.9" xmlns:na="http://www.tuhh.de/kalypsoSpree" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gml="http://www.opengis.net/gml" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:obs="zml.kalypso.org">
	<import namespace="http://www.opengis.net/gml" schemaLocation="feature.xsd"/> 
	<import namespace="zml.kalypso.org" schemaLocation="observation.xsd"/> 
<!--
