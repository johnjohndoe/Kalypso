<?xml version="1.0" encoding="UTF-8"?>
<transformationConfig xmlns="xml.model.kalypso.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<transformation className="org.kalypso.model.transformation.CalculationTransformation">
		<arg name="output" value="calcdir:.calculation"/>
		<arg name="entry0" value="startTime=#SYSTEM_TIME#"/>
	</transformation>

	<transformation className="org.kalypso.model.transformation.CopyTransformation">
		<arg name="input" value="project:modell.gml"/>
		<arg name="output" value="calcdir:calcCase.gml"/>
	</transformation>

<!-- Einzugsgebiete -->
	<transformation className="org.kalypso.model.transformation.ObservationResolver">
		<arg name="project" value="project:"/>
		<arg name="type" value="gml"/>
		<arg name="href" value="PATH=calcdir:calcCase.gml#LAYER=Einzugsgebiet#SERVICE=RESOURCE#XSD=project:.modellTyp/modell.xsd"/>
		<arg name="sourceObservation" value="Niederschlag_eingabeEingang"/>
		<arg name="targetObservation" value="Niederschlag_eingabe"/>
		<arg name="targetFolder" value="calcdir:zeitreihen"/>
	</transformation>

	<transformation className="org.kalypso.model.transformation.ObservationResolver">
		<arg name="project" value="project:"/>
		<arg name="type" value="gml"/>
		<arg name="href" value="PATH=calcdir:calcCase.gml#LAYER=Einzugsgebiet#SERVICE=RESOURCE#XSD=project:.modellTyp/modell.xsd"/>
		<arg name="sourceObservation" value="Wasserstand_gemessenEingang"/>
		<arg name="targetObservation" value="Wasserstand_gemessen"/>
		<arg name="targetFolder" value="calcdir:zeitreihen"/>
	</transformation>

<!-- Flusslaufmodelle -->
	<transformation className="org.kalypso.model.transformation.ObservationResolver">
		<arg name="project" value="project:"/>
		<arg name="type" value="gml"/>
		<arg name="href" value="PATH=calcdir:calcCase.gml#LAYER=Flusslaufmodell#SERVICE=RESOURCE#XSD=project:.modellTyp/modell.xsd"/>
		<arg name="sourceObservation" value="Wasserstand_gemessenEingang"/>
		<arg name="targetObservation" value="Wasserstand_gemessen"/>
		<arg name="targetFolder" value="calcdir:zeitreihen"/>
	</transformation>

<!-- Talsperre -->
	<transformation className="org.kalypso.model.transformation.ObservationResolver">
		<arg name="project" value="project:"/>
		<arg name="type" value="gml"/>
		<arg name="href" value="PATH=calcdir:calcCase.gml#LAYER=Talsperre#SERVICE=RESOURCE#XSD=project:.modellTyp/modell.xsd"/>
		<arg name="sourceObservation" value="Abgabe_eingang"/>
		<arg name="targetObservation" value="Abgabe"/>
		<arg name="targetFolder" value="calcdir:zeitreihen"/>
	</transformation>

<!-- Flutung -->
	<transformation className="org.kalypso.model.transformation.ObservationResolver">
		<arg name="project" value="project:"/>
		<arg name="type" value="gml"/>
		<arg name="href" value="PATH=calcdir:calcCase.gml#LAYER=Flutung#SERVICE=RESOURCE#XSD=project:.modellTyp/modell.xsd"/>
		<arg name="sourceObservation" value="Abgabe_eingang"/>
		<arg name="targetObservation" value="Abgabe"/>
		<arg name="targetFolder" value="calcdir:zeitreihen"/>
	</transformation>

	<transformation className="org.kalypso.model.transformation.ObservationResolver">
		<arg name="project" value="project:"/>
		<arg name="type" value="gml"/>
		<arg name="href" value="PATH=calcdir:calcCase.gml#LAYER=Zufluss#SERVICE=RESOURCE#XSD=project:.modellTyp/modell.xsd"/>
		<arg name="sourceObservation" value="Zugabe_eingang"/>
		<arg name="targetObservation" value="Zugabe"/>
		<arg name="targetFolder" value="calcdir:zeitreihen"/>
	</transformation>
	
	<transformation className="org.kalypso.model.transformation.CopyTransformation">
		<arg name="input" value="project:.examples/results.gml"/>
		<arg name="output" value="calcdir:results.gml"/>
	</transformation>
	

	<transformation className="org.kalypso.model.transformation.ReplaceTransformation">
		<arg name="protocol0" value="calcdir=calcdir:"/>
		<arg name="protocol1" value="localcalc=localcalc:"/>
		<arg name="file0" value="calcdir:calcCase.gml"/>
	</transformation>

</transformationConfig>