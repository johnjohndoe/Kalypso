<wsdl:definitions name="Gaja3dResourceFactory" targetNamespace="http://org.kalypso.gaja3d.service/factory"
	xmlns:wsrp="http://docs.oasis-open.org/wsrf/rp-2" xmlns:tns="http://org.kalypso.gaja3d.service/factory"
	xmlns:wsrpw="http://docs.oasis-open.org/wsrf/rpw-2" xmlns:wsrlw="http://docs.oasis-open.org/wsrf/rlw-2"
	xmlns:wsdlpp="http://www.globus.org/namespaces/2004/10/WSDLPreprocessor"
	xmlns:wsa="http://www.w3.org/2005/08/addressing" xmlns:wsaw="http://www.w3.org/2006/05/addressing/wsdl"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:wsntw="http://docs.oasis-open.org/wsn/bw-2"
	xmlns:gtwsdl0="http://docs.oasis-open.org/wsrf/rw-2" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns="http://schemas.xmlsoap.org/wsdl/">
	<wsdl:import namespace="http://docs.oasis-open.org/wsn/bw-2"
		location="../wsrf/notification/bw-2.wsdl" />
	<wsdl:import namespace="http://docs.oasis-open.org/wsrf/rpw-2"
		location="../wsrf/properties/rpw-2.wsdl" />
	<wsdl:import namespace="http://docs.oasis-open.org/wsrf/rlw-2"
		location="../wsrf/lifetime/rlw-2.wsdl" />
	<wsdl:import namespace="http://docs.oasis-open.org/wsrf/rw-2"
		location="../wsrf/resource/rw-2.wsdl" />


<!--============================================================

                      T Y P E S
                      
  ============================================================-->
<wsdl:types>
<xsd:schema targetNamespace="http://org.kalypso.gaja3d.service/factory"
    xmlns:tns="http://org.kalypso.gaja3d.service/factory"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<xsd:import 
	    namespace="http://www.w3.org/2005/08/addressing"
	    schemaLocation="../ws/addressing/ws-addr.xsd" />


	<!-- REQUESTS AND RESPONSES -->

	<xsd:element name="createResource">
		<xsd:complexType/>
	</xsd:element>
	<xsd:element name="createResourceResponse">
	<xsd:complexType>
		<xsd:sequence>
			<xsd:element ref="wsa:EndpointReference"/>
		</xsd:sequence>
	</xsd:complexType>
	</xsd:element>
        
</xsd:schema>
</wsdl:types>


<!--============================================================

                       M E S S A G E S
                      
  ============================================================-->
<wsdl:message name="CreateResourceRequest">
	<wsdl:part name="request" element="tns:createResource"/>
</wsdl:message>
<wsdl:message name="CreateResourceResponse">
	<wsdl:part name="response" element="tns:createResourceResponse"/>
</wsdl:message>



<!--============================================================

                       P O R T T Y P E
                      
  ============================================================-->
<wsdl:portType name="Gaja3dResourceFactoryPortType">

	<wsdl:operation name="createResource">
		<wsdl:input message="tns:CreateResourceRequest"/>
		<wsdl:output message="tns:CreateResourceResponse"/>
	</wsdl:operation>

</wsdl:portType>

</wsdl:definitions>
