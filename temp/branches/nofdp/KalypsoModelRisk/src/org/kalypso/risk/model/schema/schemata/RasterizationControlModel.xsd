        </xs:element>
        <xs:complexType name="DamageFunctionPropertyType">
                <xs:sequence>
                        <xs:element ref="krrcm:DamageFunction" minOccurs="0"/>
                </xs:sequence>
                <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
        </xs:complexType>
