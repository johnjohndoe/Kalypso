                        </xs:documentation>
                        <xs:documentation xml:lang="en">
                                <xs:label><![CDATA[${listProperty:http://www.opengis.net/gml#name;0;-no name-}]]></xs:label>
                        </xs:documentation>
                        <xs:documentation xml:lang="nl">
                                <xs:label><![CDATA[${listProperty:http://www.opengis.net/gml#name;0;-geen naam-}]]></xs:label>
                        </xs:documentation>
                </xs:annotation>
        </xs:element>
        <!-- =================================================================
		Statistic type
		================================================================== -->
        <xs:complexType name="StatisticClassType">
                <xs:complexContent>
                        <xs:extension base="gml:AbstractFeatureType">
                                <xs:sequence>
                                        <xs:element name="statReturnPeriod" type="xs:int">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Wiederkehrintervall [a] </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Return Period [a] </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="statMinDamage" type="xs:decimal" default="0.0">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Min. spez. Schaden an einer einzelnen Rasterzelle [€/m²] </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Min. Spec. Damage at a Single Raster Cell [€/m²] </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="statMaxDamage" type="xs:decimal" default="0.0">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Max. spez. Schaden an einer einzelnen Rasterzelle [€/m²] </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Max. Spec. Damage at a Single Raster Cell [€/m²] </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="statAverageDamage" type="xs:decimal" default="0.0">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Mittlerer spez. Schaden für eine einzelne Rasterzelle [€/m²] </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Average Spec. Damage for a Single Raster Cell [€/m²] </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="statAverageAnnualDamage" type="xs:decimal" default="0.0">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Mittlerer jährlicher Schadenserwartungswert [€/m²/a] </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Average Annual Damage [€/m²/a] </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="statSumDamage" type="xs:decimal" default="0.0">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Summe der spezifischen Schäden [€/m²] </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Summation of the Spec. Damages [€/m²] </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="statTotalDamage" type="xs:decimal" default="0.0">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Gesamtschaden [€] </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Total Damage [€] </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="statFloodedArea" type="xs:decimal" default="0.0">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> betroffene Fläche [m²] </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Flooded Area [m²] </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                </xs:sequence>
                                <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
                        </xs:extension>
                </xs:complexContent>
        </xs:complexType>
        <!-- =================================================================
		assetValueClassMember property
		================================================================== -->
        <xs:element name="assetValueClassMember" type="krrcm:AssetValueClassPropertyType">
                <xs:annotation>
                        <xs:documentation xml:lang="de"> spez. Vermögenswerte </xs:documentation>
