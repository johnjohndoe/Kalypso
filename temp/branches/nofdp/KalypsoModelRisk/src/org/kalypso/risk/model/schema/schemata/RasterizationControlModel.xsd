		Asset value classes
		================================================================== -->
        <xs:element name="AssetValueClass" type="krrcm:AssetValueClassType" substitutionGroup="gml:_Feature">
                <xs:annotation>
                        <xs:documentation xml:lang="de"> spez. Vermögenswert;<![CDATA[${property:http://www.tu-harburg.de/wb/kalypso/risk/schemata/rasterizationControlModel#name;- kein Name -}]]>
                        </xs:documentation>
                        <xs:documentation xml:lang="en"> Asset Value;<![CDATA[${property:http://www.tu-harburg.de/wb/kalypso/risk/schemata/rasterizationControlModel#name;- no name-}]]>
                        </xs:documentation>
                </xs:annotation>
        </xs:element>
        <xs:complexType name="AssetValueClassType">
                <xs:annotation>
                        <xs:appinfo xmlns:kapp="org.kalypso.appinfo">
                                <kapp:functionProperty kapp:functionId="org.kalypso.risk.model.schema.propertyFunctions.GetNameFromInscription" kapp:property="krrcm:name">
                                        <kapp:parameter>
                                                <kapp:name>shallOverwrite</kapp:name>
                                                <kapp:value>true</kapp:value>
                                        </kapp:parameter>
                                </kapp:functionProperty>
                        </xs:appinfo>
                </xs:annotation>
                <xs:complexContent>
                        <xs:extension base="kr_common:EmptyFeatureBaseType">
                                <xs:sequence>
                                        <xs:element name="name" type="xs:string" default="---">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Vermögenswert-Klasse </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Asset Value Class </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="description" type="xs:string"/>
                                        <xs:element name="administrationUnitLink" type="krrcm:AdministrationUnitLinkType" nillable="true">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de">
                                                                <xs:label>Verwaltungseinheit</xs:label>
                                                        </xs:documentation>
                                                        <xs:documentation xml:lang="en">
                                                                <xs:label>Administration unit</xs:label>
                                                        </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="value" type="xs:double" default="0.0">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> spezifischer Vermögenswert [€/m²] </xs:documentation>
                                                        <xs:documentation xml:lang="en">
                                                                <xs:label>Asset Value [€/m²]</xs:label>
                                                        </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element ref="kr_common:inscriptionMember" minOccurs="0" maxOccurs="unbounded"/>
                                </xs:sequence>
                                <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
                        </xs:extension>
                </xs:complexContent>
        </xs:complexType>
        <!-- =================================================================
		damageFunctionMember (root property)
		================================================================== -->
        <xs:element name="damageFunctionMember" type="krrcm:DamageFunctionPropertyType">
