                                        <xs:element ref="krrcm:landuseRiskTypeMember" minOccurs="0" maxOccurs="unbounded"/>
                                        <xs:element ref="krrcm:riskZoneDefinitionMember" minOccurs="0" maxOccurs="unbounded"/>
