                        <xs:extension base="kr_common:EmptyFeatureBaseType">
                                <xs:sequence>
                                        <xs:element name="name" type="xs:string">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de">Landnutzungsklasse </xs:documentation>
                                                        <xs:documentation xml:lang="en">Land-Use Class </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="description" type="xs:string">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de">Beschreibung </xs:documentation>
                                                        <xs:documentation xml:lang="en">Description </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="ordinalNumber" type="xs:int"/>
                                        <xs:element name="damageFunctionLink" type="krrcm:DamageFunctionLinkType" nillable="true">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de">Schadensfunktionsname;Liefert für den jeweiligen Nutzungstyp den Schädigungsgrad (Vulnerabilität) in Prozent in Abhängigkeit der Einstauhöhe. </xs:documentation>
                                                        <xs:documentation xml:lang="en">Name of Damage Function </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="landuseRiskTypeLink" type="krrcm:LanduseRiskTypeLinkType" nillable="true">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de">Kategorie;Für Einteilung in die Risikozonen </xs:documentation>
                                                        <xs:documentation xml:lang="en">Category </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="AssetValueClassLink" type="krrcm:AssetValueClassLinkType" nillable="true">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Vermögenswert-Klasse </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Asset Value Class </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="colorStyle" type="common:color">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Farbdarstellung;Farbdarstellung der Landnutzungsklasse in der Karte </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Color Style;Color Style of Land-Use Class in the Map </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element ref="krrcm:statisticClassMember" minOccurs="0" maxOccurs="unbounded">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Statistische Daten </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Statistic Data </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="minDamage" type="xs:double" default="0.0">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Min. mittlerer Schaden an einer einzelnen Rasterzelle [€/m²/a] </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Min. Avg. Damage at a Single Raster Cell [€/m²/a] </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="maxDamage" type="xs:double" default="0.0">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Max. mittlerer Schaden an einer einzelnen Rasterzelle [€/m²/a] </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Max. Avg. Damage at a Single Raster Cell [€/m²/a] </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="averageAnnualDamage" type="xs:double" default="0.0">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Mittlerer jährlicher Schadenserwartungswert [€/m²/a] </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Average Annual Damage [€/m²/a] </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element name="totalDamage" type="xs:double" default="0.0">
                                                <xs:annotation>
                                                        <xs:documentation xml:lang="de"> Gesamtschaden [€/a] </xs:documentation>
                                                        <xs:documentation xml:lang="en"> Total Damage [€/a] </xs:documentation>
                                                </xs:annotation>
                                        </xs:element>
                                        <xs:element ref="kr_common:inscriptionMember" minOccurs="0" maxOccurs="unbounded"/>
                                </xs:sequence>
                                <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
                        </xs:extension>
                </xs:complexContent>
        </xs:complexType>
