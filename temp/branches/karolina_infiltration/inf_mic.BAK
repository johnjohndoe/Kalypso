   write (nres,*)'bof_extra',bof_extra
   write (nres,*)'diff_percc(i,ilay,it)',diff_percc(i,ilay,it)
           write (nres,*)'m_retlay(ilay,nb)',m_retlay(ilay,nb)
              write (nres,*)'Imic_reall',Imic_reall
