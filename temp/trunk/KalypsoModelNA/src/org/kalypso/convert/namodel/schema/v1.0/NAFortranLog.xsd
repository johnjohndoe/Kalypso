<schema xmlns:naLog="http://www.tuhh.de/NAFortranLog" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gml="http://www.opengis.net/gml" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:obslink="obslink.zml.kalypso.org" xmlns:zmlinline="inline.zml.kalypso.org" targetNamespace="http://www.tuhh.de/NAFortranLog" elementFormDefault="qualified" version="1.0">
	<annotation>
		<appinfo xmlns:kapp="org.kalypso.appinfo">
			<kapp:gmlVersion>2.1.2</kapp:gmlVersion>
		</appinfo>
	</annotation>
	<import namespace="http://www.opengis.net/gml"/>
	<element name="LogMessage" type="naLog:LogType"/>
	<complexType name="LogType">
		<complexContent>
			<extension base="naLog:NonGeometricAbstractFeatureType">
				<sequence>
					<element name="log" type="naLog:RecordAssociationType" minOccurs="0" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="NonGeometricAbstractFeatureType" abstract="true">
		<complexContent>
			<restriction base="gml:AbstractFeatureType"/>
		</complexContent>
	</complexType>
	<complexType name="RecordAssociationType">
		<complexContent>
			<restriction base="gml:FeatureAssociationType">
				<sequence>
					<element ref="naLog:record"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<element name="record" type="naLog:recordType" substitutionGroup="gml:_Feature"/>
	<complexType name="recordType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element name="level" type="string">
						<annotation>
							<documentation xml:lang="de">
								<label>Level</label>
								<tooltip>Einstufung der Log Nachricht</tooltip>
								<description>Einstufung der Log Nachricht</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Level</label>
								<tooltip>Level/ Importance of the log message.</tooltip>
								<description>Level/ Importance of the log message.</description>
							</documentation>
						</annotation>
					</element>
					<element name="class" type="string"/>
					<element name="method" type="string"/>
					<element name="message" type="string">
						<annotation>
							<documentation xml:lang="de">
								<label>Nachricht</label>
								<tooltip>Text der Log Nachricht.</tooltip>
								<description>Text der Log Nachricht.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Message</label>
								<tooltip>Text of the log message.</tooltip>
								<description>Text of the log message.</description>
							</documentation>
						</annotation>
					</element>
					<element name="element" type="string">
						<annotation>
							<documentation xml:lang="de">
								<label>Element</label>
								<tooltip>Element, welches die Nachricht verursacht.</tooltip>
								<description>Element, welches die Nachricht verursacht.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Element</label>
								<tooltip>Element, which causes the message.</tooltip>
								<description>Element, which causes the message.</description>
							</documentation>
						</annotation>
					</element>
					<element name="param" type="string">
						<annotation>
							<documentation xml:lang="de">
								<label>Parameter</label>
								<tooltip>Parameter des Elementes, welcher die Nachricht verursacht.</tooltip>
								<description>Parameter des Elementes, welcher die Nachricht verursacht.</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Parameter</label>
								<tooltip>Parameter of the Element, which causes the message.</tooltip>
								<description>Parameter of the Element, which causes the message.</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
</schema>
