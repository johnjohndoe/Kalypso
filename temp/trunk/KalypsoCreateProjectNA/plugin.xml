<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.wizard"
   name="KalypsoCreateNAProject"
   version="0.0.1"
   provider-name="kalypso.org"
   class="org.kalypso.KalypsoNewNAProjectPlugin">

   <runtime>
      <library name="KalypsoCreateNAProject.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.kalypso.ui" export="true"/>
      <import plugin="org.kalypso.jwsdp"/>
   </requires>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="NA-Modell Projekt"
            id="org.kalypso.wizard"/>
      <wizard
            class="org.kalypso.wizard.KalypsoNAProjectWizard"
            icon="icons/kalypso16.gif"
            category="org.kalypso"
            name="%NewWizard.name"
            id="org.kalypso.wizard.KalypsoNAProjectWizard"/>
   </extension>

</plugin>
