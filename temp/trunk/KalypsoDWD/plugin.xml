<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.dwd"
   name="KalypsoDWD"
   version="1.0.0"
   provider-name="kalypso.org">
   <runtime>
      <library name="lib/dwdBinding.jar">
         <export name="*"/>
      </library>
      <library name="lib/servlet-api.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.kalypso.deegree" export="true"/>
      <import plugin="org.kalypso.commons" export="true"/>
      <import plugin="org.kalypso.jwsdp" export="true"/>
      <import plugin="org.kalypso.core"/>
      <import plugin="org.eclipse.ant.core"/>
   </requires>
   <extension
         point="org.eclipse.ant.core.antTasks">
      <antTask
            class="org.kalypso.dwd.DWDTask"
            library="lib/KalypsoDWDTask.jar"
            name="kalypso.dwdTask"/>
   </extension>

</plugin>
