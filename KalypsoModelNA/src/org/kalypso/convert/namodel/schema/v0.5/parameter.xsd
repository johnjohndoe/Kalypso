	<!--___VegetationCollection_________________________________________________-->
	<complexType name="IdealLandUseAssociationType">
		<complexContent>
			<restriction base="gml:FeatureAssociationType">
				<sequence>
					<element ref="par:IdealLandUse"/>
				</sequence>
			</restriction>
		</complexContent>
	</complexType>
	<element name="IdealLandUse" type="par:idealLandUseType" substitutionGroup="gml:_Feature"/>
	<complexType name="idealLandUseType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element name="idealLandUseZML" type="zmlinline:ZmlInlineIdealKcWtLaiType">
						<annotation>
							<documentation xml:lang="de">
								<label>Nutzungsparameter</label>
								<tooltip>Idealisierte Jahresvegetationsperiode der Nutungsparameter </tooltip>
								<description>Idealisierte Jahresvegetationsperiode mit kc=Verdunstungskorrekturfaktor gegenueber der pot. Verdunstung, wt=Wurzeltiefe und lai=Interzeptionsspeicherinhalt fuer ein Hydrologisches Jahr</description>
							</documentation>
							<documentation xml:lang="en">
								<label>Landuse parameter</label>
								<tooltip>idealiszed annual vegetation cycle for the landuse parameter set</tooltip>
								<description>idealiszed annual vegetation cycle for the landuse parameter set with kc=correction factor for evaporation vs. potential evaporation, wt=root depth of vegetation and lai=volume of interzeption storage (leaf area index) for a hydrological year</description>
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!--___SealingClassCollection_________________________________________________-->
