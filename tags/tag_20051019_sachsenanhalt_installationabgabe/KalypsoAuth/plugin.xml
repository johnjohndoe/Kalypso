<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.auth"
   name="KalypsoAuth Plug-in"
   version="1.0.0"
   provider-name="kalypso.org"
   class="org.kalypso.auth.KalypsoAuthPlugin">

   <runtime>
      <library name="KalypsoAuth.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.kalypso.commons"/>
   </requires>
   <extension-point id="authenticator" name="Kalypso Authenticator" schema="schema/authenticator.exsd"/>

</plugin>
