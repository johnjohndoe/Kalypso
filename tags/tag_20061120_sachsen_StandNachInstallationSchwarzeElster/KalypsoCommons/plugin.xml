<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.commons"
   name="KalypsoCommons Plug-in"
   version="0.0.1"
   provider-name="kalypso.org"
   class="org.kalypso.commons.KalypsoCommonsPlugin">

   <runtime>
      <library name="KalypsoCommons.jar">
         <export name="*"/>
      </library>
      <library name="lib/commons-io-1.0.jar">
         <export name="*"/>
      </library>
      <library name="lib/commons-lang-2.0.jar">
         <export name="*"/>
      </library>
      <library name="lib/hb15.jar">
         <export name="*"/>
      </library>
      <library name="lib/commons-logging.jar">
         <export name="*"/>
      </library>
      <library name="lib/commons-httpclient-2.0.2.jar">
         <export name="*"/>
      </library>
      <library name="lib/commons-vfs-1.0-RC8-SELFBUILD.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.junit"/>
      <import plugin="org.apache.ant" export="true"/>
      <import plugin="org.kalypso.contribs.java" export="true"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.kalypso.contribs.eclipseplatform" export="true"/>
   </requires>

</plugin>
