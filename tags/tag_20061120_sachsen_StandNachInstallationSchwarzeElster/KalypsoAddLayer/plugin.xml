<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.ui.addlayer"
   name="KaylpsoAddLayer"
   version="0.0.1"
   provider-name="kalypso.org"
   class="org.kaylpso.ui.KalypsoAddLayerPlugin">

   <runtime>
      <library name="KaylpsoAddLayerPlugin.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.kalypso.ui" export="true"/>
      <import plugin="org.kalypso.jwsdp"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.kalypso.contribs.java"/>
      <import plugin="org.kalypso.contribs.eclipseplatform"/>
   </requires>
   <extension
         id="mapviewaction"
         name="mapviewaction"
         point="org.kalypso.ui.mapviewaction">
      <MapviewAction
            title="AddLayer"
            enabled="true"
            icon="icon/add_row.gif"
            class="org.kaylpso.ui.view.action.AddThemeAction"
            tooltip="Thema zu Karte hinzufÃ¼gen"/>
   </extension>
   <extension
         id="dataimport"
         name="dataimport"
         point="org.kalypso.ui.dataimportwizard">
      <wizard
            icon="icon/file_shp.GIF"
            class="org.kaylpso.ui.wizard.shape.ImportShapeSourceWizard"
            name="%org.kalypso.ui.wizard.shape.name"
            id="org.kalypso.ui.wizard.shape"/>
      <wizard
            icon="icon/Datastore_WMS.gif"
            class="org.kaylpso.ui.wizard.wms.ImportWmsSourceWizard"
            name="%org.kalypso.ui.wizard.wms.name"
            id="org.kalypso.ui.wizard.wms"/>
      <wizard
            icon="icon/Datastore_WFS.gif"
            class="org.kaylpso.ui.wizard.wfs.ImportWfsSourceWizard"
            name="%org.kalypso.ui.wizard.wfs.name"
            id="org.kalypso.ui.wizard.wfs"/>
      <wizard
            icon="icon/fileGml.gif"
            class="org.kaylpso.ui.wizard.gml.KalypsoGmlImportWizard"
            name="%gml.name"
            id="gml"/>
      <wizard
            icon="icon/raster.GIF"
            class="org.kaylpso.ui.wizard.raster.ImportRasterSourceWizard"
            name="%org.kalypso.ui.wizard.raster.name"
            id="org.kalypso.ui.wizard.raster"/>
      <wizard
            icon="icon/file_image.gif"
            class="org.kaylpso.ui.wizard.image.ImportImageSourceWizard"
            name="%org.kalypso.ui.wizard.image.name"
            id="org.kalypso.ui.wizard.image"/>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            class="org.kalypso.ui.createGisMapView.CreateGisMapViewWizard"
            icon="icon/kalypso_gmt.gif"
            name="%org.kalypso.ui.createGisMapView.CreateGisMapViewWizard.name"
            id="org.kalypso.ui.createGisMapView.CreateGisMapViewWizard"/>
   </extension>

</plugin>
