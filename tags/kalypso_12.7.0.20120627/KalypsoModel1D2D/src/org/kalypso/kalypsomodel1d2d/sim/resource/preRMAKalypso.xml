<?xml version="1.0" encoding="UTF-8"?>
<modelspec xmlns="simspec.core.simulation.kalypso.org"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	typeID="org.kalypso.simulation.rma.preRMAKalypso">

	<input id="control" type="xs:anyURI" description="control gml-file" />
	<input id="mesh" type="xs:anyURI" description="mesh gml-file" />
	<input id="flowRelationships" type="xs:anyURI" description="flow relationships gml-file" />
	<input id="roughness" type="xs:anyURI" description="roughness model gml-file" />
	<input id="wind" type="xs:anyURI" description="wind relationships gml-file"
		optional="true" />
	<input id="restartFile0" type="xs:anyURI" description="restart results file 0"
		optional="true" />
	<input id="restartFile1" type="xs:anyURI" description="restart results file 1"
		optional="true" />
	<input id="restartFile2" type="xs:anyURI" description="restart results file 2"
		optional="true" />
	<input id="calculationUnitID" type="xs:string"
		description="ID of the (sub)-calculation unit to export" optional="true" />

	<output id="rmaVersion" type="xs:string" description="RMA-Kalypso version string" />
	<output id="control.r10" type="xs:anyURI" description="control.r10" />
	<output id="model.2d" type="xs:anyURI" description="model.2d" />
	<output id="bauwerke.txt" type="xs:anyURI" description="bauwerke.txt" />
	<output id="wind.txt" type="xs:anyURI" description="wind.txt" />
	<output id="windCoord.txt" type="xs:anyURI" description="windCoord.txt" />
	<output id="wqusw.txt" type="xs:anyURI" description="wqusw.txt" />

</modelspec>