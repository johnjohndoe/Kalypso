<!-- XXXXXXX-->
	<element name="Nummer" type="integer"/>
<!-- XXXXXXX-->
	<element name="Landnutzung" type="na:_LandnutzungType"/>
	<simpleType name="_LandnutzungType">
	<restriction base="string">
		<enumeration value="acker"/>
			<enumeration value="wald"/>
		
		</restriction>

	</simpleType>
	<element name="Name" type="string"/>
	<element name="maxQ" type="double"/>
	<element name="minQ" type="double"/>
	<element name="Ausdehnung" type="gml:PolygonPropertyType" substitutionGroup="gml:polygonProperty"/>
	<element name="Verlauf" type="gml:LineStringPropertyType" substitutionGroup="gml:lineStringProperty"/>
	<element name="Position" type="gml:PointPropertyType" substitutionGroup="gml:pointProperty"/>
	<element name="Messung" type="string"/>
	<!--<element name="Messung" type="na:_PsiLinkType"/>-->
	<complexType name="_NAModellType">
		<complexContent>
			<extension base="gml:AbstractFeatureCollectionType"/>
		</complexContent>
	</complexType>
	<!-- Einzugsgebiet -->
	<element name="Einzugsgebiet" type="na:_EinzugsgebietType" substitutionGroup="gml:_Feature"/>
	<complexType name="_EinzugsgebietType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element ref="na:Nummer"/>
					<element ref="na:Landnutzung"/>
					<element ref="na:Ausdehnung"/>
					<!--
					<element ref="na:entwaessertZuKnoten"/>
-->				
</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!--  -->
<!--	
	<element name="entwaessertZuKnoten" type="na:_entwaessertZuKnotenType"/>
	<complexType name="_entwaessertZuKnotenType">
		<attributeGroup ref="xlink:simpleLink"/>
	</complexType>
-->
	<!-- Pegel -->
	<element name="Pegel" type="na:_PegelType" substitutionGroup="gml:_Feature"/>
	<complexType name="_PegelType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element ref="na:Position"/>
					<element ref="na:Nummer"/>
					<element ref="na:Messung"/>
					<element ref="na:Einzugsgebiet"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	
<!--	<complexType name="_PsiLinkType">
		<attributeGroup ref="xlink:simpleLink"/>

	</complexType>
-->	
<!-- Knoten -->
	<element name="Knoten" type="na:_KnotenType" substitutionGroup="gml:_Feature"/>
	<complexType name="_KnotenType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element ref="na:Position"/>
					<element ref="na:Nummer"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- Flussabschnitt -->
	<element name="Flussabschnitt" type="na:_FlussabschnittType" substitutionGroup="gml:_Feature"/>
	<complexType name="_FlussabschnittType">
		<complexContent>
			<extension base="gml:AbstractFeatureType">
				<sequence>
					<element ref="na:Name"/>
					<element ref="na:Verlauf"/>
					<element ref="na:Nummer"/>
					<element ref="na:maxQ"/>
					<element ref="na:minQ"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
