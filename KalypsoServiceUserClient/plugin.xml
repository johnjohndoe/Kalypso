<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.kalypso.services.user.client"
   name="KalypsoClientUser Plug-in"
   version="1.0.0"
   provider-name="kalypso.org"
   class="org.kalypso.services.user.client.KalypsoServiceUserClientPlugin">

   <runtime>
      <library name="KalypsoClientUser.jar">
         <export name="*"/>
      </library>
      <library name="lib/kalypsoservice_user.jar">
         <export name="*"/>
      </library>
      <library name="lib/KalypsoServiceCore.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.kalypso.jwsdp"/>
      <import plugin="org.kalypso.contribs.java"/>
      <import plugin="org.kalypso.auth"/>
      <import plugin="org.kalypso.core.client"/>
   </requires>
   <extension
         id="org.kalypso.auth.authenticator"
         name="Kalypso User Service Authenticator"
         point="org.kalypso.auth.authenticator">
      <authenticator class="org.kalypso.services.user.client.UserServiceAuthenticator"/>
   </extension>
   <extension
         point="org.kalypso.core.client.serviceproxy">
      <serviceproxy
            class="org.kalypso.services.proxy.Kalypso_UserService_Impl"
            interfacename="IUserService"/>
   </extension>

</plugin>
